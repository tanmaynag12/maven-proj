/*
class BreadcrumbComponent extends React.Component {
	  constructor(props) {
	    super(props);
	    this.state = {
	    	breadcrumb:document.title,
	    	time:"",
	    	timeInMillis:this.props.timestamp,
	    	startMarkCls:'fa fa-star-o-menu',
