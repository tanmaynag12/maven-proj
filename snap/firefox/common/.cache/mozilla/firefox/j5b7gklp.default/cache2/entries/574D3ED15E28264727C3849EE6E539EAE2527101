	if(TempSubjectwiseBatchId!=undefined){
		subjectwiseBatchId = TempSubjectwiseBatchId;
	}
	var allowForAll = false;
	if($('#allowForAllShortCutVal')!=undefined && $('#allowForAllShortCutVal').val()=='true'){
		allowForAll = true;
	}
	var hdnUserRole = $('#hdnUserRole').val();
