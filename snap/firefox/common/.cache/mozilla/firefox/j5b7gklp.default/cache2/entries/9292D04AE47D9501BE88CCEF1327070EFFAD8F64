	//<iframe src="" id = "GEMSCommonIframe" width="100%" height="100%" frameborder="0"></iframe>
	
	$('#divGEMSHomeBody').html('<iframe src="" id = "GEMSCommonIframe" width="100%" height="100%" frameborder="0" ></iframe>');
	$('#GEMSCommonIframe').attr('src',url);
}


