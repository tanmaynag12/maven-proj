function eventRegistrationForEmployee(id)
{
	if($.session("eventMenu")!='ok' && $.session("eventMenu")!='no')
	{
		//alert(id);
		$.ajax({
			url:'eventRegistrationForEmployee.json',
			dataType:'json',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
	
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				
				if(jArray["flag"]==1)
				{
					$.session("eventMenu","ok");
						$('#'+id).show();
						$('#EVENTS').menu({ content: $('#EVENTS').next().html(), flyOut: true });
				}
				else
				{
					$.session("eventMenu","no");
					$('#'+id).hide();
					$('#EVENTS').menu({ content: $('#EVENTS').next().html(), flyOut: true });
				}
			},
			error:function()
			{
				
			}
		});
	}
	else if($.session("eventMenu")=='ok')
	{
		$('#'+id).show();
		$('#EVENTS').menu({ content: $('#EVENTS').next().html(), flyOut: true });
	}
	else
	{
		$('#'+id).hide();
		$('#EVENTS').menu({ content: $('#EVENTS').next().html(), flyOut: true });
	}
}

function eventRegistrationForStudent(id)
{
	if($.session("eventMenu")!='ok' && $.session("eventMenu")!='no')
	{
		//alert(id);
		$.ajax({
			url:'eventRegistrationForStudent.json',
			dataType:'json',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
	
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				
				if(jArray["flag"]==1)
				{
					$.session("eventMenu","ok");
						$('#'+id).show();
				}
				else
				{
					$.session("eventMenu","no");
					$('#'+id).hide();
				}
			},
			error:function()
			{
				
			}
		});
	}
	else if($.session("eventMenu")=='ok')
	{
		$('#'+id).show();
	}
	else
	{
		$('#'+id).hide();
	}
}

function startLoading(cnt){
	if(cnt == undefined)
		cnt= '';

	$('.preview'+cnt).addClass('loading');
}

function stopLoading(cnt){
	if(cnt == undefined)
		cnt= '';
	$('.preview'+cnt).removeClass('loading');
}


function generateBarcode(value,barcodeTargetId){

	var btype = 'code 93';


	var settings = {
			output:'css',
			bgColor:'inherit',
			color: '#000000',
			barWidth: '1',
			barHeight: '50'
				/* moduleSize: $("#moduleSize").val(),
       posX: $("#posX").val(),
       posY: $("#posY").val(),
       addQuietZone: $("#quietZoneSize").val()*/
	};
	$("#"+barcodeTargetId).barcode(value, btype, settings);
}
function loadGEMSURL(url){
	
	//<iframe src="" id = "GEMSCommonIframe" width="100%" height="100%" frameborder="0"></iframe>
	
	$('#divGEMSHomeBody').html('<iframe src="" id = "GEMSCommonIframe" width="100%" height="100%" frameborder="0" ></iframe>');
	$('#GEMSCommonIframe').attr('src',url);
}


function redirect(URL)
{
  document.location=URL;
  return false;
}

