/**
 * @name: showErrorMessage()
 * @author: vimal kr.
 * @date 17 Apr 2012
 * @param strErrorText : Error text which is to be displayed
 * @param intTimeInterval : time( in sec)  to hide the message
 */
function showErrorMessage(strErrorText,intTimeInterval) {

	$('.response-widget').find('p').html('<span class="ui-icon ui-icon-alert" style="float: left; margin-right: .3em;"></span>'+strErrorText);
	$('.response-widget').show();
	setTimeout('$(".response-widget").hide("slow")',intTimeInterval);
	$('.responsebar').html(strErrorText);
	$('.responsebar').addClass('error');
	setTimeout('$(".responsebar").removeClass("error")',intTimeInterval);
}

/**
 * @name: showSuccessMessage()
 * @author: vimal kr.
 * @date 17 Apr 2012
 * @param strErrorText : Success text which is to be displayed
 * @param intTimeInterval : time( in sec)  to hide the message
 */
function showSuccessMessage(strSuccessText,intTimeInterval) {
	//alert("In success");
	$('.response-widget').find('p').html('<span class="ui-icon ui-icon-check" style="float: left; margin-right: .3em;"></span>'+strSuccessText);
	$('.response-widget').show();
	setTimeout('$(".response-widget").hide("slow")',intTimeInterval);
	$('.responsebar').html(strSuccessText);
	$('.responsebar').addClass('success');
	setTimeout('$(".responsebar").removeClass("success")',intTimeInterval);
	
	$('.responsebar-post').html(strSuccessText);
	$('.responsebar-post').addClass('success-post');
	setTimeout('$(".responsebar-post").removeClass("success-post")',intTimeInterval);
}

function showInformation(strInfoText,intTimeInterval) {
	$('.response-widget').find('p').html('<span class="ui-icon ui-icon-info" style="float: left; margin-right: .3em;"></span>'+strInfoText);
	$('.response-widget').show();
	setTimeout('$(".response-widget").hide("slow")',intTimeInterval);
	$('.responsebar').html(strInfoText);
	$('.responsebar').addClass('info');
	setTimeout('$(".responsebar").removeClass("info")',intTimeInterval);
}



/**
 * 
 * @param amount should be of type string
 * @returns
 */
function commaFormatted(amount)
{
	var delimiter = ","; // replace comma if desired
	var a = amount.split('.',2);
	var d = a[1];
	var i = parseInt(a[0]);
	if(isNaN(i)) { return ''; }
	var minus = '';
	if(i < 0) { minus = '-'; }
	i = Math.abs(i);
	var n = new String(i);
	var a = [];
	while(n.length > 3)
	{
		var nn = n.substr(n.length-3);
		a.unshift(nn);
		n = n.substr(0,n.length-3);
	}
	if(n.length > 0) { a.unshift(n); }
	n = a.join(delimiter);
	if(d.length < 1) { amount = n; }
	else { amount = n + '.' + d; }
	amount = minus + amount;
	return amount;
}


function getCurrentYearList(){

	var yearList = new Array();
	var year = parseInt(new Date().getFullYear());
	for(var yr = year + 5 ;yr >  year - 15 ; yr--){
		yearList.push(yr+"");
	}
	return yearList;
}




jQuery.fn.shiftSelect = function() {
	var checkboxes = this;
	var lastSelected;
	var executing = false;
	jQuery(this).click( function(event) {

		if (executing)
			return;

		if ( !lastSelected ) {
			lastSelected = this;
			return;
		}

		if ( event.shiftKey ) {
			var selIndex = checkboxes.index(this);
			var lastIndex = checkboxes.index(lastSelected);
			/*
			 * if you find the "select/unselect" behavior unseemly,
			 * remove this assignment and replace 'checkValue'
			 * with 'true' below.
			 */
			var checkValue = lastSelected.checked;
			if ( selIndex == lastIndex ) {
				return true;
			}
			executing = true;
			var end = Math.max(selIndex,lastIndex);
			var start = Math.min(selIndex,lastIndex);
			for(i=start;i<=end;i++) {
				if (checkboxes[i].checked != checkValue)
					$(checkboxes[i]).click();
				$(checkboxes[i]).attr('checked','checked');
			}
			executing = false;
		}
		lastSelected = this;
	});
}

function createEditor(id)
{
	var instance = CKEDITOR.instances[id];
	if(instance)
	{
		/*if($('#'+id).val()=='')
			return;
		CKEDITOR.remove(instance);*/
		instance.destroy(true);
	}
	CKEDITOR.replace(id,
			{uiColor: '#ffffff',
		/*  filebrowserImageBrowseUrl : "http://118.139.182.191:9080/GEMS/image", */
		width: "95%",
		height: "300px",
			});
}

//by vimal
jQuery.fn.reset = function(fn) {
	return fn ? this.bind("reset", fn) : this.trigger("reset");
};
///1.required
function numberFormatValidation(id)
{
	$(function(){
		$("#"+id).keydown(function(event) {
			// Allow: backspace, delete, tab and escape
			//alert(event.keyCode);
			if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || 
					// Allow: Ctrl+A
					(event.keyCode == 65 && event.ctrlKey === true) || 
					// Allow: home, end, left, right
					(event.keyCode >= 35 && event.keyCode <= 39)) {
				// let it happen, don't do anything
				return;
			}
			else {
				// Ensure that it is a number and stop the keypress
				if ((event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 )) {
					event.preventDefault(); 
				}   
			}
			
		});
	});
}
/*******************
 * Nikhil Vanjara
 * Allow . sign with numbers
 * @param id
 */
function floatNumberFormatValidation(id)
{
	$(function(){
		$("#"+id).keydown(function(event) {
			// Allow: backspace, delete, tab and escape
			//alert(event.keyCode);
			if ( event.keyCode == 46 || event.keyCode == 8 || event.keyCode == 9 || event.keyCode == 27 || event.keyCode == 110 || event.keyCode == 190 ||
					// Allow: Ctrl+A
					(event.keyCode == 65 && event.ctrlKey === true) || 
					// Allow: home, end, left, right
					(event.keyCode >= 35 && event.keyCode <= 39)) {
				// let it happen, don't do anything
				return;
			}
			else {
				// Ensure that it is a number and stop the keypress
				if ((event.keyCode < 48 || event.keyCode > 57) && (event.keyCode < 96 || event.keyCode > 105 )) {
					event.preventDefault(); 
				}   
			}
		});
	});
}

