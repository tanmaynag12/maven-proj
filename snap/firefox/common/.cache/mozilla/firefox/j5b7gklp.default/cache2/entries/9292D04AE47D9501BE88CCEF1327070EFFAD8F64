function nthOccurrence(str,c,n) {
	var pos = str.indexOf(c, 0);
	while (n-- > 0 && pos != -1)
		pos = str.indexOf(c, pos+1);
	return pos;
}

function requiredField(id)
{

	$('#'+id).qtip({
		content: 'Required Field is Empty!..', // Set the tooltip content to the current corner
		position: {
			corner: {
				tooltip: 'bottomLeft', // Use the corner...
				target: 'topRight' // ...and opposite corner
			}
		},
		show: {
			when: false, // Don't specify a show event
			ready: true // Show the tooltip when ready
		},
		hide:{
			fixed: true // Make it fixed so it can be hovered over
		},
		style: {
			border: {
				width: 5,
				radius: 10
			},
			padding: 10, 
			textAlign: 'center',
			tip: true, // Give it a speech bubble tip with automatic corner detection
			name: 'blue' // Style it according to the preset 'cream' style
		}
	});
}


