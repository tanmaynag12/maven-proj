    		},
    		complete: function() {
    			$('.ulDMSCourseFileAttachmentTree').find('.preview-large').removeClass('loading-large');
    		},
    		success:function(jArray){
    			callback(jArray);
    		},
    		error:function(){
    		}
    	});
    };
	this.getDocument = function(categoryId,callback){
		if(categoryId == undefined || categoryId == '' || categoryId=='null' || categoryId=='undefined'){
			//jAlert('Error: define category id');
			return null;
		}		
		try{
			$.ajax({
				url:'getUploadedDocumentAjaxStudent.json?categoryid='+categoryId,
				dataType:'json',
				beforeSend: function() {
				},
				complete: function() {
				},
				async:false,
				success:function(jArray){
					gjArrUploadedDocument=jArray;
					var jsonArray = new Array();
					for(var i = 0; i < jArray.length ; i++) {
						var documentObj = new GemsDocument();
						documentObj.categoryId = jArray[i]['categoryId'];
						documentObj.eCatId = jArray[i]['eCatId'];
						documentObj.catName = jArray[i]['catName'];

						documentObj.documentName = jArray[i]['name'];
						documentObj.name = jArray[i]['name'];
						documentObj.hasProfileImage = jArray[i]['hasProfileImage'];
						documentObj.gender = jArray[i]['gender'];
						documentObj.sharedStatus = jArray[i]['sharedStatus'];
						documentObj.documentId = jArray[i]['documentId'];
						documentObj.eDocId = jArray[i]['eDocId'];
						documentObj.creationDate = jArray[i]['creationDate'];
						documentObj.ownerEmployeeId = jArray[i]['ownerStudentId'];
						documentObj.extension = jArray[i]['extension'];
						documentObj.type = jArray[i]['extension'];
						documentObj.isTemporary = false;
						documentObj.description = jArray[i]['description'];
						documentObj.documentBody = jArray[i]['documentBody'];
						jsonArray.push(documentObj);
					}
					callback(jsonArray);
				},
				error:function(){

				}

			});
		} catch(e){
			alert(e);
		}

	};

	/**
	 * student placement id is only required for getting the owner information of the document
	 */
	this.getDocumentByStudentPlacementId = function(categoryId,studentPlacementId,callback){
		if(categoryId == undefined || categoryId == '' ||  categoryId=='null' || categoryId =='undefined'){
