
$('document').ready(function(){
	//clock();
	
	$('#replayToStudentMessageModal').modal({
		keyboard:false,
		show:false
	}).css({
		width:'700px',
		'margin-left':function(){
			return -($(this).width()/2);
		},
	});

});



function showMessage(cmbBox){

	if(cmbBox=="Roster"){
		if($('#empName').val()!=0 && $('#empName').val()!=null){
			var employeeId = $('#empName option:selected').attr("id");
			sendMessage(employeeId,$('#empName option:selected').text());
		}
		else{

			jAlert("Select Employee");
		}
	}

	else if($('#'+cmbBox).val()!=0 && $('#'+cmbBox).val()!=null){
		var employeeId = $('#'+cmbBox).val();
		sendMessage(employeeId,$('#'+cmbBox+' option:selected').text());

	}
	else{

		jAlert("Select Employee");
	}


}

var int='';
function sendMessage(empId,empName){
	
	$('#viewMessagePopUp').modal('show').css({
		width: '700px',
		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});
	if(empName!=undefined)
		$('#sendEmployeeName').html('to '+empName);
	$("#msgBox").html('');

	var boiler='';

	boiler += '<table class="table-bordered" align="center"><tr><thead><th>Message Body</th></thead></tr><tr><td>'+
	'<textarea id="comment" class="commentMark" cols="60" style="background-color: rgb(255, 255, 255); overflow: hidden; width:615px; height:60px" name="commentMark" wrap="hard"></textarea></td></tr><tr><td align="center" >'+
	'<input type="button" class="btn btn-inverse btn-mini  " value="Send" onclick="saveMessage(\''+empId+'\');scrollToBottom(\'#viewMessageDetails\');" /></td></tr></table><br><br><div class="well" id="viewMessageDetails" style="overflow-y: auto; height:200px;"></div>';
	$("#msgBox").html(boiler);
	getMessageOfTwoEmployee(empId);
	var cntTime=0;
	
	var flag=true;
	interval =setInterval( function() {
		if (flag==true) {
			scrollToBottom('#viewMessageDetails');
			flag=false;
		}else {
			clearInterval( interval );
		}
	}, 200 );

	int=self.setInterval(function(){clock()},30000);
	function clock()
	{
		window.load=getMessageOfTwoEmployee(empId);
	} 
}
function scrollToBottom(id) {
	
	var objDiv = document.querySelector(id);
	objDiv.scrollTop = objDiv.scrollHeight;
}

function sendBirthdayMessage(empId,empName){
	$('#viewMessagePopUp').modal('show').css({
		width: '900px',
		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});
	if(empName!=undefined)
		$('#sendEmployeeName').html('to '+empName);
	$("#msgBox").html('');






	var boiler='';

	boiler += '<table class="table-bordered" align="center"><tr><thead><th style="font-size:12px">Message</th></thead></tr><tr><td>'+
	'<textarea id="comment" class="commentMark" cols="60" style="background-color: rgb(255, 255, 255); overflow: hidden; width:400px; height:80px" name="commentMark" wrap="hard">'+
	'many many happy returns of the day'+
	'</textarea></td></tr><tr><td align="center" >'+
	'<input type="button" class="btn btn-inverse btn-mini  " value="Send" onclick="saveMessage(\''+empId+'\');" /></td></tr></table><br><br><div class="well" id="viewMessageDetails"></div>';
	$("#msgBox").html(boiler);
	getMessageOfTwoEmployee(empId);
	var cntTime=0;


	int=self.setInterval(function(){clock()},30000);
	function clock()
	{
		window.load=getMessageOfTwoEmployee(empId);
	} 



}

function getMessageOfTwoEmployee(empId){
	var link = '';

	link +=  '<table class="table table-striped table-bordered table-condensed">';



	$.ajax({
		url:'getMessageOfTwoEmployee.json?employeeId='+empId,
		dataType:'json',
		async:false,
		success:function(jArray){

			if(jArray.length>0)	{
				//alert(jArray.length);
				for(var i=0;i<jArray.length;i++){
					
					if(jArray[i]["employeeId"]==empId)
					{

					link+= '<div class=" alert alert-success pull-left" id="'+jArray[i]["employeeMessageId"]+'-'+i+'" style="width:400px;">';
					if(jArray[i]["profileImageStatus"]=='notdefined') 
						link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[i]["employeeId"]+'\',\'employee\')" id="cmtempimg_'+i+'" src="'+jArray[i]["profileImage"]+'" style="border-radius:20px;float:left;border:0.5px solid lightgray;" width="40" height="40" border="0" alt="" /></a>';
					else
						link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[i]["employeeId"]+'\',\'employee\')" id="cmtempimg_'+i+'"  id="cmtempimg_'+i+'" src="getEmployeeProfileImageById.json?id='+jArray[i]["employeeId"]+'" style="float:left;border:0.5px solid lightgray;" width="40" height="40" border="0" alt="" /></a>';

					link+='<b style="font-size:0.8rem;margin-left:5px;margin-top:5px;">'+jArray[i]["sentBy"]+'<a href="#"></a></b><br>'+
					'<span style="margin-bottom:10px;margin-left:5px;">'+jArray[i]["date"]+' at '+jArray[i]["time"].split(" ")[1].split(".")[0]+'</span>'+
					'<h1 style="font-size: 18px; padding-bottom: 10px; margin-bottom: 10px; line-height: 22px; border-bottom: 1px solid #E5E5E5;"></h1>'+
					'<div class="name" style="">'+
					'<em>'+jArray[i]["message"]+'</em>'+

					'</div>'+
					'<br clear="all" />'+
					'<br clear="all" />'+
					'<div  class="name" align="center" style="width:350px;float:right; ">'+
					'<div class="name" style="float:left;">';
					link+='</div>'+
					'</div>'+
					'</div>';
					}
					else
					{
						link+= '<div class=" alert alert-info pull-right" id="'+jArray[i]["employeeMessageId"]+'-'+i+'" style="width:400px;">';
						

						if(jArray[i]["profileImageStatus"]=='notdefined') 
							link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[i]["employeeId"]+'\',\'employee\')" id="cmtempimg_'+i+'" src="'+jArray[i]["profileImage"]+'" style="border-radius:20px;float:left;border:0.5px solid lightgray;" width="40" height="40" border="0" alt="" /></a>';
						else
							link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[i]["employeeId"]+'\',\'employee\')" id="cmtempimg_'+i+'"  id="cmtempimg_'+i+'" src="getEmployeeProfileImageById.json?id='+jArray[i]["employeeId"]+'" style="float:left;border:0.5px solid lightgray;" width="40" height="40" border="0" alt="" /></a>';

						link+='<b style="font-size:0.8rem;margin-left:5px;margin-top:5px;">'+jArray[i]["sentBy"]+'<a href="#"></a></b><br>'+
						'<span style="margin-bottom:10px;margin-left:5px;">'+jArray[i]["date"]+' at '+jArray[i]["time"].split(" ")[1].split(".")[0]+'</span>'+
						'<h1 style="font-size: 18px; padding-bottom: 10px; margin-bottom: 10px; line-height: 22px; border-bottom: 1px solid #E5E5E5;"></h1>'+
						'<div class="name" style="">'+
						'<em>'+jArray[i]["message"]+'</em>'+

						'</div>'+
						'<br clear="all" />'+
						'<br clear="all" />'+
						'<div  class="name" align="center" style="width:350px;float:right; ">'+
						'<div class="name" style="float:left;">';
						link+='</div>'+
						'</div>'+
						'</div>';
					}
					
				}
				link += '</table>';
				$('#viewMessageDetails').html(link);
			}
			else{
				$('#viewMessageDetails').html('<div class = "alert alert-info">No messages</div>');

			}

		},
		error:function(){
		}
	});

}


function saveMessage(employeeId){
	if($("#comment").val()==''){
		jAlert("Enter message.");

	}
	else{
		$.ajax({      
			url:'sendMessageToEmployee.json?employeeId='+employeeId+'&message='+$("#comment").val(),
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();

			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(type){	

				if(type=="not saved"){

					jAlert("you cannot send message to yourself");

				}
				else{
					$('#comment').val('');
					jAlert(" message sent successfully");
					getMessageOfTwoEmployee(employeeId);
				}
			},
			error:function(){
				alert("Error");
			}
		});
	}

}


function setReadMessage(){
	$.ajax({      
		url:'setReadMessage.json',
		dataType:'text',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(type){	

			getUnreadMessage();
		},
		error:function(){
			alert("Error");
		}
	});

}

function closeDialog(){

	$('#viewMessagePopUp').modal("hide");
	if(int!='')
		clearInterval(int);


}	


function getAllStudentMessage()
{
	$('#message').val('');
	$.ajax({
		url:'getAllStudentMessage.json',
		dataType:'json',
		async:false,

		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var subcnt=1;
			var j=jArray.length;
			var boiler='';
			boiler+='<table id="dMtbl1" cellpadding="0" cellspacing="0" border="1" class="table table-striped table-bordered table-condensed" style="width: 100%"><thead>'+
			'<tr><th>Sr No.</th><th>Date</th><th>Time</th><th>Message</th>'+
			'<th>Sent By</th><th>Reply</th><th style="display:none">studentMessageId</th></tr></thead><tbody>';

			for( var i=0; i<j; i++)
			{
				if(jArray[i]["empCommStatus"]==undefined){
					var studentMessageId = jArray[i]["studentMessageId"];
					if(jArray[i]["status"]==false){
						boiler+='<tr id="'+studentMessageId+'" style="font-weight:bold">';
					}
					else{
						boiler+='<tr id="'+studentMessageId+'">';
					}

					boiler+='<td>'+subcnt+'</td>'+
					'<td>'+jArray[i]["date"]+'</td>'+
					'<td>'+jArray[i]["time"].split(" ")[1].split(".")[0]+'</td>'+
					'<td>'+jArray[i]["message"]+'</td>'+
					'<td>'+jArray[i]["sentBy"]+'</td>'+
					'<td style="text-align:center">'+
					'<input type="button" value="Reply" class="btn btn-inverse btn-mini  " onclick="sendStudentMessage(\''+jArray[i]["sentBystudentId"]+'\' ,\''+jArray[i]["sentBy"]+'\')"></td>'+
					'<td style="display:none">'+studentMessageId+'</td></tr>';
					subcnt++;
				}
			}
			boiler+='</tbody></table><br /><div id="divMeetingForCoordinator"></div>';

			$("#studentMessageDiv").html(boiler);


			$('#dMtbl1').dataTable( {
				"sDom": "<'row'<'margin-left-22'l><'pull-right'f>r>t<'row'<'margin-left-22'i><'pull-right'p>>",
				"sPaginationType": "bootstrap",
				"oLanguage": {
					"sLengthMenu": "_MENU_ records per page"
				}
			} );

		},
	});
	if($("#countStudMessage").text()!=0){
		setReadStudMessageByEmployee();
	}
}

function getUnreadStudentMessageByEmployee(){
	$.ajax({
		url:'getUnreadStudentMessageByEmployee.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			$("#countStudMessage").html(jArray[0]["studMessagecount"]);
		},
	});
}


function setReadStudMessageByEmployee(){
	$.ajax({      
		url:'setReadStudMessageByEmployee.json',
		dataType:'text',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(type){	

			getUnreadStudentMessageByEmployee();
		},
		error:function(){
			alert("Error");
		}
	});

}

function sendStudentMessage(studentId,studentName)
{
	$('#studName').html('');
	$('#replayDiv').html('');
	$('#replayToStudentMessageModal').modal('show');
	$('#studName').html("Reply To : "+studentName);
	
	var boiler ='<table class="table table-bordered"><thead>'+
					'<tr><th>Reply Message</th></tr></thead>'+
					'<tr><td><textarea id="replyStudent" style="width:650px"></textarea></td></tr>'+
					'<tr><td><div align="center">'+
					'<input type="button" class="btn btn-inverse btn-mini" value="Send Reply" onclick="sendReplayToStudent(\''+studentId+'\')"></div></td></tr>'+
				'</table>'+
				'</div>';
	$('#replayDiv').html(boiler);
}

function sendReplayToStudent(studentId)
{
	var description = $('#replyStudent').val();
	$.ajax({      
		url:'sendReplayToStudent.json?studentId='+studentId,
		dataType:'text',
		type:'POST',
		data:{studentId:studentId,description:description.toString()},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(type)
		{	
			$('#replayToStudentMessageModal').modal('hide');
			jAlert("Successfully Send Message","Success");
		},
		error:function(){
			alert("Error");
		}
	});
}
