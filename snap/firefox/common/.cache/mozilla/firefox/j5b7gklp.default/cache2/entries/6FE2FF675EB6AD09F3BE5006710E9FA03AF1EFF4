									'</tr>';
							}else{
								addSkillBoiler +='<tr>'+
								'<td><b>Mentor Name </b></td>'+
								'<td><input type="text" id="txtMentorName" name="ProjectMentorName"/></td>'+
								'</tr>';
							}
							if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
								addSkillBoiler +='<tr>'+
									'<td><b>Project Keyskill</b></td>'+
									'<td><input type="text" id="txtPrjKeyskill" name="ProjectKeyskill"/></td>'+
									'</tr>'+
									'<tr>'+
									'<td><b>Project Description </b></td>'+
