					if(studGapArr!=''){
						for(var i=0; i<totGapLen; i++){
							if(studGapArr[i]["fromYear"]>=jArray[0]["PGPGGapStarts"] && studGapArr[i]["fromYear"]<jArray[0]["PGPGGapEnds"] && studGapArr[i]["toYear"]<=jArray[0]["PGPGGapEnds"] && studGapArr[i]["toYear"]>jArray[0]["PGPGGapStarts"]){
								var temp=studGapArr[i]["toYear"] - studGapArr[i]["fromYear"];
								calculateYrs11=calculateYrs11+temp;
							}
						}
					}
					if(calculateYrs11!=jArray[0]["PGPGGapDuration"]){
						$('#PGPGGapTr').show();
						$('#txtPGPGGapStarts').val(jArray[0]["PGPGGapStarts"]);
						$('#txtPGPGGapEnds').val(jArray[0]["PGPGGapEnds"]);
					}
				}
			}
		},
		error:function(){
			////alert("in Error stream CMB");
		}
	});
}
//new gap look ends here

var statusForAddingGap='';
function showToYear(fromYear){
	var rowId = getSelectedRowId('gaptable');
	var today =new Date(); 
	var year=	today.getFullYear();
	var toYear = eval(year);
	var boiler='<option value="-1">Select</option>';
	for(var i=fromYear;i<toYear;i++)
	{
		boiler+='<option value="'+i+'">'+i+'</option>';
		
	}
	$('#'+rowId+'_tYear').html(boiler);
	
}
// For Training Tab
function Trainingpickdates(id) {
	$("#"+id+"_details").elastic();
	jQuery("#" + id + "_year", "#trainingtable").datepicker(
			{
				dateFormat : "yy",
				showWeek : false,
				changeMonth : false,
				changeYear : true,
				yearRange : "-60:+0",
				showButtonPanel : true,
				onClose : function(dateText, inst) {
					// var month = $("#ui-datepicker-div .ui-datepicker-month
					// :selected").val();
					var year = $(
							"#ui-datepicker-div .ui-datepicker-year :selected")
							.val();
					$(this).datepicker('setDate', new Date(year));
				}

			});
	
	jQuery("#" + id + "_year").datepicker(
			{
				dateFormat : "yy",
				showWeek : false,
				changeMonth : false,
				changeYear : true,
				yearRange : "-60:+0",
				showButtonPanel : true,
				onClose : function(dateText, inst) {
					// var month = $("#ui-datepicker-div .ui-datepicker-month
					// :selected").val();
					var year = $(
							"#ui-datepicker-div .ui-datepicker-year :selected")
							.val();
					$(this).datepicker('setDate', new Date(year));
				}

			});

}

var statusForAddingTraining='';
var studTrainingArr = new Array();
function viewTrainingList(){

	$.ajax({
		url:'studentTraining.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newTrainingMsg" style="display: none;">'+
			'	<table>'+
			'	<tr id="trReqPendingTraining" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
			'	</tr>'+
			'	<tr id="trReqAcceptedTraining" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
			'	</tr>'+
			'	<tr id="trSendReqTraining" style="display: none;">'+
			'		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
			'	</tr>'+
			'</table>'+
			'</div>'+
			'<table class="table table-bordered table-condensed">'+
			'<tr><th style="color: #0088CC" colspan="8"><label><b><font>Student Training Information</font></b></label></th></tr>';
			boiler+='<tr><td colspan="8"><span  style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack   freezeProfile"onclick="checkReqConditionForAddEditTraining(\'NEW\',\'\')"><i class ="fa fa-plus"></i>Add Training</a></span><span><input type="button" id="btnFreezeTraining" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForAddingTraining=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentTrainingId"]!=undefined){
						
						boiler+='<tr>'+
						'<th><b>Training Name</b></th>'+
						'<th><b>Training Company/'+juno.labelByCustomer('l.SchoolInstitute')+'</b></th>'+
						'<th><b>Training Details</b></th>'+
						'<th><b>Key Skills</b></th>'+
						'<th><b>Training Year</b></th>'+
						'<th><b>Percent/Grade</b></th>'+
						'<th colspan="4"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studTrainingArr=jArray;
						
							boiler+=	'<tr>'+
							'<td style="text-align: center;"><label>'+jArray[i]["trainingName"]+'</label></td>'+
							'<td style="text-align: center;"><label>'+jArray[i]["trainerCompanyOrInstitute"]+'</label></td>'+
							'<td style="text-align: center;"><label>'+jArray[i]["details"]+'</label></td>'+
							'<td style="text-align: center;"><label>'+jArray[i]["keySkills"]+'</label></td>'+
							'<td style="text-align: center;"><label>'+jArray[i]["year"]+'</label></td>'+
							'<td style="text-align: center;"><label>'+jArray[i]["percentOrGrade"]+'</label></td>'+
					
							'<td style="text-align: right;"><a  class="freezeProfile" onclick="checkReqConditionForAddEditTraining(\''+i+'\',\''+jArray[i]["studentTrainingId"]+'\')" href="#"><i class=" fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
							'<td style="text-align: right;"><a onclick="checkReqForDelTraining(\''+jArray[i]["studentTrainingId"]+'\')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';
							
							boiler += '<td style="text-align: right;"><a onclick ="uploadStudTrainingDocument(\''+jArray[i]["studentTrainingId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
							
							boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#training').html(boiler);
			
					}else{
						
						boiler+='</table>';
						$('#training').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForAddingTraining="ReqNotSent";
					boiler+='</table>';
					$('#training').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForAddingTraining="canChange";
					boiler+='</table>';
					$('#training').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForAddingTraining=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#training').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForAddingTraining=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#training').html(boiler);
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForAddingTraining="canChange";
					boiler+='</table>';
					$('#training').html(boiler);
				}
			
				if(statusForAddingTraining=='ReqNotSent'){
					$('#newTrainingMsg').show();
					$('#btnFreezeTraining').hide();
					$('#trSendReqBookPub').show();
					$('#trReqAcceptedBookPub').hide();
					$('#trReqPendingTraining').hide();
				}
				else if(statusForAddingTraining=='ReqAccepted'){
					$('#newTrainingMsg').show();
					$('#btnFreezeTraining').show();
					$('#trReqAcceptedBookPub').show();
					$('#trSendReqBookPub').hide();
					$('#trReqPendingTraining').hide();
				}
				else if(statusForAddingTraining=='ReqSent'){
					$('#newTrainingMsg').show();
					$('#trReqPendingTraining').show();
					$('#trReqAcceptedBookPub').hide();
					$('#trSendReqBookPub').hide();
				}
				else if(statusForAddingTraining=='canChange'){
					$('#trReqPendingTraining').hide();
					$('#trReqAcceptedBookPub').hide();
					$('#trSendReqBookPub').hide();
				}
				getstudProfileConfigurationDetailsCheckingNames1();
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
}

function checkReqConditionForAddEditTraining(indexId, studentTrainingId){
	if(statusForAddingTraining=="canChange"){
		addNewTrainingNew(indexId, studentTrainingId);
	}
	else if(statusForAddingTraining=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingTraining=="ReqAccepted"){
		 $('#btnFreezeTraining').show();
		 addNewTrainingNew(indexId, studentTrainingId);
	}
	else if(statusForAddingTraining=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="training";
					sendRequestForEditing(tab);
				}
					});
	}
	getstudProfileConfigurationDetailsCheckingNames1();
}

function addNewTrainingNew(indexId, trainingId){

	var addSkillBoiler='';
	
		addSkillBoiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
								'<th colspan="2" style="color: #0088CC"><h4Student Training Information</h4></th>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Training Name </b><b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtTrainingName2" name="studentTrainingName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Training Company/'+juno.labelByCustomer('l.SchoolInstitute')+'</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtTrainingCompanyInstitute" name="trainingCompInstiName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Training Details</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtTrainingDetails" name="trainingDetailsName"/></td>'+
							'</tr>'+
							'<tr>'+
							'<td><b>Training Year</b> <b style="color:red;font-size:16px;" >*</b></td>'+
							'<td><select id="cmbTrainingYrOfpass" name="trainingYearName"></select></td>'+
						'</tr>'+
						'<tr>'+
						'<td><b>Percent/Grade</b> <b style="color:red;font-size:16px;" >*</b></td>'+
						'<td><input type="text" id="txtpercentOrGrade" name="TrainingPerGradeName"/></td>'+
					'</tr>'+
					'<tr>'+
								'<td><b>Key Skills</b></td>'+
								'<td><input type="text" id="keySkillId" name="keySkillsName"/></td>'+
							'</tr>'+	
							'<tr>'+
								'<td></td>'+
								'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnAddNewStudCerti" onclick="saveNewStudTrainingNew(\''+trainingId+'\')"/>'+
								'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="	viewTrainingList();" /></td>'+
							'</tr>'+
							
							'</tbody>'+
							'</table>';
	
	
		$('#training').html(addSkillBoiler);
	//$('.validnum').numeric();
	
	var currentdate = new Date();
	var curYear = (currentdate.getFullYear());
	var yearListOf20 = '<option value="0">Select</option>';
	
	for ( var year = curYear; year > curYear - 60 ; year--)
		yearListOf20 += '<option value='+year+'>' + year + '</option>';
	$('#cmbTrainingYrOfpass').html(yearListOf20);
	
	if(indexId!="NEW"){
		$('#txtTrainingName2').val(studTrainingArr[indexId]["trainingName"]);
		$('#txtTrainingCompanyInstitute').val(studTrainingArr[indexId]["trainerCompanyOrInstitute"]);
		$('#txtTrainingDetails').val(studTrainingArr[indexId]["details"]);
		$('#cmbTrainingYrOfpass').val(studTrainingArr[indexId]["year"]);
		$('#txtpercentOrGrade').val(studTrainingArr[indexId]["percentOrGrade"]);
	}
}

function saveNewStudTrainingNew(trainingId,studentId){
	var inserttype='';
	if(trainingId==undefined || trainingId==''){
		trainingId='';
		inserttype='new';
	}else{
		inserttype='edit';
	}
	
	var trainingName=$('#txtTrainingName2').val();
	var keySkillId=$('#keySkillId').val();
	var trainerCompanyOrInstitute=$('#txtTrainingCompanyInstitute').val();
	var trainingDetails=$('#txtTrainingDetails').val();
	var year=$('#cmbTrainingYrOfpass').val();
	var percentOrGrade=$('#txtpercentOrGrade').val();
	//PercentGrade
	var cnt=0;
	if(trainingName==''){
		jAlert("Please enter Training name");
		cnt=1;
	}
	else if(trainerCompanyOrInstitute==''){
		jAlert("Please enter Training Company/Institute");
		cnt=1;
	}
	else if(trainingDetails==''){
		jAlert("Please enter Training Details");
		cnt=1;
	}
	else if(year==0){
		jAlert("Please select year of passing");
		cnt=1;
		
	}
	else if(percentOrGrade==''){
		jAlert("Please enter Percent/Grade");
		cnt=1;
	}
	
	if(cnt==0){
		$.ajax({
			url:'addStudentTraining.json?inserttype='+inserttype,
			data:{trainingName:trainingName,trainerCompanyOrInstitute:trainerCompanyOrInstitute,trainingDetails:trainingDetails,year:year,percentOrGrade:percentOrGrade,trainingId:trainingId,keySkillId:keySkillId},
			dataType:'text',
			type:'POST',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				viewTrainingList();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function checkReqForDelTraining(trainingId){
	
	if(statusForAddingTraining=="canChange"){
		deleteStudTraining(trainingId);
	}
	else if(statusForAddingTraining=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingTraining=="ReqAccepted"){
		 $('#btnFreezeTraining').show();
		 deleteStudTraining(trainingId);
	}
	else if(statusForAddingTraining=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="training";
					sendRequestForEditing(tab);
				}
					});
	}
}
function deleteStudTraining(trainingId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudentTraining.json?trainingId='+trainingId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						viewTrainingList();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}

function uploadStudTrainingDocument(trainingId){
	if(trainingId != '' && trainingId != 'null' && trainingId != undefined){
		if(statusForAddingTraining=='canChange' || statusForAddingTraining=='ReqAccepted'){
			getDegreeCategoryId(gTabDegreeName.TabName_TRAINING,trainingId);
		}
		else if(statusForAddingTraining=='ReqSent'){
			jAlert("The request you have sent for editing is pending");
		}else if(statusForAddingTraining=='ReqNotSent'){
			$('#btnFreezeTraining').show();
			jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r) {
				if(r ==true) {
					var tab="training";
					sendRequestForEditing(tab);
				}
			});
		}   
	} else
			jAlert('Student award record not found!');
	
}

// ******************************************************************************************************Validations
$(document)
		.ready(
				function() {
					jQuery(function() {
						$('.studPerDetails').validated(function() {
							addStudentPersonalDetails();
						});
						$('.studGuardian').validated(function() {
							addStudentGuardianDetails();
						});
					});
					
					// ----------10  TH TAB VALIDATIONS

					jQuery("#txt10marks").validate(
									{
										expression : "if (VAL.match(/^[0-9 /./]{0,4}$/)) return true; else return false;",
										message : "Please Enter valid Total Marks"
									});
					jQuery("#txt10outof").validate(
									{
										expression : "if (VAL.match(/^[0-9 /./]{0,4}$/)) return true; else return false;",
										message : "Please Enter valid Out Of Marks"

									});
					jQuery("#yrOfPassingId").validate(
									{
										expression : "if (VAL && VAL.match(/^[0-9 /./]{4,4}$/)) return true; else return false;",
										message : "Please Enter valid passing Year"
									});
					jQuery("#tenthboard").validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid Board Name"
									});
					jQuery("#tenthschool").validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid School Name"
									});
					jQuery("#tenthplace").validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid Place Name"
									});
					/*jQuery("#tenthremark").validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid Remark Name"
									});*/
					$('.stud10Th').validated(function() {
						checkEditStatusFor10th();
					});
					// ***********************************************************************12th
					// validations

					jQuery("#txt12marks").validate(
							{
								expression : "if (VAL.match(/^[0-9 /./]{0,4}$/)) return true; else return false;",
								message : "Please Enter valid Total Marks"
							});
					jQuery("#txt12outof").validate(
							{
								expression : "if (VAL.match(/^[0-9 /./]{0,4}$/)) return true; else return false;",
								message : "Please Enter valid Out Of Marks"

							});					
					jQuery("#twelthYrOfPassing").validate(
							{
								expression : "if (VAL && VAL.match(/^[0-9 /./]{4,4}$/)) return true; else return false;",
								message : "Please Enter valid passing Year"
							});
					jQuery("#twelthBoard")
							.validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid Board Name"
									});
					jQuery("#studStreamCombo")
							.validate(
									{
										expression : "if (VAL =='Select') return false; else return true;",
										message : "Please Select Stream"
									});
					if($('#studStreamCombo').val()=='Science'){
						jQuery("#txt12Physicsmarks")
								.validate(
										{
											expression : "if (VAL ==''||VAL == 0) return false; else return true;",
											message : "Please Insert Marks of Physics"
										});
						jQuery("#txt12Physicsoutof")
								.validate(
										{
											expression : "if (VAL ==''||VAL == 0) return false; else return true;",
											message : "Please Insert Out of Marks of Physics"
										});
						jQuery("#txt12Chemistrymarks")
								.validate(
										{
											expression : "if (VAL ==''||VAL == 0) return false; else return true;",
											message : "Please Insert Marks of Chemistry"
										});
						jQuery("#txt12Chemistryoutof")
								.validate(
										{
											expression : "if (VAL ==''||VAL == 0) return false; else return true;",
											message : "Please Insert Out of Marks of Chemistry"
										});						
					}
					jQuery("#studtwelthSchool")
							.validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid School Name"
									});

					jQuery("#studTwelthPlace")
							.validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid Place Name"
									});

					/*jQuery("#studtwelthRemarks")
							.validate(
									{
										expression : "if (VAL) return true; else return false;",
										message : "Please Enter valid Remark Name"
									});*/

					$('.stud12Th').validated(function() {
						//addStudentHscDetails();
						checkEditStatusFor12th();
					});

					// applicant student passport form validation.......
					jQuery("#studpassportcountrycode").validate({
										expression : "if(VAL && VAL.match(/^[0-9 /./]{0,7}$/)) return true; else return false;",
										message : "Please Enter valid country code"
									});
					/*
					 * jQuery("#studpassportno").validate({ expression: "if (VAL &&
					 * VAL.match(/^[a-zA-Z0-9 /./]{1,25}$/)) return true; else
					 * return false;", message:"Please Enter valid passport
					 * number" });
					 */
					jQuery("#studpassportissueplace").validate({
										expression : "if (VAL && VAL.match(/^[a-zA-Z /./,/]{1,25}$/)) return true; else return false;",
										message : "Please Enter valid place of issue"
									});
					jQuery("#studvisanature").validate({
										expression : "if (VAL && VAL.match(/^[a-zA-Z /./]{1,25}$/)) return true; else return false;",
										message : "Please Enter valid nature of visa"
									});
					/*
					 * jQuery("#studvisaid").validate({ expression: "if (VAL &&
					 * VAL.match(/^[a-zA-Z0-9 /./]{1,25}$/)) return true; else
					 * return false;", message:"Please Enter valid visa number"
					 * });
					 */
					$('.studPassport').validated(function() {
						addStudentPassportDetails();
					});

				});

// For Work Experience Tab
function WorkExppickdate(id) {

	var frdt = id + "_fromYear";
	$(function() {
		var dates = $("#" + id + "_fromYear,#" + id + "_toYear")
				.datepicker(
						{
							defaultDate : "+1w",
							dateFormat : "yy",
							changeMonth : false,
							showWeek : true,
							changeYear : true,
							showButtonPanel : true,
							yearRange : "-60:+0",
							onSelect : function(selectedDate) {
								var option = this.id == frdt ? "minDate"
										: "maxDate", instance = $(this).data(
										"datepicker"), date = $.datepicker
										.parseDate(
												instance.settings.dateFormat
														|| $.datepicker._defaults.dateFormat,
												selectedDate, instance.settings);
								dates.not(this).datepicker("option", option,
										date);
							},
							onClose : function(dateText, inst) {
								var year = $(
										"#ui-datepicker-div .ui-datepicker-year :selected")
										.val();
								$(this).datepicker('setDate', new Date(year));
							},
						});
	});

}

// new work exp look starts
var studWorkexpArr= new Array();
function viewWorkExpTab444(){
	$.ajax({
		url:'studentWorkExp.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newWorkExpMsg" style="display: none;">'+
			'	<table>'+
			'	<tr id="trReqPendingWorkExp" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
			'	</tr>'+
			'	<tr id="trReqAcceptedWorkExp" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
			'	</tr>'+
			'	<tr id="trSendReqWorkExp" style="display: none;">'+
			'		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
			'	</tr>'+
			'</table>'+
			'</div>'+
			'<table class="table table-bordered table-condensed">'+
			'<tr><th style="color: #0088CC" colspan="8"><label><b><font>Student Work Experiences</font></b></label></th></tr>';
			boiler+='<tr><td colspan="8"><span style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack freezeProfile" onclick="checkReqConditionForAddEditWorkExp(\'NEW\',\'\')" ><i class="fa fa-plus"></i> Add Experience</a></span><span><input type="button" id="btnFreezeWorkExp" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForAddingWorkExp=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentEmployeementId"]!=undefined){
						
						boiler+='<tr>'+
						'<th><b>Designation</b></th>'+
						'<th><b>Company/Employer</b></th>'+
						'<th><b>Currently working?</b></th>'+
						'<th><b>Duration</b></th>'+
						'<th><b>Location</b></th>'+
						'<th><b>keyskill</b></th>'+
						'<th colspan="3"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studWorkexpArr=jArray;
							
							boiler+=	'<tr>'+
								'<td style="text-align: center;"><label>'+jArray[i]["designation"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["employer"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["pastOrCurrent"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["fromYear"]+'</label>To<label>'+jArray[i]["toYear"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["place"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["keySkill"]+'</label></td>'+
						
								'<td style="text-align: right;"><a onclick="checkReqConditionForAddEditWorkExp(\''+i+'\',\''+jArray[i]["studentEmployeementId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
								'<td style="text-align: right;"><a onclick="checkReqForDelWorkExp('+jArray[i]["studentEmployeementId"]+')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';
								
								boiler += '<td style="text-align: right;"><a onclick = "uploadStudWorkExpDocument(\''+jArray[i]["studentEmployeementId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
								
								boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#workExp').html(boiler);
			
					}else{
						
						boiler+='</table>';
						$('#workExp').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForAddingWorkExp="ReqNotSent";
					boiler+='</table>';
					$('#workExp').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForAddingWorkExp="canChange";
					boiler+='</table>';
					$('#workExp').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForAddingWorkExp=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#workExp').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForAddingWorkExp=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#workExp').html(boiler);
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForAddingWorkExp="canChange";
					boiler+='</table>';
					$('#workExp').html(boiler);
				}
			
				if(statusForAddingWorkExp=='ReqNotSent'){
					$('#newWorkExpMsg').show();
					$('#btnFreezeWorkExp').hide();
					$('#trSendReqWorkExp').show();
					$('#trReqAcceptedWorkExp').hide();
					$('#trReqPendingWorkExp').hide();
				}
				else if(statusForAddingWorkExp=='ReqAccepted'){
					$('#newWorkExpMsg').show();
					$('#btnFreezeWorkExp').show();
					$('#trReqAcceptedWorkExp').show();
					$('#trSendReqWorkExp').hide();
					$('#trReqPendingWorkExp').hide();
				}
				else if(statusForAddingWorkExp=='ReqSent'){
					$('#newWorkExpMsg').show();
					$('#trReqPendingWorkExp').show();
					$('#trReqAcceptedWorkExp').hide();
					$('#trSendReqWorkExp').hide();
				}
				else if(statusForAddingWorkExp=='canChange'){
					$('#trReqPendingWorkExp').hide();
					$('#trReqAcceptedWorkExp').hide();
					$('#trSendReqWorkExp').hide();
				}
				getstudProfileConfigurationDetailsCheckingNames1();
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
}

function checkReqConditionForAddEditWorkExp(indexId, studWorkExpId){
	//addNewProjectNew
	if(statusForAddingWorkExp=="canChange"){
		addNewWorkExpNew(indexId, studWorkExpId);
	}
	else if(statusForAddingWorkExp=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingWorkExp=="ReqAccepted"){
		 $('#btnFreezeWorkExp').show();
		 addNewWorkExpNew(indexId, studWorkExpId);
	}
	else if(statusForAddingWorkExp=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="workExp";
					sendRequestForEditing(tab);
				}
					});
	}
	getstudProfileConfigurationDetailsCheckingNames1();	
}

function checkReqForDelWorkExp(studWorkExpId){
	
	if(statusForAddingWorkExp=="canChange"){
		deleteStudWorkExpNew(studWorkExpId);
	}
	else if(statusForAddingWorkExp=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingWorkExp=="ReqAccepted"){
		 $('#btnFreezeWorkExp').show();
		 deleteStudWorkExpNew(studWorkExpId);
	}
	else if(statusForAddingWorkExp=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="workExp";
					sendRequestForEditing(tab);
				}
					});
	}
}

function deleteStudWorkExpNew(studWorkExpId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudentWorkExp.json?employeementId='+studWorkExpId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						viewWorkExpTab444();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}

function saveNewStudWorkExpNew(workExpId){
	
	if(workExpId==undefined){
		workExpId='';
	}

	var designation2=$('#txtDesignation2').val();
	var companyEmp2=$('#txtCompanyEmp2').val();
	var location2=$('#txtLocation2').val();
	var timePeriod2=$('#txtTimePeriod2').val();
	var CurrentlyWorking2=$('#cmbCurrentlyWorking2').val();
	var durationFrom2=$('#txtDurationFrom2').val();
	var durationTo2=$('#txtDurationTo2').val();
	var jobProfile2=$('#txtJobProfile2').val();
	
	var role = $('#txtRole').val();
	var rank = $('#txtRank').val();
	var achievements = $('#txtAchieveMents').val();
	var keySkill =$('#txtKeySkill').val();
	var annualCTC = $('#txtannualCTC').val();
	
	var cnt=0;
	if(designation2==''){
		jAlert("Please enter Designation");
		cnt=1;
	}
	else if(companyEmp2==''){
		jAlert("Please enter company");
		cnt=1;
	}
	else if(location2==''){
		jAlert("Please enter location");
		cnt=1;
	}
	else if(timePeriod2==''){
		jAlert("Please enter time period");
		cnt=1;
	}
	/*else if(timePeriod2.match(".")){
		jAlert("Time period should not be in decimal point");
		cnt=1;
	}*/
	else if(CurrentlyWorking2==-1){
		jAlert("Please salect current working status");
		cnt=1;
	}
	else if(durationFrom2==''){
		jAlert("Please enter duration from");
		cnt=1;
	}
	else if(durationTo2==''){
		if(CurrentlyWorking2=='No'){
			jAlert("Please enter duration to");
			cnt=1;
		}
	}
	/*else if(isNaN(duration)){
		jAlert("Please enter duration in numbers only");
		cnt=1;
	}*/
	
	if(cnt==0){
		$.ajax({
			url:'addStudentWorkExp.json',
			data:{workExpId:workExpId,designation2:designation2,companyEmp2:companyEmp2,location2:location2,timePeriod2:timePeriod2,CurrentlyWorking2:CurrentlyWorking2,durationFrom2:durationFrom2,
				durationTo2:durationTo2,jobProfile2:jobProfile2,role:role,rank:rank,achievements:achievements,keySkill:keySkill,annualCTC:annualCTC},
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				viewWorkExpTab444();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function addNewWorkExpNew(indexId, studWorkExpId){

	var addSkillBoiler='';
	
		addSkillBoiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
							'<th colspan="2" style="color: #0088CC"><h4>Please Enter your Experience</h4></th>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Designation <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><input type="text" id="txtDesignation2" name="workDesignationName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Company/Employer <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><input type="text" id="txtCompanyEmp2" name="workCompanyEmpName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Location <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><input type="text" id="txtLocation2" name="workLocationName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Time period (in months) <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><input type="text" id="txtTimePeriod2" class="validnum" name="workTimePeriodName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Currently working? <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><select id="cmbCurrentlyWorking2" onchange="hideWorkExpTxtFromTo()" name="currentlyWorkingName">'+
								'<option value="-1">Select</option>'+
								'<option value="Yes">Yes</option>'+
								'<option value="No">No</option>'+
								'</select></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Duration <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td>From&nbsp;<input type="text" id="txtDurationFrom2" onblur="hideWorkExpTxtFromTo()" name="workDurationFrom"/>&nbsp;&nbsp;&nbsp;&nbsp;- To&nbsp;<input type="text" id="txtDurationTo2" name="workDurationTo"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Job profile</b></td>'+
								'<td><input type="text" id="txtJobProfile2" name="workJobProfileName"/></td>'+
							'</tr>'+
						'<tr>'+
							'<td><b>Role </b></td>'+
							'<td><input type="text" id="txtRole" name="workRoleName"/></td>'+
						'</tr>'+
						'<tr>'+
						'<td><b>Rank </b></td>'+
						'<td><input type="text" id="txtRank" name="workRankName"/></td>'+
					'</tr>'+
					'<tr>'+
					'<td><b>Achievements </b></td>'+
					'<td><input type="text" id="txtAchieveMents" name="AchieveMentsDetail"/></td>'+
				'</tr>'+
				'<tr>'+
					'<td><b>Key Skill </b></td>'+
					'<td><input type="text" id="txtKeySkill" name="Keyskill"/></td>'+
				'</tr>';
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="XLRI"){
					addSkillBoiler +='<tr>'+
						'<td><b>CTC </b></td>'+
						'<td><input type="text" id="txtannualCTC" name="annualCTC"/></td>'+
					'</tr>';
				}
				addSkillBoiler +='<tr>'+
					'<td></td>'+
					'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnAddNewStudProj" onclick="saveNewStudWorkExpNew('+studWorkExpId+')"/>'+
					'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="viewWorkExpTab444()" /></td>'+
				'</tr>'+
				'</tbody>'+
				'</table>';
	
	
	$('#workExp').html(addSkillBoiler);
	$('.validnum').numeric();
	
	//date picker starts
	$( "#txtDurationFrom2" ).datepicker({
		dateFormat: "M dd,yy",
		showWeek: true,
		changeMonth: true,
		changeYear: true,
		maxDate:new Date(),
		yearRange:"-60:+0",
		onSelect: function(selected) {
			//hideWorkExpTxtFromTo();
			var date = $("#txtDurationFrom2").datepicker("getDate");
			if (date) {
				//$("#txtDurationTo2").datepicker("setDate", date);
				
				$("#txtDurationTo2").datepicker("option","minDate", selected);
			}
			//$("#txtDurationTo2").datepicker("option","minDate", selected);
		},
		firstDay:1   
	});

	$( "#txtDurationTo2" ).datepicker({
		dateFormat: "M dd,yy",
		showWeek: true,
		changeMonth: true,
		changeYear: true,
		maxDate:new Date(),
		yearRange:"-60:+0",
	});

	// date picker ends
	
	if(indexId!="NEW"){
		$('#txtDesignation2').val(studWorkexpArr[indexId]["designation"]);
		$('#txtCompanyEmp2').val(studWorkexpArr[indexId]["employer"]);
		$('#txtLocation2').val(studWorkexpArr[indexId]["place"]);
		$('#txtTimePeriod2').val(studWorkexpArr[indexId]["timePeriod"]);
		$('#cmbCurrentlyWorking2').val(studWorkexpArr[indexId]["pastOrCurrent"]);
		$('#txtDurationFrom2').val(studWorkexpArr[indexId]["fromYear"]);
		$('#txtDurationTo2').val(studWorkexpArr[indexId]["toYear"]);
		$('#txtJobProfile2').val(studWorkexpArr[indexId]["jobProfile"]);
		$('#txtRole').val(studWorkexpArr[indexId]["role"]);
		$('#txtRank').val(studWorkexpArr[indexId]["rank"]);
		$('#txtAchieveMents').val(studWorkexpArr[indexId]["achievement"]);
		$('#txtKeySkill').val(studWorkexpArr[indexId]["keySkill"]);
		$('#txtannualCTC').val(studWorkexpArr[indexId]["annualCTC"]);
		
		
		if(studWorkexpArr[indexId]["pastOrCurrent"]=='Yes'){
			$('#txtDurationTo2').attr('disabled', true);
			$('#txtDurationTo2').val('Still working');
		}
		else{
			$('#txtDurationTo2').attr('disabled', false);
		}
	}
}

function hideWorkExpTxtFromTo(){
	var currVal=$('#cmbCurrentlyWorking2').val();
	if(currVal=="-1"){
		jAlert("Please select your current working status");
	}
	else if(currVal=="No"){
		$('#txtDurationTo2').attr('disabled', false);
		$('#txtDurationTo2').val('');
	}
	else if(currVal=="Yes"){
		$('#txtDurationTo2').attr('disabled', true);
		$('#txtDurationTo2').val('Still working');
	}
}

function uploadStudWorkExpDocument(studWorkExpId){
	if(studWorkExpId != '' && studWorkExpId != 'null' && studWorkExpId != undefined){
		if(statusForAddingWorkExp=='canChange' || statusForAddingWorkExp=='ReqAccepted'){
			getDegreeCategoryId(gTabDegreeName.TabName_WORKEX,studWorkExpId);
		}
		else if(statusForAddingWorkExp=='ReqSent'){
			jAlert("The request you have sent for editing is pending");
		}else if(statusForAddingWorkExp=='ReqNotSent'){
			$('#btnFreezeWorkExp').show();
			jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r) {
				if(r ==true) {
					var tab="workExp";
					sendRequestForEditing(tab);
				}
			});
		}   
	} else
			jAlert('Student work experience record not found!');
	
}
// new work exp look ends
var statusForAddingWorkExp='';

// ---------------------------------Hide Show End Year And End Month
function hideHowForWorkExpTab() {
	if ($('#CurrentOrPastFlag').is(':checked')) {
		$("#toYearTr").hide();
	} else {
		$("#toYearTr").show();
	}
}
// save Work Experience Detais On PopUp

function addStudentWorkExpDetails() {
	var pastOrCurrent1 = "";
	var toYear = "";
	if ($('#CurrentOrPastFlag').is(':checked')) {

		pastOrCurrent1 = $('input[name = "pastOrCurrent1"]:checked').val();
		toYear = 'NA';
	} else {
		pastOrCurrent1 = 'NA';
		toYear = $('input[name = "toYear"]').val();
	}

	var designation = $('input[name = "designation"]').val();
	var employer = $('input[name = "employer"]').val();
	var timePeriod = $('input[name="timePeriod"]').val();
	var fromYear = $('input[name = "fromYear"]').val();

	var jobProfile = $('input[name = "jobProfile"]').val();
	var place = $('#placeTxt').val();
	// CurrentOrPastFlagCheckBox=$('input[name="CurrentOrPastFlagCheckBox"]').val();
	var employeementId = $('input[name="employeementId"]').val();
	$.ajax({
		url : 'addStudentWorkExp.json',
		data : {
			designation : designation,
			employer : employer,
			timePeriod : timePeriod,
			fromYear : fromYear,
			toYear : toYear,
			jobProfile : jobProfile,
			place : place,
			pastOrCurrent1 : pastOrCurrent1,
			employeementId : employeementId
		},
		dataType : 'json',
		type : 'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if (jArray[0].responseFlag == '1')
				$('#studWorkExp').modal('hide');
			showSuccessMessage(jArray[0].responseText, 5000);
			viewWorkExpList();
			getCustomAlertMessage("alert_create");
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

//new achievement look starts here
var studAchievementArr = new Array();
function viewAchievementsTab666(){

	$.ajax({
		url:'fetchStudentAward.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newAchievementMsg" style="display: none;">'+
			'	<table>'+
			'	<tr id="trReqPendingAchievement" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
			'	</tr>'+
			'	<tr id="trReqAcceptedAchievement" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
			'	</tr>'+
			'	<tr id="trSendReqAchievement" style="display: none;">'+
			'		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
			'	</tr>'+
			'</table>'+
			'</div>'+
			'<table class="table table-bordered table-condensed">'+
			'<tr><th style="color: #0088CC" colspan="8"><label><b><font>Student Achievements</font></b></label></th></tr>';
			boiler+='<tr><td colspan="8"><span  style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack   freezeProfile" onclick="checkReqConditionForAddEditAward(\'NEW\',\'\')" ><i class="fa fa-plus"></i>Add Award</a></span><span><input type="button" id="btnFreezeAchievement" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini  btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForAddingAchievement=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentAwardId"]!=undefined){
						
						boiler+='<tr>'+
						'<th><b>Award name</b></th>'+
						'<th><b>Description</b></th>'+
						'<th><b>Year</b></th>'+
						'<th><b>Achievement Type</b></th>'+
						'<th><b>Achievement Proficiency</b></th>'+
						'<th colspan="3"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studAchievementArr=jArray;
						
							boiler+=	'<tr>'+
								'<td style="text-align: center;"><label>'+jArray[i]["awardName"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["description"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["year"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["achievementType"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["achievementproficiency"]+'</label></td>'+
						
								'<td style="text-align: right;"><a  class="freezeProfile" onclick="checkReqConditionForAddEditAward(\''+i+'\',\''+jArray[i]["studentAwardId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
								'<td style="text-align: right;"><a onclick="checkReqForDelAward('+jArray[i]["studentAwardId"]+')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';
								
								boiler += '<td style="text-align: right;"><a onclick = "uploadStudAwardDocument(\''+jArray[i]["studentAwardId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
								
								boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#achievement').html(boiler);
			
					}else{
						
						boiler+='</table>';
						$('#achievement').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForAddingAchievement="ReqNotSent";
					boiler+='</table>';
					$('#achievement').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForAddingAchievement="canChange";
					boiler+='</table>';
					$('#achievement').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForAddingAchievement=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#achievement').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForAddingAchievement=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#achievement').html(boiler);
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForAddingAchievement="canChange";
					boiler+='</table>';
					$('#achievement').html(boiler);
				}
			
				if(statusForAddingAchievement=='ReqNotSent'){
					$('#newAchievementMsg').show();
					$('#btnFreezeAchievement').hide();
					$('#trSendReqAchievement').show();
					$('#trReqAcceptedAchievement').hide();
					$('#trReqPendingAchievement').hide();
				}
				else if(statusForAddingAchievement=='ReqAccepted'){
					$('#newAchievementMsg').show();
					$('#btnFreezeAchievement').show();
					$('#trReqAcceptedAchievement').show();
					$('#trSendReqAchievement').hide();
					$('#trReqPendingAchievement').hide();
				}
				else if(statusForAddingAchievement=='ReqSent'){
					$('#newAchievementMsg').show();
					$('#trReqPendingAchievement').show();
					$('#trReqAcceptedAchievement').hide();
					$('#trSendReqAchievement').hide();
				}
				else if(statusForAddingAchievement=='canChange'){
					$('#trReqPendingAchievement').hide();
					$('#trReqAcceptedAchievement').hide();
					$('#trSendReqAchievement').hide();
				}
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
}

function checkReqConditionForAddEditAward(indexId, studAwardId){
	//addNewProjectNew
	if(statusForAddingAchievement=="canChange"){
		addNewAwardNew(indexId, studAwardId);
	}
	else if(statusForAddingAchievement=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingAchievement=="ReqAccepted"){
		 $('#btnFreezeAchievement').show();
		 addNewAwardNew(indexId, studAwardId);
	}
	else if(statusForAddingAchievement=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="achievement";
					sendRequestForEditing(tab);
				}
					});
	}
	getstudProfileConfigurationDetailsCheckingNames1();	
}

function checkReqForDelAward(studAwardId){
	
	if(statusForAddingAchievement=="canChange"){
		deleteStudAwardNew(studAwardId);
	}
	else if(statusForAddingAchievement=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingAchievement=="ReqAccepted"){
		 $('#btnFreezeAchievement').show();
		 deleteStudAwardNew(studAwardId);
	}
	else if(statusForAddingAchievement=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="achievement";
					sendRequestForEditing(tab);
				}
					});
	}
}

function deleteStudAwardNew(studAwardId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudentAcievement.json?awardId='+studAwardId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						viewAchievementsTab666();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}

function addNewAwardNew(indexId, studAwardId){

	var addSkillBoiler='';
	
		addSkillBoiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
								'<th colspan="2" style="color: #0088CC"><h4>Please Enter your award</h4></th>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Award Name</b><b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtAwardName2" name="achieveAwardName"/></td>'+
							'</tr>'+
							'<tr>';
							if($('#customerHelper').val() == "MEGHE"){
								addSkillBoiler += '<td><b>Agency Name</b> <b style="color:red;font-size:16px;" >*</b></td>';
							}else{
								addSkillBoiler += '<td><b>Description</b> <b style="color:red;font-size:16px;" >*</b></td>';
							}
							
							addSkillBoiler += '<td><input type="text" id="txtAwardDesc2" name="achieveDescriptionName"/></td></tr>'+
							
							'<tr>'+
							'<td><b>Achievement Type</b> </td>'+
							'<td><select id="achievementTypeid">'+
							'<option value="0">Select</option><option value="National">National</option>'+
							'<option value="International">International</option>';
							if ($('#customerHelper').val() == "MEGHE"){
								addSkillBoiler += '<option value="School">School</option><option value="College">College</option><option value="Other">Other</option>';
							}
							
							addSkillBoiler += '</select></td>'+
						'</tr>'+
						
						'<tr>'+
						'<td><b>Achievement Proficiency</b> </td>'+
						'<td><select id="achievementproficiencyid">'+
						'<option value="0">Select</option><option value="Professional">Professional</option>'+
						'<option value="Academic">Academic</option>';
						
						if ($('#customerHelper').val() == "MEGHE"){
								addSkillBoiler += '<option value="Co-curricular">Co-curricular</option>';
							}
						addSkillBoiler += '</select></td>'+
					'</tr>'+
						
							'<tr>'+
								'<td><b>Year</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><select id="cmbAwardYrOfpass2" name="achieveYearName"></select></td>'+
							'</tr>'+
							'<tr>'+
								'<td></td>'+
								'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnAddNewStudCerti" onclick="saveNewStudAwardNew('+studAwardId+')"/>'+
								'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="viewAchievementsTab666()" /></td>'+
							'</tr>'+
							'</tbody>'+
							'</table>';
	
	
	$('#achievement').html(addSkillBoiler);
	//$('.validnum').numeric();
	
	var currentdate = new Date();
	var curYear = (currentdate.getFullYear() + 6);
	var yearListOf20 = '<option value="0">Select</option>';
	
	for ( var year = curYear; year > curYear - 60 ; year--)
		yearListOf20 += '<option value='+year+'>' + year + '</option>';
	$('#cmbAwardYrOfpass2').html(yearListOf20);
	
	if(indexId!="NEW"){
		$('#txtAwardName2').val(studAchievementArr[indexId]["awardName"]);
		$('#txtAwardDesc2').val(studAchievementArr[indexId]["description"]);
		$('#cmbAwardYrOfpass2').val(studAchievementArr[indexId]["year"]);
		if(studAchievementArr[indexId]["achievementType"]!=""){
		$('#achievementTypeid').val(studAchievementArr[indexId]["achievementType"]);
		}
		if(studAchievementArr[indexId]["achievementproficiency"]!=""){
			$('#achievementproficiencyid').val(studAchievementArr[indexId]["achievementproficiency"]);
		}
	}
}

function saveNewStudAwardNew(studAwardId){
	
	if(studAwardId==undefined){
		studAwardId='';
	}

	var awardName2=$('#txtAwardName2').val();
	var awardDesc2=$('#txtAwardDesc2').val();
	var awardPassYr2=$('#cmbAwardYrOfpass2').val();
	var achievementType = $('#achievementTypeid option:selected').val();
	var achievementproficiency = $('#achievementproficiencyid  option:selected').val();
	
	var cnt=0;
	if(awardName2==''){
		jAlert("Please enter award name");
		cnt=1;
	}
	else if(awardPassYr2==0){
		jAlert("Please select year of passing");
		cnt=1;
	}else if(awardDesc2==''){
		jAlert("Please enter award description");
		cnt=1;
	}
	
	
	if(cnt==0){
		$.ajax({
			url:'addStudentAward.json',
			data:{awardName2:awardName2,awardDesc2:awardDesc2,awardPassYr2:awardPassYr2,studAwardId:studAwardId,
				achievementType:achievementType,
				achievementproficiency:achievementproficiency
				},
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				viewAchievementsTab666();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function uploadStudAwardDocument(studAwardId){
	if(studAwardId != '' && studAwardId != 'null' && studAwardId != undefined){
		if(statusForAddingAchievement=='canChange' || statusForAddingAchievement=='ReqAccepted'){
			getDegreeCategoryId(gTabDegreeName.TabName_ACHIEVEMENT,studAwardId);
		}
		else if(statusForAddingAchievement=='ReqSent'){
			jAlert("The request you have sent for editing is pending");
		}else if(statusForAddingAchievement=='ReqNotSent'){
			$('#btnFreezeAchievement').show();
			jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r) {
				if(r ==true) {
					var tab="achievement";
					sendRequestForEditing(tab);
				}
			});
		}   
	} else
			jAlert('Student award record not found!');
	
}

//new achievement look ends here
var statusForAddingAchievement='';

function  columnColorFormatter(cellValue, opts, rowObject) {
	
	if(rowObject.status=="accepted")
		return "<img src='img/event/green dot.png' style='width:15px'/>";
	else if(rowObject.status=="pending")
		return "&nbsp;<img src='img/event/yellow dot.png'/>";
	else if(rowObject.status=="rejected")
		return "&nbsp;<img src='img/event/red dot.png'/>";
	else
		return "";
}


var gArray= new Array();
function getstudProfileConfigurationDetailsCheckingNames1(){

	$.ajax({
	url : 'getAllStudentProfileConfigurationDetails.json',
	dataType : 'json',
	type : 'GET',
	async : false,
	success : function(jArray) {
		gArray= jArray;
		getstudProfileConfigurationDetailsCompareNamesAndDisable1();
		
	},
	error : function() {
		 jAlert("Something went wrong, Please try after some time","Error");
	}
});
}

function getstudProfileConfigurationDetailsCompareNamesAndDisable1(){
	
	for (var i = 0; i < gArray.length; i++) {
		
//Certificate Details...................................................................
		if ((gArray[i]["isCheckedText"] == "certficateName") && ($('input[name = "certficateName"]')!=undefined) && (($('input[name = "certficateName"]').val())!=null) &&  ($.trim($('input[name = "certficateName"]').val())!="") && ($('input[name = "certficateName"]').val().length>0)) {
			$('input[name = "certficateName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "InstCompanyName") && ($('input[name = "InstCompanyName"]')!=undefined) && (($('input[name = "InstCompanyName"]').val())!=null) &&  ($.trim($('input[name = "InstCompanyName"]').val())!="") && ($('input[name = "InstCompanyName"]').val().length>0)) {
			$('input[name = "InstCompanyName"]').attr("disabled", true);
		}
		
		if ((gArray[i]["isCheckedText"] == "certYearOfPassingName") && ($('select[name = "certYearOfPassingName"]')!=undefined) && (($('select[name = "certYearOfPassingName"]').val())!=null)  && ($('select[name = "certYearOfPassingName"]').val() !="0")) {
			$('select[name = "certYearOfPassingName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "meritOtherName") && ($('input[name = "meritOtherName"]')!=undefined) && (($('input[name = "meritOtherName"]').val())!=null) &&  ($.trim($('input[name = "meritOtherName"]').val())!="") && ($('input[name = "meritOtherName"]').val().length>0)) {
			$('input[name = "meritOtherName"]').attr("disabled", true);
		}

		if ( (gArray[i]["isCheckedText"] == "certGradePercentageName") && ($('input[name = "certGradePercentageName"]')!=undefined) && (($('input[name = "certGradePercentageName"]').val())!=null) &&  ($.trim($('input[name = "certGradePercentageName"]').val())!="") && ($('input[name = "certGradePercentageName"]').val().length>0)) {
			$('input[name = "certGradePercentageName"]').attr("disabled", true);
		}

//Project Details.........................................................................................................................................................................................
		if ((gArray[i]["isCheckedText"] == "studProjectName") && ($('input[name = "studProjectName"]')!=undefined) && (($('input[name = "studProjectName"]').val())!=null) &&  ($.trim($('input[name = "studProjectName"]').val())!="") && ($('input[name = "studProjectName"]').val().length>0)) {
			$('input[name = "studProjectName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studProjectTypeName") && ($('select[name = "studProjectTypeName"]')!=undefined) && (($('select[name = "studProjectTypeName"]').val())!=null) && ($('select[name = "studProjectTypeName"]').val()!="0")) {
			$('select[name = "studProjectTypeName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studDegreeName") && ($('select[name = "studDegreeName"]')!=undefined) && (($('select[name = "studDegreeName"]').val())!=null) && ($('select[name = "studDegreeName"]').val()!="0")) {
			$('select[name = "studDegreeName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studClassYearName") && ($('select[name = "studClassYearName"]')!=undefined) && (($('select[name = "studClassYearName"]').val())!=null) && ($('select[name = "studClassYearName"]').val()!="-1")) {
			$('select[name = "studClassYearName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studDomainName") && ($('input[name = "studDomainName"]')!=undefined) && (($('input[name = "studDomainName"]').val())!=null) &&  ($.trim($('input[name = "studDomainName"]').val())!="") && ($('input[name = "studDomainName"]').val().length>0)) {
			$('input[name = "studDomainName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "projectTeamSizeName") && ($('input[name = "projectTeamSizeName"]')!=undefined) && (($('input[name = "projectTeamSizeName"]').val())!=null) &&  ($.trim($('input[name = "projectTeamSizeName"]').val())!="") && ($('input[name = "projectTeamSizeName"]').val().length>0)) {
			$('input[name = "projectTeamSizeName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studDurationInMonthName") && ($('input[name = "studDurationInMonthName"]')!=undefined) && (($('input[name = "studDurationInMonthName"]').val())!=null) &&  ($.trim($('input[name = "studDurationInMonthName"]').val())!="") && ($('input[name = "studDurationInMonthName"]').val().length>0)) {
			$('input[name = "studDurationInMonthName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "ProjectCompanyName") && ($('input[name = "ProjectCompanyName"]')!=undefined) && (($('input[name = "ProjectCompanyName"]').val())!=null) &&  ($.trim($('input[name = "ProjectCompanyName"]').val())!="") && ($('input[name = "ProjectCompanyName"]').val().length>0)) {
			$('input[name = "ProjectCompanyName"]').attr("disabled", true);
		}

		if ( (gArray[i]["isCheckedText"] == "proTechnologyName") && ($('input[name = "proTechnologyName"]')!=undefined) && (($('input[name = "proTechnologyName"]').val())!=null) &&  ($.trim($('input[name = "proTechnologyName"]').val())!="") && ($('input[name = "proTechnologyName"]').val().length>0)) {
			$('input[name = "proTechnologyName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "projectContributionName") && ($('input[name = "projectContributionName"]')!=undefined) && (($('input[name = "projectContributionName"]').val())!=null) &&  ($.trim($('input[name = "projectContributionName"]').val())!="") && ($('input[name = "projectContributionName"]').val().length>0)) {
			$('input[name = "projectContributionName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "ProjectResponsibilityName") && ($('input[name = "ProjectResponsibilityName"]')!=undefined) && (($('input[name = "ProjectResponsibilityName"]').val())!=null) &&  ($.trim($('input[name = "ProjectResponsibilityName"]').val())!="") && ($('input[name = "ProjectResponsibilityName"]').val().length>0)) {
			$('input[name = "ProjectResponsibilityName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "projectRemarkName") && ($('input[name = "projectRemarkName"]')!=undefined) && (($('input[name = "projectRemarkName"]').val())!=null) &&  ($.trim($('input[name = "projectRemarkName"]').val())!="") && ($('input[name = "projectRemarkName"]').val().length>0)) {
			$('input[name = "projectRemarkName"]').attr("disabled", true);
		}

//Work-Exp Details.................................................................................................................................................................................................................................................................
		if ((gArray[i]["isCheckedText"] == "workDesignationName") && ($('input[name = "workDesignationName"]')!=undefined) && (($('input[name = "workDesignationName"]').val())!=null) &&  ($.trim($('input[name = "workDesignationName"]').val())!="") && ($('input[name = "workDesignationName"]').val().length>0)) {
			$('input[name = "workDesignationName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "workCompanyEmpName") && ($('input[name = "workCompanyEmpName"]')!=undefined) && (($('input[name = "workCompanyEmpName"]').val())!=null) &&  ($.trim($('input[name = "workCompanyEmpName"]').val())!="") && ($('input[name = "workCompanyEmpName"]').val().length>0)) {
			$('input[name = "workCompanyEmpName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "workLocationName") && ($('input[name = "workLocationName"]')!=undefined) && (($('input[name = "workLocationName"]').val())!=null) &&  ($.trim($('input[name = "workLocationName"]').val())!="") && ($('input[name = "workLocationName"]').val().length>0)) {
			$('input[name = "workLocationName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "workTimePeriodName") && ($('input[name = "workTimePeriodName"]')!=undefined) && (($('input[name = "workT␀␀␀imePeriodName"]').val())!=null) &&  ($.trim($('input[name = "workTimePeriodName"]').val())!="") && ($('input[name = "workTimePeriodName"]').val().length>0)) {
			$('input[name = "workTimePeriodName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "workJobProfileName") && ($('input[name = "workJobProfileName"]')!=undefined) && (($('input[name = "workJobProfileName"]').val())!=null) &&  ($.trim($('input[name = "workJobProfileName"]').val())!="") && ($('input[name = "workJobProfileName"]').val().length>0)) {
			$('input[name = "workJobProfileName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "currentlyWorkingName") && ($('select[name = "currentlyWorkingName"]')!=undefined) && (($('select[name = "currentlyWorkingName"]').val())!=null)  && ($('select[name = "currentlyWorkingName"]').val()!="-1")) {
			$('select[name = "currentlyWorkingName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "workDurationFrom") && ($('input[name = "workDurationFrom"]')!=undefined) && (($('input[name = "workDurationFrom"]').val())!=null)  && ($('input[name = "workDurationFrom"]').val().length>0)) {
			$('input[name = "workDurationFrom"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "workDurationTo") && ($('input[name = "workDurationTo"]')!=undefined) && (($('input[name = "workDurationTo"]').val())!=null) && ($('input[name = "workDurationTo"]').val().length>0)) {
			$('input[name = "workDurationTo"]').attr("disabled", true);
		}
		
//Gap Details....................................................................................................................................................................................................................................................
		if ((gArray[i]["isCheckedText"] == "studenGapReason") && ($('input[name = "studenGapReason"]')!=undefined) && (($('input[name = "studenGapReason"]').val())!=null) &&  ($.trim($('input[name = "studenGapReason"]').val())!="") && ($('input[name = "studenGapReason"]').val().length>0)) {
			$('input[name = "studenGapReason"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studentGapFromYear") && ($('select[name = "studentGapFromYear"]')!=undefined) && (($('select[name = "studentGapFromYear"]').val())!=null) && ($('select[name = "studentGapFromYear"]').val()!="0")) {
			$('select[name = "studentGapFromYear"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "studentGapToYear") && ($('select[name = "studentGapToYear"]')!=undefined) && (($('select[name = "studentGapToYear"]').val())!=null) &&  ($('select[name = "studentGapToYear"]').val()!="0")) {
			$('select[name = "studentGapToYear"]').attr("disabled", true);
		}
		
//Training Details...................................................................................................................................................................................................................................
		if ((gArray[i]["isCheckedText"] == "studentTrainingName") && ($('input[name = "studentTrainingName"]')!=undefined) && (($('input[name = "studentTrainingName"]').val())!=null) &&  ($.trim($('input[name = "studentTrainingName"]').val())!="") && ($('input[name = "studentTrainingName"]').val().length>0)) {
			$('input[name = "studentTrainingName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "trainingCompInstiName") && ($('input[name = "trainingCompInstiName"]')!=undefined) && (($('input[name = "trainingCompInstiName"]').val())!=null) &&  ($.trim($('input[name = "trainingCompInstiName"]').val())!="") && ($('input[name = "trainingCompInstiName"]').val().length>0)) {
			$('input[name = "trainingCompInstiName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "trainingDetailsName") && ($('input[name = "trainingDetailsName"]')!=undefined) && (($('input[name = "trainingDetailsName"]').val())!=null) &&  ($.trim($('input[name = "trainingDetailsName"]').val())!="") && ($('input[name = "trainingDetailsName"]').val().length>0)) {
			$('input[name = "trainingDetailsName"]').attr("disabled", true);
		}

		if ( (gArray[i]["isCheckedText"] == "trainingYearName") && ($('select[name = "trainingYearName"]')!=undefined) && (($('select[name = "trainingYearName"]').val())!=null) && ($('select[name = "trainingYearName"]').val()!="0")) {
			$('select[name = "trainingYearName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "TrainingPerGradeName") && ($('input[name = "TrainingPerGradeName"]')!=undefined) && (($('input[name = "TrainingPerGradeName"]').val())!=null) &&  ($.trim($('input[name = "TrainingPerGradeName"]').val())!="") && ($('input[name = "TrainingPerGradeName"]').val().length>0)) {
			$('input[name = "TrainingPerGradeName"]').attr("disabled", true);
		}
		
//Achievements Details.........................................................................................................................................................................................................................................................
		if ((gArray[i]["isCheckedText"] == "achieveAwardName") && ($('input[name = "achieveAwardName"]')!=undefined) && (($('input[name = "achieveAwardName"]').val())!=null) &&  ($.trim($('input[name = "achieveAwardName"]').val())!="") && ($('input[name = "achieveAwardName"]').val().length>0)) {
			$('input[name = "achieveAwardName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "achieveDescriptionName") && ($('input[name = "achieveDescriptionName"]')!=undefined) && (($('input[name = "achieveDescriptionName"]').val())!=null) &&  ($.trim($('input[name = "achieveDescriptionName"]').val())!="") && ($('input[name = "achieveDescriptionName"]').val().length>0)) {
			$('input[name = "achieveDescriptionName"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "achieveYearName") && ($('select[name = "achieveYearName"]')!=undefined) && (($('select[name = "achieveYearName"]').val())!=null)  && ($('select[name = "achieveYearName"]').val()!="0")) {
			$('select[name = "achieveYearName"]').attr("disabled", true);
		}

		
	}
	
