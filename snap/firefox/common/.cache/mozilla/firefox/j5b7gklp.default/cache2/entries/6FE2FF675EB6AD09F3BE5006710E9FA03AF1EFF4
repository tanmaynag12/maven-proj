function elasticText(id){
	$("#"+id+"_company").elastic();
	$("#"+id+"_projectName").elastic();
	$("#"+id+"_responsibility").elastic();
	$("#"+id+"_contribution").elastic();
} 
function validYear(value)
{
	if (value == "select")
		return [ false, " Please select Year!!", "" ];
	else
		return [ true, "", "" ];
}

function validMonth(value)
{
	if (value == "select")
		return [ false, " Please select Month!!", "" ];
	else
		return [ true, "", "" ];
}


// new certificate look starts here
var studCertificateArr = new Array();
function viewStudCertificatesTab555(){

	$.ajax({
		url:'studentCertificates.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newCertiMsg" style="display: none;">'+
			'	<table>'+
			'	<tr id="trReqPendingCerti" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
			'	</tr>'+
			'	<tr id="trReqAcceptedCerti" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
			'	</tr>'+
			'	<tr id="trSendReqCerti" style="display: none;">'+
			'		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
			'	</tr>'+
			'</table>'+
			'</div>'+
			'<table class="table table-bordered table-condensed">'+
			'<tr><th style="color: #0088CC" colspan="6"><label><b><font>Student Certificates</font></b></label></th></tr>';
			boiler+='<tr><td colspan="6"><span style="float:right;"><a  class="btn btn-inverse btn-mini  btnColorBlack   freezeProfile" onclick="checkReqConditionForAddEditCerti(\'NEW\',\'\')" ><i class="fa fa-plus"></i>Add Certificate</a></span><span><input type="button" id="btnFreezeCertificate" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForAddingCertificate=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentCertificateId"]!=undefined){
						
						boiler+='<tr>'+
						'<th><b>Certification name</b></th>'+
						'<th><b>Institue/Company</b></th>'+
						'<th><b>Year of passing</b></th>'+
						'<th><b>Date Of Certification</b></th>'+
						'<th><b>Certificate Specialization</b></th>'+
						'<th><b>Certificate Keyskill</b></th>'+
						'<th colspan="3"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studCertificateArr=jArray;
						
							boiler+=	'<tr>'+
								'<td style="text-align: center;"><label>'+jArray[i]["certification"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["companyORInstitute"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["year"]+'</label></td>'+
						        '<td style="text-align: center;"><label>'+jArray[i]["dateOfCertification"]+'</label></td>'+
                                '<td style="text-align: center;"><label>'+jArray[i]["certificateSpec"]+'</label></td>'+
                                '<td style="text-align: center;"><label>'+jArray[i]["certificateKeyskill"]+'</label></td>'+

								'<td style="text-align: right;"><a onclick="checkReqConditionForAddEditCerti(\''+i+'\',\''+jArray[i]["studentCertificateId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
								'<td style="text-align: right;"><a onclick="checkReqForDelCerti('+jArray[i]["studentCertificateId"]+')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';
								
								boiler += '<td style="text-align: right;"><a onclick = "uploadStudCertiDocument(\''+jArray[i]["studentCertificateId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
								
								boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#certificates').html(boiler);
			
					}else{
						
						boiler+='</table>';
						$('#certificates').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForAddingCertificate="ReqNotSent";
					boiler+='</table>';
					$('#certificates').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForAddingCertificate="canChange";
					boiler+='</table>';
					$('#certificates').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForAddingCertificate=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#certificates').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForAddingCertificate=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#certificates').html(boiler);
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForAddingCertificate="canChange";
					boiler+='</table>';
					$('#certificates').html(boiler);
				}
			
				if(statusForAddingCertificate=='ReqNotSent'){
					$('#newCertiMsg').show();
					$('#btnFreezeCertificate').hide();
					$('#trSendReqCerti').show();
					$('#trReqAcceptedCerti').hide();
					$('#trReqPendingCerti').hide();
				}
				else if(statusForAddingCertificate=='ReqAccepted'){
					$('#newCertiMsg').show();
					$('#btnFreezeCertificate').show();
					$('#trReqAcceptedCerti').show();
					$('#trSendReqCerti').hide();
					$('#trReqPendingCerti').hide();
				}
				else if(statusForAddingCertificate=='ReqSent'){
					$('#newCertiMsg').show();
					$('#trReqPendingCerti').show();
					$('#trReqAcceptedCerti').hide();
					$('#trSendReqCerti').hide();
				}
				else if(statusForAddingCertificate=='canChange'){
					$('#trReqPendingCerti').hide();
					$('#trReqAcceptedCerti').hide();
					$('#trSendReqCerti').hide();
				}
				getstudProfileConfigurationDetailsCheckingNames1();
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
}

function checkReqConditionForAddEditCerti(indexId, studCertiId){
	//addNewProjectNew
	if(statusForAddingCertificate=="canChange"){
		addNewCertiNew(indexId, studCertiId);
	}
	else if(statusForAddingCertificate=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingCertificate=="ReqAccepted"){
		 $('#btnFreezeCertificate').show();
		 addNewCertiNew(indexId, studCertiId);
	}
	else if(statusForAddingCertificate=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="certificate";
					sendRequestForEditing(tab);
				}
					});
	}
	
	getstudProfileConfigurationDetailsCheckingNames1();	
}

function checkReqForDelCerti(studCertiId){
	
	if(statusForAddingCertificate=="canChange"){
		deleteStudCertiNew(studCertiId);
	}
	else if(statusForAddingCertificate=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingCertificate=="ReqAccepted"){
		 $('#btnFreezeCertificate').show();
		 deleteStudCertiNew(studCertiId);
	}
	else if(statusForAddingCertificate=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="certificate";
					sendRequestForEditing(tab);
				}
					});
	}
}

function deleteStudCertiNew(studCertiId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudCertificates.json?certificateId='+studCertiId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						viewStudCertificatesTab555();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}

function addNewCertiNew(indexId, studCertiId){

	var addSkillBoiler='';
	
		addSkillBoiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
								'<th colspan="2" style="color: #0088CC"><h4>Please Enter your Certificate</h4></th>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Certificate Name</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" name="certficateName" id="txtCertiName2" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>'+juno.labelByCustomer('l.SchoolInstitute')+'/Company </b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" name="InstCompanyName" id="txtCertiInstComp2" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Year of passing</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><select id="cmbCertiYrOfpass2" name="certYearOfPassingName"></select></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Date Of Certification </b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtCertificationDate" readonly="readonly"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Certificate Specialization</b></td>'+
								'<td><input type="text" id="txtCertSpeci" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Certificate Keyskill</b></td>'+
								'<td><input type="text" id="txtCertKeySkill"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Merit/Other</b></td>'+
								'<td><input type="text" name="meritOtherName" id="txtCertMerit2" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Grade percentage</b></td>'+
								'<td><input type="text" name="certGradePercentageName" id="txtCertGrade2" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td></td>'+
								'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen" id="btnAddNewStudCerti" onclick="saveNewStudCertiNew('+studCertiId+')"/>'+
								'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed" onclick="viewStudCertificatesTab555()" /></td>'+
							'</tr>'+
							'</tbody>'+
							'</table>';
	
	
	$('#certificates').html(addSkillBoiler);
	//$('.validnum').numeric();
	
	var currentdate = new Date();
	var curYear = (currentdate.getFullYear() + 6);
	var yearListOf20 = '<option value="0">Select</option>';
	
	for ( var year = curYear; year > curYear - 60 ; year--)
		yearListOf20 += '<option value='+year+'>' + year + '</option>';
	$('#cmbCertiYrOfpass2').html(yearListOf20);
	
	if(indexId!="NEW"){
		$('#txtCertiName2').val(studCertificateArr[indexId]["certification"]);
		$('#txtCertiInstComp2').val(studCertificateArr[indexId]["companyORInstitute"]);
		$('#cmbCertiYrOfpass2').val(studCertificateArr[indexId]["year"]);
		$('#txtCertMerit2').val(studCertificateArr[indexId]["remark"]);
		$('#txtCertGrade2').val(studCertificateArr[indexId]["scoreGrade"]);
		$('#txtCertificationDate').val(studCertificateArr[indexId]["dateOfCertification"]);
		$('#txtCertSpeci').val(studCertificateArr[indexId]["certificateSpec"]);
		$('#txtCertKeySkill').val(studCertificateArr[indexId]["certificateKeyskill"]);
	}
	
	$( "#txtCertificationDate" ).datepicker({
		dateFormat : 'M d,yy',
		showWeek : true,
		changeMonth : true,
		changeYear : true,
		yearRange : "-80:+0",
		firstDay : 1,
		//maxDate : new Date(currentYear, CurrentMonth, CurrentDate)
	});
}

function saveNewStudCertiNew(studCertiId){
	
	if(studCertiId==undefined){
		studCertiId='';
	}

	var certiName2=$('#txtCertiName2').val();
	var caerInstComp2=$('#txtCertiInstComp2').val();
	var certPassYr2=$('#cmbCertiYrOfpass2').val();
	var certMerit2=$('#txtCertMerit2').val();
	var certGrade2=$('#txtCertGrade2').val();
	var certDate =$('#txtCertificationDate').val()!=null ?$('#txtCertificationDate').val():"";
	var CertSpeci=$('#txtCertSpeci').val()!=null ?$('#txtCertSpeci').val():"";
	var CertKeySkill =$('#txtCertKeySkill').val()!=null ?$('#txtCertKeySkill').val():"";
	if(certDate==''){
		jAlert("Please enter date of certification ");
		return;
	}
	var cnt=0;
	if(certiName2==''){
		jAlert("Please enter certificate name");
		cnt=1;
	}
	else if(caerInstComp2==''){
		jAlert("Please enter institute/company");
		cnt=1;
	}
	else if(certPassYr2==0){
		jAlert("Please select year of passing");
		cnt=1;
	}
	if(cnt==0){
		$.ajax({
			url:'addStudentCertificates.json',
			data:{certiName2:certiName2,caerInstComp2:caerInstComp2,certPassYr2:certPassYr2,certMerit2:certMerit2,certGrade2:certGrade2,studCertiId:studCertiId,certDate:certDate,
			CertSpeci:CertSpeci,CertKeySkill:CertKeySkill},
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				viewStudCertificatesTab555();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function uploadStudCertiDocument(studCertId){
	if(studCertId != '' && studCertId != 'null' && studCertId != undefined){
		if(statusForAddingCertificate=='canChange' || statusForAddingCertificate=='ReqAccepted'){
			getDegreeCategoryId(gTabDegreeName.TabName_CERTIFICATION,studCertId);
		}
		else if(statusForAddingCertificate=='ReqSent'){
			jAlert("The request you have sent for editing is pending");
		}else if(statusForAddingCertificate=='ReqNotSent'){
			$('#btnFreezeCertificate').show();
			jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r) {
				if(r ==true) {
					var tab="certificate";
					sendRequestForEditing(tab);
				}
			});
		}   
	} else
			jAlert('Student Certificate record not found!');
	
}

// new certificate look ends here

var statusForAddingCertificate='';

//new project look starts
var studProjectArr = new Array();
function showProjTabDetailsNew333(){
	var universityName = $('#universityName').val();
	$.ajax({
		url:'studentProjects.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newProjMsg">'+
				'<table>'+
					'<tr id="trReqPendingProj" style="display: none;">'+
					'	<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
					'</tr>'+
					'<tr id="trReqAcceptedProj" style="display: none;">'+
					'	<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
					'</tr>'+
					'<tr id="trSendReqProj" style="display: none;">'+
					'	<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
					'</tr>'+
				'</table>'+
				'</div>'+
				'<table class="table table-bordered table-condensed">'+
			'<tr><th style="color: #0088CC" colspan="9"><label><b><font>Student Projects</font></b></label></th></tr>';
			boiler+='<tr><td colspan="9"><span style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack   freezeProfile" onclick="checkReqConditionForAddEditProject(\'NEW\',\'\')" ><i class="fa fa-plus"></i> Add Project</a></span><span><input type="button" id="btnFreezeProject" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForAddingProject=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentProjectUndertakenId"]!=undefined){
						
						boiler+='<tr>'+
						'<th><b>Project Name</b></th>'+
						'<th><b>Project Type</b></th>';
						if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
							boiler+='<th><b>Qualification Name</b></th>';
						}
						if (universityName != undefined && universityName == "D.Y. Patil Edu. Soc. Deemed University"){
							boiler+='<th><b>Subject</b></th>';
						}else{
							boiler+='<th><b>Domain</b></th>';
						}
						boiler+='<th><b>Team Size</b></th>'+
						'<th><b>Duration (in months)</b></th>';
						if (universityName != undefined && universityName == "D.Y. Patil Edu. Soc. Deemed University"){
							boiler+='<th><b>Guide Name</b></th>';
						}else{
							boiler+='<th><b>Mentor Name</b></th>';
						}
						if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
							boiler+='<th><b>Project Keyskill</b></th>'+
							'<th><b>Project Description </b></th>';
						}
						boiler+='<th colspan="3"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studProjectArr=jArray;
							boiler+=	'<tr>'+
								'<td><label>'+jArray[i]["projectName"]+'</label></td>'+
								'<td><label>'+jArray[i]["projectTypeName"]+'</label></td>';
							
								if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
									if(jArray[i]["DegreeName"]!=undefined){
										boiler+='<td><label>'+jArray[i]["DegreeName"]+'</label></td>';
									}
									else{
										boiler+='<td><label>-</label></td>';
									}
								}
									
								boiler+='<td><label>'+jArray[i]["domain"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["teamSize"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["duration"]+'</label></td>'+
								'<td style="text-align: center;"><label>'+jArray[i]["mentorName"]+'</label></td>';
								if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
									boiler+='<td style="text-align: center;"><label>'+jArray[i]["prjKeyskill"]+'</label></td>'+
									'<td style="text-align: center;"><label>'+jArray[i]["prjDescription"]+'</label></td>';
								}
								boiler+='<td style="text-align: right;"><a onclick="checkReqConditionForAddEditProject(\''+i+'\',\''+jArray[i]["studentProjectUndertakenId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
								'<td style="text-align: right;"><a onclick="checkReqForDelProject('+jArray[i]["studentProjectUndertakenId"]+')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';
								
								boiler += '<td style="text-align: right;"><a onclick = "uploadStudProjDocument(\''+jArray[i]["studentProjectUndertakenId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
								
								boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#projects').html(boiler);
			
					}else{
						//boiler+='<tr><td colspan="5"><span><input type="button" style="float:right;"value="+ Add Skill" class="btn btn-inverse btn-mini  " onclick="checkReqConditionForAddEdit(\'\',\'\',\'\')" /></span></td></tr>';
						//boiler+='<tr>';
						boiler+='</table>';
						$('#projects').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForAddingProject="ReqNotSent";
					boiler+='</table>';
					$('#projects').html(boiler);
					/*if()
						postGraduationDetailsPart2('ADD',degreeType);*/
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForAddingProject="canChange";
					boiler+='</table>';
					$('#projects').html(boiler);
					/*if()
						postGraduationDetailsPart2('ADD',degreeType);*/
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForAddingProject=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#projects').html(boiler);
					/*if()
						postGraduationDetailsPart2('ADD',degreeType);*/
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForAddingProject=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#projects').html(boiler);
					/*if()
						postGraduationDetailsPart2('ADD',degreeType);*/
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForAddingProject="canChange";
					boiler+='</table>';
					$('#projects').html(boiler);
				}
			
				if(statusForAddingProject=='ReqNotSent'){
					$('#newProjMsg').show();
					$('#btnFreezeProject').hide();
					$('#trSendReqProj').show();
					$('#trReqAcceptedProj').hide();
					$('#trReqPendingProj').hide();
				}
				else if(statusForAddingProject=='ReqAccepted'){
					$('#newProjMsg').show();
					$('#btnFreezeProject').show();
					$('#trReqAcceptedProj').show();
					$('#trSendReqProj').hide();
					$('#trReqPendingProj').hide();
				}
				else if(statusForAddingProject=='ReqSent'){
					$('#newProjMsg').show();
					$('#trReqPendingProj').show();
					$('#trReqAcceptedProj').hide();
					$('#trSendReqProj').hide();
				}
				else if(statusForAddingProject=='canChange'){
					$('#trReqPendingProj').hide();
					$('#trReqAcceptedProj').hide();
					$('#trSendReqProj').hide();
				}
				
				getstudProfileConfigurationDetailsCheckingNames1();
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
}

function checkReqConditionForAddEditProject(indexId, studProjId){
	
	//addNewProjectNew
	if(statusForAddingProject=="canChange"){
		addNewProjectNew(indexId, studProjId);
	}
	else if(statusForAddingProject=="ReqSent"){
			jAlert("The request you have sent for editing is pending");s
	}
	else if(statusForAddingProject=="ReqAccepted"){
		 $('#btnFreezeProject').show();
		 addNewProjectNew(indexId, studProjId);
	}
	else if(statusForAddingProject=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="project";
					sendRequestForEditing(tab);
				}
					});
	}
	getstudProfileConfigurationDetailsCheckingNames1();
}

function checkReqForDelProject(studProjId){
	
	if(statusForAddingProject=="canChange"){
		deleteStudProjectNew(studProjId);
	}
	else if(statusForAddingProject=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingProject=="ReqAccepted"){
		 $('#btnFreezeProject').show();
		 deleteStudProjectNew(studProjId);
	}
	else if(statusForAddingProject=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="project";
					sendRequestForEditing(tab);
				}
					});
	}
}

function deleteStudProjectNew(studProjId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteNewStudentProjectNew.json?studProjId='+studProjId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						showProjTabDetailsNew333();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}

function saveNewStudProjNew(projectUndertakenId){
	
	if(projectUndertakenId==undefined){
		projectUndertakenId='';
	}
	
	var projectName=$('#txtProjName').val();
	var projectType=$('#cmbProjType').val();
	var degreeName=$('#cmbDegname2').val();
	var semOrYear=$('#cmbSemYr2').val();
	var domain=$('#txtDomainName').val();
	var responsibility=$('#txtResponsibilty').val();
	var company=$('#txtCompanyName').val();
	var teamSize=$('#txtTeamSize2').val();
	var contribution=$('#txtContribution2').val();
	var duration=$('#txtDurationInMonth').val();
	var remarks=$('#txtRemarks').val();
	var technology=$('#txtTechnology2').val();
	var mentorName=$('#txtMentorName').val()!=null ?$('#txtMentorName').val():"";
	var prjKeyskill=$('#txtPrjKeyskill').val()!=null ?$('#txtPrjKeyskill').val():"";
	var prjDesc=$('#txtPrjDesc').val()!=null ?$('#txtPrjDesc').val():"";
	var statusOfIntro = $('input[name = "statusOfIntro"]:checked').val();
	var reviewOfLiterature = $('input[name = "reviewOfLiterature"]:checked').val();
	var methodology = $('input[name = "methodology"]:checked').val();
	var result = $('input[name = "result"]:checked').val();
	var discussion = $('input[name = "discussion"]:checked').val();
	var conclusion = $('input[name = "conclusion"]:checked').val();
	var summary = $('input[name = "summary"]:checked').val();
	
	var cnt=0;
	if(projectName==''){
		jAlert("Please enter Project name");
		cnt=1;
	}
	else if(projectType==0){
		jAlert("Please select Project type");
		cnt=1;
	}
	else if(degreeName==0){
		jAlert("Please select Qualification name");
		cnt=1;
	}
	else if(semOrYear==-1){
		jAlert("Please select Class/Year");
		cnt=1;
	}
	else if(domain==''){
		jAlert("Please enter Domain");
		cnt=1;
	}
	else if(teamSize==''){
		jAlert("Please enter team size");
		cnt=1;
	}
	else if(duration==''){
		jAlert("Please enter duration in months");
		cnt=1;
	}
	else if(isNaN(duration)){
		jAlert("Please enter duration in numbers only");
		cnt=1;
	}
	
	if(cnt==0){
		$.ajax({
			url:'addStudentProjects.json',
			data:{projectName:projectName,company:company,technology:technology,domain:domain,responsibility:responsibility,projectType:projectType,degreeName:degreeName,semOrYear:semOrYear,teamSize:teamSize,contribution:contribution,duration:duration,remarks:remarks,projectUndertakenId:projectUndertakenId,
			mentorName:mentorName,prjKeyskill:prjKeyskill,prjDesc:prjDesc,statusOfIntro:statusOfIntro,reviewOfLiterature:reviewOfLiterature,methodology:methodology,result:result,discussion:discussion,conclusion:conclusion,summary:summary},
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				showProjTabDetailsNew333();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function addNewProjectNew(indexId, studProjId){
	var universityName = $('#universityName').val();
	var addSkillBoiler='';
	
		addSkillBoiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
							'<th colspan="2" style="color: #0088CC"><h4>Please Enter your project</h4></th>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Project Name <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><input type="text" name="studProjectName" id="txtProjName" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Project Type <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><select id="cmbProjType" name="studProjectTypeName"><option value="0">Select</option></select></td>'+
							'</tr>';
							if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
								addSkillBoiler += '<tr>'+
									'<td><b>Qualification Name <b style="color:red;font-size:16px;" >*</b></b></td>'+
									'<td><select id="cmbDegname2" name="studDegreeName"><option value="0">Select</option></select></td>'+
									'</tr>';
							}
							addSkillBoiler += '<tr>'+
								'<td><b>Class/Year <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><select id="cmbSemYr2" name = "studClassYearName">'+
								'<option value="-1">Select</option>'+
								'<option value="1">1</option>'+
								'<option value="2">2</option>'+
								'<option value="3">3</option>'+
								'<option value="4">4</option>'+
								'<option value="5">5</option>'+
								'<option value="6">6</option>'+
								'<option value="7">7</option>'+
								'<option value="8">8</option>'+
								'<option value="9">9</option>'+
								'<option value="10">10</option>'+
								'</select></td>'+
							'</tr>';
							if (universityName != undefined && universityName == "D.Y. Patil Edu. Soc. Deemed University"){
								addSkillBoiler +='<tr>'+
									'<td><b>Subject <b style="color:red;font-size:16px;" >*</b></b></td>'+
									'<td><input type="text" id="txtDomainName" name="studDomainName" /></td>'+
									'</tr>';
							}else{
								addSkillBoiler +='<tr>'+
									'<td><b>Domain <b style="color:red;font-size:16px;" >*</b></b></td>'+
									'<td><input type="text" id="txtDomainName" name="studDomainName" /></td>'+
									'</tr>';
							}
							if (universityName != undefined && universityName != "D.Y. Patil Edu. Soc. Deemed University"){
								addSkillBoiler +='<tr>'+
									'<td><b>Team Size <b style="color:red;font-size:16px;" >*</b></b></td>'+
									'<td><input type="text" name="projectTeamSizeName" id="txtTeamSize2" class="validnum" /></td>'+
									'</tr>';
							}
							addSkillBoiler +='<tr>'+
								'<td><b>Duration (in months) <b style="color:red;font-size:16px;" >*</b></b></td>'+
								'<td><input type="text" name="studDurationInMonthName" id="txtDurationInMonth" class="validnum" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Company</b></td>'+
								'<td><input type="text" id="txtCompanyName" name="ProjectCompanyName"/></td>'+
							'</tr>';
							if (universityName != undefined && universityName == "D.Y. Patil Edu. Soc. Deemed University"){
							    addSkillBoiler += '<tr>' +
							        '<td><b>Project Status</b></td>' +
							        '<td>' +
							            '<input name="statusOfIntro" type="checkbox" id="statusIntro" value="statusOfIntroduction" /> Status Of Introduction ' +
							            '<input name="reviewOfLiterature" type="checkbox" id="reviewOfLiter" value="reviewOfLiterature" /> Review of Literature <br>' +
							            '<input name="methodology" type="checkbox" id="methodolog" value="methodology" /> Methodology &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' +
							            '<input name="result" type="checkbox" id="results" value="result" /> Result <br>' +
							            '<input name="discussion" type="checkbox" id="discussions" value="discussion" /> Discussion &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;' +
							            '<input name="conclusion" type="checkbox" id="conclusions" value="conclusion" /> Conclusion <br>' +
							            '<input name="summary" type="checkbox" id="summarys" value="summary" /> Summary ' +
							        '</td>' +
							        '</tr>';
							}

							if (universityName != undefined && universityName == "D.Y. Patil Edu. Soc. Deemed University"){
								addSkillBoiler +='<tr>'+
									'<td><b>Guide Name </b></td>'+
