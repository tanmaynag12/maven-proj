function SaveEmpProfileImg2()
	{
		var hdnUserRole = $('#hdnUserRole').val();
		var url = 'checkProfileImage.json';
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_checkProfileImage.json';
		}
		$.ajax({
				url:url,
				dataType:'json',
				async:false,			
				success:function(jArray){	
					if(jArray['isImage'] == false){		
						var boiler=	'<div class="thumbnail" id="empProfileImage2342" style="height:120px;width:120px;margin-left:20px"><img src="img/boy.gif" alt="Photo" name="Photo" style="background-color: #FFFFFF" width="120px" height="120px" border="1" align="right"><br></div>';
							boiler+='&nbsp&nbsp&nbsp&nbsp&nbsp<div class="btn btn-info" style="width:110px;height:20px">Upload Image<input style="margin-top:-40px;margin-left:-35px;opacity:0;" type="file" name="fileData" id="profilePictureUpload" onclick="empProfilePic();" accept="image/*"></div>';	
							$('#profileImgModalBodyDiv').html(boiler);
							$('#profileImgModal').modal('show');
					}
				},
				error:function(){
				}
			});
	}
	
	function base64ToBlob(base64, mime) 
{
    mime = mime || '';
    var sliceSize = 1024;
    var byteChars = window.atob(base64);
    var byteArrays = [];

    for (var offset = 0, len = byteChars.length; offset < len; offset += sliceSize) {
        var slice = byteChars.slice(offset, offset + sliceSize);

        var byteNumbers = new Array(slice.length);
        for (var i = 0; i < slice.length; i++) {
            byteNumbers[i] = slice.charCodeAt(i);
        }

        var byteArray = new Uint8Array(byteNumbers);

        byteArrays.push(byteArray);
    }

    return new Blob(byteArrays, {type: mime});
}
	
	function empProfilePic(){
	var Maxsize =  5242880;	
	$image_crop = $('#image_demo').croppie({
		enableExif: true,
		enableOrientation: true,
		viewport: {
			width:200,
			height:200,
			type:'square' //circle
		},
		boundary:{
			width:300,
			height:300
		}
	});

	$('#profilePictureUpload').on('change', function(){
		var reader = new FileReader();
		reader.onload = function (event) {
			$image_crop.croppie('bind', {
				url: event.target.result
			}).then(function(){
				console.log('jQuery bind complete');
			});
		}
		reader.readAsDataURL(this.files[0]);
		$('#uploadimageModal').modal('show');
	});

	$(function() {
		$('.vanilla-rotate').on('click', function(ev) {
			$image_crop.croppie('rotate', parseInt($(this).data('deg')));
		});
	});

	$('.crop_image').click(function(event){
		$image_crop.croppie('result', {
			type: 'canvas',
			size: 'viewport'
		}).then(function(response){
			console.log(response);
			var base64ImageContent = response.replace(/^data:image\/(png|jpg);base64,/, "")
			var blob = base64ToBlob(base64ImageContent, 'image/png');                
			var formData = new FormData();
			formData.append('fileData', blob);
			$.ajax({
				url:"uploadEmployeeProfileImage.json",
				type: "POST",
				contentType: false,
				processData: false,
				data: formData,
			name : 'fileData',
			size: Maxsize,
			onSubmit : function(data) {
				//if(extension.toString().toLowerCase() == 'png' || extension.toString().toLowerCase() == 'jpg',extension.toString().toLowerCase() == 'jpeg'){
					startLoading();
					this.setData({
						'fileSize':Maxsize
					});
			},
			success:function(data)
			{
				$('#uploadimageModal').modal('hide');

				if(isNaN(data)){
					//showErrorMessage('File size is larger than specified',10000);
					jAlert("File size is larger than specified !!");
				} else {
					var imageBoiler = 
							'<img src="getEmployeeProfileImage.json?type=profile&someparam='+data+'" alt="Photo" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" />';
 
					$('#empProfileImage2342').html(imageBoiler);	
					jAlert("Image uploaded successfully !!!");
					imageStatus = true;
					showSuccessMessage('Image uploaded. ',10000); 
					$('#profileImgModal').modal('hide');
				}
				$('#uploaded_image').html(data);
				$image_crop.croppie('destroy');
			}
		});
});
	
});
}	
