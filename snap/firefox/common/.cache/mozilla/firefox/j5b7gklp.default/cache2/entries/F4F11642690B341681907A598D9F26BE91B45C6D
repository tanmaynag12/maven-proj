$(document).ready(function(){
	var person = $('#hdnUserRole').val();
	if(person!=undefined){
		flushSession();
		if(person.toLowerCase() != 'student' && person != 'alumnistudent'){
			person = 'Employee';
		} else if(person == 'alumnistudent'){
			person = 'Student';
		}
		initUserPostDocument(person);
	}
});

function initUserPostDocument(person){
	var aluminiRingPostId = '';
	$('document').ready(function() {
		try{
			new AjaxUpload('aluminiUploadButton',{
				action : 'attachFileWithAluminiPost.json',
				data : {},
				name : 'fileData',
				size:AllowedFileSize.DOCUMENT_UPLOAD,
				onSubmit : function(file, extension) {
					startLoading();
					this.setData({
						'aluminiRingPostId':aluminiRingPostId,
						'fileSize':AllowedFileSize.DOCUMENT_UPLOAD
					});
				},
				onComplete:function(file, response) {
					stopLoading();
					var jArray = eval('(' + response + ')');
					showAluminiPostAttachment(jArray,'showAluminiPostUploadDiv',true,person);
				//	refreshTemporaryAttachmentList(aluminiRingPostId,AllowedFileSize.DOCUMENT_UPLOAD);
					
				}
			});
			}catch(e){
				
			}
	});
}

function getAluminiPostAttachement(aluminiRingPostId,callback){
	getDocumentCategoryByAluminiPostId(aluminiRingPostId,function(categoryId){
		var uploadDocument = new UploadDocument();
		uploadDocument.getDocument(categoryId, function(jArray){
			showQuotationAttachment(jArray,undefined,true);
			callback(jArray);
		});
	});
}

function getDocumentCategoryByAluminiPostId(aluminiRingPostId,callback){
	$.ajax({
		url:'getDocumentCategoryByAluminiPostId.json',
		dataType:'text',
		data:'aluminiRingPostId='+aluminiRingPostId,
		async:false,
		success:function(categoryId){
			callback(categoryId);
		}
	});
}


function showAluminiPostAttachment(jArray,tableId,isCreator,person, thumbnailFlag = ''){
	var attachementBoiler = '';
	if(thumbnailFlag=='thumbnail-ForTesting'){
		attachementBoiler=`<div style="overflow-y: auto;overflow-x: hidden;padding-bottom:70px;"><hr style="width:90%;"/><span style="">${jArray.length} Attachments</span><br/>`;
		jArray.forEach(function(jObj, i){
			var attachmentFileName = jArray[i]["name"].toString(); 
			var documentId = jArray[i]["documentId"];
			var encDocumentId = jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];
			var isTemporary = jArray[i]["isTemporary"];
			var documentType = jArray[i]["documentType"];
			var trId = 'trAttachmentFile-'+encDocumentId+i;

			var fileType = jArray[i]["type"];
			iconPath = gFileTypeIcon[fileType];
			
			var thumbnailUrl = juno.label['url.download.json'].setIndexedArgs(encDocumentId, "THUMBNAIL", encodeURIComponent(attachmentFileName));
			var doc_url = juno.label['url.download.json'].setIndexedArgs(encDocumentId, "UPLOADED", encodeURIComponent(attachmentFileName));
			var li_boiler = '';
			var click_event = '';
			if(!isTemporary){
				
				if(documentType == 'uploaded'){
					click_event = `junoDocumentOrImageViewer('${encDocumentId}','${fileType.toLowerCase()}');`;
					li_boiler+=`<li><a href="javascript:void(0)" onclick="junoDocumentOrImageViewer('${encDocumentId}','${fileType.toLowerCase()}');"><i class="fa fa-eye fa-lg"></i> View</a></li>`;
					li_boiler+=`<li><a href="${doc_url}" target="_blank"><i class="fa fa-download fa-lg"></i> Download</a></li>`;
				} else {
					li_boiler+=`<li><a href="javascript:void(0)" onclick="showTextDocumentForAlumini('${encDocumentId}','${person}');"><i class="fa fa-download fa-lg"></i> Download</a></li>`;
				}

				if(isCreator){
					li_boiler+=`<li><a href="javascript:void(0)" onclick="removeAttachmentAjax('${trId}','${encDocumentId}','saved');"><i class="fa fa-trash fa-lg"></i> Remove</a></li>`;
				}

				if(!isCreator){
					li_boiler+=`<li><a id = "stdbtn-${encDocumentId}" documenttype="${documentType}" person="${person}" class="btn-stdbtn" title="Save to drive" href="javascript:void(0)"><i class="fa fa-hdd-o fa-lg"></i> Save to drive</a></li>`;
				}
				
			}else{

			}
			attachementBoiler += `<div id="${trId}" class="custom-section-box2"
				style="margin:5px;display:initial;padding:10px;float:left;width:190px;border: 1px solid #BFBFBF;box-shadow: 2px 2px 5px #aaaaaa;overflow: initial;">
				<div style="height:100px;overflow:hidden;border: 1px solid #BFBFBF;box-shadow: 0px 0px 5px 1px #aaaaaa;cursor:pointer;">
					<img src="${thumbnailUrl}" style="width:100%;" onclick="${click_event}"/>
				</div>
				
				<div class="btn-group pull-right" style="margin-left:10px;text-transform: none;font-weight:normal;margin-top: 5px;">
                      <span data-toggle="dropdown" style="cursor: pointer;padding:0px 10px;" title="Settings"><i class="fa fa-ellipsis-v fa-lg" aria-hidden="true"></i></span>
                      <ul class="dropdown-menu">${li_boiler}</ul>
                </div>
                <div class="text-overflow-ellipsis"><img src="${iconPath}" style="height:10px;"/> ${attachmentFileName}</div>
			</div>`;
		});
		attachementBoiler+=`<br/><hr style="width:90%;"/></div>`;
	}else{
		attachementBoiler = "<table class = 'table'>";

		for(var i = 0 ; i < jArray.length; i++){
			var attachmentFileName = jArray[i]["name"].toString(); 
			var documentId = jArray[i]["documentId"];
			var encDocumentId = jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];


			var isTemporary = jArray[i]["isTemporary"];
			var documentType = jArray[i]["documentType"];
			//attachmentFileName = attachmentFileName.substring(attachmentFileName.lastIndexOf('/')+1);
			//var fileName = attachmentFileName.replace(/ /g,'~').trim();
			var trId = 'trAttachmentFile-'+encDocumentId+i;

			var fileType = jArray[i]["type"];
			iconPath = gFileTypeIcon[fileType];

			attachementBoiler += 
				'<tr id = "'+trId+'"><td><img src="'+iconPath+'" style="height:30px;"/>&nbsp;&nbsp;&nbsp;&nbsp;'+attachmentFileName+'</td>';

			if(!isTemporary){
				if(documentType == 'uploaded'){
					attachementBoiler += 
						'<td>'+(juno.label['a.download.json'].setIndexedArgs(encDocumentId, "UPLOADED", encodeURIComponent(attachmentFileName), '<i class="fa fa-download fa-lg"></i>'))+'&nbsp;';
				} else {
					attachementBoiler += 
						'<td><a class="btn btn-mini" title="Download" href = "javascript:void(0)" onclick="showTextDocumentForAlumini(\''+encDocumentId+'\',\''+person+'\')"><i class="fa fa-download fa-lg"></i></a>&nbsp;';
				}	

				if(isCreator){
					attachementBoiler += '<a class="btn btn-mini" href = "javascript:void(0)" title = "Remove" onclick="removeAttachmentAjax(\''+trId+'\',\''+encDocumentId+'\',\'saved\')"><i class="fa fa-trash fa-lg"></i></a>&nbsp;'; 
				}

				if(!isCreator){
					attachementBoiler += '<span id = "stdbtn-'+encDocumentId+'" documenttype="'+documentType+'" person="'+person+'" class="btn btn-mini btn-stdbtn" title="Save to drive"><i class="fa fa-hdd-o fa-lg"></i></span>&nbsp';
				}
				var availableFilesArr = ['doc','docx','ppt','pptx','xls','xlsx','jpg','png', 'gif', 'jpeg'];
				if(fileType != undefined && (availableFilesArr.indexOf(fileType.toLowerCase())!=-1)){
					attachementBoiler += '<a class="btn btn-mini docListOpt btnDocOpts" href="javascript:void(0);" title="Preview" onclick="junoDocumentOrImageViewer(\''+encDocumentId+'\',\''+fileType.toLowerCase()+'\');"><i class="fa fa-eye fa-lg"></i></a>';
				}
				//if(fileType.toLowerCase()=='doc' || fileType.toLowerCase()=='docx' || fileType.toLowerCase()=='ppt' || fileType.toLowerCase()=='pptx' || fileType.toLowerCase()=='xls' || fileType.toLowerCase()=='xlsx')



			} else {
				attachementBoiler += 
					'<td><a class="btn btn-mini" href = "javascript:void(0)" title = "Remove" src = "img/remove-icon.png" onclick = "removeAttachmentAjax(\''+trId+'\',\''+encDocumentId+'\',\'temporary\',\''+documentType+'\')"><i class="fa fa-trash fa-lg"></i></a>&nbsp;&nbsp;<i class="fa fa-exclamation-triangle fa-lg juno-text-warining" title = "Unsaved"></i>';
			}
			attachementBoiler += '</td>';
			attachementBoiler +=  '</tr>';

			//alert(attachementBoiler);
		}
		attachementBoiler += "</table>";
	}
	if(tableId == undefined)
		$('#showQuotationAttachment').html(attachementBoiler);
	else
		$('#'+tableId).html(attachementBoiler);
	//alert(person);
	$('.btn-stdbtn').unbind();
	$('.btn-stdbtn').bind('click',function(){
		var elementId = $(this).attr('id');
		var documentType = $(this).attr('documenttype');
		var documentId = elementId.split('-')[1];
		new DriveTransfer({
			elementId:elementId,
			documentId:documentId,
			documentType:documentType,
			person:person,
			success:function(responseObject){
				var documentId = responseObject.documentId;
				jAlert("");
				getCustomAlertMessage('alert_create');
				//alert(newDocumentId);
			},
		});
	});
	//alert(jArray[i]);
	//alert("tableId: "+tableId);
}

/**
*
* @param trId
* @param documentId
* @param docuementType Temporary/Saved
*/
function removeAttachmentAjax(trId,documentId,saveOrTemp,documentType){
	if(saveOrTemp == 'saved'){
		removeUploadedDocumentById(documentId,function(jObject){
			$('#'+trId).remove();
		});
	} else {
		removeTemporaryAttachmentAjax(documentId,documentType,function(){
			$('#'+trId).remove();
			//refreshTemporaryAttachmentList();
		});
	}
}

function refreshTemporaryAttachmentList(aluminiRingPostId,fileSize)
{
	try {
		$.ajax({
			url : 'attachFileWithAluminiPost.json',
			dataType : 'json',
			data:{aluminiRingPostId:aluminiRingPostId,fileSize:fileSize},
			type:'POST',
			success : function(jArray) {
				//populateTemporaryAttachmentList(jArray);
			},
		});
	} catch (e) {

	}
}

/**
documentId is index of the session array where documents have been kept 
 */
function removeTemporaryAttachmentAjax(documentId,documentType,callback){
	try{
		$.ajax({
			url:'removeTemporaryAluminiPostAttachment.json',
			dataType:'json',
			type:'POST',
			data:{d__documentId:documentId,documentType:documentType},
			success:function(){
				try{
					callback();
				}catch(e){}
				
			}	
		});
	}catch(e){
		alert(e);
	}
}


function flushSession(){
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'flushSessionForAluminiPost.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_flushSessionForAluminiPost.json';
	}
	$.ajax({
		url:url,
		type:'POST',
		success:function(){
			
		}
	});
}

function initShowAddDocumentToAluminiPostFromDMSDialog(){
	new AttachFromDMS({
		onSubmit:function(documentJSONStringified){
			//alert(documentJSONStringified.length);
			insertAndShowSelectedFileToAluminiPost(documentJSONStringified,'Employee');
		},
	});
}

function initStudentShowAddDocumentToAluminiPostFromDMSDialog(){
	$("script[src='js/jquery.dataTables.js']").remove();
	$("script[src='js/DT_bootstrap.js").remove();
	$("script[src='js/paging.js").remove();
	$("link[href='css/DT_bootstrap.css']").remove();
	$.getScript( "js/jquery.dataTables.js" );
    $.getScript( "js/DT_bootstrap.js" );
    $.getScript( "js/paging.js" );
    $.getScript( "css/DT_bootstrap.css" );
	new AttachFromDMSForStudent({
		onSubmit:function(documentJSONStringified){
			//alert(documentJSONStringified.length);
			insertAndShowSelectedFileToAluminiPost(documentJSONStringified,'Student');
		},
	});
}


function insertAndShowSelectedFileToAluminiPost(documentJSONStringified,person){
	insertSelectedFilesToAluminiPost(documentJSONStringified,function(jArray){
		showAluminiPostAttachment(jArray,'showAluminiPostUploadDiv',true,person);
	});		
}
function insertSelectedFilesToAluminiPost(documentJSONStringified,callback){
	$.ajax({
		url:'insertSelectedFilesToAluminiPost.json',
		dataType:'json',
		type:'POST',
		data:{documentIdsWithTypeAndName:documentJSONStringified,targetCategoryId:''},
		success:function(jArray){
			callback(jArray);				
		},error:function(){
			
		}
	});
}

function showTextDocumentForAlumini(documentId,person){
	var object = null;
	
	if(person.toLowerCase() == 'employee'){
		object = new UploadDocument();
	} else {
		object = new UploadStudentDocument();
	}
	object.getTextDocumentWithAttachment(documentId,function(jObject){
		var boiler = '<table class="table table-bordered table-condensed">'+
		'<tr>'+
			'<td>';
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="INDIRA_PUNE" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DES")
			{
				boiler+='<div style="height:300px; width:100%; overflow: auto;" id="editing_field1">'+decodeURIComponent(jObject["documentBody"])+'</div>';
			}else{
				boiler+='<div style="height:300px; width:100%; overflow: auto;" id="editing_field1">'+jObject["documentBody"]+'</div>';
			}
			boiler+='</td>'+
		'</tr>'+
		'<tr><td><div class = "label label-info" style = "font-size:14px;height:15px">Description:</div></td></tr>'+
		'<tr>'+
			'<td>';
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="INDIRA_PUNE" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DES")
			{
				boiler+='<div style="height:200px; width:100%; overflow: auto;" id="editing_field2">'+decodeURIComponent(jObject["description"])+'</div>';
			}else{
				boiler+='<div style="height:200px; width:100%; overflow: auto;" id="editing_field2">'+jObject["description"]+'</div>';
			}
			boiler+='</td>'+
		'</tr>'+
		'<tr><td><div class = "label label-info" style = "font-size:14px;height:15px">Attachments:</div></td></tr>'+
		'<tr>'+
		'<td>'+
		'<table width = "100%" class = "table"  id = "tblAttachementList"></table>'+
		'</td>'+
		'</tr>'+
		'</table>';
		$('#showTextDocumentDialog').find('.documentBody').html(boiler);
		//createEditorForAlumini('editing_field1');
		//CKEDITOR.instances.editing_field1.setData(jObject["documentBody"]);
		$('#showTextDocumentDialog').find('.documentName').html(jObject["name"]);
		object.getAttachmentListAjax(documentId,function(jArray){
			object.showDocumentAttachmentList(jArray,'tblAttachementList',documentId,false);
			$('#showTextDocumentDialog').modal('show');
		});
	});
}


function createEditorForAlumini(id)
{
	var instance = CKEDITOR.instances[id];
	if(instance)
	{
		/*if($('#'+id).val()=='')
			return;
		CKEDITOR.remove(instance);*/
		instance.destroy(true);
	}
	CKEDITOR.replace(id,
			{uiColor: '#ffffff',
		width: "95%",
		height: "50%",
		toolbarCanCollapse:false,
		removePlugins: 'toolbar'
	});
