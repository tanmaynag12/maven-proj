(function($) {
	/*var fileTypes = [ 'doc', 'docx', 'xls', 'xlsx', 'pdf', 'jpg', 'text',
	                        'png', 'gif', 'ppt', 'zip','pptx'];*/
	$.fn.extend({
		uploadDocument: function(options={}){
			const { uploadButtonLabel='Upload File' } = options;
			var currDom = $(this);
			var dom_id = currDom.attr('id');
			//SessionKey = CommonUploadFilesSession+divId
			
			$.flushDocumentsUploads(dom_id);
			
			currDom.html(`<div>
			<div class="upload-btn-wrapper" style="width:200px;">
			<button class="btn btn-info full-upload-btn" type="button"><i class="fa fa-upload" aria-hidden="true"></i> ${uploadButtonLabel}</button>
			<input type="file" id="${dom_id}_fileUploadBtn" name="fileData" accept="application/pdf"/>
				</div>
			<div id="${dom_id}_savedFileList" class="uploadFileList_common"></div>
			<div id="${dom_id}_uploadedFileList" class="uploadFileList_common"></div>
			</div>`);


			$.junoDocumentInit(dom_id, options);
			return currDom;
		},
		
		DmsViewDocuments: function(options={}){
			$(this).html($.DmsViewDocuments(options));
		}
		
	});

	$.junoDocumentInit = function(divId, options={}){
		const {
			limit=10, 
					fileTypes=[ 'doc', 'docx', 'xls', 'xlsx', 'pdf', 'jpg', 'text',
					            'png', 'gif', 'ppt', 'zip','pptx'],
					showDefaultDocumentList=true,
					success,
					crop=false,
					resource,
					resourceId,
					documentList=[],
					onChange,
					fileSizeLimitInBytes=AllowedFileSize.DOCUMENT_UPLOAD
		} = options;
		
		if(onChange!=undefined){
			$('#'+divId).data('fileArr', []);
			$('#'+divId).data('call_on_change', onChange);
		}
		
		var commonSessionName = divId;
		if(documentList.length>0){
			$('#'+divId+'_savedFileList').DmsViewDocuments({
				data: documentList,
				isCreator:true,
				person:''
			});
		}
		if(resource!=undefined && resourceId!=undefined) {
			$.getDocumentsByResouce({
				eleId:divId, 
				resourse:resource, 
				resourseId:resourceId, 
				isCreator:true
			});
		}
		
		$.ajax('flushCommonAttachFileFromSession.json?sessionName='+commonSessionName);
		if(crop){

			$image_crop = $('#image_demo').croppie({
				enableExif: true,
				enableOrientation: true,
				viewport: {
					width:200,
					height:200,
					type:'square' //circle
				},
				boundary:{
					width:300,
					height:300
				}
			});

			var name='#'+divId+'_fileUploadBtn';
			$(name).on('change', function(){
				var reader = new FileReader();
				reader.onload = function (event) {
					$image_crop.croppie('bind', {
						url: event.target.result
					}).then(function(){
						console.log('jQuery bind complete');
					});
				}

				console.log(this);
				reader.readAsDataURL(this.files[0]);
				$('#uploadimageModal').modal('show');


			});
			$image_crop.croppie('destroy');
			$('.crop_image').click(function(event){
				$image_crop.croppie('result', {
					type: 'canvas',
					size: 'viewport'
				}).then(function(response){
					console.log(response);
					var base64ImageContent = response.replace(/^data:image\/(png|jpg);base64,/, "")
							var blob = base64ToBlob(base64ImageContent, 'image/png');                
					var formData = new FormData();
					formData.append('fileData', blob);
					new AjaxUpload({
						url:"attachFileInSession.json",
						type: "POST",
						contentType: false,
						processData: false,
						data: formData,
						name : 'fileData',
						size: Maxsize,
						onSubmit : function(data) {
							//if(extension.toString().toLowerCase() == 'png' || extension.toString().toLowerCase() == 'jpg',extension.toString().toLowerCase() == 'jpeg'){
							startLoading();
							this.setData({
								'fileSize':Maxsize,
								sessionName:commonSessionName
							});
						},
						success:function(data,response)
						{
							$('#uploadimageModal').modal('hide');


							var jArray = JSON.parse(response);
							if(showDefaultDocumentList){
								//$.junoDocumentShowUploadedAttachmentList(jArray, divId+'_uploadedFileList',true,'', commonSessionName);
								$('#'+divId+'_uploadedFileList').DmsViewDocuments({
									data: jArray,
									isCreator:true,
									person:'',
									commonSessionName:commonSessionName
								});
							}else if(success!=undefined){
								success(jArray, commonSessionName);
							}
							//$('#uploaded_image').html(data);
							$image_crop.croppie('destroy');
						}
					});

				});
			});
		}
		else
		{
			new AjaxUpload(divId+'_fileUploadBtn',{
				action : '/restApi/attachFileInSession.json',
				data : {},
				name : 'fileData',
				size: fileSizeLimitInBytes,
				onSubmit : function(file, extension) {

					if(($('#'+divId+'_savedFileList tr').length + $('#'+divId+'_uploadedFileList tr').length)>=limit){
						jAlert('Only '+limit+' File(s) are Allowed to Upload!');
						return false;
					}else if(fileTypes.includes(extension.toString().toLowerCase())){
						this.setData({
							'limit':limit,
							'fileSize': fileSizeLimitInBytes,
							sessionName:commonSessionName
						});
					} else {
						jAlert('File Format not supported ! \n Please use '+fileTypes.join(', ')+' file types only');
						return false;
					}
				},
				onComplete:function(file, response) {
					var jArray = JSON.parse(response);
					if(showDefaultDocumentList){
						//$.junoDocumentShowUploadedAttachmentList(jArray, divId+'_uploadedFileList',true,'', commonSessionName);
						$('#'+divId+'_uploadedFileList').DmsViewDocuments({
							data: jArray,
							isCreator:true,
							person:'',
							commonSessionName:commonSessionName
						});
					}else if(success!=undefined){
						success(jArray, commonSessionName);
					}
					
					if(onChange!=undefined){
						onChange(jArray);
					}
				}
			});
		}
	}
	
	$.junoDocumentOrImageViewer = function(docId, fileType, isFullScreenView=false){
		var link = (juno.label['url.download.json'].setIndexedArgs(docId, "UPLOADED", ""));
		juno.documentPreview(link, fileType, isFullScreenView);
	}
	
	$.junoDocumentOrImageViewer2 = function(docId, fileType, targetViewId=''){
		var link = (juno.label['url.download.json'].setIndexedArgs(docId, "UPLOADED", ""));
		
		var frame_width = (($(document).width())-100)+'px';
		var frame_height = '500px';
		var imgFilesArr = ['jpg','png', 'gif', 'jpeg'];
		var officeFilesArr = ['xls','xlsx', 'doc', 'docx','ppt', 'pptx'];
		
		var _encLink = encodeURIComponent(window.location.origin+'/'+link);
		var boiler = '';
		if(fileType != undefined && (imgFilesArr.indexOf(fileType.toLowerCase())!=-1)){
			boiler = '<img src="'+link+'"/>';
		}else if(fileType != undefined && (fileType.toLowerCase()=='pdf')){
			boiler ='<iframe style="width:100%;height:500px;" src="Viewer.js/index.html#../'+link+'&f=.pdf" allowfullscreen webkitallowfullscreen/>';
		}else if(fileType != undefined && (officeFilesArr.indexOf(fileType.toLowerCase())!=-1)){
			boiler = '<iframe src="https://view.officeapps.live.com/op/embed.aspx?src='+_encLink+'" style="width:100%; height:800px;" frameborder="0"></iframe>';
		}else{
			boiler = '<iframe src="https://docs.google.com/gview?url='+_encLink+'&embedded=true" style="width:100%; height:800px;" frameborder="0"></iframe>';
		}
		
		$(targetViewId).html(boiler);
	}
	
	$.DmsViewDocuments = function(options={}){
		const {
			data=[],
			isCreator=false,
			person,
			commonSessionName='',
			isDownload=true,
			viewFullScreen=false,
			targetViewId
		} = options;
		
		if(commonSessionName!='' && $('#'+commonSessionName).length>0){
			$('#'+commonSessionName).data('fileArr', data);
		}
		var attachementBoiler = "<table class = 'table attachment-table-cls nonPrintable' data-commonSessionName='"+commonSessionName+"'><tbody>";
		data.forEach((jObj, i)=>{
			var {
				name:attachmentFileName='',
				documentId,
				eDocId,
				isTemporary=false,
				documentType='UPLOADED',
				type:fileType='txt',
			} = jObj;
			
			var encDocumentId = eDocId!=undefined?eDocId:documentId;
			var trId = 'trAttachmentFile-'+encDocumentId;
			var iconPath = gFileTypeIcon[fileType];
			
			attachementBoiler += 
				'<tr id = "'+trId+'" class="commonDmsAttachTr nonPrintable" >\
				<td style="width: 210px;" class="nonPrintable"><div class="warp-text-and-expand-on-hover"><img src="'+iconPath+'" style="height:30px;margin-right:10px;"/>'+attachmentFileName+'</div></td>';

			attachementBoiler += '<td style="width: 90px;" class="nonPrintable">';
			if(!isTemporary){
				if(documentType == 'uploaded' || documentType=='UPLOADED'){
					if(isDownload) {
					attachementBoiler += 
						''+(juno.label['a.download.json'].setIndexedArgs(encDocumentId, "UPLOADED", encodeURIComponent(attachmentFileName), '<i class="fa fa-download fa-lg"></i>'))+'&nbsp;';
					}else {
						attachementBoiler +='&nbsp;';
					}
				} else if('PATH'==documentType){
					attachementBoiler+=`<a href="downloadWithPath.json?id=${encDocumentId}&n=${encodeURIComponent(attachmentFileName)}&t=${fileType}" target="_blank"><i class="fa fa-download fa-lg"></i></a>`;
				}else {
					attachementBoiler += 
						'<a class="btn btn-mini" title="Download" href = "javascript:void(0)" onclick="showTextDocumentForAlumini(\''+encDocumentId+'\',\''+person+'\')"><i class="fa fa-download fa-lg"></i></a>&nbsp;';
				}
				if(isCreator){
					attachementBoiler += '<a class="btn btn-mini" href = "javascript:void(0)" title = "Remove" onclick="$.junoDocumentRemoveAttachmentAjax(this,\''+encDocumentId+'\',\'saved\', '+i+')"><i class="fa fa-trash fa-lg"></i></a>&nbsp;'; 
				}
				if(!isCreator){
					//attachementBoiler += '<span id = "stdbtn-'+encDocumentId+'" documenttype="'+documentType+'" person="'+person+'" class="btn btn-mini btn-stdbtn" title="Save to drive"><i class="fa fa-hdd-o fa-lg"></i></span>&nbsp';
				}
				
				var availableFilesArr = ['pdf', 'doc','docx','ppt','pptx','xls','xlsx','jpg','png', 'gif', 'jpeg'];
				if(fileType != undefined && (availableFilesArr.indexOf(fileType.toLowerCase())!=-1)){
					if(targetViewId!=undefined) {
						attachementBoiler += '<a class="btn btn-mini docListOpt btnDocOpts docPreviewBtnCls" href="javascript:void(0);" title="Preview" onclick="$.junoDocumentOrImageViewer2(\''+encDocumentId+'\',\''+fileType.toLowerCase()+'\', \''+targetViewId+'\');"><i class="fa fa-eye fa-lg nonPrintable"></i></a>';
					}else {
						attachementBoiler += '<a class="btn btn-mini docListOpt btnDocOpts" href="javascript:void(0);" title="Preview" onclick="$.junoDocumentOrImageViewer(\''+encDocumentId+'\',\''+fileType.toLowerCase()+'\', '+viewFullScreen+');"><i class="fa fa-eye fa-lg nonPrintable"></i></a>';
					}
				}
			} else {

				var downloadLink22 = (juno.label['a.download.json'].setIndexedArgs(encDocumentId, "TEMP", encodeURIComponent(attachmentFileName)+'&sessVar=CommonUploadFilesSession'+commonSessionName, '<i class="fa fa-download fa-lg"></i>'));
				attachementBoiler += 
					'<a class="btn btn-mini" \
					href = "javascript:void(0)" title = "Remove" src = "img/remove-icon.png" \
					onclick = "$.junoDocumentRemoveAttachmentAjax(this,\''+encDocumentId+'\',\'temporary\','+i+')"><i class="fa fa-trash fa-lg"></i></a>\
					&nbsp;&nbsp;'+downloadLink22+'&nbsp;&nbsp;\
					<i class="fa fa-exclamation-triangle fa-lg juno-text-warining" title = "Unsaved"></i>';
			}
			attachementBoiler += '</td>';
			attachementBoiler +=  '</tr>';	
			
		});
		
		
		if(data.length==0) 
			attachementBoiler+= `<i class="fa fa-exclamation-triangle"></i> No Documents Available!`;
			
		attachementBoiler += "</tbody></table>";
		
		
		return attachementBoiler;	
		
	}
	
	$.junoDocumentShowUploadedAttachmentList = function(jArray,tableId,isCreator,person, commonSessionName, isDownload=true){
		var attachementBoiler = "<table class = 'table attachment-table-cls' data-commonSessionName='"+commonSessionName+"'>";
		for(var i = 0 ; i < jArray.length; i++){
			var attachmentFileName = jArray[i]["name"].toString(); 
			var documentId = jArray[i]["documentId"];
			var encDocumentId = jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];
			var isTemporary = jArray[i]["isTemporary"];
			var documentType = jArray[i]["documentType"];
			var trId = 'trAttachmentFile-'+encDocumentId;

			var fileType = jArray[i]["type"];
			iconPath = gFileTypeIcon[fileType];

			attachementBoiler += 
				'<tr id = "'+trId+'" class="commonDmsAttachTr"><td><img src="'+iconPath+'" style="height:30px;"/>&nbsp;&nbsp;&nbsp;&nbsp;'+attachmentFileName+'</td>';

			if(!isTemporary){
				if(documentType == 'uploaded'){
					if(isDownload) {
					attachementBoiler += 
						'<td  class="nonPrintable">'+(juno.label['a.download.json'].setIndexedArgs(encDocumentId, "UPLOADED", encodeURIComponent(attachmentFileName), '<i class="fa fa-download fa-lg"></i>'))+'&nbsp;';
					}else {
						attachementBoiler += 
								'<td class="nonPrintable">&nbsp;';
					}
				} else {
					attachementBoiler += 
						'<td  class="nonPrintable"><a class="btn btn-mini" title="Download" href = "javascript:void(0)" onclick="showTextDocumentForAlumini(\''+encDocumentId+'\',\''+person+'\')"><i class="fa fa-download fa-lg"></i></a>&nbsp;';
				}
				if(isCreator){
					attachementBoiler += '<a class="btn btn-mini" href = "javascript:void(0)" title = "Remove" onclick="$.junoDocumentRemoveAttachmentAjax(this,\''+encDocumentId+'\',\'saved\', '+i+')"><i class="fa fa-trash fa-lg"></i></a>&nbsp;'; 
				}
				if(!isCreator){
					//attachementBoiler += '<span id = "stdbtn-'+encDocumentId+'" documenttype="'+documentType+'" person="'+person+'" class="btn btn-mini btn-stdbtn" title="Save to drive"><i class="fa fa-hdd-o fa-lg"></i></span>&nbsp';
				}
				var availableFilesArr = ['pdf', 'doc','docx','ppt','pptx','xls','xlsx','jpg','png', 'gif', 'jpeg'];
				if(fileType != undefined && (availableFilesArr.indexOf(fileType.toLowerCase())!=-1)){
					attachementBoiler += '<a class="btn btn-mini docListOpt btnDocOpts" href="javascript:void(0);" title="Preview" onclick="$.junoDocumentOrImageViewer(\''+encDocumentId+'\',\''+fileType.toLowerCase()+'\');"><i class="fa fa-eye fa-lg"></i></a>';
				}
			} else {

				var downloadLink22 = (juno.label['a.download.json'].setIndexedArgs(encDocumentId, "TEMP", encodeURIComponent(attachmentFileName)+'&sessVar=CommonUploadFilesSession'+commonSessionName, '<i class="fa fa-download fa-lg"></i>'));
				attachementBoiler += 
					'<td  class="nonPrintable">\
					<a class="btn btn-mini" \
					href = "javascript:void(0)" title = "Remove" src = "img/remove-icon.png" \
					onclick = "$.junoDocumentRemoveAttachmentAjax(this,\''+encDocumentId+'\',\'temporary\','+i+')"><i class="fa fa-trash fa-lg"></i></a>\
					&nbsp;&nbsp;'+downloadLink22+'&nbsp;&nbsp;\
					<i class="fa fa-exclamation-triangle fa-lg juno-text-warining" title = "Unsaved"></i>';
			}
			attachementBoiler += '</td>';
			attachementBoiler +=  '</tr>';

			//alert(attachementBoiler);
		}
		if(jArray.length==0) attachementBoiler+= `<i class="fa fa-exclamation-triangle"></i> No Documents Available!`;
		attachementBoiler += "</table>";
		if(tableId == undefined && $('#showQuotationAttachment').length!=0)
			$('#showQuotationAttachment').html(attachementBoiler);
		else if(tableId != undefined)
			$('#'+tableId).html(attachementBoiler);
		
		return attachementBoiler;	
	}

	function removeFileFromDataFileArray(index=-1, commonSessionName=''){
		try{
			if(index>=0 && commonSessionName!='' && $('#'+commonSessionName).length>0){
				$('#'+commonSessionName).data('fileArr').splice(index, 1);
				const call_on_change = $('#'+commonSessionName).data('call_on_change').
				call_on_change($('#'+commonSessionName).data('fileArr'));
			}
		}catch(e){
			console.log(e);
		}
	}
	
	
	$.junoDocumentRemoveAttachmentAjax = function(thisDom,documentId,saveOrTemp,arrIndex){
		if(saveOrTemp == 'saved'){
			removeUploadedDocumentById(documentId,function(jObject){
				$(thisDom).closest('tr.commonDmsAttachTr').remove();
			});
		} else {
			var commonSessionName = $(thisDom).closest('table.attachment-table-cls').attr('data-commonSessionName');
			$.ajax({
				url:'flushCommonAttachFileFromSession.json?d__docId='+documentId+'&sessionName='+commonSessionName,
				type:'POST',
				success:function(txt){
					if(txt=='done'){
						$(thisDom).closest('tr.commonDmsAttachTr').remove();
						
						try{
							const call_on_change = $('#'+commonSessionName).data('call_on_change');
							if(call_on_change!=null){
								call_on_change('TEsting');
							}
						}catch(e){
							console.log(e);
						}
					}
				}
			});
		}
	}
	
	
	
	$.getDocumentsByResouce = function(options){
		var promise=new Promise(function (resolve,reject){
			const {eleId, resourse, resourseId, isCreator=false, isDownload=true} = options;
			$('#'+eleId+'_uploadedFileList').html('');
			$.ajax({
				url:'getallDocumentsByResourseAndResourseId.json',
				dataType:'json',
				data:{resId:resourseId,
					res:resourse
				},
				success:function(jArray){
					//$.junoDocumentShowUploadedAttachmentList(jArray, eleId+'_savedFileList',isCreator,'', eleId, isDownload);
					$('#'+eleId+'_savedFileList').DmsViewDocuments({
						data: jArray,
						isCreator:isCreator,
						person:'',
						commonSessionName:eleId,
						isDownload:isDownload
					});
					resolve(jArray);
				}
			});	
		});
		return promise;
	}
	
	$.flushDocumentsUploads = function(divId)
	{
		$('#'+divId+'_uploadedFileList').html('');
		$.ajax('flushCommonAttachFileFromSession.json?sessionName='+divId);
	}
