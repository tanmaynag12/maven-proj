var gRoasterIdArray = new Array();
var gDepartmentName = new Array();
var gBatchSemester  = new Array();
var gDepartmentId = new Array();
function replaceAll(txt, replace, with_this){
	return txt.replace(new RegExp(replace, 'g'),with_this);
}
/**
 * @author jignesh vaghela
 *	@name showStudentShare()
