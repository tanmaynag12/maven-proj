function deleteComment(documentThreadId,documentType,counter,documentTypeId)
{
	$.ajax({
		url:'deleteCommentAjax.json?documentThreadId='+documentThreadId+'&documentType='+documentType+'&counter='+counter,
		dataType:'json',
		success:function(jArray){

			$('#'+documentType+'-'+documentTypeId+'-'+counter).remove().fadeIn('slow');

		},
		error:function(){

		}
	});
}

function deletePrivateComment(documentPrivateBucketThreadId,documentType,counter,documentTypeId)
{
	$.ajax({
		url:'deletePrivateCommentAjax.json?documentPrivateBucketThreadId='+documentPrivateBucketThreadId,
		dataType:'json',
		success:function(jArray){

			$('#private-'+documentType+'-'+documentTypeId+'-'+counter).remove().fadeIn('slow');

		},
		error:function(){

		}
	});
}

/**
 * @author jignesh vaghela
 *	@name getSharedFiles()
 *	@since 3rd May 2012
 */
function getSharedFiles()
{
	$.ajax({
		url:'getSharedTextFilesAjax.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(jArray.length > 0)
			{
				gjArrTextDocument = jArray;
				showTextDocumentIcons();
			}

		},
		error:function(){

		}
	});

	$.ajax({
		url:'getSharedUploadedFilesAjax.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			if(jArray.length > 0)
			{
				gjArrUploadedDocument = jArray;
				showUploadedDocumentIcon();
			}
		},
		error:function(){

		}
	});
}

function showSharedEmployeeList(documentId,documentType,inputid)
{
	$('#trshowComment'+inputid).hide();
	$('#trshowStudentShare'+inputid).hide();
	$('#trshowPrivateComment'+inputid).hide();

	var employeeShare='';
	var doctypeId='';
	var studsize='';
	var empsize='';
	employeeShare='<div id="viewSharedEmployeesAndStudents'+inputid+'"">'+
	
	'<h3><a href="#viewStudentSharedList'+inputid+'">View Shared Students</a></h3>';


	employeeShare+='<div id="viewStudentSharedList'+inputid+'">';

	$.ajax({
		url:'showSharedStudentListAjax.json?d__documentId='+documentId+'&documentType='+documentType,
		dataType:'json',
		async:false,
		success:function(jArray){

			var size=jArray[0].length;
			var alumcnt=0;
			if(jArray[0].length>0)
			{
					employeeShare+='<div class="navbar navbar-static"><div class="navbar-inner">'+
		            '<div class="container" style="width: auto;">'+
		            	'<a class="brand" href="#">Shared Students</a><ul class="nav">'+
		            '<li class="dropdown">'+
		              '<a href="#" class="dropdown-toggle" data-toggle="dropdown">Add Private Comment <b class="caret"></b></a>'+
		              '<ul class="dropdown-menu">';	
					for(var i=0; i<jArray[1].length; i++)
						employeeShare+='<li><a href="javascript:void(0)" onclick="addPrivateChat('+jArray[1][i]["documentPrivateBucketId"]+','+documentId+',\''+documentType+'\',\'student\')">'+jArray[1][i]["threadName"]+'</a></li>';
					
						employeeShare+='<li id="createNewThread" class="divider"></li>';
						if(jArray[0][0]["owner"]=='yes')
							employeeShare+='<li><a href="javascript:void(0)" onclick="newThread('+documentId+',\''+documentType+'\',\'student\')">Create New Thread</a></li>';
					
					employeeShare+= '  </ul>'+
		            ' </li>'+
		           '</ul></div></div><table style="width:100%; margin-left:0%; margin-right:0%;">'+
					'<tr><th colspan="5">Shared Students<a href="javascript:void(0);" style="float:right;" onclick="javascript:$(\'#trshowEmployeeShare'+inputid+'\').hide();"><img src="img/remove.png" /></a></th></tr><tr>';
					
						for(var i=0; i<size; i++)
						{
							if((size-1)==i)
							{
								if(documentType=='textfile')
									doctypeId=jArray[0][i]["documentBucketId"];
								else
									doctypeId=jArray[0][i]["documentPathBucketId"];
								
								if(jArray[0][i]["profileImageStatus"]=='notdefined')
									employeeShare+='<td colspan="'+(5-alumcnt)+'" style="width:150px;"><div class="members shadow" id="sharedStud'+i+'"><img onmouseover="showPhotoDialog(\'studimg_'+i+'\',\''+jArray[0][i]["studentId"]+'\',\'student\')" id="studimg_'+i+'" src="'+jArray[0][i]["profileImage"]+'"  width="50px" height="50px" />';
								else
									employeeShare+='<td colspan="'+(5-alumcnt)+'" style="width:150px;"><div class="members shadow" id="sharedStud'+i+'"><img onmouseover="showPhotoDialog(\'studimg_'+i+'\',\''+jArray[0][i]["studentId"]+'\',\'student\')" id="studimg_'+i+'" src="getStudentProfileImageById.json?id='+jArray[0][i]["studentId"]+'"  width="50px" height="50px" />';
								
								employeeShare+='<label class="labelCircle">'+jArray[0][i]["studentName"]+'</label><input type="checkbox" class="sharedEmp" value="sharedEmp'+i+'" name="'+jArray[0][i]["employeeId"]+'" /></div></td>';
							}
							else
							{
								if(alumcnt=4)
								{
									alumcnt=0;
									employeeShare+='</tr><tr>';
								}
									if(documentType=='textfile')
										doctypeId=jArray[0][i]["documentBucketId"];
									else
										doctypeId=jArray[0][i]["documentPathBucketId"];
			
									if(jArray[0][i]["profileImageStatus"]=='notdefined')
										employeeShare+='<td style="width:150px;"><div class="members shadow" id="sharedStud'+i+'"><img onmouseover="showPhotoDialog(\'studimg_'+i+'\',\''+jArray[0][i]["studentId"]+'\',\'student\')" id="studimg_'+i+'" src="'+jArray[0][i]["profileImage"]+'"  width="50px" height="50px" />';
									else
										employeeShare+='<td style="width:150px;"><div class="members shadow" id="sharedStud'+i+'"><img onmouseover="showPhotoDialog(\'studimg_'+i+'\',\''+jArray[0][i]["studentId"]+'\',\'student\')" id="studimg_'+i+'" src="getStudentProfileImageById.json?id='+jArray[0][i]["studentId"]+'"  width="50px" height="50px" />';
									
									employeeShare+='<label class="labelCircle">'+jArray[0][i]["studentName"]+'</label><input type="checkbox" class="sharedEmp" value="sharedEmp'+i+'" name="'+jArray[0][i]["employeeId"]+'" /></div></td>';
									
									alumcnt++;
							}
						}
					employeeShare+='</tr></table></div>';
			}
		},
		error:function(){

		}
	});


	//employeeShare+='</div><h3><a href="#viewEmployeeSharedList'+inputid+'">View Shared Employees</a></h3><div id="viewEmployeeSharedList'+inputid+'">';

	$.ajax({
		url:'showSharedEmployeeListAjax.json?d__documentId='+documentId+'&documentType='+documentType,
		dataType:'json',
		async:false,
		success:function(jArray){

			var size=jArray[0].length;
			empsize=jArray.length;
			
			var alumcnt=0;
			if(jArray[0].length>0)
			{
				employeeShare+='<div class="navbar navbar-static"><div class="navbar-inner">'+
	                '<div class="container" style="width: auto;">'+
	            '<a class="brand" href="#">Shared Employees</a><ul class="nav">'+
	                '<li class="dropdown">'+
	                  '<a href="#" class="dropdown-toggle" data-toggle="dropdown">Add Private Comment <b class="caret"></b></a>'+
	                  '<ul class="dropdown-menu">';
				for(var i=0; i<jArray[1].length; i++)
					employeeShare+='<li><a href="javascript:void(0)" onclick="addPrivateChat('+jArray[1][i]["documentPrivateBucketId"]+','+documentId+',\''+documentType+'\',\'employee\')">'+jArray[1][i]["threadName"]+'</a></li>';
				
					employeeShare+='<li id="createNewThread" class="divider"></li>';
					if(jArray[0][0]["owner"]=='yes')
						employeeShare+='<li><a href="javascript:void(0)" onclick="newThread('+documentId+',\''+documentType+'\',\'employee\')">Create New Thread</a></li>';
				
					employeeShare+= '  </ul>'+
	               ' </li>'+
	              '</ul></div></div><table style="width:100%; margin-left:0%; margin-right:0%;">'+
				'<tr><th colspan="5">Shared Employees<a href="javascript:void(0);" style="float:right;" onclick="javascript:$(\'#trshowEmployeeShare'+inputid+'\').hide();"><img src="img/remove.png" /></a></th></tr><tr>';
				
					for(var i=0; i<size; i++)
					{
						if((size-1)==i)
						{
							if(documentType=='textfile')
								doctypeId=jArray[0][i]["documentBucketId"];
							else
								doctypeId=jArray[0][i]["documentPathBucketId"];
							
							if(jArray[0][i]["profileImageStatus"]=='notdefined')
								employeeShare+='<td colspan="'+(5-alumcnt)+'" style="width:150px;"><div class="members shadow" id="sharedEmp'+i+'"><img onmouseover="showPhotoDialog(\'empimg_'+i+'\',\''+jArray[0][i]["employeeId"]+'\',\'employee\')" id="empimg_'+i+'" src="'+jArray[0][i]["profileImage"]+'"  />';
							else
								employeeShare+='<td colspan="'+(5-alumcnt)+'" style="width:150px;"><div class="members shadow" id="sharedEmp'+i+'"><img onmouseover="showPhotoDialog(\'empimg_'+i+'\',\''+jArray[0][i]["employeeId"]+'\',\'employee\')" id="empimg_'+i+'" src="getEmployeeProfileImageById.json?id='+jArray[0][i]["employeeId"]+'" />';
							
							employeeShare+='<label class="labelCircle">'+jArray[0][i]["employeeName"]+'</label><input type="checkbox" class="sharedEmp" value="sharedEmp'+i+'" name="'+jArray[0][i]["employeeId"]+'" /></div></td>';
						}
						else
						{
							if(alumcnt==4)
							{
								alumcnt=0;
								employeeShare+='</tr><tr>';
							}
								if(documentType=='textfile')
									doctypeId=jArray[0][i]["documentBucketId"];
								else
									doctypeId=jArray[0][i]["documentPathBucketId"];
		
								if(jArray[0][i]["profileImageStatus"]=='notdefined')
									employeeShare+='<td style="width:150px;"><div class="members shadow" id="sharedEmp'+i+'"><img onmouseover="showPhotoDialog(\'empimg_'+i+'\',\''+jArray[0][i]["employeeId"]+'\',\'employee\')" id="empimg_'+i+'" src="'+jArray[0][i]["profileImage"]+'"  />';
								else
									employeeShare+='<td style="width:150px;"><div class="members shadow" id="sharedEmp'+i+'"><img onmouseover="showPhotoDialog(\'empimg_'+i+'\',\''+jArray[0][i]["employeeId"]+'\',\'employee\')" id="empimg_'+i+'" src="getEmployeeProfileImageById.json?id='+jArray[0][i]["employeeId"]+'" />';
								
								employeeShare+='<label class="labelCircle">'+jArray[0][i]["employeeName"]+'</label><input type="checkbox" class="sharedEmp" name="'+jArray[0][i]["employeeId"]+'" value="sharedEmp'+i+'" /></div></td>';
								
								alumcnt++;
						}
					}
			
				employeeShare+='</tr></table></div>';
			}
		},
		error:function(){

		}
	});
	employeeShare+='</div></div>';

	$('#trshowEmployeeShare'+inputid).show();
	$('#showEmployeeShare'+inputid).html(employeeShare);
	
	 $("#viewSharedEmployeesAndStudents"+inputid).accordion('destroy');
	   
		$('#viewSharedEmployeesAndStudents'+inputid).accordion({ header: "h3",
	   	 autoHeight: false, collapsible: true, active:false
		});
}


function newThread(documentId,documentType,src)
{
	var thread='<div id="threadModal"  class="modal hide">'+
	'<div class="modal-header">'+
	'<button type="button" class="close" data-dismiss="modal">Ãƒâ€”</button>'+
	'<h3>Thread Name</h3></div>'+
	'<div class="modal-body" align="center">'+	
	'		<table class="smalltable">	'+
	'			<tr><th colspan="2"></th></tr>'+
	'			<tr><td><label>Thread Name</label></td><td><input type="text" id="sharethradename" /></td></tr>'+
	'		</table>'+
	'</div>'+
	'<div class="modal-footer"><input type="button" value="Save" class="btn btn-inverse btn-mini  " onclick="saveThreadName('+documentId+',\''+documentType+'\',\''+src+'\')" /> <a href="#" class="btn btn-inverse btn-mini  " data-dismiss="modal" style="margin-left:15px;">Close</a></div>'+
	'</div>';
	
	$('#showPrivateCommentDiv').html(thread);
	
	$('#threadModal').modal('show');
	
}

function addPrivateChat(documentPrivateBucketId,documentId,documentType,src)
{
	
		var employee = new Array();
		
		$('.sharedEmp').each(function(){
			if($(this).attr('checked'))
			{
				employee.push($(this).attr('name'));
			}
		});
		var flag=0;
		if(employee.length>0)
			flag=1;
		
		if(flag==1)
		{
			$.ajax({
				url:'addPrivateChat.json?documentPrivateBucketId='+documentPrivateBucketId+'&documentType='+documentType+'&employee='+employee+'&source='+src,
				dataType:'json',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){
					
					jAlert("Successfully Added");

				},
				error:function(){

				}
			});
		}
		else
		{
			jAlert("Please select at least one Person.");
		}
	
}

function saveThreadName(documentId,documentType,src)
{
	var sharethradename=$('#sharethradename').val();
	
	$.ajax({
		url:'addNewThreadToSharing.json?d__documentId='+documentId+'&documentType='+documentType+'&sharethradename='+sharethradename,
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			
			var thread='<li><a href="javascript:void(0)" onclick="addPrivateChat('+jArray[0]["documentPrivateBucketId"]+','+documentId+',\''+documentType+'\',\''+src+'\')">'+jArray[0]["threadName"]+'</a></li>';
			
			$('#createNewThread').before(thread);

		},
		error:function(){

		}
	});
}

