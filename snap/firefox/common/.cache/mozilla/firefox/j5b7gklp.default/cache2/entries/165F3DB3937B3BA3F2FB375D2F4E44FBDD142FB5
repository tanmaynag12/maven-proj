function unshareEmployee(documentId,inputid,docuTypeBucketId,documentType)
{
	$.ajax({
		url:'unshareEmployeeAjax.json?docuTypeBucketId='+docuTypeBucketId+'&documentType='+documentType,
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			showSharedEmployeeList(documentId,documentType,inputid);

		},
		error:function(){

		}
	});
}

/**
 * @author jignesh vaghela
 *	@name unshareEmployee()
 *	@since 15th May 2012
 */


function showExistsshareEmployeeList(documentId,inputid,documentType)
{

	var size = newshareEmployeeName.length;
	var count=parseInt(size)/9;
	var colspanNo=parseInt(size)%9;
	var  employeeShare='<table class="smalltable">'+
	'<tr><th colspan="8">Shared Employees</th><th align="right"><a href="javascript:void(0);" onclick="javascript:$(\'#trshowEmployeeShare'+inputid+'\').hide();"><img src="img/remove.png" /></a></th></tr><tr>';
	if(count==0 && colspanNo>0)
	{
		for(var i=0; i<colspanNo; i++)
		{
			if(colspanNo==(i-1))
			{
				employeeShare+='<td colspan="'+colspanNo+'" style="width:120px;"><table width="100px;"><tr><td width="40px" height="50px"><img src="'+newshareEmployeeImage[i]+'" alt="boy" width="40px" height="50px" /></td></tr>'+
				'<tr><td style="width:120px"><label>'+newshareEmployeeName[i]+'</label></td></tr></table></td>';
			}
			else
			{
				employeeShare+='<td width="100px"><table class="smalltable"><tr><td style="width=100px;" height="100px"><img src="'+newshareEmployeeImage[i]+'" alt="boy" width="40px" height="50px" /></td></tr>'+
				'<tr><td style="width:120px;"><label>'+newshareEmployeeName[i]+'</label></td></tr></table></td>';
			}
		}
	}
	else
	{
		var k=0;
		for(var i=0; i<size; i++)
		{
			if(count==0)
			{
				var tempSize=parseInt(9)-parseInt(colspanNo);
				for(var j=i; j<colspanNo; j++)
				{

					employeeShare+='<td style="width:120px"><table><tr><td style="vertical-align: top;"><img src="'+newshareEmployeeImage[i]+'" alt="boy" width="40px" height="50px" /><a href="javascript:void(0);" title="Unshare Employee" onclick="unshareEmployee('+documentId+','+inputid+','+newshareEmployeeTypeId[i]+',\''+documentType+'\')"><img src="img/cloaseshare.jpg" alt="unshare"  width="15px" height="15px" style="margin-bottom:40px;margin-left:5px;" /></a></td></tr>';
					employeeShare+='<tr><td><label style="width:50px;font-size:10px;">'+newshareEmployeeName[i]+'</label></td></tr></table></td>';
					i++;
				}

				employeeShare+='<td style="width:120px"><table><tr><td style="vertical-align: top;"><img src="'+newshareEmployeeImage[i]+'" alt="boy" width="40px" height="50px" /><a href="javascript:void(0);" title="Unshare Employee" onclick="unshareEmployee('+documentId+','+inputid+','+newshareEmployeeTypeId[i]+',\''+documentType+'\')"><img src="img/cloaseshare.jpg" alt="unshare"  width="15px" height="15px" style="margin-bottom:40px;margin-left:5px;" /></a></td></tr>';
				employeeShare+='<tr><td colspan="'+tempSize+'"><label style="width:50px; font-size:10px;">'+newshareEmployeeName[i]+'</label></td></tr></table></td>';
				i++;
			}
			else
			{
				if(k==9)
				{
					employeeShare+='</tr><tr>';

					employeeShare+='<td style="width:120px"><table><tr><td style="vertical-align: top;"><img src="'+newshareEmployeeImage[i]+'" alt="boy" width="40px" height="50px" /><a href="javascript:void(0);" title="Unshare Employee" onclick="unshareEmployee('+documentId+','+inputid+','+newshareEmployeeTypeId[i]+',\''+documentType+'\')"><img src="img/cloaseshare.jpg" alt="unshare"  width="15px" height="15px" style="margin-bottom:40px;margin-left:5px;" /></a></td></tr>';
					employeeShare+='<tr><td style="width:120px"><label style="width:50px; font-size:10px;">'+newshareEmployeeName[i]+'</label></td></tr></table></td>';
					k=0;
					count--;
				}
				else
				{
					employeeShare+='<td style="width:120px"><table><tr><td style="vertical-align: top;"><img src="'+newshareEmployeeImage[i]+'" alt="boy" width="40px" height="50px" /><a href="javascript:void(0);" title="Unshare Employee" onclick="unshareEmployee('+documentId+','+inputid+','+newshareEmployeeTypeId[i]+',\''+documentType+'\')"><img src="img/cloaseshare.jpg" alt="unshare"  width="15px" height="15px" style="margin-bottom:40px;margin-left:5px;" /></a></td></tr>';
					employeeShare+='<tr><td style="width:120px"><label style="width:50px; font-size:10px;">'+newshareEmployeeName[i]+'</label></td></tr></table></td>';
				}
			}
			k++;

		}
	}


	var sizeShare = unshareEmployeeImage.length;
	if(sizeShare > 0)
	{
		employeeShare+='<table class="gridtable">';
		employeeShare+='<tr><th colspan="4">Already exist of shared employee List</th></tr>'+
		'<tr><th><label>Employee Image</label></th><th><label>Employee Name</label></th><th><label>Shared Date</label></th></tr>';
		for(var i=0; i<sizeShare; i++)
		{

			employeeShare+='<tr><td><img src="'+unshareEmployeeImage[i]+'" width="50px" height="50px" /></td>'+
			'<td><label>'+unshareEmployeeName[i]+'</label></td>'+
			'<td><label>'+unshareEmployeeShareDt[i]+'</label></td></tr>';
		}
		employeeShare+='</table>';
	}

	$('#trshowEmployeeShare'+inputid).show();
	$('#showEmployeeShare'+inputid).html(employeeShare);


}

/**
 * @author jignesh vaghela
 *	@name for comment purpose 
 *	@since 5th May 2012
 */

(function($){$.fn.extend({elastic:function(){var mimics=['paddingTop','paddingRight','paddingBottom','paddingLeft','fontSize','lineHeight','fontFamily','width','fontWeight'];return this.each(function(){if(this.type!='textarea'){return false;}
var $textarea=$(this),$twin=$('<div />').css({'position':'absolute','display':'none'}),lineHeight=parseInt($textarea.css('lineHeight'),4)||parseInt($textarea.css('fontSize'),'10'),minheight=parseInt($textarea.css('height'),4)||lineHeight*3,maxheight=parseInt($textarea.css('max-height'),10)||Number.MAX_VALUE,goalheight=0,i=0;$twin.appendTo($textarea.parent());var i=mimics.length;while(i--){$twin.css(mimics[i].toString(),$textarea.css(mimics[i].toString()));}
function setHeightAndOverflow(height,overflow){curratedHeight=Math.floor(parseInt(height,10));if($textarea.height()!=curratedHeight){$textarea.css({'height':curratedHeight+'px','overflow':overflow});}}
function update(){var textareaContent=$textarea.val().replace(/<|>/g,' ').replace(/\n/g,'<br />').replace(/&/g,"&amp;");var twinContent=$twin.html();if(textareaContent+'&nbsp;'!=twinContent){$twin.html(textareaContent+'&nbsp;');if(Math.abs($twin.height()+lineHeight-$textarea.height())>3){var goalheight=$twin.height()+lineHeight;if(goalheight>=maxheight){setHeightAndOverflow(maxheight,'auto');}else if(goalheight<=minheight){setHeightAndOverflow(minheight,'hidden');}else{setHeightAndOverflow(goalheight,'hidden');}}}}
$textarea.css({'overflow':'hidden'}).bind('focus',function(){self.periodicalUpdater=window.setInterval(function(){update();},50);}).bind('blur',function(){clearInterval(self.periodicalUpdater);});update();});}});})(jQuery);
