var unshareStudentImage=new Array();
var unshareStudentShareDt=new Array();
var unshareStudentName=new Array();

var newshareStudentImage=new Array();
var newshareStudentShareDt=new Array();
var newshareStudentName=new Array();
var newshareStudentTypeId=new Array();




function showStudentShare(id,inputid,batchIndexId,documentid,documenttype,semesterName,src,srcid)
{
	$('#trshowComment'+inputid).hide();
	$('#trshowEmployeeShare'+inputid).hide();
	$('#trshowPrivateComment'+inputid).hide();
	
	var share='<div class="shadow"><div class="modal-header"><button type="button" class="close" data-dismiss="modal" onclick="javascript:$(\'#trshowStudentShare'+inputid+'\').fadeOut(1000);" style="color:red;">x</button><h5>Student Sharing with '+semesterName+'</h5></div>'+
	'<table  class="table"><tr><td colspan="2"><table class="table"><tr><th colspan="5">By Batch</th></tr>'+
	'<tr><td> <input type="text" id="student-batch-input-'+id+inputid+'" /></td><td><input type="checkbox" id="checkAllBatch'+id+inputid+'" /><label>My Batches</label></td>'+
	'<td colspan="3" align="center"><input type="button" id="btnStudBatch'+id+inputid+'" value="Submit" class="btn btn-inverse btn-mini  " /></td></tr></table></td></tr>'+
	'<tr><td colspan="2"><table class="table"><tr><th colspan="5">By Student Name</th></tr>'+
	'<tr><td> <input type="text" id="student-name-input-'+id+inputid+'" /></td><td><input type="checkbox" id="checkAllStudent'+id+inputid+'" /><label>All Students</label></td>'+
	'<td colspan="2" align="center"><input type="button" id="btnStud'+id+inputid+'" value="Submit" class="btn btn-inverse btn-mini  " /></td></tr></table></td></tr>'+		
	'</table></div>';
	$('#showStudentShare'+inputid).html(share);
	var semcapid=gBatchSemCapId[batchIndexId];
	var semId=gSemesterId[batchIndexId];
	
	var subId = $('#cmdloadsubject').val();
	if(subId==undefined)
		subId=0;
	$.ajax({
		url:'GetBatchStudentMappingAjax.json?semcapid='+semcapid+'&semesterName='+semesterName+'&semId='+semId+
		'&subId='+subId,
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$(document).ready(function() {

				$("#student-batch-input-"+id+inputid).tokenInput(jArray, {
					theme: "facebook",
					preventDuplicates: true
				});   

				$("#checkAllBatch"+id+inputid).click(function() {
					if($(this).attr('checked'))
					{
						for(var i=0; i<jArray.length; i++)
						{
							$("#student-batch-input-"+id+inputid).tokenInput("add", {id: jArray[i]["id"], name: jArray[i]["name"]});
						}
					}
					else
					{
						$("#student-batch-input-"+id+inputid).tokenInput("clear");
					}
				});
			});
		},
		error:function(){

		}
	});

	$.ajax({
		url:'getStudentMappingAllStudentofSemAjax.json?semcapid='+semcapid+'&semesterName='+semesterName+'&semId='+semId,
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$(document).ready(function() {

				$("#student-name-input-"+id+inputid).tokenInput(jArray, {
					theme: "facebook",
					preventDuplicates: true
				}); 

				$("#checkAllStudent"+id+inputid).click(function() {
					if($(this).attr('checked'))
					{
						for(var i=0; i<jArray.length; i++)
						{
							$("#student-name-input-"+id+inputid).tokenInput("add", {id: jArray[i]["id"], name: jArray[i]["name"]});
						}
					}
					else
					{
						$("#student-name-input-"+id+inputid).tokenInput("clear");
					}
				});
			});
		},
		error:function(){

		}
	});
	$('#trshowStudentShare'+inputid).show();

	$("#btnStudBatch"+id+inputid).click(function() {

		var sharedToStudent=$("#student-batch-input-"+id+inputid).val();
		var studBatchArray = new Array();

		studBatchArray=sharedToStudent.split(",");
		$.ajax({
			url:'saveStudentBatchDocumentSharingAjax.json?d__documentId='+documentid+'&documentType='+documenttype+'&sharedToStudent='+studBatchArray,
			dataType:'json',
			async:false,
			beforeSend:function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				
				
				if(src=='dms')
				{
					$('#'+srcid).html('');
					var uploadDocument =new	UploadDocument();
					uploadDocument.getDocument(gNodeId, function(jArray){
						uploadDocument.showDocument({
							elementId:srcid,
							documentList:jArray,
						});
					});
				}	
				else
				{
					
					$('.tblFileIconList').html('');
					getTextFiles(gNodeId);
					getUploadedFiles(gNodeId);
				}
			},
			error:function(){

			}
		});

	});


	$("#btnStud"+id+inputid).click(function() {

		var sharedToStudent=$("#student-name-input-"+id+inputid).val();
		var studPersonalArray = new Array();

		studPersonalArray=sharedToStudent.split(",");
		$.ajax({
			url:'saveStudentPersonalDocumentSharingAjax.json',
			dataType:'json',
			type:'POST',
			data:{d__documentId:documentid,documentType:documenttype,sharedToStudent:studPersonalArray.toString()},
			async:false,
			beforeSend:function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){

				if(src=='dms')
				{
					$('#'+srcid).html('');
					var uploadDocument =new	UploadDocument();
					uploadDocument.getDocument(gNodeId, function(jArray){
						uploadDocument.showDocument({
							elementId:srcid,
							documentList:jArray,
						});
					});
					
					
					
				}	
				else
				{
					$('.tblFileIconList').html('');
					getTextFiles(gNodeId);
					getUploadedFiles(gNodeId);
					shareCourseFileDocument();
					
				}
			},
			error:function(){

			}
		});

	});

}
/**
 * @author jignesh vaghela
 *	@name showEmployeeShare()
 *	@since 30th Apr 2012
 */
var unshareEmployeeImage=new Array();
var unshareEmployeeShareDt=new Array();
var unshareEmployeeName=new Array();

var newshareEmployeeImage=new Array();
var newshareEmployeeShareDt=new Array();
var newshareEmployeeName=new Array();
var newshareEmployeeTypeId=new Array();
function showEmployeeShare(id,inputid,roasterIndexId,documentid,documenttype,departmentName,src,srcid)
{
	$('#trshowComment'+inputid).hide();
	$('#trshowStudentShare'+inputid).hide();
	$('#trshowPrivateComment'+inputid).hide();
	var share='<table class="gridtable">'+
	'<tr><th colspan="3">Employee Sharing with '+departmentName+' Staff</th><th align="right"><a href="#" onclick="javascript:$(\'#trshowEmployeeShare'+inputid+'\').hide();"><img src="img/remove.png" /></a></th></tr>'+
	'<tr><td><label>With Employee Name :</label></td><td> <input type="text" id="employee-name-input-'+id+inputid+'" /></td>'+
	'<td colspan="2"><input type="checkbox" id="chkAll'+id+inputid+'" /><label>select All</label></td></tr>'+	
	'<tr><td colspan="3"><table class="smalltable"><tr><td><label>Can View :</label><input type="checkbox" value="view" id="view'+inputid+'" /></td>'+
	'<td><label>Can Edit :</label><input type="checkbox" value="edit" id="edit'+inputid+'" /></td>'+
	'</tr></table></td><td align="center"><div class="login"><input type="button" value="Submit" id="btnSubmit'+id+inputid+'" /></div></td>'+
	'</table>';
	$('#showEmployeeShare'+inputid).html(share);

	var roasterId=gRoasterIdArray[roasterIndexId];
	$.ajax({
		url:'loadEmployeeListofSharing.json?roasterId='+roasterId,
		dataType:'json',
		async:false,
		beforeSend:function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			$(document).ready(function() {
				$("#employee-name-input-"+id+inputid).tokenInput(jArray, {
					theme: "facebook",
					preventDuplicates: true
				});
				$("#btnSubmit"+id+inputid).click(function() {		        	
					var sharedToEmployee=$("#employee-name-input-"+id+inputid).val();
					var empIdArray = new Array();
					empIdArray=sharedToEmployee.split(",");
					
					var view=$('#view'+inputid).attr('checked');
					var edit=$('#edit'+inputid).attr('checked');
					
					$.ajax({
						url:'saveDocumentSharingAjax.json?d__documentId='+documentid+'&documentType='+documenttype+'&sharedToEmployee='+empIdArray+'&view='+view+'&edit='+edit,
						dataType:'json',
						async:false,
						beforeSend:function() {
							startCommonAjaxSpinner();
						},
						complete: function() {
							stopCommonAjaxSpinner();
						},
						success:function(jArray){
					
							if(src=='dms')
							{
								$('#'+srcid).html('');
								var uploadDocument =new	UploadDocument();
								uploadDocument.getDocument(gNodeId, function(jArray){
									uploadDocument.showDocument({
										elementId:srcid,
										documentList:jArray,
									});
								});
								
								
							}	
							else
							{
								$('.tblFileIconList').html('');
								getTextFiles(gNodeId);
								getUploadedFiles(gNodeId);
							}
						},
						error:function(){

						}
					});

				});
				$("#chkAll"+id+inputid).click(function() {
					if($(this).attr('checked'))
					{
						for(var i=0; i<jArray.length; i++)
						{
							$("#employee-name-input-"+id+inputid).tokenInput("add", {id: jArray[i]["id"], name: jArray[i]["name"]});
						}
					}
					else
					{
						$("#employee-name-input-"+id+inputid).tokenInput("clear");
					}
				});
			});

		},
		error:function(){

		}
	});


	$('#trshowEmployeeShare'+inputid).show();

}
/**
 * @author jignesh vaghela
 *	@name loadEmployeeRoasterMappingAjax()
 *	@since 1st May 2012
 */

function loadEmployeeRoasterMappingAjax()
{
	if(gCategoryType=='courseFile')
	{
		var instCourseId=$('#cmbloadCourse').val();
	
		$.ajax({
			url:'loadEmployeeRoasterMappingMin.json?type=required&instCourseId='+instCourseId,
			dataType:'json',
			async:false,
			success:function(jArray){
				var j=jArray.length;
				var roasterId = 0;
				var trustName = 1;
				var campusName = 2;
				var instituteName = 3;
				var courseName = 4;
				for(var i=0; i<j; i++)
				{
					gRoasterIdArray[i]=jArray[i][roasterId];
					
					if(jArray[i][trustName]!=undefined && jArray[i][trustName].toString()!='null')
						gDepartmentName[i]=jArray[i][trustName];
					else if(jArray[i][campusName]!=undefined && jArray[i][campusName].toString()!='null')
						gDepartmentName[i]=jArray[i][campusName];
					else if(jArray[i][instituteName]!=undefined && jArray[i][instituteName].toString()!='null')
						gDepartmentName[i]=jArray[i][instituteName];
					else if(jArray[i][courseName]!=undefined && jArray[i][courseName].toString()!='null')
						gDepartmentName[i]=jArray[i][courseName];
				}
			},
			error:function(){
	
			}
		});
	}
	else
	{
		
		$.ajax({
			url:'loadEmployeeRoasterMappingMin.json?type=notrequired&instCourseId=0',
			dataType:'json',
			async:false,
			success:function(jArray){
				var j=jArray.length;
				var roasterId = 0;
				var trustName = 1;
				var campusName = 2;
				var instituteName = 3;
				var courseName = 4;
				for(var i=0; i<j; i++)
				{
					gRoasterIdArray[i]=jArray[i][roasterId];
					
					if(jArray[i][trustName]!=undefined && jArray[i][trustName].toString()!='null')
						gDepartmentName[i]=jArray[i][trustName];
					else if(jArray[i][campusName]!=undefined && jArray[i][campusName].toString()!='null')
						gDepartmentName[i]=jArray[i][campusName];
					else if(jArray[i][instituteName]!=undefined && jArray[i][instituteName].toString()!='null')
						gDepartmentName[i]=jArray[i][instituteName];
					else if(jArray[i][courseName]!=undefined && jArray[i][courseName].toString()!='null')
						gDepartmentName[i]=jArray[i][courseName];
				}
			},
			error:function(){
	
			}
		});
	}
	loadStudentBatchAjax();
}

/**
 * @author jignesh vaghela
 *	@name loadEmployeeRoasterMappingAjax()
 *	@since 23rd june 2012
 */
var gBatchSemCapId = new Array();
var gSemesterId = new Array();
function loadStudentBatchAjax()
{
	if(gCategoryType=='mentoring')
	{
		loadStudentMentoring();
	}	
	else if(gCategoryType=='courseFile')
	{
		var instCourseId=$('#cmbloadCourse').val();
		$.ajax({
			url:'loadStudentBatch.json?type=required&instCourseId='+instCourseId,
			dataType:'json',
			async:false,
			success:function(jArray){
				var j=jArray.length;
				for(var i=0; i<j; i++)
				{
					gBatchSemCapId[i]=jArray[i]["semesterBatchId"];
					gBatchSemester[i]=jArray[i]["semesterName"];
					gSemesterId[i]=jArray[i]["semesterId"];
				}
			},
			error:function(){

			}
		});
	}	
	else
	{
		$.ajax({
			url:'loadStudentBatch.json?type=notrequired&instCourseId=0',
			dataType:'json',
			async:false,
			success:function(jArray){
				var j=jArray.length;
				for(var i=0; i<j; i++)
				{
					gBatchSemCapId[i]=jArray[i]["semesterBatchId"];
					gBatchSemester[i]=jArray[i]["semesterName"];
					gSemesterId[i]=jArray[i]["semesterId"];
				}
			},
			error:function(){

			}
		});
	}
	
}

/**
 * @author jignesh vaghela
 *	@name showComment()
 *	@since 5th May 2012
 */
function showComment(inputid,documentTypeId,documentType,gcnt)
{
	var link='';
	$('#trshowStudentShare'+inputid).hide();
	$('#trshowEmployeeShare'+inputid).hide();
	$('#trshowPrivateComment'+inputid).hide();
	$.ajax({
		url:'loadEmployeeDocumentComment.json?documentTypeId='+documentTypeId+'&documentType='+documentType,
		dataType:'json',
		async:false,
		success:function(jArray){
			var size =jArray[0].length;
			var temp=new Array();
			
			if(documentType=='textfile')
				temp=gjArrTextDocument;
			else
				temp=gjArrUploadedDocument;
			
			link='<div id="commentaccord">'+
				'<h3><a href="#">Public Comment</a></h3>';
			
			link+='<div id="commonChat">'+
			'<div style="width:100%;"><a href="javascript:void(0)" style="float:right;" onclick="javascript:$(\'#trshowComment'+inputid+'\').hide();"><img src="img/remove.png" /></a></div>'+
			'<div id="showComment'+inputid+'">'+
			'<div class="commentPanel" id="commentPanel-first-'+inputid+'" align="left">'+
			'<div class="commentBox" align="right" id="commentBox-'+inputid+'">';

			if(temp[gcnt]["hasProfileImage"]== 'no')
			{
				link+='<img src="'+jArray[gcnt]["profileImage"]+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
				gProfileImgPath=jArray[gcnt]["profileImage"];
			}
			else if(temp[gcnt]["hasProfileImage"]=='yes')
			{
				link+='<img src="getEmployeeProfileImage.json?id='+temp[gcnt]["employeeId"]+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
				gProfileImgPath= 'getEmployeeProfileImage.json?id='+temp[gcnt]["employeeId"];
			}

			link+='<input type="hidden" id="owner" value="" />'+
			'<label id="record-">';
			
			if(documentType=='textfile')
				link+='<textarea class="commentMark" id="commentMark-first-'+inputid+'" onblur="if (this.value==\'\') this.value = \'Write a comment\'" onfocus="if (this.value==\'Write a comment\') this.value = \'\'" onKeyPress="return SubmitComment(this,event,\''+documentType+'\','+temp[gcnt]["documentId"]+','+inputid+');" wrap="hard" name="commentMark" style=" background-color:#fff; overflow: hidden;" cols="60">Write a comment</textarea>';
			else
				link+='<textarea class="commentMark" id="commentMark-first-'+inputid+'" onblur="if (this.value==\'\') this.value = \'Write a comment\'" onfocus="if (this.value==\'Write a comment\') this.value = \'\'" onKeyPress="return SubmitComment(this,event,\''+documentType+'\','+temp[gcnt]["documentPathId"]+','+inputid+');" wrap="hard" name="commentMark" style=" background-color:#fff; overflow: hidden;" cols="60">Write a comment</textarea>';
			link+='</label>'+
			'<br clear="all" />'+
			'</div>'+
			'</div>'+
			'</div>';
			if(size>0)
			{
				for(var i=0; i<size; i++)
				{
					$('#'+documentType+'-'+documentTypeId+'-'+i).remove();
				}
				for(var i=1;i<=gCommentCnt; i++)
				{
					$('#'+documentType+'-'+documentTypeId+'-'+i).remove();
				}
				
					for(var i=0; i<size; i++)
					{
						if(jArray[0][i]["person"]=='employee')
						{
							link+='<div class="commentPanel" id="'+documentType+'-'+documentTypeId+'-'+i+'" align="left">';
	
							if(jArray[0][i]["profileImageStatus"]=='notdefined') 
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[0][i]["employeeId"]+'\',\'employee\')" id="cmtimg_'+i+'" src="'+jArray[0][i]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
							else
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[0][i]["employeeId"]+'\',\'employee\')" id="cmtimg_'+i+'" src="getEmployeeProfileImageById.json?id='+jArray[0][i]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
	
							link+='<label class="name"><b>'+jArray[0][i]["employeeName"]+'<a href="#"></a></b>'+
							'<div class="name" style="text-align:justify;float:left;">'+
							'<em>'+jArray[0][i]["comment"]+'</em>'+
	
							'</div>'+
							'<br clear="all" />'+
							'<br clear="all" />'+
							'<div  class="name" align="center" style="width:350px;float:right; ">'+
							'<div class="name" style="float:left;">';
	
							
							link+='<span style="margin-bottom:10px;float:right;">'+jArray[0][i]["date"]+' at '+jArray[0][i]["time"];
	
							link+='&nbsp;<a href="javascript:void(0)" onclick="javascript:deleteComment('+jArray[0][i]["documentBucketThreadId"]+',\''+documentType+'\','+i+','+documentTypeId+')" class="c_delete">Delete</a>'+
							'<span id="clike-panel-">'+
							'<a href="javascript: void(0)"  onclick="javascript: replyToComment(\''+documentType+'\','+documentTypeId+','+i+');">reply</a>'+
							'</span>'+
							'</span>'+
							'</div>'+
							'</div>'+
							'</div>';
						}
						else
						{
							link+='<div class="commentPanel" id="'+documentType+'-'+documentTypeId+'-'+i+'" align="left">';
							
							if(jArray[0][i]["profileImageStatus"]=='notdefined') 
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[0][i]["studentId"]+'\',\'student\')" id="cmtimg_'+i+'" src="'+jArray[0][i]["studentId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
							else
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+i+'\',\''+jArray[0][i]["studentId"]+'\',\'student\')" id="cmtimg_'+i+'" src="getStudentProfileImageById.json?id='+jArray[0][i]["studentId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
	
							link+='<label class="name"><b>'+jArray[0][i]["studentName"]+'<a href="#"></a></b>'+
							'<div class="name" style="text-align:justify;float:left;">'+
							'<em>'+jArray[0][i]["comment"]+'</em>'+
	
							'</div>'+
							'<br clear="all" />'+
							'<br clear="all" />'+
							'<div  class="name" align="center" style="width:350px;float:right; ">'+
							'<div class="name" style="float:left;">';
	
							
							link+='<span style="margin-bottom:10px;float:right;">'+jArray[0][i]["date"]+' at '+jArray[0][i]["time"];
	
							link+='&nbsp;<a href="javascript:void(0)" onclick="javascript:deleteComment('+jArray[0][i]["documentBucketThreadId"]+',\''+documentType+'\','+i+','+documentTypeId+')" class="c_delete">Delete</a>'+
							'<span id="clike-panel-">'+
							'<a href="javascript: void(0)"  onclick="javascript: replyToComment(\''+documentType+'\','+documentTypeId+','+i+');">reply</a>'+
							'</span>'+
							'</span>'+
							'</div>'+
							'</div>'+
							'</div>';
						}
						gCommentCnt++;
					}
			}
			link+='</div>';
			
			for(var i=1; i<jArray.length; i++)
			{
				if(jArray[i][0]!=undefined)
				{
					link+='<h3><a href="#">'+jArray[i][0]["threadName"]+' Comment</a></h3>';
					link+='<div id="'+jArray[i][0]["threadName"]+'-'+jArray[i][0]["documentPrivateThreadId"]+'">';
					link+='<div style="width:100%;"><a href="javascript:void(0)" style="float:right;" onclick="javascript:$(\'#trshowComment'+inputid+'\').hide();"><img src="img/remove.png" /></a></div>'+
					'<div id="showComment-private'+inputid+'">'+
					'<div class="commentPanel" id="commentPanel-first-private-'+inputid+'" align="left">'+
					'<div class="commentBox" align="right" id="commentBox-private'+inputid+'">';
	
					if(temp[gcnt]["hasProfileImage"]== 'no')
					{
						link+='<img src="'+jArray[gcnt]["profileImage"]+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
						gProfileImgPath=jArray[gcnt]["profileImage"];
					}
					else if(temp[gcnt]["hasProfileImage"]=='yes')
					{
						link+='<img src="getEmployeeProfileImage.json?id='+temp[gcnt]["employeeId"]+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
						gProfileImgPath= 'getEmployeeProfileImage.json?id='+temp[gcnt]["employeeId"];
					}
	
					link+='<input type="hidden" id="owner" value="" />'+
					'<label id="record-">'+
					'<textarea class="commentMark" id="commentMark-first-private-'+inputid+'" onblur="if (this.value==\'\') this.value = \'Write a comment\'" onfocus="if (this.value==\'Write a comment\') this.value = \'\'" onKeyPress="return SubmitPrivateComment(this,event,\'textfile\','+documentTypeId+','+inputid+','+jArray[i][0]["documentPrivateThreadId"]+');" wrap="hard" name="commentMark" style=" background-color:#fff; overflow: hidden;" cols="60">Write a comment</textarea>'+
					'</label>'+
					'<br clear="all" />'+
					'</div>'+
					'</div>'+
					'</div>';
					for(var j=1; j<jArray[i].length; j++)
					{
						if(jArray[0][i]["person"]=='employee')
						{
							link+='<div class="commentPanel" id="private-'+documentType+'-'+documentTypeId+'-'+i+'" align="left">';
		
							if(jArray[0][i]["profileImageStatus"]=='notdefined') 
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'privatecmtempimg_'+i+'\',\''+jArray[i][j]["employeeId"]+'\',\'employee\')" id="privatecmtimg_'+i+'" src="'+jArray[i][j]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
							else
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'privatecmtempimg_'+i+'\',\''+jArray[i][j]["employeeId"]+'\',\'employee\')" id="privatecmtimg_'+i+'" src="getEmployeeProfileImageById.json?id='+jArray[i][j]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
		
							link+='<label class="name"><b>'+jArray[i][j]["employeeName"]+'<a href="#"></a></b>'+
							'<div class="name" style="text-align:justify;float:left;">'+
							'<em>'+jArray[i][j]["comment"]+'</em>'+
		
							'</div>'+
							'<br clear="all" />'+
							'<br clear="all" />'+
							'<div  class="name" align="center" style="width:350px;float:right; ">'+
							'<div class="name" style="float:left;">';
		
		
							link+='<span style="margin-bottom:10px;float:right;">'+jArray[i][j]["date"]+' at '+jArray[i][j]["time"];
		
							link+='&nbsp;<a href="javascript:void(0)" onclick="javascript:deletePrivateComment('+jArray[i][j]["documentPrivateBucketThreadId"]+',\''+documentType+'\','+i+','+documentTypeId+')" class="c_delete">Delete</a>'+
							'<span id="clike-panel-">'+
							'<a href="javascript: void(0)"  onclick="javascript: replyToPrivateComment(\''+documentType+'\','+documentTypeId+','+i+','+jArray[i][j]["documentPrivateBucketThreadId"]+');">reply</a>'+
							'</span>'+
							'</span>'+
							'</div>'+
							'</div>'+
							'</div>';
						}
						else
						{
							link+='<div class="commentPanel" id="private-'+documentType+'-'+documentTypeId+'-'+i+'" align="left">';
							
							if(jArray[0][i]["profileImageStatus"]=='notdefined') 
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'privatecmtempimg_'+i+'\',\''+jArray[i][j]["studentId"]+'\',\'employee\')" id="privatecmtimg_'+i+'" src="'+jArray[i][j]["studentId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
							else
								link+='<a href="#"><img onmouseover="showPhotoDialog(\'privatecmtempimg_'+i+'\',\''+jArray[i][j]["studentId"]+'\',\'employee\')" id="privatecmtimg_'+i+'" src="getStudnentProfileImageById.htm?id='+jArray[i][j]["studentId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
		
							link+='<label class="name"><b>'+jArray[i][j]["studentName"]+'<a href="#"></a></b>'+
							'<div class="name" style="text-align:justify;float:left;">'+
							'<em>'+jArray[i][j]["comment"]+'</em>'+
		
							'</div>'+
							'<br clear="all" />'+
							'<br clear="all" />'+
							'<div  class="name" align="center" style="width:350px;float:right; ">'+
							'<div class="name" style="float:left;">';
		
		
							link+='<span style="margin-bottom:10px;float:right;">'+jArray[i][j]["date"]+' at '+jArray[i][j]["time"];
		
							link+='&nbsp;<a href="javascript:void(0)" onclick="javascript:deletePrivateComment('+jArray[i][j]["documentPrivateBucketThreadId"]+',\''+documentType+'\','+i+','+documentTypeId+')" class="c_delete">Delete</a>'+
							'<span id="clike-panel-">'+
							'<a href="javascript: void(0)"  onclick="javascript: replyToPrivateComment(\''+documentType+'\','+documentTypeId+','+i+','+jArray[i][j]["documentPrivateBucketThreadId"]+');">reply</a>'+
							'</span>'+
							'</span>'+
							'</div>'+
							'</div>'+
							'</div>';
						}
					}
					
				}
			}
			link+='</div>';

		},
		error:function(){

		}
	});
	$('#commentConfig'+inputid).html(link);
	
	$("#commentaccord").accordion('destroy');
	   
	$('#commentaccord').accordion({ header: "h3",
   	 autoHeight: false, collapsible: true, active:false
	});
	$(document).ready(function(){$('.commentMark').elastic(); });
	$('#trshowComment'+inputid).show();
}
/**
 * @author jignesh vaghela
 *	@name replyToComment()
 *	@since 5th May 2012
 */
function replyToComment(documentType,documentTypeId,counter)
{
	$('#temp-'+documentType+'-'+documentTypeId+'-'+counter).remove();

	var comment='<div class="commentPanel" id="temp-'+documentType+'-'+documentTypeId+'-'+counter+'" align="left">'+
	'<div class="commentBox" align="right" id="commentBox-">'+
	'<img src="'+gProfileImgPath+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />'+
	'<input type="hidden" id="owner" value="" />'+
	'<label id="record-">'+
	'<textarea class="commentMark" id="commentMark-" onblur="if (this.value==\'\') this.value = \'Write a comment\'" onfocus="if (this.value==\'Write a comment\') this.value = \'\'" onKeyPress="return SubmitComment(this,event,\''+documentType+'\','+documentTypeId+','+counter+');" wrap="hard" name="commentMark" style=" background-color:#fff; overflow: hidden;" cols="60">Write a comment</textarea>'+
	'</label><a href="javascript:void(0)" onclick="removeCurrentComment(\'temp-'+documentType+'-'+documentTypeId+'-'+counter+'\')"><img src="img/cloaseshare.jpg" /></a>'+
	'<br clear="all" />'+
	'</div>'+
	'</div>'+
	'</div>';

	$('#'+documentType+'-'+documentTypeId+'-'+counter).after($(comment).fadeIn('slow'));
	
}

function replyToPrivateComment(documentType,documentTypeId,counter,documentPrivateBucketThreadId)
{
	$('#temp-'+documentType+'-'+documentTypeId+'-'+counter).remove();

	var comment='<div class="commentPanel" id="temp-'+documentType+'-'+documentTypeId+'-'+counter+'" align="left">'+
	'<div class="commentBox" align="right" id="commentBox-">'+
	'<img src="'+gProfileImgPath+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />'+
	'<input type="hidden" id="owner" value="" />'+
	'<label id="record-">'+
	'<textarea class="commentMark" id="commentMark-" onblur="if (this.value==\'\') this.value = \'Write a comment\'" onfocus="if (this.value==\'Write a comment\') this.value = \'\'" onKeyPress="return SubmitPrivateComment(this,event,\''+documentType+'\','+documentTypeId+','+counter+','+documentPrivateBucketThreadId+');" wrap="hard" name="commentMark" style=" background-color:#fff; overflow: hidden;" cols="60">Write a comment</textarea>'+
	'</label><a href="javascript:void(0)" onclick="removeCurrentComment(\'temp-'+documentType+'-'+documentTypeId+'-'+counter+'\')"><img src="img/cloaseshare.jpg" /></a>'+
	'<br clear="all" />'+
	'</div>'+
	'</div>'+
	'</div>';

	$('#private-'+documentType+'-'+documentTypeId+'-'+counter).after($(comment).fadeIn('slow'));

}

function removeCurrentComment(id)
{
	$('#'+id).remove();
}

/**
 * @author jignesh vaghela
 *	@name SubmitComment()
 *	@since 5th May 2012
 */
function SubmitComment(id,event,documentType,documentTypeId,counter)
{
	var key = event.keyCode;
	// If the user has pressed enter
	if (key == 13) {

		var sourceid=id.id;
		var commentdisp=$('#'+id.id).val();

		$.ajax({
			url:'SubmitCommentAjax.json?documentType='+documentType+'&documentTypeId='+documentTypeId+'&comment='+commentdisp,
			dataType:'json',
			success:function(jArray){

				gCommentCnt++;
				var comment='';
					
					comment='<div class="commentPanel" id="'+documentType+'-'+documentTypeId+'-'+gCommentCnt+'" align="left">';


					if(jArray[0]["profileImageStatus"]=='notdefined') 
						comment+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+counter+'\',\''+jArray[0]["employeeId"]+'\',\'employee\')" id="cmtimg_'+counter+'" src="'+jArray[0]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
					else
						comment+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+counter+'\',\''+jArray[0]["employeeId"]+'\',\'employee\')" id="cmtimg_'+counter+'" src="getEmployeeProfileImageById.json?id='+jArray[0]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';

					comment+='<label class="name"><b>'+jArray[0]["employeeName"]+'<a href="#"></a></b>'+
					'<div class="name" style="text-align:justify;float:left;">'+
					'<em>'+jArray[0]["comment"]+'</em>'+

					'</div>'+
					'<br clear="all" />'+
					'<br clear="all" />'+
					'<div  class="name" align="center" style="width:350px;float:right; ">'+
					'<div class="name" style="float:left;">';

					alert(jArray[0]["documentBucketThreadId"]);
					comment+='<span style="margin-bottom:10px;float:right;">'+jArray[0]["date"]+' at '+jArray[0]["time"];

					comment+='&nbsp;<a href="javascript:void(0)" onclick="javascript:deleteComment('+jArray[0]["documentBucketThreadId"]+',\''+documentType+'\','+gCommentCnt+','+documentTypeId+')" class="c_delete">Delete</a>'+
					'<span id="clike-panel-">'+
					'<a href="javascript: void(0)"  onclick="javascript: replyToComment(\''+documentType+'\','+documentTypeId+','+gCommentCnt+');">reply</a>'+
					'</span>'+
					'</span>'+
					'</div>'+
					'</div>'+
					'</div>';
					

				if(sourceid.indexOf("commentMark-first") != -1)
				{
					$('#temp-'+documentType+'-'+documentTypeId+'-'+counter).remove().fadeOut('slow');
					$('#commentPanel-first-'+counter).after($(comment).fadeIn('slow'));
				}
				else
				{
					$('#temp-'+documentType+'-'+documentTypeId+'-'+counter).remove().fadeOut('slow');
					$('#'+documentType+'-'+documentTypeId+'-'+counter).after($(comment).fadeIn('slow'));
				}
			},
			error:function(){

			}
		});
		return false;
	}
	else {
		return true;
	}

}

function SubmitPrivateComment(id,event,documentType,documentTypeId,counter,documentPrivateBucketThreadId)
{
	var key = event.keyCode;
	if (key == 13) {

		var sourceid=id.id;
		var commentdisp=$('#'+id.id).val();

		$.ajax({
			url:'SubmitPrivateCommentAjax.json?documentPrivateBucketThreadId='+documentPrivateBucketThreadId+'&comment='+commentdisp,
			dataType:'json',
			success:function(jArray){

				gCommentCnt++;
				var comment='';
					
					comment='<div class="commentPanel" id="private-'+documentType+'-'+documentTypeId+'-'+gCommentCnt+'" align="left">';


					if(jArray[0]["profileImageStatus"]=='notdefined') 
						comment+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+counter+'\',\''+jArray[0]["employeeId"]+'\',\'employee\')" id="cmtimg_'+counter+'" src="'+jArray[0]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';
					else
						comment+='<a href="#"><img onmouseover="showPhotoDialog(\'cmtempimg_'+counter+'\',\''+jArray[0]["employeeId"]+'\',\'employee\')" id="cmtimg_'+counter+'" src="getEmployeeProfileImageById.json?id='+jArray[0]["employeeId"]+'" style="float:left; padding-right:9px;" width="40" height="40" border="0" alt="" /></a>';

					comment+='<label class="name"><b>'+jArray[0]["employeeName"]+'<a href="#"></a></b>'+
					'<div class="name" style="text-align:justify;float:left;">'+
					'<em>'+jArray[0]["comment"]+'</em>'+

					'</div>'+
					'<br clear="all" />'+
					'<br clear="all" />'+
					'<div  class="name" align="center" style="width:350px;float:right; ">'+
					'<div class="name" style="float:left;">';

					
					comment+='<span style="margin-bottom:10px;float:right;">'+jArray[0]["date"]+' at '+jArray[0]["time"];

					comment+='&nbsp;<a href="javascript:void(0)" onclick="javascript:deletePrivateComment('+jArray[0]["documentPrivateBucketThreadId"]+',\''+documentType+'\','+counter+','+documentTypeId+')" class="c_delete">Delete</a>'+
					'<span id="clike-panel-">'+
					'<a href="javascript: void(0)"  onclick="javascript: replyToPrivateComment(\''+documentType+'\','+documentTypeId+','+counter+','+jArray[0]["documentPrivateBucketThreadId"]+');">reply</a>'+
					'</span>'+
					'</span>'+
					'</div>'+
					'</div>'+
					'</div>';

				if(sourceid.indexOf("commentMark-first-private") != -1)
				{
					$('#temp-'+documentType+'-'+documentTypeId+'-'+counter).remove().fadeOut('slow');
					$('#commentPanel-first-private-'+counter).after($(comment).fadeIn('slow'));
				}
				else
				{
					$('#temp-'+documentType+'-'+documentTypeId+'-'+counter).remove().fadeOut('slow');
					$('#private-'+documentType+'-'+documentTypeId+'-'+counter).after($(comment).fadeIn('slow'));
				}

				counter++;

			},
			error:function(){

			}
		});
		return false;
	}
	else {
		return true;
	}
}

/**
 * @author jignesh vaghela
