        <script type="text/javascript">
$(document).ready(function(){
    if (annyang) {
      // Let's define our first command. First the text we expect, and then the function it should call
      var commands = {
        'show attendence': function(search) {showAttendencePage();},
        'show Assessments': function(search) {showAssignmentPage();},
        'show Profile' : function(search){window.open("stu_studentProfile.htm","_blank");},
        'show Syllabus' : function(search){window.open("studentCourseFileNew.htm","_blank");},
        'show Calender' : function(search){window.open("studentScheduleAcademicCalender.htm","_blank");},
        'show Timetable' : function(search){window.open("stu_StudentTimeTable.htm","_blank");},
        'show Library Detail' : function(search){window.open("stu_studentLibraryDetails.htm","_blank");},
        'show Fees Detail' : function(search){window.open("studentFinanceDetails.htm","_blank");},
        'show Leave Detail' : function(search){window.open("studentLeaveApplication.htm","_blank");},
        'show Hostel Detail' : function(search){window.open("studentHostelHomeDetails.htm","_blank");},
        'show Task' : function(search){getJobTrayAlerts();},
        'show DMS' : function(search){window.open("manageDocument.json","_blank");},
      };
      annyang.addCommands(commands);
      SpeechKITT.annyang();
      SpeechKITT.setStylesheet('https://cdnjs.cloudflare.com/ajax/libs/SpeechKITT/0.3.0/themes/flat.css');
      SpeechKITT.vroom();
    }
});
</script>

