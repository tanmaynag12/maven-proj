$(document).ready(function(){
	try{
		getMyLocalIp();
	}catch(e){}
	
	try{
	autoSelected_GemsDateFilters();
	if($.cookie("cookiesChznArr")!=null){
		gCookieChzxReArrayList = $.cookie("cookiesChznArr").split('~@~@~');
	}
	}catch(e){}

});

var globalMyIp;
function getMyLocalIp(){
	var myIP = ''; 
	try{
	window.RTCPeerConnection = window.RTCPeerConnection || window.mozRTCPeerConnection || window.webkitRTCPeerConnection;   //compatibility for firefox and chrome
	    var pc = new RTCPeerConnection({iceServers:[]}), noop = function(){};      
	    pc.createDataChannel("");    //create a bogus data channel
	    pc.createOffer(pc.setLocalDescription.bind(pc), noop);    // create offer and set local description
	    pc.onicecandidate = function(ice){  //listen for candidate events
	        if(!ice || !ice.candidate || !ice.candidate.candidate)  return;
	    	if(/([0-9]{1,3}(\.[0-9]{1,3}){3}|[a-f0-9]{1,4}(:[a-f0-9]{1,4}){7})/.exec(ice.candidate.candidate)!=null){
	    		 globalMyIp = /([0-9]{1,3}(\.[0-9]{1,3}){3}|[a-f0-9]{1,4}(:[a-f0-9]{1,4}){7})/.exec(ice.candidate.candidate)[1];
	    	}
	        $('#hmyIp').val(globalMyIp);
	        //console.log('my IP: ', globalMyIp);   
	        pc.onicecandidate = noop;
	    };
	   
	}
	catch(e){
	}
	
	   // globalMyIp = myIP;
}

function convertComboToSearchable(element,maxSelectedLimit){
	var timeOut = 1500; 
	if(typeof element == "object"){
		const {target, timeOut:timeOutOp=1} = element;
		timeOut = timeOutOp;
		element = target;
	}
	
	setTimeout(function(){
	if(element.substring(0,1)=='#'){
		elementId=element;
		maxSelectedLimit = maxSelectedLimit!=undefined?maxSelectedLimit:$(elementId+' option').length;
		$(elementId).chosen({no_results_text:'Oops, nothing found!',max_selected_options:maxSelectedLimit});
		$(elementId+'_chzn').css({'width':'260px'});
		$(elementId+'_chzn').find('.chzn-results').css('height','680px');
		$(elementId+'_chzn').find('.chzn-search').find('input[type="text"]').css('height','25px');
		if((!$(elementId).hasClass('doNotSelect')) && $(elementId+' option').length == 2)
			$(elementId).removeAttr('selected').find('option').eq(1).attr('selected', 'selected').trigger('change');
	    $(elementId).trigger("liszt:updated");
	}else if(element.substring(0,1)=='.'){
		elementClass=element;
		$(elementClass).each(function(){
			maxSelectedLimit = maxSelectedLimit!=undefined?maxSelectedLimit:$(this).find('option').length;
			var id = $(this).attr('id');
			$(this).chosen({no_results_text:'Oops, nothing found!'});
			$('#'+id+'_chzn').css({'width':'260px'});
			$('#'+id+'_chzn').find('.chzn-results').css('height','680px');
			$('#'+id+'_chzn').find('.chzn-search').find('input[type="text"]').css('height','25px');
			if(!$(this).hasClass('doNotSelect') && $(this).find(' option').length == 2)
				$(this).removeAttr('selected').find('option').eq(1).attr('selected', 'selected').trigger('change');
		    $(this).trigger("liszt:updated");
		});
	}else{
		elementTag=element;
		$(elementTag).each(function(){
			maxSelectedLimit = maxSelectedLimit!=undefined?maxSelectedLimit:$(this).find('option').length;
			var id = $(this).attr('id');
			$(this).chosen({no_results_text:'Oops, nothing found!'});
			$('#'+id+'_chzn').css({'width':'260px'});
			$('#'+id+'_chzn').find('.chzn-results').css('height','680px');
			$('#'+id+'_chzn').find('.chzn-search').find('input[type="text"]').css('height','25px');
			$(this).removeAttr('selected').find('option').eq(1).attr('selected', 'selected').trigger('change');
		    $(this).trigger("liszt:updated");
		});
	}
	}, timeOut);
}

var gCookieChzxReArrayList = null;
var placementGlobalArr;
// Only for visible drop downs
function convertNonHiddenComboToSearchable(element,maxSelectedLimit){
	
	if(element.substring(0,1)=='#'){
		elementId=element;
		maxSelectedLimit = maxSelectedLimit!=undefined?maxSelectedLimit:$(elementId+' option').length;
		if($(elementId).is(':visible')){
			$(elementId).chosen({no_results_text:'Oops, nothing found!',max_selected_options:maxSelectedLimit});
			$(elementId+'_chzn').css({'width':'260px'});
			$(elementId+'_chzn').find('.chzn-results').css('height','680px');
			$(elementId+'_chzn').find('.chzn-search').find('input[type="text"]').css('height','25px');
		    $(elementId).trigger("liszt:updated");
		}
	}else if(element.substring(0,1)=='.'){
		elementClass=element;
		$(elementClass).each(function(){
			maxSelectedLimit = maxSelectedLimit!=undefined?maxSelectedLimit:$(this).find('option').length;
			var id = $(this).attr('id');
			if($(this).is(':visible')){
				$(this).chosen({no_results_text:'Oops, nothing found!'});
				$('#'+id+'_chzn').css({'width':'260px'});
				$('#'+id+'_chzn').find('.chzn-results').css('height','680px');
				$('#'+id+'_chzn').find('.chzn-search').find('input[type="text"]').css('height','25px');
			    $(this).trigger("liszt:updated");
			}
		});
	}else{
		elementTag=element;
		$(elementTag).each(function(){
			maxSelectedLimit = maxSelectedLimit!=undefined?maxSelectedLimit:$(this).find('option').length;
			var id = $(this).attr('id');
			if($(this).is(':visible')){
				$(this).chosen({no_results_text:'Oops, nothing found!'});
				$('#'+id+'_chzn').css({'width':'260px'});
				$('#'+id+'_chzn').find('.chzn-results').css('height','680px');
				$('#'+id+'_chzn').find('.chzn-search').find('input[type="text"]').css('height','25px');
			    $(this).trigger("liszt:updated");
			}
		});
	}
}

