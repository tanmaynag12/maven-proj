<script>
	$(function() {
		$("#helpPanel").resizable().removeClass().addClass("helpPanel hide");
		
		$("#helpPanel").draggable({
			handle: "#helpPanelTitle",
			containment: ".draggable-area", scroll: false
		}).removeClass().addClass("helpPanel hide");
		
		$("#helpButton").draggable({
			containment: ".draggable-area", scroll: false
		});
		
		$("#helpButton").on("click", function(){
			$("#helpPanel").show();
			$("#helpButton").hide();
		});
		
		$("#helpPanelClose").on("click", function(){
			$("#helpPanel").hide();
			$("#helpButton").show();
		});
		
		$("#helpPanelOpen").on("click", function(){
			$("#helpPanel").hide();
			$("#helpButton").show();
			
			window.open("helpPanelFull.jsp?help=studentHome");
		});
	});
</script> -->

