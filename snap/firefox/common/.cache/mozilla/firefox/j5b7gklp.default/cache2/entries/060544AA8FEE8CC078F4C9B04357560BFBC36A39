<script type="text/javascript">
	function onAttachFileFunction(){
		$("script[src='js/jquery.tree.js']").remove();
		$("script[src='js/ajaxupload.js").remove();
		$("script[src='scripts/dms/dmsStudent.js").remove();
		$("script[src='scripts/dms/attachFromDMSForSharingStudent.js").remove();
