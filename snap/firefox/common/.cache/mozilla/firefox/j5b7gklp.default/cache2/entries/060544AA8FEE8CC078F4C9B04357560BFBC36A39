<script type="text/javascript">
	$('document').ready(function(){
		$('.ui-datepicker').addClass('notranslate');
		$('input').addClass('notranslate');
		$('select').addClass('notranslate');
		$('.label').addClass('notranslate');
		$('.socialicon').on('hover', function () { $(this).effect('bounce', { times: 1 }, 500); });		
				
		function loadjscssfile(filename, filetype){
			try
			{
				if (filetype == "js") { 
					var fileref = document.createElement('script');
					fileref.setAttribute("type", "text/javascript");
					fileref.setAttribute("src", filename);
				} else if (filetype == "css") {
					var fileref = document.createElement("link");
					fileref.setAttribute("rel", "stylesheet");
					fileref.setAttribute("type", "text/css");
					fileref.setAttribute("href", filename);
				}
				if (typeof fileref != "undefined")
					document.getElementsByTagName("head")[0].appendChild(fileref);
			}
			catch(err)
			{
				 
			}
		}
		try
		{
			loadjscssfile("js/jquery.youtube.player.min.js", "js");
			loadjscssfile("js/swfobject.js", "js");
			loadjscssfile("css/youtube-player.css", "css");
		}
		catch(err)
		{
			 
		}
	});
	function showHelpVideos() {
		
		var player;
		var config = {
			playlistAppendTo : '#playlist', // selector
			// Custom playlist
			playlist : {
				title : 'help videos',
				videos : [ 
				           {id : 'cZVMU56NXug', title : 'Academic Configuration'},
				           {id : 'O8p6PWxMxMs', title : 'Feedback Configuration'},
				           {id : '9BoN9_lTa0g', title : 'Student-Side Feedback'},
				           {id : 'ayfTlNOJdjc', title : 'Test Configuration'},
				           {id : '2viO0ZAFp8I', title : 'Students Attempting Test'},
				           {id : '-fo8PccBduA', title : 'Test Evaluation'},
				           {id : '1I0YOHO5mJA', title : 'Student Result View'},
				           {id : '0julvY4Bfos', title : 'Paper Setting (Random)'}
				         ]
			}
		};
		try
		{
			player = $('.youtube-player').player(config);
		}
		catch(err)
		{
			 
		}
		$('#youtubeHelpModal').modal({
			keyboard : false,
			backdrop : 'static'
		}).css({
			width : '850px',
			'margin-left' : function() {
				return -($(this).width() / 2);
			},
		});
		;
		
	}
</script>
