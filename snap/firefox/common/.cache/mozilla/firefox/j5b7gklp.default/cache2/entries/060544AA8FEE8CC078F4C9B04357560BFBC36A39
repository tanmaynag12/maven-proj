juno.downloadDocumentV2 = (key, bucketName, fileName)=>{
	

fetch('/api/v1/dms/' + bucketName + '/objects/' + key, {
		    headers: {
		        "x-api-key": 'juno-api-key',
		    }
		})
		.then(response => {
		    if (!response.ok) {
		        throw new Error("Failed to fetch the file");
		    }
		    return response.blob(); 
		})
		.then(blob => {
		    const link = document.createElement('a');
		    link.href = URL.createObjectURL(blob);
		    link.download = fileName; 
		    document.body.appendChild(link);
		    link.click(); 
		    document.body.removeChild(link); 
		})
		.catch(error => {
		    console.error('Error downloading the file:', error);
		});
}
juno.uploadDocumentToDMSModuleV2 = (file, note, category, stringifiedRequestBody, resource, resourceId)=>{
	const formData = new FormData();
    formData.append("file", file);
    formData.append("note", note);
    formData.append("category", category);
    formData.append("requestBody", stringifiedRequestBody);
    formData.append("resource", resource);
    formData.append("resourceId", resourceId);
	return fetch('/api/v1/dms/02652e0c-9288-9b54-b774-ecb1d784b5f8/object/module?publicAccessURL=false', {   
				method: "POST", 
				body: formData,
			    headers: {
			    	/* "Content-Type":"application/octet-stream", */
			    	"userId":'21669',
			        "x-api-key": 'juno-api-key',
			    }
			})
			.then(response => {
		        if (!response.ok) {
		            return `Upload failed, response code `;
		        }
			    return response.json(); 
			})
			.catch(error => {
			  	console.error("Upload error:", error);
		     	return "Upload failed due to an error";
			});
}
juno.getDMSModuleDocumentsV2 = (category, stringifiedRequestBody) => {
    const headers = {
    	"userId":'21669',
        "x-api-key": 'juno-api-key',
        "Content-Type": "application/json; charset=utf-8"
    };

    const options = {
        method: "POST",
        headers: headers,
        body: stringifiedRequestBody
    };
    
    return fetch('/api/v1/dms/common/module?category='+category, options)
        .then(response => {
            if (!response.ok) {
                return Promise.reject(`Failed to receive document list, response code `);
            }
            return response.json();
        })
        .then(responseBody => {
            return responseBody; 
        })
        .catch(error => {
            console.error("Error making the request:", error);
            throw error;
        });
}

juno.getDMSModuleDocumentsByResourceV2 = (resource, resourceId) => {
    const headers = {
    	"userId":'21669',
        "x-api-key": 'juno-api-key',
        "Content-Type": "application/json; charset=utf-8"
    };

    const options = {
        headers: headers,
    };
    
    return fetch('/api/v1/dms/common/resource?resource='+resource+'&resourceId='+resourceId, options)
        .then(response => {
            if (!response.ok) {
                return Promise.reject(`Failed to receive document list, response code `);
            }
            return response.json();
        })
        .then(responseBody => {
            return responseBody; 
        })
        .catch(error => {
            console.error("Error making the request:", error);
            throw error;
        });
}



