<script type="text/javascript">
const createTokenTransport = async () => {
	  const controller = new AbortController();
	  const timeoutId = setTimeout(() => controller.abort(), 1000);
	  try {
	    fetch(window.location.origin + "/getLoggedInUserDetails.json", {
	      credentials: "include",
	      headers: {
	        "x-api-key":'juno-api-key',
	      },
	      signal: controller.signal,
	    })
	      .then((res) => res.json())
	      .then((data) => {
	    	if(data){ 
				const id = data["id"];
				document.cookie = "userId=" + id.replace('\"','').replace('\"','');
				localStorage.setItem('user', JSON.stringify(data));
			}
	        fetch(window.location.origin + "/api/v1/transport/token/generate", {
	          method: "POST",
	          headers: {
	            credentials: "include",
	            "Content-Type": "application/json",
	            "x-api-key": 'juno-api-key',
	          },
	          signal: controller.signal,
	          body: JSON.stringify({ junoCampusUserId: data.id }),
	        })
	          .then((response) => response.text())
	          .then((data) => {
	            document.cookie = "authorization=" + data;
	            fetch(window.location.origin+"/api/v1/transport/user/role",{
					  headers: {
	  		            credentials: "include",
	  		            "authorization": data,
	  		            "Content-Type": "application/json",
	  		            "x-api-key": 'juno-api-key',
	  		          }
				  }).then((res)=>res.json()).then((role)=>{
					  if(role.length > 0){
						  window.location.href = '/transport/'+role[0].toLowerCase()+'/dashboard.html';
					  }
				  })
	            fetch(window.location.origin + "/api/v1/transport/user/id", {
	  		          headers: {
	  		            credentials: "include",
	  		            "authorization": data,
	  		            "Content-Type": "application/json",
	  		            "x-api-key": 'juno-api-key',
	  		          },}).then(res=>res.text()).then(id =>{
						document.cookie = "userId=" + id.replace('\"','').replace('\"','');
					})	            
	            if (data) {
	              // todo: fetch() config
	            }
	          });
	      });
	  } catch (error) {
	    console.error(error);
	  } finally {
	    clearTimeout(timeoutId);
	  }
	};
		
	const createTokenWorkflow = async () => {
		  const controller = new AbortController();
		  const timeoutId = setTimeout(() => controller.abort(), 1000);
		  try {
			 fetch(window.location.origin + "/getLoggedInUserDetails.json", {
		      credentials: "include",
		      headers: {
		        "x-api-key":'juno-api-key',
		      },
		      signal: controller.signal,
		    }).then(res=>res.json()).then(data=> {
				if(data){ const id = data["id"];
					document.cookie = "userId=" + id.replace('\"','').replace('\"','');
				localStorage.setItem('user', JSON.stringify(data));
				}
				
		    }).finally(()=>{
		    	window.location.href = "/workflow/user.html"
		    })
		    
		  } catch (error) {
		    console.error(error);
		  } finally {
		    clearTimeout(timeoutId);
		  }
		};
		
		const createTokenDMS = async () => {
			  const controller = new AbortController();
			  const timeoutId = setTimeout(() => controller.abort(), 1000);
			  try {
				 fetch(window.location.origin + "/getLoggedInUserDetails.json", {
			      credentials: "include",
			      headers: {
			        "x-api-key":'juno-api-key',
			      },
			      signal: controller.signal,
			    }).then(res=>res.json()).then(data=> {
					if(data){ const id = data["id"];
						document.cookie = "userId=" + id.replace('\"','').replace('\"','');
					localStorage.setItem('user', JSON.stringify(data));

					const bucketName = data["bucket-name"];
					document.cookie = "bucket-name=" + bucketName.replace('\"','').replace('\"','');
					}
					
					
			    }).finally(()=>{
			    	window.location.href = "/dms/my-files.html"
			    })
			    
			  } catch (error) {
			    console.error(error);
			  } finally {
			    clearTimeout(timeoutId);
			  }
			};
</script>


<script type="text/javascript">

		
 var cntTime=0;
 var int=self.setInterval(function(){clock();},300000);
 function clock()
 {
	//window.load=getUnreadMessageAndAnnouncement();
	window.load=notify();
	//window.load=updateAllStudentforSearchCommon();
 }
 var gLocalIP='';
 var gPublicIP='';
 function notify(){
	
		var hdnUserRole = $('#hdnUserRole').val();
		var url = 'getServerUserNotification.json';
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_getServerUserNotification.json';
		}
	    $.ajax({
			url : url,
			dataType : 'json',
			type : 'POST',
			success : function(jArray) {

				if (jArray.length > 0) {
					var flagType = jArray[0]["flagType"];
					var message = decodeURIComponent(jArray[0]["message"]);
					var blockUI = jArray[0]["blockUI"];
					$("#serverNotificationBar").html('<center><b style ="font-size:13px;text-align:center;">'+message+'</b></center>'+
					'<i title="Dismiss" class="fa fa-window-close" style="float:right;width:40px;margin-top:-10px" onclick="$(\'#serverNotificationBar\').hide()">');
				
					if (flagType == "success"){
						$("#serverNotificationBar").show();
						$("#serverNotificationBar").css('background-color', '#DFF0D8');//green color
					}else if(flagType == "backup"){
						$("#serverNotificationBar").show();
						$("#serverNotificationBar").css('background-color', '#F2DEDE');//red color
					}else 
						$("#serverNotificationBar").hide();
					$("#serverNotificationBar").css('color', '#000000');
				}else{
					$("#serverNotificationBar").hide();
				}

				},
			});
	
	
	}
	function getUnreadMessageAndAnnouncement() {
		$.ajax({
			url : 'getUnreadMessageAndAnnouncement.json',
			dataType : 'json',
			success : function(jArray) {
				if (jArray[0]["count"] == 0)
					$("#NoticeCounter").hide();
				else
					$("#NoticeCounter").html(jArray[0]["count"]).show();

				if (jArray[0]["dutyCount"] == 0)
					$("#DutiesCounter").hide();
				else
					$("#DutiesCounter").html(jArray[0]["dutyCount"]).show();

				if (jArray[0]["inboxCount"] == 0)
					$("#InboxCounter").hide();
				else
					$("#InboxCounter").html(jArray[0]["inboxCount"]).show();

				$("#countAnnouncement").html(jArray[0]["count"]).show();
			},
		});
	}

	function setAllUnreadMessagesAndAnnouncementsAsRead() {
		$.ajax({
			url : 'setAllUnreadAnnouncementsAndMessagesAsRead.json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function() {
				getUnreadMessageAndAnnouncement();
			},
			error : function() {
			}
		});
	}
	/*function updateAllStudentforSearchCommon()
	{
		$.ajax({
			url:'createStudentSearchMasterData.json',
			data:'',
			dataType:'json',
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				
			}
		});
	}*/
	$(document).ready(function() {
		clock();
	});
</script>
	
	
	
 
