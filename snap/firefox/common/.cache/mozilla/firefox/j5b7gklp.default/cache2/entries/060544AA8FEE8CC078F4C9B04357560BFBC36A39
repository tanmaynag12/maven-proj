<script type="text/javascript">


setInterval(function(){ breadcrumbBoxDateTimer(); }, 1000);//myTimer
var gTimeInMillis =  1744361303417;


function breadcrumbBoxDateTimer() {
	gTimeInMillis=gTimeInMillis+1000;
	var dObj = new Date(gTimeInMillis);
	var time=dObj.toLocaleTimeString();

	
	/***Add Bookmark Icon***/
	if($('#menuNameSpan').length==0 && $.trim(firstTimebreadCrumbs)!='' && $('.breadCrumbs').length>0){
		var dropdownLst = '';
		

		$(".breadCrumbs").html("<span id='menuNameSpan'></span><b  style='font-size:13px;font-weight: normal;' class='pull-right'> "+$("#dateTime").val()+" <span class='breadCrumbs_timestamp' style='margin-left:5px;'>"+time+"</span>"+dropdownLst+"</b>");
		
		
		$('#menuNameSpan').html(firstTimebreadCrumbs);
		
	}else if($.trim(firstTimebreadCrumbs)==''){
		$(".breadCrumbs").html("<b  style='font-size:13px;font-weight: normal;' class='pull-right'> "+$("#dateTime").val()+" <span class='breadCrumbs_timestamp' style='margin-left:5px;'>"+time+"<span></b>");
	}
	$('.breadCrumbs_timestamp').text(time);
}

var menuShortcutId;
var pid;
var boiler;
var firstTimebreadCrumbs = "";
var shrtBoiler="";
$(document).ready(function() {
	var isForShortcut = false;
	getMenuList(isForShortcut);
});


function getMenuList(isForShortcut){
	var redirectStatusHideMenuProp = $("#redirectStatusHideMenuProp").val();
	var redirectStatusHideMenu = $("#redirectStatusHideMenu").val();
	if(redirectStatusHideMenu!=undefined && redirectStatusHideMenuProp == "true"){
		return;
	}
	var i_menuName = 0;
	var i_menuLink = 1;
	var i_menuMasterId = 2;
	var i_menuSuperMasterId = 3;
	var i_menuShortcutMappingId = 4;
	var i_children = 5;
	
	var title1=document.title;
	var buildItem = function(item){
		var html ='<li>'+'<a menuShortcutId="'+item[i_menuShortcutMappingId]+'" pid="'+item[i_menuMasterId]+'" href="'+item[i_menuLink]+'">'+item[i_menuName]+'</a>';
		if (item[i_children]) {		    	
			html += "<ul>";
			$.each(item[i_children], function (index, sub) {
				html += buildItem(sub);
			});
			html += "</ul>";
		}
		html += "</li>";

		/***Add link in Shortcut***/
		if(item[i_menuShortcutMappingId]!=0 && item[i_menuShortcutMappingId]!=undefined){
			shrtBoiler = '<li id='+item[i_menuShortcutMappingId]+' class="shortcutMenuLink"><a menuShortcutId="'+item[i_menuShortcutMappingId]+'" pid="'+item[i_menuMasterId]+'" href="'+item[i_menuLink]+'" class="j-fa fa-bookmark-o">'+item[i_menuName]+'</a></li>';
			$('#shortcutMenuLinDiv').after(shrtBoiler);
		}
		/******/
		return html;
	};
	var generateTree = function(jArray){
		var obj=jArray;
		//boiler += '<li>'+'<a menushortcutid="0" pid="20001" href="#" style="padding-right: 23px;">----Manu</a><ul>';
		$.each(obj, function (index, item) {
			boiler +=buildItem(item);
		});
		//boiler += '</ul></li>'
	};
	
	apiCall_getMenuListByRoleDBMin(isForShortcut).then(function(jArray){
			
		    boiler = ''+//<a class="animateddrawer" id="ddsmoothmenu-mobiletoggle" href="#"><span></span>
			'<div id="global-nav-id" class="ddsmoothmenu">'+
			'<ul>';
			generateTree(jArray);	
			boiler +='</ul></div>';
			$('#menuRole').html(boiler);	
			ddsmoothmenu.init({
				mainmenuid: "global-nav-id", //menu DIV id
				orientation: 'h', //Horizontal or vertical menu: Set to "h" or "v"
				classname: 'ddsmoothmenu', //class added to menu's outer DIV
				//customtheme: ["#1c5a80", "#18374a"],
				contentsource: "markup" //"markup" or ["container_id", "path_to_menu_file"]
			});

			 $(document).ready(
					function() {
						$(".breadCrumbs").text('');
						var loc = window.location.href;
						loc = loc.substring(loc.lastIndexOf('/') + 1);
						var currentObject = $('#global-nav-id').find('a[href ="' + loc + '"]').eq(0);
						var breadCrum = new Array();
						breadCrum.push(currentObject.text());
						console.log($(currentObject).attr("pid"));
						pid = $(currentObject).attr("pid");
						menuShortcutId = $(currentObject).attr("menuShortcutId");
						while (currentObject.parent().parent().prev().prop('tagName') == 'A') {
							breadCrum.push(currentObject.parent().parent().prev().text());
							currentObject = currentObject.parent().parent().prev();
						}
						var temp = new Array();
						var i = 0;
						var j = breadCrum.length;
						while (i < j) {
							temp.push(breadCrum.pop());
							i++;
						}
						$(".breadCrumbs").html(temp.join(" <i class='fa fa-angle-double-right'></i><i style=\"color: transparent;margin-left: -6px;\">&raquo;</i> "));
						firstTimebreadCrumbs = temp.join(" <i class='fa fa-angle-double-right'></i><i style=\"color: transparent;margin-left: -6px;\">&raquo;</i> ");
						if(firstTimebreadCrumbs==""){
							firstTimebreadCrumbs=title1;
						}
					});
	});
}

function apiCall_getMenuListByRoleDBMin(isForShortcut){
	return new Promise((resolve, reject)=>{
		//window.api_masterMenuList
		if(window.api_masterMenuList==undefined){
			var hdnUserRole = $('#hdnUserRole').val();
			var url = 'getMenuListByRoleDBMin.json?isForShortcut='+isForShortcut;
			if(hdnUserRole == "student" || hdnUserRole == "Student"){
				url = 'stu_getMenuListByRoleDBMin.json?isForShortcut='+isForShortcut;
			}
			$.ajax({
				url : url,
				dataType : 'json',
				async:true,
				success : function(jArray) 
				{
					resolve(jArray);
				},
				error:function(){
					jAlert("Error...!");
				}
			});
		}else{
			resolve(window.api_masterMenuList);
		}
	});
}

$(document).ready(function() {
	
	var loc = window.location.href;
	loc = loc.substring(loc.lastIndexOf('/') + 1);
	var currentObject = $('#global-nav-id').find('a[href ="' + loc + '"]');
	if(currentObject.text()!="")
		document.title = currentObject.text();
	else{
		try{
			var title1=document.title;
			title1=title1.replace(/GEMS - /g, '').replace(/GEMS /g, '').replace(/GEMS/g, '').replace(/gems/g, '');
			document.title=title1;
		}catch(e){}
	}
	console.log(loc);
});

function addMenuInShortcut(){
	var menuMasterId = $('#selectedMenuId').attr('value');
	var isForShortcut = true;
	$.ajax({
		url:'addSelectedMenuInShortcut.json?menuMasterId='+menuMasterId,
		dataType:'json',
		async:false,
		beforeSend:function() {
			//startCommonAjaxSpinner();
		},
		complete: function() {         
			//stopCommonAjaxSpinner();
		},
		success:function(jArray){
			$('.shortcutMenuLink').remove(); 
			getMenuList(isForShortcut);
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
}

function deleteMenuInShortcut(){
	var menuMasterId = $('#selectedMenuId').attr('value');
	var isForShortcut = true;
	$.ajax({
		url:'deleteSelectedMenuInShortcut.json?menuMasterId='+menuMasterId,
		dataType:'json',
		async:false,
		beforeSend:function() {
			//startCommonAjaxSpinner();
		},
		complete: function() {         
			//stopCommonAjaxSpinner();
		},
		success:function(jArray){
			$('.shortcutMenuLink').remove(); 
			getMenuList(isForShortcut);
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
}

function dropLinkFromShortcut(ev) {
    ev.preventDefault();
    var data = ev.dataTransfer.getData("text");
    ev.target.appendChild(document.getElementById(data));
}

