function setEulaValue()
{
	$.ajax({
		url:'setEulaValue.json',
		dataType:'text',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(msg){
