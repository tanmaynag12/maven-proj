function getbooksIssuedCount()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getbooksIssuedCount.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getbooksIssuedCount.json';
	}
	$.ajax({
		url:url,
		dataType:'text',
		async:false,
		beforeSend: function() {
		
		},
		complete: function() {
			
		},
		success:function(count){
			
			if(count != null){
				
				$("#bookIssueCount").html(count+' <span style="font-size:11px;text-transform:none;">Issued</span>');
				//alert(jArray["postIds"]);
			}
			else
				{
				$("#bookIssueCount").text('-');
				}
				
		},
		error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
	});
	
}

function getPlacementDetail(flag)
{
	$("#divalumpost").hide();
	$('#tabResult').show();
	//alert(flag);
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'GetPlacementAndProjectPlacementListforStudent.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_GetPlacementAndProjectPlacementListforStudent.json';
	}
	$.ajax({
		url:url,
		dataType:'json',
		async:false,
		beforeSend: function() {
		//	$("#tabResult").html('<p style="text-align:center;"><img src="img/loading-image.gif"></p>');
		},
		complete: function() {
			
		},
		success:function(jArray){
			var boiler='<table class="table table-bordered table-condensed">'+
			'<tr><th colspan=5  style="text-align:center;background-color:#e5f2fc;">Placement</th></tr>'+
			'<tr style="text-align:center;"><th>Company Name</th><th>Post</th><th>Job Type</th><th>Register</th></tr>';
			
			if(jArray.length==0)
			{
				$("#tabResult").html('');
			}
			
			if(jArray != null)
			{
				if(flag==''){
				$("#companyCount").text(jArray.length);
				
				if($("#companyCountPlc") != undefined && $("#companyCountPlc") != 'undefined')
					$("#companyCountPlc").text(jArray.length);
				//alert(jArray["postIds"]);
				}
				else
				{
					if(jArray.length==0)
					{
						$("#tabResult").html('<p style="text-align:center;">No Details found</p>');
					}
					else
					{
					for(var i=0;i<jArray.length;i++)
					{
						boiler+= '<tr><td>'+jArray[i]["companyName"]+'</td>';
						boiler+= '<td>'+jArray[i]["post"]+'</td>';
						boiler+= '<td>'+jArray[i]["type"]+'</td>';
						/*boiler+= '<td><a href="javascript:void(0);" onclick=fetchCriteria('+jArray[i]["projPlacId"]+','+jArray[i]["jobTypeId"]+')>View</a></td>';*/
						if(jArray[i]["registeredStatus"]=='1')
							boiler+= '<td>Registered</td></tr>';
						else
							boiler+= '<td><a href="javascript:void(0);" onclick=openPlacement('+jArray[i]["jobTypeId"]+')>Apply</a></td></tr>';
					}
					boiler+= '</table>';
					$("#tabResult").html(boiler);
					}
					
				}
			}
			else
			{
				$("#companyCount").text('-');
			}	
		},
		error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}



function fetchCriteria(placementId,type)
{
	var boiler = '<table class="table table-bordered">'+
						'<tr>'+
							'<th style="background-color:#dfeffc;">HSC & SSC Criteria</th>' +
							'<td>'+
									'<table>'+
										'<tr>'+
											'<th>10th Percentage :</th>'+
											'<td><lable id="ssclbl'+placementId+'"></lable></td>'+
										'</tr>'+
										'<tr>'+
											'<th>12th Percentage :</th>'+
											'<td><lable id="hsclbl'+placementId+'"></lable></td>'+
										'</tr>'+
										'<tr>'+
											'<th>Gap :</th>'+
											'<td><lable id="sscHscGaplbl'+placementId+'"></lable></td>'+
										'</tr>'+
									'</table>'+
							'</td>'+
						'</tr>'+
						'<tr>'+
								'<th style="background-color:#dfeffc;">Qualification Criteria :</th>'+
								'<td>'+
									'<table id="tlbDegreeCriteria'+placementId+'" class="table-bordered">'+
									'</table>'+
								'</td>'+
						'</tr></table>';
	$("#tabResult").html(boiler);
	
	$.ajax({
		url : 'fetchSscHscCriteriaForCreatePlacement.json',
		data : "placementId=" + placementId,
		dataType : 'json',
		async:false,
		beforeSend : function() {
			
		},
		complete : function() {
			
		},
		success : function(jArray) {
			if(jArray.length > 0)
			{
				if(jArray[0]["sscPercent"] != undefined)
					{
						$("#ssclbl"+placementId).html(jArray[0]["sscPercent"] + " %");
					}
				else
				{
					$("#ssclbl"+placementId).html("-");
				}
				
				if(jArray[0]["hscPercent"] != undefined)
					{
						$("#hsclbl"+placementId).html(jArray[0]["hscPercent"] + " %");
					}
				else
					{
						$("#hsclbl"+placementId).html("-");
					}
				
				if(jArray[0]["gap"] != undefined)
				{
					$("#sscHscGaplbl"+placementId).html(jArray[0]["gap"] + " Yrs");
				}
				else
				{
					$("#sscHscGaplbl"+placementId).html("-");
				}
			}
			else
			{
				$("#ssclbl"+placementId).html("-");
				$("#hsclbl"+placementId).html("-");
				$("#sscHscGaplbl"+placementId).html("-");
			}
			
			
			
		},
		error : function() {
			jAlert("Error in job SSC HSC CRITERIA", "Error");
		}
	});
	
	showDegreeCriteria(placementId,type);
}

function openPlacement(type)
{
	if(type==1)
	{
		window.open("viewPlacements.htm?placementFlag=INTERN","_self");
	}
	else
	{
		window.open("registerforPlacements.htm","_self");
	}
}

