	window.location.replace('stu_StudentTimeTable.htm');
}

function showTodaysConflictTimetableForStudent()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getCurrentConlictScheduleDetailsForstudent.json?date='+ todaysDate();
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getCurrentConlictScheduleDetailsForstudent.json?date='+ todaysDate();
	}
	$.ajax({
		url:url,
		dataType:'json',
		beforeSend:function() {
			
		},
		complete: function() {

		},
		success:function(jArray){
			if(jArray.length>0)
			{
				var todaysConflictSchedule='<table class="table table-condensed alert-info"> <tbody><tr><td style="text-align:center;" colspan="'+jArray.length+'">Session conflict Details</td></tr>';
				todaysConflictSchedule+='<tr style="text-align:center;">';
				for(var i=0 ; i<jArray.length;i++)
				{

					if(jArray[i]["subjectName"]!=undefined)
					{
						todaysConflictSchedule +='<td style="text-align:center;"><i>'+ jArray[i]["startTime"] +'-'+ jArray[i]["endTime"] +'</i></td>';
					}	
				}	
				todaysConflictSchedule +='</tr><tr>';
				for(var i=0 ; i<jArray.length;i++)
				{
					if(jArray[i]["subjectName"]!=undefined)
					{
						todaysConflictSchedule +='<td style="text-align:center;">'+jArray[i]["shortName"]+'</td>';
					}	

				}	
				todaysConflictSchedule += '</tr><tr>';

				for(var i=0 ; i<jArray.length;i++)
				{
					if(jArray[i]["subjectName"]!=undefined)
					{

						todaysConflictSchedule +='<td style="text-align:center;">'+ jArray[i]["infra"]+'</td>';
					}
					else
					{
						todaysConflictSchedule += '<td></td>';
					}

				}
				todaysConflictSchedule +='</tr></tbody></table>';
				$('#viewTimetable').append(todaysConflictSchedule);
			}

		},
		error:function(){
			getCustomAlertMessage('alert_error');
		}
	});
}
function showAttachmentFromStudentScheduler(src){
	var docArr = $(src).data('uploded');
	var textFileseArr = $(src).data('textFiles');
	var sub = $(src).data('sub');
	var boiler = '';
	var boilernew = '';
	
	for(var i in textFileseArr){
		var iconPath = gFileTypeIcon[textFileseArr[i].dext];
		var downloadLinkTmp = '<a title="View" href="javascript:void(0)" onclick="showTextDocumentForAlumini(\''+textFileseArr[i].eDocId+'\',\''+textFileseArr[i].user+'\')"><i class="fa fa-files-o fa-2x" />  '+textFileseArr[i].dName+'</a><br>';
		boiler+=downloadLinkTmp;
	}
	for(var i in docArr){
		var iconPath = gFileTypeIcon[docArr[i].dext];
		var downloadLinkTmp = (juno.label['a.download.json'].setIndexedArgs((docArr[i].eDocPathId),"UPLOADED", encodeURIComponent(docArr[i].dName),'<img src="'+iconPath+'" width="25px" height="25px"/>'+docArr[i].dName+'<br>'));
		boiler+=downloadLinkTmp;
	}
	
	boilernew = '<table class="table table-bordered table-condensed"><tr><th style="width:150px;">Attachment</th><td>'+boiler+'</td></tr></table>';
	
	$('#gemsCommonDialogModal .modal-header').html(sub);
	$('#gemsCommonDialogModal .modal-body').html(boilernew);
	$('#gemsCommonDialogModal').modal('show');
}

function checkStudentProfileImage()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'checkProfileImage.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_checkProfileImage.json';
	}
	$.ajax({
			url:url,
			dataType:'json',
			async:false,			
			success:function(jArray){	
				if(jArray['isImage'] == false){		
					var boiler=	'<div class="thumbnail" id="studProfileImage2342" style="height:120px;width:120px;margin-left:20px"><img src="getStudentProfileImage.json?id=-1249993431" alt="Photo" name="Photo" style="background-color: #FFFFFF" width="120px" height="120px" border="1" align="right"><br></div>';
					boiler+='&nbsp&nbsp&nbsp&nbsp&nbsp<div class="btn btn-info" style="width:110px;height:20px">Upload Image<input style="margin-top:-40px;margin-left:-35px;opacity:0;" type="file" name="fileData" id="profilePictureUpload" onclick="studProfilePic();" accept="image/*"></div>';	
					$('#profileImgModalBodyDiv').html(boiler);
					$('#profileImgModal').modal('show');
	}
			},
			error:function(){
			}
		});
}

function base64ToBlob(base64, mime) 
{
    mime = mime || '';
    var sliceSize = 1024;
    var byteChars = window.atob(base64);
    var byteArrays = [];

    for (var offset = 0, len = byteChars.length; offset < len; offset += sliceSize) {
        var slice = byteChars.slice(offset, offset + sliceSize);

        var byteNumbers = new Array(slice.length);
        for (var i = 0; i < slice.length; i++) {
            byteNumbers[i] = slice.charCodeAt(i);
        }

        var byteArray = new Uint8Array(byteNumbers);

        byteArrays.push(byteArray);
    }

    return new Blob(byteArrays, {type: mime});
}
function studProfilePic(){
	var Maxsize =  5242880;	
	$image_crop = $('#image_demo').croppie({
		enableExif: true,
		enableOrientation: true,
		viewport: {
			width:200,
			height:200,
			type:'square' //circle
		},
		boundary:{
			width:300,
			height:300
		}
	});

	$('#profilePictureUpload').on('change', function(){
		var reader = new FileReader();
		reader.onload = function (event) {
			$image_crop.croppie('bind', {
				url: event.target.result
			}).then(function(){
				console.log('jQuery bind complete');
			});
		}
		reader.readAsDataURL(this.files[0]);
		$('#uploadimageModal').modal('show');
	});

	$(function() {
		$('.vanilla-rotate').on('click', function(ev) {
			$image_crop.croppie('rotate', parseInt($(this).data('deg')));
		});
	});

	$('.crop_image').click(function(event){
		$image_crop.croppie('result', {
			type: 'canvas',
			size: 'viewport'
		}).then(function(response){
			console.log(response);
			var base64ImageContent = response.replace(/^data:image\/(png|jpg);base64,/, "")
			var blob = base64ToBlob(base64ImageContent, 'image/png');                
			var formData = new FormData();
			formData.append('fileData', blob);
			$.ajax({
				url:"uploadStudentProfileImage.json",
				type: "POST",
				contentType: false,
				processData: false,
				data: formData,
			name : 'fileData',
			size: Maxsize,
			onSubmit : function(data) {
				//if(extension.toString().toLowerCase() == 'png' || extension.toString().toLowerCase() == 'jpg',extension.toString().toLowerCase() == 'jpeg'){
					startLoading();
					this.setData({
						'fileSize':Maxsize
					});
			},
			success:function(data)
			{
				$('#uploadimageModal').modal('hide');

				if(isNaN(data)){
					//showErrorMessage('File size is larger than specified',10000);
					jAlert("File size is larger than specified !!");
				} else {
					var imageBoiler = 
						'<img src="getStudentProfileImage.json?type=profile&someparam='+data+'" alt="Photo" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" />';

					$('#studProfileImage2342').html(imageBoiler);	
					jAlert("Image uploaded successfully !!!");					
					imageStatus = true;
					showSuccessMessage('Image uploaded. ',10000); 
					$('#profileImgModal').modal('hide');
				}
				//$('#uploaded_image').html(data);
				$image_crop.croppie('destroy');
			}
		});
});
	
});
