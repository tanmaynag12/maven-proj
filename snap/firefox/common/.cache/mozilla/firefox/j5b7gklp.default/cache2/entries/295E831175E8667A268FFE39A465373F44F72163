var dates = {
 		convert:function(d) {
 			// Converts the date in d to a date-object. The input can be:
 			//   a date object: returned without modification
 			//  an array      : Interpreted as [year,month,day]. NOTE: month is 0-11.
 			//   a number     : Interpreted as number of milliseconds
 			//                  since 1 Jan 1970 (a timestamp) 
 			//   a string     : Any format supported by the javascript engine, like
 			//                  "YYYY/MM/DD", "MM/DD/YYYY", "Jan 31 2009" etc.
 			//  an object     : Interpreted as an object with year, month and date
 			//                  attributes.  **NOTE** month is 0-11.
 			return (
 					d.constructor === Date ? d :
 						d.constructor === Array ? new Date(d[0],d[1],d[2]) :
 							d.constructor === Number ? new Date(d) :
 								d.constructor === String ? new Date(d) :
 									typeof d === "object" ? new Date(d.year,d.month,d.date) :
 										NaN
 			);
 		},
 		compare:function(a,b) {
 			// Compare two dates (could be of any type supported by the convert
 			// function above) and returns:
 			//  -1 : if a < b
 			//   0 : if a = b
 			//   1 : if a > b
 			// NaN : if a or b is an illegal date
 			// NOTE: The code inside isFinite does an assignment (=).
 			return (
 					isFinite(a=this.convert(a).valueOf()) &&
 					isFinite(b=this.convert(b).valueOf()) ?
 							(a>b)-(a<b) :
 								NaN
 			);
 		},
 		inRange:function(d,start,end) {
 			// Checks if date in d is between dates in start and end.
 			// Returns a boolean or NaN:
 			//    true  : if d is between start and end (inclusive)
 			//    false : if d is before start or after end
 			//    NaN   : if one or more of the dates is illegal.
 			// NOTE: The code inside isFinite does an assignment (=).
 			return (
 					isFinite(d=this.convert(d).valueOf()) &&
 					isFinite(start=this.convert(start).valueOf()) &&
 					isFinite(end=this.convert(end).valueOf()) ?
 							start <= d && d <= end :
 								NaN
 			);
 		}
 };



var months = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
var days = ["Sun","Mon","Tue","Wed","Thu","Fri","Sat"];
var academicYrStDate = new Date();
var academicYrEndDate = new Date();
var header = '';

function getAcademicCalendarDetails()
{
	$.ajax({
		url:'getActualAcademicCalendarForHomePage.json', 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){

			var currDate = new Date();
			currDate.setHours(0, 0, 0, 0);

			/********************To check entry for date is available or not***************/
			var holidayDateArray = new Array();
			var eventDateArray = new Array();

			var minStDate =  dates.convert(jArray[0][0]);
			var maxEndDate = dates.convert(jArray[0][1]);

			$('#fromDate').datepicker("option", "minDate", minStDate);
			var minMonth = minStDate.getMonth();
			var maxMonth = maxEndDate.getMonth();
			var minYear = minStDate.getFullYear();
			var maxYear = maxEndDate.getFullYear();

			var monthCol = eval(maxMonth - minMonth);
			if(monthCol<0)
				monthCol = eval(12 + monthCol);


			header = $('#department :selected').text()+"<br/>"+$('#semester :selected').text();

			var boiler = '<thead id="thead"><tr><th style="display:none;" colspan="'+(((monthCol+1)*3)+1)+'" id="calendarHeader"></th></tr>'+
			'<tr><th style="width:100px;"> Academic<br/>Calendar</th></tr></thead>'+
			'<tbody id="tbody">'+
			'<tr><th style="width:100px;">1</th></tr><tr><th style="width:100px;">2</th></tr><tr><th style="width:100px;">3</th></tr>'+
			'<tr><th style="width:100px;">4</th></tr><tr><th style="width:100px;">5</th></tr><tr><th style="width:100px;">6</th></tr>'+
			'<tr><th style="width:100px;">7</th></tr><tr><th style="width:100px;">8</th></tr><tr><th style="width:100px;">9</th></tr>'+
			'<tr><th style="width:100px;">10</th></tr><tr><th style="width:100px;">11</th></tr><tr><th style="width:100px;">12</th></tr>'+
			'<tr><th style="width:100px;">13</th></tr><tr><th style="width:100px;">14</th></tr><tr><th style="width:100px;">15</th></tr>'+
			'<tr><th style="width:100px;">16</th></tr><tr><th style="width:100px;">17</th></tr><tr><th style="width:100px;">18</th></tr>'+
			'<tr><th style="width:100px;">19</th></tr><tr><th style="width:100px;">20</th></tr><tr><th style="width:100px;">21</th></tr>'+
			'<tr><th style="width:100px;">22</th></tr><tr><th style="width:100px;">23</th></tr><tr><th style="width:100px;">24</th></tr>'+
			'<tr><th style="width:100px;">25</th></tr><tr><th style="width:100px;">26</th></tr><tr><th style="width:100px;">27</th></tr>'+
			'<tr><th style="width:100px;">28</th></tr><tr><th style="width:100px;">29</th></tr><tr><th style="width:100px;">30</th></tr>'+
			'<tr><th style="width:100px;">31</th></tr></tbody>';

			//$('#commonDialogHeading').html('Academic Calendar');
			$('#scheduler').html(boiler);
			$('#homeSchedulerContent').show();
			$('#homeContent').hide();
			/*$('#commonDialog').css({
				width:'75%','margin-left':function(){
					return -($(this).width()/2);
				},
			}).modal('show');*/
			

			var tbl = document.getElementById('scheduler');
			for(var c= 1; c<=31; c++)
				$(tbl.rows[c].cells[0]).attr('class', 'headlabel');


			academicYrStDate.setFullYear(minYear, minMonth, 1);
			academicYrStDate.setHours(0, 0, 0, 0);

			academicYrEndDate.setFullYear(maxYear, (maxMonth+1), 0);	// get last day of maxMonth
			academicYrEndDate.setHours(0, 0, 0, 0);

			for(var col=0; col<=monthCol; col++)
			{
				var month = (minMonth+col)%12;
				if(col>0 && month==0)
					minYear++;
				addMonthCol(month, minYear);
			}

			/************************To Add Holidays **********************/

			for(var h=0; h<jArray[1].length; h++)
			{
				var holidayDate = dates.convert(jArray[1][h][0]);		//"holidayDate"
				holidayDate.setHours(0, 0, 0, 0);
				holidayDate = holidayDate.toString().split("00")[0].split(" ").join("_");

				var holidayBoiler = '';
				if($.inArray(holidayDate.toString(),holidayDateArray)<0)
				{
					holidayDateArray.push(holidayDate);
					holidayBoiler = '<br/><div id="div-'+holidayDate+'" style="color:#0033cc" title="Holiday">'+jArray[1][h][1];
					$('#'+holidayDate).append(holidayBoiler);		//"holidayReason"
				}
				else
				{
					holidayBoiler = ' &<br/>'+jArray[1][h][1];
					$('#div-'+holidayDate).append(holidayBoiler);		//"holidayReason"
				}
			}

			/********************************To Add Events *******************************/

			/*for(var e=0; e<jArray[2].length; e++)
			{
				var eventDate = dates.convert(jArray[2][e][0]);
				eventDate.setHours(0, 0, 0, 0);

				var color="#cc0000";
				alert(e+"============"+jArray[2][e][5]);

				alert(jArray[2][e][5]=="true");
				if(jArray[2][e][5]=="true")
				{
					alert("In true");
					color="#006600";
				}
				else
					alert("In else");

				eventDate = eventDate.toString().split("00")[0].split(" ").join("_");

				var eventBoiler = '';
				if($.inArray(eventDate.toString(),eventDateArray)<0)
				{
					eventDateArray.push(eventDate);
					eventBoiler = '<div id="div-'+eventDate+'" style="color:'+color+'"><u>Event</u> :-<br/>'+jArray[2][e][0]+'</div>';
					$('#'+eventDate).append(eventBoiler);
				}
				else
				{
					eventBoiler = ' &<br/>'+jArray[2][e][0];
					$('#div-'+eventDate).append(eventBoiler);
				}*/

				var milestoneDates = new Array();
				
				for(var m=0; m<jArray[2].length; m++)
				{
					var color="red";

					if(jArray[2][m][5]=="true")
						color="green";
					
					var milestoneDate = dates.convert(jArray[2][m][0]);
					milestoneDate.setHours(0, 0, 0, 0);
					milestoneDate = milestoneDate.toString().split("00")[0].split(" ").join("_");

					var uniqMilestone = milestoneDate+"-"+jArray[2][m][2];
					if($.inArray(uniqMilestone,milestoneDates)<0)
					{
						milestoneDates.push(uniqMilestone);
						var roleType = $('#roleType').val();
						if(roleType!="nonteaching" && roleType!="empadmin")
						{
							var milestoneBoiler = ' <br/><div id="div-'+milestoneDate+"-"+jArray[2][m][2]+'" style="color:'+color+'"><span id="'+milestoneDate+"-"+jArray[2][m][2]+'">'+
							jArray[2][m][1]+':-<br/>'+jArray[2][m][3]+'</span></div>';
							$('#'+milestoneDate).append(milestoneBoiler);
						}
						else
						{
							var milestoneBoiler = ' <br/><div id="div-'+milestoneDate+"-"+jArray[2][m][2]+'" style="color:'+color+'"><a id="'+milestoneDate+"-"+jArray[2][m][2]+'">'+
							jArray[2][m][1]+':-<br/>'+jArray[2][m][3]+'</a></div>';
							$('#'+milestoneDate).append(milestoneBoiler);
						}
					}
					else
						$('#'+milestoneDate+"-"+jArray[2][m][2]).append(' & '+jArray[2][m][3]);
				}
			//}

			/********************************To Add evaluation schemas *******************************/
			for(var e=0; e<jArray[3].length; e++)
			{

				var evalDate = dates.convert(jArray[3][e][0]);
				evalDate.setHours(0, 0, 0, 0);

				evalDate = evalDate.toString().split("00")[0].split(" ").join("_");
				var evalBoiler = '<br/><a onclick="viewConductionSchema('+jArray[3][e][3]+',\''+evalDate+'\')">'+
				jArray[3][e][1]+' of '+jArray[3][e][2]+'</a><br/>';
				$('#'+evalDate).append(evalBoiler);
			}

			$('#planTable').show();
		},
		error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}

function getBackToHome()
{
	$('#scheduler').html('');
	$('#homeSchedulerContent').hide();
	$('#homeContent').show();
}

function addMonthCol(month, year)
{

	var tbl = document.getElementById('scheduler'); // table reference
	var header = tbl.rows[1].insertCell(tbl.rows[1].cells.length);
	var name = months[month]+" "+year; 
	header.setAttribute('class', 'headlabel');
	header.setAttribute('colspan','3');
	var div = document.createElement('h5'); // create Label element
	div.setAttribute('id', 'col_'+month);
	div.setAttribute('style', 'background:#E4F1FC;color: black;');
	header.appendChild(div);
	$('#col_'+month).text(name);

	for (var i = 2; i <tbl.rows.length; i++) 				// Creating Cells
		createCell(month,i);
}

function createCell(month,i)
{
	var tbl = document.getElementById('scheduler'); // table reference
	var academicYrStMonth = academicYrStDate.getMonth();
	var cell=tbl.rows[i].insertCell(tbl.rows[i].cells.length);

	if(academicYrStMonth!=month)	//	Date not available in Month
	{
		var div = document.createElement('label'); // create DIV element
		cell.setAttribute('style', 'background-color:#CAC6C6;width:50px;min-width:0px');
		cell.appendChild(div);
	}
	else
	{
		var day = academicYrStDate.getDay();
		var Id=academicYrStDate+"";
		Id = Id.split("00")[0].split(" ").join("_")+"Day";
		cell.setAttribute('style', 'width:50px;min-width:0px;text-align:left');
		cell.setAttribute('id',Id);
		$('#'+Id).text(days[day]);
	}

	cell=tbl.rows[i].insertCell(tbl.rows[i].cells.length);
	cell.setAttribute('colspan','2');
	var div = document.createElement('label'); // create DIV element
	if(academicYrStMonth!=month)	//	Date not available in Month
	{
		Id = month+"-"+i;
		div.setAttribute('id', Id);
		cell.setAttribute('style', 'background-color:#CAC6C6;');
		div.setAttribute('style', 'background-color:#CAC6C6;margin-bottom:0px;');
		cell.appendChild(div);
	}
	else
	{
		Id=academicYrStDate+"";
		Id = Id.split("00")[0].split(" ").join("_");
		cell.setAttribute('style', 'margin-bottom:0px;text-align:left;');
		div.setAttribute('style', 'line-height:13px;font-size:12px;');
		div.setAttribute('id', Id);
		cell.appendChild(div);

		academicYrStDate.setTime(academicYrStDate.getTime()+86400000);
	}
}


function bulletinStatus()
{

	$('.compose').toggle('slow');
	$('#divalumpost').hide();
	$('#divalumpost').fadeIn(2000);
	/*var anchor=$('#bulletinStatus').attr('bulletintype');
		if(anchor=="COMPOSE"){
			$('#bulletinStatus').html('<i class="fa fa-times-circle fa-lg"></i>');
			$('#bulletinStatus').attr('bulletinType','HIDE')
		}
	else{
		$('#bulletinStatus').html('<i className="fa fa-envelope fa-lg"></i>&nbsp;Compose&nbsp;');
		$('#divalumpost').hide();
		$('#divalumpost').fadeIn(2000);
	}
		*/
	$(".timeline").fadeToggle(1000);	
}

function getAdmissionLink(studentId)
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getAdmissionLink.json?studentId='+studentId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getAdmissionLink.json';
	}
	$.ajax({
		url:url,
		dataType:'text',
		beforeSend: function() {
		},
		complete: function() {
		},
		success:function(link){
			if(link != "NO_LINK")
			{
				$('#redirectAdmissionLink').attr('href',link);
				$('#transDiv').toggle();
			}
		},error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
			
	});

}

function getAcademicInformation(studentId){
	var customerIdentifiecationObj=$('#customerHelper').val();
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getAcademicInformationNew.json?studentId='+studentId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getAcademicInformationNew.json';
	}
	$.ajax({
		//url:'getAcademicInformation.json',
		url:url,
		// dataType:'json',
		beforeSend: function() {
			$('#rollNo').find('.modal-body').html('<div class="preview-large "></div>');
			$('#rollNo').find('.preview-large').addClass('loading-large');
		},
		complete: function() {
			$('.studentAcademicInfo').find('.preview-large').removeClass('loading-large');
		},
		success:function(jObject){
			var studUniqueNoLabel=$('#studUniqueNo').val();
			var studentDashBoardObj = eval('('+jObject+')');
			var courseName = studentDashBoardObj.AcademicInfo.courseName;
			var studUniqueNo = studentDashBoardObj.AcademicInfo.studUniqueNo;
			var rollNo = studentDashBoardObj.AcademicInfo.rollNo;
			var divisionName = studentDashBoardObj.AcademicInfo.divisionName;
			var academicBatch = studentDashBoardObj.AcademicInfo.academicBatch;
			var semesterName = studentDashBoardObj.AcademicInfo.semesterName;
			var houseName=studentDashBoardObj.AcademicInfo.houseName;
			var parentProgramName = studentDashBoardObj.AcademicInfo.parentProgramName;
			var studentUniqueId = studentDashBoardObj.AcademicInfo.studentUniqueId;
			var textToDisplay='';
			if(customerIdentifiecationObj !=''&& (customerIdentifiecationObj == 'CHANAKYA'||customerIdentifiecationObj == 'INDIRA_PUNE')){
				textToDisplay=studUniqueNoLabel+":"+studUniqueNo;
			}
			else if (customerIdentifiecationObj =='JGI' ){
				textToDisplay = juno.labelByCustomer('l.Customerwise.RollNo.Label')+": "+studUniqueNo;				
			}
			else if (customerIdentifiecationObj =='CMR_UNIVERSITY'){
				textToDisplay = juno.labelByCustomer('l.Customerwise.RollNo.Label')+": "+rollNo;				
			}
			else {
				if( customerIdentifiecationObj == 'MIT'){
					textToDisplay = studUniqueNo+ "<br>Roll No. :"+rollNo;
				}else if( customerIdentifiecationObj == 'VCET' && parentProgramName!=''){
					textToDisplay = "Roll No. :"+rollNo;
					courseName = courseName +" [ "+parentProgramName+" ]";
				}else if($('#studDashboardLblStudSide').val() == true || $('#studDashboardLblStudSide').val() == "true"){
					textToDisplay = "Enrollment No. :"+rollNo;
				}else{
					textToDisplay = "Roll No. :"+rollNo;
				}
				
			}
			if($('#studentUniqueNumber').val()==true || $('#studentUniqueNumber').val()=="true"){
				textToDisplay+="<br>Student Unique Id : "+studentUniqueId;
			}
			textToDisplay+="<br>"+semesterName+"<br>"+courseName+"<br>"+divisionName;
			if(houseName!='')
				textToDisplay+="<br/>House: "+houseName;
			$('#rollNo').html(textToDisplay);
			
		},error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
			
	});
}

function calculteAttendence()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getStudentAttendancePercentageSP.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getStudentAttendancePercentageSP.json';
	}
	$.ajax({
	//		url:'getStudentAttendancePercentage.json',
		url: url,	
		dataType:'text',
		async:false,
		beforeSend: function() {
		},
		complete: function() {
		},
		success:function(per){
			if(per != "Problem"){
				$("#attendencePer").text(per+"%");
			}else{
				$("#attendencePer").text("-");
			}
			getAssignments();
		},
		error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}
function studentSubjectCount()
{
	try {
		var hdnUserRole = $('#hdnUserRole').val();
		var url = 'getCurrentSubjectCount.json';
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_getCurrentSubjectCount.json';
		}
		$.ajax({
			url:url, // StudentCourseFileSubjectList.json
			dataType:'text',
			async:false,
			beforeSend: function() {
			},
			complete: function() {
			},
			success:function(subjectCount){			
				$("#subjectListCountNew").text(subjectCount);
				//$("#companyCount").text(subjectCount);
			},
			error:function(){
				getCustomAlertMessage("alert_error");
			}
		});
	} catch (e) {
		getCustomAlertMessage("alert_error");
	}
}

function getAssignments()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getStudentAssignments.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getStudentAssignments.json';
	}
	$.ajax({
		url:url,
		dataType:'json',
		async:false,
		beforeSend: function() {
		
		},
		complete: function() {
			
		},
		success:function(jArray){
			if(jArray != null){
				$("#assignmentCount").text(jArray["CountAssignment"]);
				$("#assignmentPostIds").val(jArray["postIds"]);
			
				//alert(jArray["postIds"]);
			}
			else
				{
				$("#assignmentCount").text('-');
				}
			
			getPlacementDetail('');
				
		},
		error:function(){
			jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}

