var monthArray = ["Jan","Feb","Mar","Apr","May","Jun","Jul","Aug","Sep","Oct","Nov","Dec"];
var days = ['Sunday', 'Monday', 'Tuesday', 'Wednesday', 'Thursday', 'Friday', 'Saturday' ];
	



$(document).ready(function(){
	
	  $("#txtFromDate1").datepicker({
			dateFormat:"M dd,yy",
			showWeek: true,
			changeMonth: true,
			changeYear: true,
			yearRange : "-60:+0",

		});

		$("#txtToDate1").datepicker({
			dateFormat:"M dd,yy",
			showWeek: true,
			changeMonth: true,
			changeYear: true,
			yearRange : "-60:+0",

		});

	  
});

function todaysDate(dt)
{
	monthArray = ["Jan","Feb","Mar", "Apr", "May", "Jun", "Jul", "Aug","Sep","Oct","Nov", "Dec"];
	var today = new Date();
	if(dt)
	{
		today = new Date(dt);
	}
	
    //var today = new Date();
    var strDay = today.getDate();
    var strMonth = today.getMonth();
    var strYear = today.getYear();

    if (strYear < 1000)
        strYear += 1900;
    strMonth = monthArray[strMonth];
    return strMonth + " " +strDay + "," + strYear;
}

function showTodaysTimetableForStudent()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getTodaysScheduleForStudentLoggedIn.json?date='+ todaysDate();
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getTodaysScheduleForStudentLoggedIn.json?date='+ todaysDate();
	}
	$.ajax({
		//url:'getTodaysWorkloadByStudentLoggedin.json?date='+ todaysDate() ,
		url:url,
		dataType:'json',
		beforeSend:function() {
//			$('#viewTimetable').html('<div style="width:100%; text-align:center;"><i class="fa fa-spinner fa-pulse fa-fw fa-4x"></i></div>');
		},
		complete: function() {
		
		},
		success:function(mainjArray){
			
			var jArray = mainjArray[0]["timetable"];
			/*var TodaysWorloadBoiler = '<table  id="schedule1" class="table table-bordered table-condensed" style="font-family:Source Sans Pro;border-top: 2px solid rgb(127, 127, 127);border-bottom: 2px solid rgb(127, 127, 127);border-left:2px solid rgb(127, 127, 127);border-right:2px solid rgb(127, 127, 127);">';

			TodaysWorloadBoiler += '<tr style="height:2px;"><td colspan="10"><div style="text-align:center"><b>Today\'s Schedule</b>'+*/
			
			
			var TodaysWorloadBoiler = '<fieldset class="custom-fieldset" style="boarder:1px solid #bfbdbd !important"><legend style="font-size:11px"><i class="fa fa-clock-o"></i> TODAY\'S  SCHEDULE</legend><div><table  id="schedule1" class="table table-condensed alert-info">';

			TodaysWorloadBoiler += '<div style="text-align:center;background-color:#BDC3C7;"></div>'+
			
			//'<a class="pull-right" href="stu_timetableStudent.htm">View All</a>'+
			
			'</div></td></tr><tr>';
         
			if(jArray.length > 0 )
			{
				$('#viewTimetable').show();
				var j=jArray.length;
				
				//alert(j);
				
				TodaysWorloadBoiler += '<tr>';
				for(var i=0 ; i<j;i++)
				{
					if(jArray[i]["subjectWisebatchId"]!=undefined)
					{
						TodaysWorloadBoiler +='';
            TodaysWorloadBoiler +='<td style="text-align:center;font-size:11px;"><div> <i>'+ jArray[i]["startTimeHHMMA"] +'-'+ jArray[i]["endTimeHHMMA"] +'</i></td></div>';
						
					}	
					
				}	
				TodaysWorloadBoiler += '</tr><tr>';
				for(var i=0 ; i<j;i++)
				{
					if(jArray[i]["subjectWisebatchId"]!=undefined)
					{
						
						TodaysWorloadBoiler +='';
						if(jArray[i]["recess"]=="")
						{
							TodaysWorloadBoiler +='<td style="text-align:center;font-size:11px;">'+jArray[i]["subShortName"].gemsDecode();
							if(jArray[i]["documents"].length>0 || jArray[i]["documentUpl"].length>0){
								TodaysWorloadBoiler +='&nbsp;&nbsp;<i title="Attachment" class="fa fa-paperclip fa-lg" id="showAttachment-paperclip-'+i+'" onclick="showAttachmentFromStudentScheduler(this)"></i>';
							}
							TodaysWorloadBoiler +='</td></div>';
						}
						else
						{
							TodaysWorloadBoiler += '<td style="text-align:center;font-size:11px;">'+jArray[i]["recess"]+'</td></div>';
						}
						
					}	
					
				}	
				TodaysWorloadBoiler += '</tr><tr>';
				
				for(var i=0 ; i<j;i++)
				{
					if(jArray[i]["subjectWisebatchId"]!=undefined)
					{
						TodaysWorloadBoiler +='';
						if(jArray[i]["recess"]=="")
						{
							if(jArray[i]["infraName"]==undefined)
								TodaysWorloadBoiler +='<td>-</td></div>';
							else
                TodaysWorloadBoiler +='<td style="text-align:center;font-size:11px;">'+ jArray[i]["infraName"]+'</td></div>';
						}
						
						
						else
						{
							TodaysWorloadBoiler += '<td></td></div></fieldset>';
						}
						
					}	
					
				}	
				
				//TodaysWorloadBoiler += '<div style="text-align:center;"><span style="width:100%; font-style: ; font-size:12px; color:#7f7f7f;">(no lectures)</span></div>';
				$('#viewTimetable').html(TodaysWorloadBoiler);
				
				
				/*TodaysWorloadBoiler += '</tr>'+
					'</tbody></table>'; 					
				$('#viewTimetable').html(TodaysWorloadBoiler);	*/
				/*$('#containerTimeTable').slimScroll({
				    //position: 'bottom',
					start: 'bottom',
				    width: '780px',
				    height:'auto',
		    		scrollBy: '10px',
		    		railVisible: true,
		    		wheelStep: 6,
		    	    alwaysVisible: true
				});*/
			}	
			else{
				if(mainjArray[0]["holiday"]!='' && mainjArray[0]["holiday"]!=undefined)
					TodaysWorloadBoiler += '<tr><td colspan="10" style="text-align:center;background-color:;"><span style="width:100%;color:#7f7f7f;">(No schedule for Today) <i class="icon icon-calendar"></i> Holiday: </span><span style="width:100%;color:#ff6666;font-weight:bold;">'+(mainjArray[0]["holiday"])+'</span></td></tr>';
				else
					TodaysWorloadBoiler += '<tr><td colspan="10" style="text-align:center;background-color:;"><span style="width:100%; color:#7f7f7f;">(No schedule for Today)</span></td></tr>';
			}
			$('#viewTimetable').html(TodaysWorloadBoiler);
			
			
			for(var i=0 ; i<j;i++)
			{
				$('#showAttachment-paperclip-'+i).data('sub',jArray[i]["subShortName"]);
				$('#showAttachment-paperclip-'+i).data('textFiles',jArray[i].documents);
				$('#showAttachment-paperclip-'+i).data('uploded',jArray[i].documentUpl);
			}
			/***************Swapnil Raut* Weekly Task**************/
			
			var date = new Date();
			var day = date.getDay();

			while(day>0)
			{
				date = new Date(date.getTime()-86400000);
				day--;
			}
			fromDate = date;
			//weeklyWorkload();
			
			/***************Swapnil Raut* Weekly Task**************/
		},
		error:function(){
			
		}
	});
}

var fromDate = new Date();

function weeklyWorkload(){
	$.ajax({
		url:'getDailyWorkloadForStudent.json',
		dataType:'json',
		beforeSend:function() {
			$('#viewWorkload').html('<div style="width:100%; text-align:center;"><img src="img/ajax.gif"/></div>');
		},
		complete: function() {
			
		},
		success:function(jArray){
			
			var j=jArray.length;
			
			if(j>0){
				var boiler ='<table id="schedule1" class="table table-bordered table-condensed">';
				for(var i=0; i<j; i++)
				{
					var fromDate = new Date(jArray[i][3]);
					var toDate = new Date(jArray[i][4]);
					
					var frDayOfMonth = fromDate.getDate();
					var frMonth = fromDate.getMonth();
					var frYear = fromDate.getFullYear();
					
					var toDayOfMonth = toDate.getDate();
					var toMonth = toDate.getMonth();
					var toYear = toDate.getFullYear();
					
					var evalTypeName = jArray[i][5];
					var evalUni = jArray[i][6];
	
					boiler +=   '<tr><td><div style="font-size: 11px; font-weight: 600;">'+evalTypeName+'</div>'+
								'<div>'+evalUni+' - '+jArray[i][7]+'</div>'+
								'<div>'+monthArray[frMonth]+' '+frDayOfMonth+','+frYear+' - '+monthArray[toMonth]+' '+toDayOfMonth+','+toYear;
								if(jArray[i][8]!=undefined)
									boiler +=	'your\'s on'+jArray[i][8];
								
					boiler +=	'</div></td></tr>';
					
				}
				
				boiler+='</table></div>';
				$('#viewWorkload').html(boiler);
			}else{
				
			/*	boiler+='</table>';
				$('#viewWorkload').html(boiler);
			}else{*/
				$('#viewWorkload').html('<span style="width:100%; font-style: italic; font-size:medium; color:#C4C3C3;">(no workload)</span>');
			}
		},
		error:function(){
			
		}
	});
}

/*function dates(){
	
