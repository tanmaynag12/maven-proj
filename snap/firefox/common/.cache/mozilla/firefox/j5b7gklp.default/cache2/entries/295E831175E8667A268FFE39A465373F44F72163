function loadCourses()
{
//	var selectedCourseId = $('#cmbloadCourse').val();
	$.ajax({
		url:'courseListAttributeForCourseCoordinator.json?linkURL='+ window.location.toString(),
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray)
		{
			var size=jArray.length;
			if(size>0)
			{
				type=jArray[0]["level"];
				var typepfmapped='<option value="0">Select</option>';
				for(var i=0;i<jArray.length;i++)
				{
					//if(selectedCourseId!=jArray[i]["instituteCourseId"])
						typepfmapped += '<option value = "'+jArray[i]["instituteCourseId"]+'">'+jArray[i]["instituteName"]+'</option>';
				}
				$('#cmbCourseId').html(typepfmapped);
			}
		},
		error:function(){
		}
	});
}

function getSemesterReport()
{
	var instituteCourseId = $('#cmbCourseId').val();

	if(instituteCourseId>0)
	{
		$.ajax({
			url:'getRunningSemestersByInstituteCourseId.json'+'?instituteCourseId='+instituteCourseId, 
			dataType:'json',
			beforeSend:function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				var j=jArray.length;
				var semesterBoiler = '<option value="0">Select</option>';
				for(var i=0;i<j;i++)
					semesterBoiler +='<option value="'+jArray[i]["semesterId"]+'_'+jArray[i]["strengthId"]+'">'+jArray[i]["semesterName"]+'</option>';

				$('#cmbBatchId').html(semesterBoiler);
			},
			error:function(){
				jAlert("Something went wrong, Please try after some time","Error");
			}
		});
	}
	else
		requiredMessageShow("department", "Please select correct Programme");
}

function getAllBatchesByStrength()
{
	var instituteCourseStrengthId = $('#cmbBatchId').val().split("_")[1];
	var semesterId = $('#cmbBatchId').val().split("_")[0];
	
	
	$.ajax({
		url:'batchSemesterCapacityListByStrengthIdAndSemesterId.json?instituteCourseStrengthId='+instituteCourseStrengthId+'&semesterId='+semesterId,
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){

			var html = '<option value="0">Select</option>';
			var len = jArray.length;
			for ( var i = 0; i < len; i++)
				html += '<option value="'+jArray[i]["semesterBatchIdId"]+'">'+jArray[i]["semesterBatchName"]+ '</option>';

			$("#cmbCapacity").html(html);
		},
		
	});
}

function getSectionWiseTimeTableContent()
{
	var startDate = $('#txtFromDate').val();
	var endDate = $('#txtToDate').val();
	
	/*if(startDate == ""){
		jAlert("Please select from date");
		return;
	}
	if(endDate == ""){
		jAlert("Please select to date");
		return;
	}*/
	
	$.ajax({
		url : 'getBetweenDatesTimetableForStudent.json',
			dataType : 'json',
			data : {startDate:startDate,endDate:endDate},
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();

			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				var day = "";
				var weekDays = ["Monday", "Tuesday", "Wednesday", "Thursday", "Friday", "Saturday", "Sunday"];
				var boiler = '<table class="table table-bordered" id="tbl_SubjectwiseTimetable"><tr><th>Date & Day</th><th>Session No.</th>'+
					'<th>Course Name</th><th>Start Time</th><th>End Time</th><th>Faculty Name</th>'+
					'<th>Class Room / Lab</th></tr><tbody id="tbodyTimeTable"></tbody></table>';
				$('#viewSectionTable').html(boiler);
				if(jArray != undefined && jArray.length > 0){
					for(var i = 0; i < jArray.length; i++){
						var temboiler = '<tr></td>';
						if(day != jArray[i].day){
							temboiler +='<td id="tdLectureDate_'+jArray[i].day+'" rowspan="1" style="background-color:#E1F5A9">'+jArray[i].lectureDate+" ("+(jArray[i].day != undefined ? weekDays[jArray[i].day] : "")+')</td>';
						day = jArray[i].day;
						}
						else
						{
							var rowspan = $('#tdLectureDate_'+day).attr('rowspan');
							rowspan = eval(rowspan) + 1;
							$('#tdLectureDate_'+day).attr('rowspan', rowspan);
						}
						temboiler +='<td>'+jArray[i].sessionCount+'</td><td>'+jArray[i].sub+'</td>';
						temboiler +='<td>'+jArray[i].lStartTime+'</td>'+
						'<td>'+jArray[i].lEndTime+'</td><td>'+jArray[i].allocatedEmployeeNameList+'</td>'+
						'<td>'+(jArray[i].infraDisplayName).split('$')[1]+'</td>'+
						'</tr>';
						$('#tbodyTimeTable').append(temboiler);
					}
				}
				else
					boiler += '<tr><td colspan="9">No data found!!!</td></tr>';
				
				
				$('#btnExcel').remove();
				$('<br><input type="button" id="btnExcel" class="btn btn-mini btn-inverse" value="Export to Excel" onclick="tableToExcel(\'tbl_SubjectwiseTimetable\', \'test\');" >').insertAfter($('#divTimetableReport'));
			},
			error : function() {
				alert("error in fetching Course");
			}
	});
}

function getTimeTableDetails(){
	window.location.replace('stu_StudentTimeTable.htm');
}

function showTodaysConflictTimetableForStudent()
{
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getCurrentConlictScheduleDetailsForstudent.json?date='+ todaysDate();
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getCurrentConlictScheduleDetailsForstudent.json?date='+ todaysDate();
	}
	$.ajax({
		url:url,
		dataType:'json',
		beforeSend:function() {
			
		},
		complete: function() {

		},
		success:function(jArray){
			if(jArray.length>0)
			{
				var todaysConflictSchedule='<table class="table table-condensed alert-info"> <tbody><tr><td style="text-align:center;" colspan="'+jArray.length+'">Session conflict Details</td></tr>';
				todaysConflictSchedule+='<tr style="text-align:center;">';
				for(var i=0 ; i<jArray.length;i++)
				{

					if(jArray[i]["subjectName"]!=undefined)
					{
						todaysConflictSchedule +='<td style="text-align:center;"><i>'+ jArray[i]["startTime"] +'-'+ jArray[i]["endTime"] +'</i></td>';
					}	
				}	
				todaysConflictSchedule +='</tr><tr>';
				for(var i=0 ; i<jArray.length;i++)
				{
					if(jArray[i]["subjectName"]!=undefined)
					{
						todaysConflictSchedule +='<td style="text-align:center;">'+jArray[i]["shortName"]+'</td>';
					}	

				}	
				todaysConflictSchedule += '</tr><tr>';

				for(var i=0 ; i<jArray.length;i++)
				{
					if(jArray[i]["subjectName"]!=undefined)
					{

						todaysConflictSchedule +='<td style="text-align:center;">'+ jArray[i]["infra"]+'</td>';
					}
					else
					{
						todaysConflictSchedule += '<td></td>';
					}

				}
				todaysConflictSchedule +='</tr></tbody></table>';
				$('#viewTimetable').append(todaysConflictSchedule);
			}

		},
		error:function(){
			getCustomAlertMessage('alert_error');
		}
	});
}
function showAttachmentFromStudentScheduler(src){
	var docArr = $(src).data('uploded');
	var textFileseArr = $(src).data('textFiles');
	var sub = $(src).data('sub');
	var boiler = '';
	var boilernew = '';
	
	for(var i in textFileseArr){
		var iconPath = gFileTypeIcon[textFileseArr[i].dext];
		var downloadLinkTmp = '<a title="View" href="javascript:void(0)" onclick="showTextDocumentForAlumini(\''+textFileseArr[i].eDocId+'\',\''+textFileseArr[i].user+'\')"><i class="fa fa-files-o fa-2x" />  '+textFileseArr[i].dName+'</a><br>';
		boiler+=downloadLinkTmp;
	}
	for(var i in docArr){
		var iconPath = gFileTypeIcon[docArr[i].dext];
		var downloadLinkTmp = (juno.label['a.download.json'].setIndexedArgs((docArr[i].eDocPathId),"UPLOADED", encodeURIComponent(docArr[i].dName),'<img src="'+iconPath+'" width="25px" height="25px"/>'+docArr[i].dName+'<br>'));
		boiler+=downloadLinkTmp;
	}
	
	boilernew = '<table class="table table-bordered table-condensed"><tr><th style="width:150px;">Attachment</th><td>'+boiler+'</td></tr></table>';
	
	$('#gemsCommonDialogModal .modal-header').html(sub);
	$('#gemsCommonDialogModal .modal-body').html(boilernew);
	$('#gemsCommonDialogModal').modal('show');
}

