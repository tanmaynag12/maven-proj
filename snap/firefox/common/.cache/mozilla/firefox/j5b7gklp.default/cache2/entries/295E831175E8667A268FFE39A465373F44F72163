function setEulaValue()
{
	$.ajax({
		url:'setEulaValue.json',
		dataType:'text',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(msg){
					$('#myModal').modal('hide');
					//jAlert("You have accepted the declaration");
		},
		error:function(){
		}
	});
}

function getNextYearAdmission()
{
	//window.open('subjectChoice.htm');
	
	$.ajax({
		url:'getNextYearAdmission.json',
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
					if(jArray[0]["linkOpen"]=='Preference')
					{
						window.location.replace('stu_studentSubjectPreferance.htm');
					}
					else if(jArray[0]["linkOpen"]=='SubjectChoice')
					{
						window.location.replace('subjectChoice.htm');
					}
					else if(jArray[0]["linkOpen"]=='WithoutSubjectChoice')
					{
						window.location.replace('stu_studentSubjectChoiceAndMarkEntry.htm');
					}
					else if(jArray[0]["linkOpen"]=='NoAdmissionProcess')
					{
						jAlert("Admission Process Not Started","Message");
					}
					else if(jArray[0]["linkOpen"]=='Connection Error')
					{
						jAlert("Connection Error","Message");
					}
		},
		error:function(){
		}
	});
}

