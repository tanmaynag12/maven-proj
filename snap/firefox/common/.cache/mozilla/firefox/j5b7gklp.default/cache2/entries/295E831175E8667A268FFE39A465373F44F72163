	var url = 'getUnreadStudentAnnouncement.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getUnreadStudentAnnouncement.json';
	}
	$.ajax({
		url:url,
		dataType:'json',
		async:false,
		beforeSend:function() {
			
		},
		complete: function() {
			
		},
		success:function(jArray){
				if(jArray.length > 0 ){
					$("#announcementCount").text(jArray[0]["count"]);
					$("#countAnnouncement").text(jArray[0]["count"]);
				}
				
			},
		error:function(){
			return;
		}
	});
}

