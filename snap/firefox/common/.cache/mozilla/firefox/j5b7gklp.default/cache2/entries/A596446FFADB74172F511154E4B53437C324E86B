function showAnnouncementAttachment(jArray,tableId,isCreator){
	var attachementBoiler = "";
	for(var i = 0 ; i < jArray.length; i++){
		var attachmentFileName = jArray[i]["name"].toString(); 
		var documentId = jArray[i]["documentId"];
		var isTemporary = jArray[i]["isTemporary"];
		//var fileType = jArray[i]["extension"].toLowerCase();
		
		//attachmentFileName = attachmentFileName.substring(attachmentFileName.lastIndexOf('/')+1);
		//var fileName = attachmentFileName.replace(/ /g,'~').trim();
		var trId = 'div-'+documentId;
		
		var fileType = jArray[i]["type"];
		iconPath = gFileTypeIcon[fileType];
		if(!isTemporary){
			attachementBoiler += 
				'<div class="hvr-curl-bottom-right" id = "'+trId+'" style="background-color:#eee;height:65px;width:280px;margin:2px;" onmouseover="showDownloads('+documentId+')" onmouseleave="hideButton('+documentId+');">'+
				'<div style="float:left;border-right:1px solid lightgray;padding:5px;margin-top:2px;"><img src="'+iconPath+'"></div>'+
				'<p style="font-size:12px;padding:5px;margin-bottom:0px;" title="'+attachmentFileName+'">&nbsp;&nbsp;&nbsp;&nbsp;'+attachmentFileName.substring(0,25)+'</p>';
			attachementBoiler += '<div id="buttonDownload-'+documentId+'" style="display:none;"><a style="margin-left:10px;" class="btn btn-default" href = "'+(juno.label['url.download.json'].setIndexedArgs(documentId, "UPLOADED", encodeURIComponent(attachmentFileName)))+'"  target="_blank">Download</a>&nbsp;';
			
			if(fileType=='doc' || fileType=='docx' || fileType=='ppt' || fileType=='pptx' || fileType=='xls' || fileType=='xlsx')
			{
				attachementBoiler += '<a class="btn btn-default" href = "javascript:void(0);" onclick="showDocumentPreview('+documentId+')">Preview</a>';
			}
			
			/*if(isCreator){
				attachementBoiler += '<img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAttachmentAjax(\''+trId+'\',\''+documentId+'\',\'saved\')" />'; 
			}*/
			
		} else {
			
			attachementBoiler += 
				'<div class="hvr-curl-bottom-right download" id = "'+trId+'" style="background-color:#eee;height:65px;width:32%;margin:2px;" onmouseover="showDownloads('+documentId+')" onmouseleave="hideButton('+documentId+');">'+
				'<div style="float:left;border-right:1px solid lightgray;padding:5px;margin-top:2px;"><img src="'+iconPath+'"></div>'+
				'<p style="font-size:12px;padding:5px;" title="'+attachmentFileName+'">&nbsp;&nbsp;&nbsp;&nbsp;'+attachmentFileName.substring(0,30)+'</p>';
			
			attachementBoiler += '<div id="buttonDownload-'+documentId+'" style="display:none;">'+
				'<a class="btn btn-default" style="margin-left:10px;" href = "javascript:void(0);" onclick = "removeAttachmentAjax(\''+trId+'\',\''+documentId+'\',\'temporary\')" >Remove Attachment</a></div>';
		}
		attachementBoiler += '</div></div>';
	}
	//attachementBoiler += "</table>";
	if(tableId == undefined)
		$('#showAnnouncementAttachment').html(attachementBoiler);
	else
		$('#'+tableId).html(attachementBoiler);
}

function showDownloads(documentId)
{
	$("#buttonDownload-"+documentId).show();
}

function hideButton(documentId)
{
	$("#buttonDownload-"+documentId).hide();
}

function showDocumentPreview(documentId, fileType)
{
	var domain = "http://"+window.location.host;
	var path = domain+'/'+(juno.label['url.download.json'].setIndexedArgs((documentId), "UPLOADED", 'NA'));
	var encodedPath = encodeURIComponent(path);
	var boiler = '';
	if(fileType != undefined && (fileType == 'jpg' || fileType == 'png'|| fileType == 'gif'))
		boiler = '<img style="width:100%;height:550px;" src="https://view.officeapps.live.com/op/view.aspx?src='+encodedPath+'"/>';
	else
		boiler = '<iframe style="width:100%;height:550px;" src="https://view.officeapps.live.com/op/view.aspx?src='+encodedPath+'"/>';
	
	$('#showDocumentPreviewDialog').find('.documentBody').html(boiler);
	$('#showDocumentPreviewDialog').modal('show').css({
		width: '100%',
		height:'100%',
		left:'0px',
		top:'0px',
		margin:'0'
	});
	
	$('#showDocumentPreviewDialog').css('z-index', '1100');
}



/**
 *
 * @param trId
 * @param documentId
 * @param docuementType Temporary/Saved
 */
function removeAttachmentAjax(trId,documentId,docuementType){
	if(docuementType == 'saved'){
		removeUploadedDocumentById(documentId,function(jObject){
			$('#'+trId).remove();
			refreshTemporaryAttachmentList();
		});
	} else {
		removeTemporaryAttachmentAjax(documentId,function(){
			$('#'+trId).remove();
			refreshTemporaryAttachmentList();
		});
	}
}

/**
documentId is index of the session array where documents have been kept 
 */
function removeTemporaryAttachmentAjax(documentId,callback){
	try{
		$.ajax({
			url:'removeTemporaryAnnouncementAttachment.json',
			dataType:'json',
			type:'POST',
			data:{d__documentId:documentId},
			success:function(){
				try{
					callback();
				}catch(e){
					
				}
			}	
		});
		
	}catch(e){
		alert(e);
	}
}

function getTemporaryAttachmentList(callback){
	$.ajax({
		url:'getTemporaryAnnouncementAttachment.json',
		dataType:'json',
		success:function(jArray){
			callback(jArray);
		}
	});
}

function refreshTemporaryAttachmentList(){
	$.ajax({
		url:'getTemporaryAnnouncementAttachment.json',
		dataType:'json',
		success:function(jArray){
			showAnnouncementAttachment(jArray,"showAnnouncementAttachment",true);
			
			}
	});
}

function getDocumentCategoryByAnnoucementId(announcementId,callback){
	$.ajax({
		url:'getDocumentCategoryByAnnoucementId.json',
		dataType:'text',
		data:'announcementId='+announcementId,
		success:function(categoryId){
			callback(categoryId);
		}
	});
}

function flushSession(){
	$.ajax({
		url:'flushSessionForAnnouncement.json',
		type:'POST',
		success:function(){
			
		}
	});
}

/**************For forwarding announcement******************/
function setAndShowExistingAttachmentForAnnoucement(announcementId,callback){
	$.ajax({
		url:'setAndShowExistingAttachmentForAnnoucement.json',
		dataType:'json',
		data:'announcementId='+announcementId,
		success:function(jArray){
			callback(jArray);
		}
	});
}␀␀␀␀␀␀␀␀`␀␀x`Îü␀␀␀20250331133932-8l˜_␀␀éÀm—I␀␀␀https://erp.cmrit.ac.in/scripts/instituteheadjs/announcementAttachment.js␀␀␀␀␀␀ﬁ$␀␀/**
