function removeAttachmentAjax(trId,documentId,docuementType){
	if(docuementType == 'saved'){
		removeUploadedDocumentById(documentId,function(jObject){
			$('#'+trId).remove();
			refreshTemporaryAttachmentList();
		});
	} else {
		removeTemporaryAttachmentAjax(documentId,function(){
			$('#'+trId).remove();
			refreshTemporaryAttachmentList();
		});
	}
}

/**
documentId is index of the session array where documents have been kept 
