function removeTemporaryAttachmentAjax(documentId,callback){
	try{
		$.ajax({
			url:'removeTemporaryAnnouncementAttachment.json',
			dataType:'json',
			type:'POST',
			data:{d__documentId:documentId},
			success:function(){
				try{
					callback();
				}catch(e){
					
				}
			}	
		});
		
	}catch(e){
		alert(e);
	}
}

