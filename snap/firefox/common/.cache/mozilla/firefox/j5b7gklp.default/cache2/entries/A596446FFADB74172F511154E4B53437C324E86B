/**
 * @vimal
 */



var gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip'];

$(document).ready(function(){
	
$(".download").mouseenter(function(){ 
	alert('kk');
	$("#buttonDownload").show();
	  
  });
 $(".download").mouseleave(function(){
	 
	  $("#buttonDownload").hide();
	  
  });
});


/**
 * 
 * @param announcementId annoucement is passed at the time of 
 * edit of the annoucement
 */
function initFileUpload(announcementId){
	var elementId = 'uploadFile';
	if(announcementId != undefined && announcementId != ''){
		elementId = 'uploadFileEdit';
	}
	$('document').ready(function() {
		new AjaxUpload(elementId,{
			action : 'attachFileWithAnnouncement.json',
			data : {},
			name : 'fileData',
			size:AllowedFileSize.DOCUMENT_UPLOAD,
			onSubmit : function(file, extension) {
				startLoading();
				this.setData({
					'announcementId':announcementId,
					'fileSize':AllowedFileSize.DOCUMENT_UPLOAD
				});
			},
			onComplete : function(file, response) {
				//alert('HI');
				stopLoading();
				var jArray = eval('(' + response + ')');
				//alert(jArray.length);
				if(announcementId == undefined || announcementId == '')
					showAnnouncementAttachment(jArray,undefined,true);
				else
					showAnnouncementAttachment(jArray,'divAnnouncementUploadedFiles',true);
			}
		});
	});
}




function getAnnouncementAttachement(announcementId,callback){
	
	try{
		$.ajax({
			url:'getAnnouncementAttachment.htm?announcementId='+announcementId,
			dataType:'json',
			success:function(jArray){
				callback(jArray);
			},
			error:function(){
				
			}
		});
	}catch(e){
		alert(e);
	}
	
}

