function saveEditComment(commentThreadId)
{
var comment=$("#txtCommentEdit-"+commentThreadId).val().trim();
		
	if(comment == ""){
		jAlert("Please Enter Comment");
		return;
	}
	
	$.ajax({
			url:'editCommentThread.json',
			dataType:'json',
			data:{commentThreadId:commentThreadId,comment:comment},
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(commentjArray){
				//alert(commentjArray.length);
				/*var k = 0;
				var boiler=	'<table style="margin-left:5%;margin-top:3px;width:94%;background-color:#F3F3F3;">'+
								'<tr id="tr'+commentjArray[k]["commentThreadId"]+'">'+
									'<td rowspan="2" style="vertical-align:top;padding:4px;width:30px;"><img src="'+commentjArray[k]["ImageURL"]+'" height="20px" width="20px"></td>'+
									'<td><p style="display:inline;"><b>'+commentjArray[k]["name"]+':</b></p><p style="display:inline;float:right" class="muted">'+commentjArray[k]["commentDateTime"]+'</p></td>'+    
								'</tr>'+
								'<tr>'+
								//change here
									'<td><div id="commentData-'+commentjArray[k]["commentThreadId"]+'"><p class="muted">'+commentjArray[k]["comment"]+'</div><a href="#linkDiv" title="Remove" onclick="deleteCommentThread('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">remove</i></a><a href="javascript:void(0);" onclick="editComment('+commentjArray[k]["commentThreadId"]+')" title="Edit"><i id="commentModifyId-'+commentjArray[k]["commentThreadId"]+'" style="float:right">edit</i></a></p></td>'+
								'</tr>'+
							'</table>';
				
				$('#tableShowHideComment-'+alumniRingPostId).after(boiler);
				$("#txtNewCommen-"+alumniRingPostId).val("");*/
			},
	        error : function() {
	        	jAlert("Error posting your comment, Please try after some time");
			}
	       
	    });

}














 function editComment(id)
 {
	 var textVal=$('#actionEdit-'+id).text();
	 if(textVal=="edit")
		  {
		  var data=$('#commentData-'+id).text();
		  $('#actionEdit-'+id).html('<span id="actionSave-'+id+'"><a href="javascript:void(0);" onClick="saveComment('+id+')"><i style="float:right;margin-left:5px;">save</i></a></span><span id="actionCancle-'+id+'"><a href="javascript:void(0);" onclick="cancleComment(\''+id+'\',\''+data+'\')"><i style="float:right;margin-left:5px;">cancel</i></span>');
		  var boiler='<input type="text" style="width:80%; margin:5px;border-radius:0px; display:inline;" id="txtCommentEdit-'+id+'" value="'+data+'">';
		  $("#commentData-"+id).html(boiler);
		  document.getElementById('txtCommentEdit-'+id).contentEditable=true;
		  document.getElementById('txtCommentEdit-'+id).focus();
		  }
			  
 }
function saveComment(id)
{
saveEditComment(id);
var editedComment=$('#txtCommentEdit-'+id).val();
var boiler=''+editedComment;
$('#commentData-'+id).html(boiler);
$('#actionEdit-'+id).html('<a href="javascript:void(0);" onclick="editComment('+id+')"><i style="float:right;margin-left:5px;">edit</i></a>');
}

function cancleComment(id,data)
{
	var editedComment=$('#txtCommentEdit-'+id).val();
	var boiler=''+editedComment;
	$('#actionEdit-'+id).html('<a href="javascript:void(0);" onclick="editComment('+id+')"><i style="float:right;margin-left:5px;">edit</i></a>');
	$('#commentData-'+id).html(data);
}”ò¿Ÿ®␀␀]␀␀␀␀␀␀␀␀␀∏OÄ␀␀␀␀␀␀document␀␀␀¢Ñ
