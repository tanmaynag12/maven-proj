	$('#tempMsg').hide();
	var link='';
		
	link='<div class="post"></div>'+
	'<div style="background-color: white; border:1px solid; border-color:#c4cde0; min-height: 90px; width: 98%;" id="txttempwritediv">'+
	'<textarea style="width: 94.5%;" class="commentMark" id="txttempwrite"></textarea>'+
	'<div>'+
	'<div class="pull-right"><input type="button" class="btn btn-primary btn-mini" value="post" onclick="saveBulletin('+userId+')" /></div>'+
	'</div>'+
	'</div>'+
	'<div class="alert alert-info" id="tempMsg" style="display:none;"></div>';

tourStates1 = [
	          	{
	          		html: link,
	          		buttons: {},
	          		focus: 1,
	          		position: { container: '#reply'+replyid, x: 35, y: -20, width: 300, arrow: 'lt' },
	          		//submit: tourSubmitFunc
	          	}];
	$.prompt(tourStates1, { opacity: 0.3 });
}

function saveBulletin(toid)
{
	var txttempwrite= $('#txttempwrite').val();
	
	$.ajax({
		url:'addAlumniPostNew.json',
		//url:'saveBulletin.json?toid='+toid+'&txttempwrite='+txttempwrite,
		dataType:'text',
		data:{
			alumUserId:'Individual-'+toid,
			alumniTextPost:encodeURIComponent(txttempwrite)
		},
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(msg){
			
			//$('#tempMsg').html("Your Post is sent Successfully");
			//$('#tempMsg').show();
			$('#txttempwritediv').hide();
			getAlumniPost(5);
			
			
		},
		error:function(){

		}
	});
}

