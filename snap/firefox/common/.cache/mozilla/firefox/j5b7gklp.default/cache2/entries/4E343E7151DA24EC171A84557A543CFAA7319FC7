$(document).ready(function(){

	
	function createLightCKEditor(id) {
		var instance = CKEDITOR.instances[id];
		if (instance)
			CKEDITOR.remove(instance);
		CKEDITOR.replace(id, {
			uiColor : '#FFFFFF',
			height : '20%',
			toolbar : [ [ 'Font', 'FontSize', '-', 'Bold', 'Italic',
					'Underline', 'Strike', 'Subscript', 'Superscript',
					'RemoveFormat', '-', 'NumberedList', 'BulletedList', '-',
					'JustifyLeft', 'JustifyCenter', 'JustifyRight',
					'JustifyBlock', '-' ] ]
		});
		//$('#cke_top_'+id+' .cke_toolbox').hide();
	}
	
	getAlumniPost(5);
});
function getAlumniPost(limit){
	try{
		$("#divalumpost").show();
		$("#tabResult").hide();
	}catch(e){
		
	}
	$('.displayBoxes').hide();
	$('#postHome').show();
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getAlumniPost.json';
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getAlumniPost.json';
	}
		$.ajax({
			url:url,
			data:{limit:limit},
			dataType:'json',
			beforeSend: function() {
				$('#divalumpost').html('<div id="divPostLoading" style="text-align: center;width: 100%;"><img src="img/loading-image.gif"></div> ');
			},
			complete: function() {
				//$('#divPostLoading').hide();
			},
			success:function(jArray){
				paintAlumniPost(jArray);
			},
			error:function(){
		
			}
		});
}
function getSpecificAlumniPost(alumniRingPostIds){
	$('.displayBoxes').hide();
	$('#postHome').show();
		$.ajax({
			url:'getAlumniPostByAlumniRingPostIds.json?alumniRingPostIds='+alumniRingPostIds,
			dataType:'json',
			beforeSend: function() {
				$('#divalumpost').html('<div id="divPostLoading" style="text-align: center;width: 100%;"><img src="img/loading-image.gif"></div> ');
			},
			complete: function() {
				//$('#divPostLoading').hide();
			},
			success:function(jArray){
				if(jArray.length > 0)
					paintAlumniPost(jArray);
				else{
					$('#tabResult').html('<p style="text-align:center;font-size:14px;">No Details found</p>');
					$("#divalumpost").hide();
				}
			},
			error:function(){
		
			}
		});
}


function paintAlumniPost(jArray){
	$('#divalumpost').html('');
	var replyid=1;
	var template = '';
	var isAlumniHomeHtm = false;
	if(window.location.pathname.includes('alumniHome.htm')) isAlumniHomeHtm = true;
	//alert(isAlumniHomeHtm);	
    for(var i=jArray.length-1; i>=0; i--){
	
		template = '';
        var id=jArray[i]["alumniRingPostDetailId"];
        var des=jArray[i]["description"];
        try{
        	des = decodeURIComponent(des);
		}catch(e){
			des = jArray[i]["description"];
		}
		
		template += '<div id="alumPost'+jArray[i]["alumniRingPostDetailId"]+'" style="background-color: #FFF; border: 1px solid #C0C0C0; font-size: 11px;margin-bottom: 5px;">';
		if(!jArray[i]["latestPost"])
			template += '<div style="padding: 5px; background-color: #F7F7F7; border-bottom: 1px solid #F0F0F0;">';
		else
			template += '<div style="padding: 5px; background-color: #E7F7E7; border-bottom: 1px solid #F0F0F0;">';

		template += '<img src="'+jArray[i]["ImageURL"]+'" style="margin-right: 10px; height:25px;width:25px;" onclick="showPostofUser('+jArray[i]["userId"]+')">';
		if(!jArray[i]["selfPost"]){
			template += '<strong>'+jArray[i]["name"]+'</strong>';
			template += ' shared this with <strong>You</strong>';
		}/*else if(jArray[i]["userId"]==jArray[i]["userToId"] && jArray[i]["sharedWith"]=='none'){
            template += '<strong>Admin</strong>';
            template += ' shared this with <strong>You</strong>';
        }*/else{
			template += '<strong>You</strong>';
			template += ' shared this with <strong>'+jArray[i]["sharedWith"]+'</strong>';
		}

		template += '</div>';
		template += '<div class="ignoreGEMSStyling" style="padding: 15px;overflow:auto;margin-left: 10px;" id="description'+jArray[i]["alumniRingPostDetailId"]+'" contenteditable="false">';
		try{
			template += decodeURIComponent(jArray[i]["description"]);
		}catch(e){
			template += jArray[i]["description"];
		}
		template += '<div id="attachment-'+jArray[i]["alumniRingPostId"]+'"></div>';
		template += '</div>';
		template += '<div style="padding: 5px; background-color: #FAFAFA; border-bottom: 1px solid #F0F0F0; font-size: 11px;">';
		if(!jArray[i]["selfPost"])
		{
			if($('#showCommentHide').attr('showhide')=='hide'){
				template += ''; 
				template += ''; 
			}else{
				template += '<a data-toggle="popover" title=""  data-content="" data-original-title="" id="reply'+replyid+'" href="javascript:void(0)" onclick="replyToBulletin('+jArray[i]["userId"]+',\''+jArray[i]['type']+'\',\''+jArray[i]['status']+'\','+replyid+',\''+jArray[i]["id"]+'\')">Reply</a>';
				template += ' - ';	
			}
		}//new
			template +='<a href="javascript:void(0);" onclick="removePost('+jArray[i]["alumniRingPostDetailId"]+')" >Remove</a>';
			template +='<a href="javascript:void(0);" onclick="removeAllPost(\''+jArray[i]["alumniRingPostDetailId"]+'\',\''+jArray[i]["alumniRingPostId"]+'\')" >Remove All</a>'; //jm
	
		if(isAlumniHomeHtm){
			template +='<div id="shareSocialDiv-'+i+'" style="font-size: 10px;"></div>';
		}
		/*template += ' - ';
		template +='<a id="forward" href="javascript:void(0);" onclick="forwardPost(\''+id+'\',\''+des+'\')" >Forward</a>';*/
//		if($('#customerHelper').val() != 'DYPATIL') // coustmer condition by awanish
		template +='<span style="float: right;"> '+jArray[i]["date"]+' '+jArray[i]["time"]+'</span>';
		template += '</div>';
		template += '<div style="width:100%;display:block;" class="commentbox"><div id="divNewCommen-'+jArray[i]["alumniRingPostId"]+'" class="'+$('#showCommentHide').attr('showhide')+'"><input type="text" id="txtNewCommen-'+jArray[i]["alumniRingPostId"]+'" style="width:80%;margin:5px;border-radius:0px; display:inline;" placeholder="Enter Comment..." class="tComment"><button id="btnNewCommen-'+jArray[i]["alumniRingPostId"]+'" onclick="savePostComment('+jArray[i]["alumniRingPostId"]+')" class="btn btn-mini btn-primary" style="border-radius:0px;width:15%;float:right;margin-left:0px; display:inline;">Send</button></div></div>';
		template += '</div>';
		
		replyid++;
		
		$('#divalumpost').prepend(template);

		if(isAlumniHomeHtm){
			$("#shareSocialDiv-"+i).jsSocials({
				url: des,
				text: "Share Post",
				showCount: false,
				showLabel: true,
				shares: [
				         "email",
				         "twitter",
				         { share: "facebook", label: "Share" },
				         "googleplus",
				         "linkedin",
				         "pinterest",
				         "whatsapp"
				         ]
			});
		}
		var docArray = jArray[i]["attachments"];
		if(docArray.length > 0){
			var isCreator = false;
			if(jArray[i]["selfPost"]){
				isCreator = true;
			}
			var person = $('#hdnUserRole').val();
			if(person.toLowerCase() != 'student' && person != 'alumnistudent'){
				person = 'Employee';
			} else if(person == 'alumnistudent'){
				person = 'Student';
			}
			showAluminiPostAttachment(docArray,'attachment-'+jArray[i]["alumniRingPostId"],isCreator,person);
		}
		var commentjArray = eval( '('+jArray[i]["commentjArray"]+')' );
		var commentLength = commentjArray.length;
		if($('#showCommentHide').attr('showhide')=='show'){
		var showHideBoiler='<table id="tableShowHideComment-'+jArray[i]["alumniRingPostId"]+'" style="margin-left:5%; display:none;margin-top:3px;width:94%;background-color:#F3F3F3;" class="'+$('#showCommentHide').attr('showhide')+'">'+
		'<tr id="tr_showComments-'+jArray[i]["alumniRingPostId"]+'">'+
		'<td><a href="javascript:void(0);" onclick="showHiddenComment('+jArray[i]["alumniRingPostId"]+')">Show all comments('+commentLength+')</a></td>'+    
		'</tr>'+
		'<tr id="tr_hideComments-'+jArray[i]["alumniRingPostId"]+'" style="display:none;">'+
		'<td><a href="javascript:void(0);" onclick="hideShownComment('+jArray[i]["alumniRingPostId"]+')">Hide comments</a></td>'+     
		'</tr>'+
		'</table>';
		$("#divNewCommen-"+jArray[i]["alumniRingPostId"]).after(showHideBoiler);

		for(var k=0;k<commentLength;k++){
			var boiler='';
			if(commentLength>3 && k<(commentLength-3)){
				boiler= '<table class="hiddenComment-'+jArray[i]["alumniRingPostId"]+'" style="display:none;margin-left:5%;margin-top:3px;width:94%;background-color:#F3F3F3;" class"'+$('#showCommentHide').attr('showhide')+'">'+
				'<tbody id="tr-'+commentjArray[k]["commentThreadId"]+'">'+
				'<tr>'+
				'<td rowspan="2" style="vertical-align:top;padding:4px;width:30px;"><img src="'+commentjArray[k]["ImageURL"]+'" height="20px" width="20px"></td>'+
				'<td><p style="display:inline;"><b>'+commentjArray[k]["name"]+':</b></p><p style="display:inline;float:right" class="muted"></p></td>'+    //'+commentjArray[k]["commentDateTime"]+'
				'</tr>'+
				'<tr>'+
				'<td>'+
				'<p class="muted"><span id="commentData-'+commentjArray[k]["commentThreadId"]+'">'+commentjArray[k]["comment"]+'</span>';
				if(commentjArray[k]["isOwner"])
				{	
				boiler+='<span id="parentAction"><span id="actionRemove-'+commentjArray[k]["commentThreadId"]+'"><a href="#linkDiv" title="Remove" onclick="deleteCommentThread('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">remove</i></a></span>'+
'<span id="actionEdit-'+commentjArray[k]["commentThreadId"]+'"><a href="javascript:void(0);" onclick="editComment('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">edit</i></a></span>';
				}
				boiler+='</span></p>'+

				'</td>'+
				'</tr>'+
				'</tbody>'+
				'</table>';
			}
			else{
				boiler= '<table class="comment-'+jArray[i]["alumniRingPostId"]+'" style="margin-left:5%;margin-top:3px;width:94%;background-color:#F3F3F3;" class"'+$('#showCommentHide').attr('showhide')+'">'+
				'<tbody id="tr-'+commentjArray[k]["commentThreadId"]+'">'+
				'<tr>'+
				'<td rowspan="2" style="vertical-align:top;padding:4px;width:30px;"><img src="'+commentjArray[k]["ImageURL"]+'" height="20px" width="20px"></td>'+
				'<td><p style="display:inline;"><b>'+commentjArray[k]["name"]+':</b></p><p style="display:inline;float:right" class="muted">'+($('#customerHelper').val() == 'DYPATIL' ? "" :commentjArray[k]["commentDateTime"])+'</p></td>'+   //customer condition by awanish 
				'</tr>'+
				'<tr>'+
				'<td>'+
				'<p class="muted"><span id="commentData-'+commentjArray[k]["commentThreadId"]+'">'+commentjArray[k]["comment"]+'</span>';
			//change
				if(commentjArray[k]["isOwner"]){	
					boiler+='<span id="parentAction"><span id="actionRemove-'+commentjArray[k]["commentThreadId"]+'"><a href="#linkDiv" title="Remove" onclick="deleteCommentThread('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">remove</i></a></span>'+
							'<span id="actionEdit-'+commentjArray[k]["commentThreadId"]+'"><a href="javascript:void(0);" onclick="editComment('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">edit</i></a></span>';
				}
				boiler+=			'</span></p>'+
				'</td>'+
				'</tr>'+
				'</tbody>'+
				'</table>';
			}
			$('#tableShowHideComment-'+jArray[i]["alumniRingPostId"]).after(boiler);
			$("#txtNewCommen-"+jArray[i]["alumniRingPostId"]).val("");
		}
		}	
		if(commentLength>3){
			$('#tableShowHideComment-'+jArray[i]["alumniRingPostId"]).show();
		}
		else{
			$('#tableShowHideComment-'+jArray[i]["alumniRingPostId"]).hide();
		}
	}
}

function showHiddenComment(alumniRingPostId){
	$('.hiddenComment-'+alumniRingPostId).show();
	$('#tr_showComments-'+alumniRingPostId).hide();
	$('#tr_hideComments-'+alumniRingPostId).show();
}
function hideShownComment(alumniRingPostId){
	$('.hiddenComment-'+alumniRingPostId).hide();
	$('#tr_showComments-'+alumniRingPostId).show();
	$('#tr_hideComments-'+alumniRingPostId).hide();
}
function showAllPostedToUser(alumniRingPostId){
	$.ajax({
		url:'showAllPostedToUser.json?alumniRingPostId='+alumniRingPostId,
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var modalBoiler ='			<div id="divSharedWithModal" class="modal hide fade">' +
						'				<div class="modal-header">' +
						'					<button type="button" class="close" data-dismiss="modal">x</button>' +
						'					<h3>You shared the post with...</h3>' +
						'				</div>' +
						'				<div class="modal-body">' +
						'					<div id="divSharedWithModalBody"></div>' +
						'				</div>' +
						'				<div class="modal-footer">' +
						'					<input type="button" class="btn btn-inverse btn-mini"' +
						'						data-dismiss="modal" value="Close" />' +
						'				</div>' +
						'			</div>';
			$('#sharedWithModalDiv').html(modalBoiler);
			var boiler ='<table class="table">';
			for(var i=0;i<jArray.length;i++){
				boiler +='<tr>';
				boiler +='<td><img src="'+jArray[i]["ImageURL"]+'"/></td>';
				boiler +='<td><strong>'+jArray[i]["name"]+'</strong></td>';
				boiler +='</tr>';
			}
			boiler +='</table>';
			$('#divSharedWithModalBody').html(boiler);
			$('#divSharedWithModal').modal('show');
		},
		error:function(){

		}
	});
}
$(function(){
//pankaj try catch to avoid error in file uploading
$('#post-alum').click(function(){
	globalPostType = $('.shareType').val();
	var alumniTextPost='';
	  let emailSubject = "";

	if(globalPostType=='status'){
		try{
		alumniTextPost= encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData());//$('#alumniTextPost').val();
		}catch(Exception){alumniTextPost="";}
	}else if(globalPostType=='videos'){
		try{
			var textPost= encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData()); //$('#alumniTextPost').val();
		}catch(Exception){alumniTextPost="";}
		var url = $('div.liveurl div.inner div.url').html();
		var urlboiler='<a href="'+url+'" target="_blank">'+url+'</a>';
		$('div.liveurl div.inner div.url').html(urlboiler);
		
		var title=$('div.liveurl div.inner div.title').html();
		var titleBoiler='<b>'+title+'</b>';
		$('div.liveurl div.inner div.title').html(titleBoiler);
		
		//alumniTextPost=textPost+'<div class="row-fluid">'+$('div.liveurl div.inner').html()+'</div>';
		//$('div.liveurl div.inner').html()
		alumniTextPost=textPost+encodeURIComponent('<div class="row-fluid">'+$('#alumniLinkPost').val()+'</div>');
				
	}else if(globalPostType=='photos'){
		try{
		alumniTextPost=encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData());//$('#alumniTextPost').val();
	}catch(Exception){alumniTextPost="";}
	}else if(globalPostType=='location'){
		try{
		alumniTextPost=encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData());//$('#alumniTextPost').val();
	}catch(Exception){alumniTextPost="";}
		var lng = $( "input[name='lng']" ).val();
		var lat = $( "input[name='lat']" ).val();
		
		if(lat!=undefined && lng!=undefined && lat!=0 && lng!=0 && lat!='' && lng!=''){
			alumniTextPost += '<br><strong>'+$('#geocomplete').val()+'</strong><br/>';
			alumniTextPost += '<img class="img-responsive" src="http://maps.googleapis.com/maps/api/staticmap?size=500x150&maptype=roadmap\&markers=size:mid%7Ccolor:red%7C'+lat+','+lng+'&zoom=15&sensor=false" />';
		}
	}else if(globalPostType == 'sms'){
		try{
			alumniTextPost = $('#alumniTextPost').val();
		}
		catch(Exception){
			alumniTextPost = "";
		}
	}else if(globalPostType == 'email'){
		try{
			if(ckeditorFlag != undefined)
				ckeditorFlag = false;
				emailSubject = $("#emailSubject").val();
			alumniTextPost= encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData());//$('#alumniTextPost').val();
		}
		catch(Exception){
			alumniTextPost = "";
		}
	}
	
	
	var alumUserId='';
	var selfPostFlag=false;
	if($('#postScope').val()!='' && $('#postScope').val()!=undefined){
		alumUserId=$('#postScope').val();
		$('#postScope').focus();
		selfPostFlag=true;
	}else{
		selfPostFlag=false;
	}

	if(selfPostFlag){
		$.ajax({
			url:'addAlumniPostNew.json',
			dataType:'json',
			type:'POST',
			data:{alumUserId:alumUserId,alumniTextPost:alumniTextPost, postType : globalPostType,emailSubject: emailSubject},
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
				try{
				CKEDITOR.instances.alumniTextPost.setData("");
				}catch(Exception){}
				//$('#alumniTextPost').val("");
				$('#alumniLinkPost').val("");
				$('#alumniDocumentPostText').val('');
				$('#showAluminiPostUploadDiv').html('');				
				$('#postScope').tokenInput("clear");
				$('.liveurl').hide();
				//$('#alumniTextPost').focus();
				/*for(name in CKEDITOR.instances){
				    CKEDITOR.instances[name].destroy(true);
				}*/
				$('#post-alum').focus();
				//changeMessageType(globalPostType);
			},
			success:function(jArray){
				getAlumniPost(5);
			},
			error:function(){
			}
		});
	}
});



$('#post-alum-home').click(function(){
	
	var alumniTextPost=CKEDITOR.instances['alumniTextPost'].val().getData();
	var alumUserId='';
	var selfPostFlag=false;
	if($('#postScope').val()!='' && $('#postScope').val()!=undefined){
		alumUserId=$('#postScope').val();	
		selfPostFlag=false;
	}else{
		alumUserId=$('#alumUserId').val();
		selfPostFlag=true;
	}
	
	$.ajax({
		url:'addAlumniPostNew.json',
		dataType:'json',
		type:'POST',
		data:{alumUserId:alumUserId,alumniTextPost:alumniTextPost},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
			changeMessageType('Text');
			//$('#alumniTextPost').focus();
			CKEDITOR.instances.alumniTextPost.setData("");
			//$('#alumniTextPost').val("");
			//$('#alumniLinkPost').val("");
			$('#alumniDocumentPostText').val('');
			$('#showAluminiPostUploadDiv').html('');				
			$('#postScope').tokenInput("clear");
			$('.liveurl').hide();
			
			for(name in CKEDITOR.instances){
			    CKEDITOR.instances[name].destroy(true);
			}
			//$('#alumniTextPost').val("");
			$('#post-alum-home').focus();
		},
		success:function(jArray){
			if(selfPostFlag){
				var alum='<div class="row-fluid" id="alumPost'+jArray["alumniRingPostId"]+'"><div style="background-color: white; border:1px solid; border-color:#c4cde0; margin-top:20px; min-height: 90px;" class="span12">';
				var alumRemoveAll ='<div class="row-fluid" id="alumPostAllRemove'+jArray["alumniRingPostId"]+'"><div style="background-color: white; border:1px solid; border-color:#c4cde0; margin-top:20px; min-height: 90px;" class="span12">'; 
				if(jArray['status']=='yes' && jArray['type']=='employee'){ 
					alum+='<div class="modal-header post"><img src="getEmployeeProfileImageById.json?id='+jArray["id"]+'" /><label style="margin-left:50px; font-weight:bold;"> '+jArray["name"]+'</label><div class="postclose"><a href="javascript:void(0)"><img src="img/close.png" style="border:0px;" onclick="removePost('+jArray["alumniRingPostId"]+')" /></a></div><div class="name" style="float:right;"> Created '+jArray["date"]+' at '+jArray["time"]+'</div></div>';
					alumRemoveAll+='<div class="modal-header post"><img src="getEmployeeProfileImageById.json?id='+jArray["id"]+'" /><label style="margin-left:50px; font-weight:bold;"> '+jArray["name"]+'</label><div class="postclose"><a href="javascript:void(0)"><img src="img/close.png" style="border:0px;" onclick="removeAllPost('+jArray["alumniRingPostId"]+')" /></a></div><div class="name" style="float:right;"> Created '+jArray["date"]+' at '+jArray["time"]+'</div></div>';
                 }
				else if(jArray['status']=='yes' && jArray['type']=='student') {
					alum+='<div class="modal-header post"><img src="getStudentProfileImageById.json?id='+jArray["id"]+'" /><label style="margin-left:50px; font-weight:bold;"> '+jArray["name"]+'</label><div class="postclose"><a href="javascript:void(0)"><img src="img/close.png" style="border:0px;" onclick="removePost('+jArray["alumniRingPostId"]+')" /></a></div><div class="name" style="float:right;"> Created '+jArray["date"]+' at '+jArray["time"]+'</div></div>';
					alumRemoveAll+='<div class="modal-header post"><img src="getStudentProfileImageById.json?id='+jArray["id"]+'" /><label style="margin-left:50px; font-weight:bold;"> '+jArray["name"]+'</label><div class="postclose"><a href="javascript:void(0)"><img src="img/close.png" style="border:0px;" onclick="removeAllPost('+jArray["alumniRingPostId"]+')" /></a></div><div class="name" style="float:right;"> Created '+jArray["date"]+' at '+jArray["time"]+'</div></div>';
				}else {
					alum+='<div class="modal-header post"><img src="'+jArray["profileImage"]+'" /><label style="margin-left:50px; font-weight:bold;"> '+jArray["name"]+'</label><div class="name" style="float:right;"> Created '+jArray["date"]+' at '+jArray["time"]+'</div></div>';
					alumRemoveAll+='<div class="modal-header post"><img src="'+jArray["profileImage"]+'" /><label style="margin-left:50px; font-weight:bold;"> '+jArray["name"]+'</label><div class="name" style="float:right;"> Created '+jArray["date"]+' at '+jArray["time"]+'</div></div>';
                     }
                     alum += '<div style="margin-top:10px; margin-left:50px;">' + alumniTextPost + '</div><br>';
                    alumRemoveAll += '<div style="margin-top:10px; margin-left:50px;">' + alumniTextPost + '</div><br>';
 
				//alum+='<div style="margin-top:10px; margin-left:50px;">'+alumniTextPost+'</div>'+ // jm

			}
			showSuccessMessage("Posted successfully",5000);
		},
		error:function(){

		}
	});
});
});

