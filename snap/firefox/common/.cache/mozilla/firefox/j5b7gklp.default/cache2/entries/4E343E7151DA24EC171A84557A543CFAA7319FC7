function removeAllPost(alumnipostDetailId, alumniRingPostId) {
    $.ajax({
        url: 'removeAllAlumniPost.json?alumnipostDetailId=' + alumnipostDetailId + '&alumniRingPostId=' + alumniRingPostId,
        success: function (jArray) {
        $('#alumPost'+alumnipostDetailId).fadeOut(1200);
        },       
    });
}

function savePostComment(alumniRingPostId){
	
	var comment=$("#txtNewCommen-"+alumniRingPostId).val().trim();
	
		if(comment == ""){
		jAlert("Please Enter Comment");
		return;
	}
	
	$.ajax({
			url:'savePostCommentThread.json?comment='+escape(comment)+'&alumniRingPostId='+alumniRingPostId,
			dataType:'json',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(commentjArray){
				
				var k = 0;
				
				var boiler=	'<table style="margin-left:5%;margin-top:3px;width:94%;background-color:#F3F3F3;">'+
				'<tbody id="tr-'+commentjArray[k]["commentThreadId"]+'">'+
								'<tr>'+
									'<td rowspan="2" style="vertical-align:top;padding:4px;width:30px;"><img src="'+commentjArray[k]["ImageURL"]+'" height="20px" width="20px"></td>'+
									'<td><p style="display:inline;"><b>'+commentjArray[k]["name"]+':</b></p><p style="display:inline;float:right" class="muted">'+($('#customerHelper').val() == 'DYPATIL' ? "" : commentjArray[k]["commentDateTime"])+'</p></td>'+  // customer helper condition by awanish  
								'</tr>'+
								'<tr>'+
								//change here
									'<td><p class="muted"><span id="commentData-'+commentjArray[k]["commentThreadId"]+'">'+commentjArray[k]["comment"]+'</span><span id="parentAction"><span id="actionRemove-'+commentjArray[k]["commentThreadId"]+'"><a href="#linkDiv" title="Remove" onclick="deleteCommentThread('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">remove</i></a></span>'+
									'<span id="actionEdit-'+commentjArray[k]["commentThreadId"]+'"><a href="javascript:void(0);" onclick="editComment('+commentjArray[k]["commentThreadId"]+')"><i style="float:right;margin-left:5px;">edit</i></a></span>';
									'</span></p></td>'+
								'</tr>'+
								'</tbody>'
							'</table>';
				
				$('#tableShowHideComment-'+alumniRingPostId).after(boiler);
				$("#txtNewCommen-"+alumniRingPostId).val("");
			},
	        error : function() {
	        	jAlert("Error posting your comment, Please try after some time");
			}
	       
	    });
}

function deleteCommentThread(commentThreadId){
	$.ajax({
		url:'deleteCommentThread.json?commentThreadId='+commentThreadId,
		dataType:'text',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(text){
			$("#tr-"+commentThreadId).hide();
			/*var commCnt=$("#lblCommentCnt").text();
			$("#lblCommentCnt").html(parseInt(commCnt)-1);*/
		},
        error : function() {
        	alert("error in deleteCommentThread");
		}
       
    });
}
