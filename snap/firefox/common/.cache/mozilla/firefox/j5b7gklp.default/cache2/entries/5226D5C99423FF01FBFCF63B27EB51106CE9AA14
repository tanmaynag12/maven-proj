		$.ajax({
			url:'getDocumentCategoryByResearchPublicationId.json',
			data:'employeeProfileDetailId='+employeeProfileId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}



function addResearchPublicationResponseAfterSubmit(response,postData) {
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#researchtable").getGridParam("selrow");
	if(gr != null){
		$("#researchtable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#researchtable").jqGrid('setCell',gr,'employeeReasearchId',jObject["employeeReasearchId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}

function getEmployeeProfileDetailsWithChaptersPublished(employeePublicationId)
{
	$.ajax({
		url:'getEmployeeProfileDetailsWithChaptersPublished.json?employeePublicationId='+employeePublicationId,
		dataType:'json',
		beforeSend: function() {
			$('#loadingDiv').show();
			$(".ui-dialog .ui-dialog-titlebar-close").hide();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var htm= '<table class="smalltable" id="empChapTable"><tr><th>Employee Name</th><th>No. Of Chapters</th><th>Save Chapters</th></tr>';	 
			for(var i = 0 ; i < jArray.length; i++){
				var empChapters=0;
				empChapters=(jArray[i]["empChapters"]!=undefined)?parseInt(jArray[i]["empChapters"]):'';
				if(jArray[i]["empId"]!=undefined){

					htm += '<tr><td><label>'+jArray[i]["empFullName"]+'</label></td>'+
					'<td><input type="text" class="txtEmpChapters" id="empProfile_'+jArray[i]["empProfileDetailId"]+'" value="'+empChapters+'"/></td>'+
					'<td><button class="btn btn-mini btn-info" onclick="saveEmpChapters('+jArray[i]["empProfileDetailId"]+',this)">Save</button></td></tr>';
				}

				if(jArray[i]["other"]!=undefined){

					htm += '<tr><td><label>'+jArray[i]["other"]+'</label></td><td>'+
					'<input type="text" class="txtEmpChapters" id="empProfile_ '+jArray[i]["empProfileDetailId"]+'"  value="'+empChapters+'"/></td>'+
					'<td><button class="btn btn-mini btn-info" onclick="saveEmpChapters('+jArray[i]["empProfileDetailId"]+',this)">Save</button></td></tr>';
				}
			}

			htm += '</table>';
			$('#empChapterList').html(htm);
		},
		error:function(){
			alert('Error in Employee Chapters Published');
		}
	});
}

function saveEmpChapters(empProfileDetailId,element)
{
	var empChapters = $('#empProfile_'+empProfileDetailId).val();
	if(empChapters=='')
		alert('Enter No. Of Chapter');
	else
	{
		$.ajax({
			url:'saveEmpPublicationChapters.json?empProfileDetailId='+empProfileDetailId+'&empChapters='+empChapters,
			type:'POST',
			dataType:'text',
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(txt){
				var currHtml='<button class="btn btn-mini btn-info" onclick="saveEmpChapters('+empProfileDetailId+',this)">Save</button>&nbsp;&nbsp;<i class="icon-ok"></i>Saved';
				$(element).parent().html(currHtml);
			},
			error:function(){
				jAlert("Something went wrong, Please try after some time","Error");
			}
		});
	}
}


