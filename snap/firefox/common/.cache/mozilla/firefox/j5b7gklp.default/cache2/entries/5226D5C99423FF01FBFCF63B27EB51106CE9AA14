//deAllocate member




function deAllocateMember(type){
	var employeeProfileDetail = new Array();


	$("input[name=chkEmployeeName]:checked").each(function() {

		employeeProfileDetail.push($(this).val());
	}); 
	if(employeeProfileDetail.length > 0){

		$.ajax({
			url:'deAllocateMember.json?employeeProfileDetailId='+employeeProfileDetail,	
			dataType:'json',
			success:function(jArray){
				if(jArray[0].responseFlag == '1'){
					showSuccessMessage(jArray[0].responseText,10000);

					if(type==gEmployeeProfileTab.TRAINING)
					{
						//getTrainingDetails();
						viewEmpTrainingNew();
					}
					else if(type==gEmployeeProfileTab.SEMINAR){
						//getSeminarDetail();
						viewEmpSeminarNew();
					}
					else if(type==gEmployeeProfileTab.CONFERENCE){
						//getConferenceDetails();
						viewNewEmpConference();
					}
					else if(type==gEmployeeProfileTab.BOOKMONOGRAPH){
						//getBooks_Monograph();
						viewBookMonographNew();

					}
					else if(type==gEmployeeProfileTab.WORKSHOP){
						//getWorkshopDetail();
						viewEmpWorkShopNew();
					}
					else if(type==gEmployeeProfileTab.RESEARCH){
						//getResearchPublication();
						viewResearchPublicationNew();
					}
					else if(type==gEmployeeProfileTab.PATENT){
						//getPatentDetails();
						viewNewEmpPatents();
					}
					else if(type==gEmployeeProfileTab.CONSULTANCY){
						//getConsultancyDetail();
						viewEmpConsultancyNew();
					}
					else if(type==gEmployeeProfileTab.MEMBERSHIP){
						//getMembershipDetails();
						viewEmpMembershipNew();
					}

					$('#employeeList').modal('hide');
					$("#divModalAddingNewPaperPublication").modal('show');
					$('#divModalAddingNewBooksAndMonograph').modal('show');
					$('#divModalAddingNewPatentsAndCopyRigts').modal('show');
					$('#divModalAddingNewProfessionalSocietyMembership').modal('show');
					$('#divModalAddingNewEventConferences').modal('show');
					$('#divModalAddingNewSeminar').modal('show');
					$('#divModalAddingNewWorkshop').modal('show');
					$('#divModalAddingNewTraining').modal('show');
					$('#divModalAddingNewEventConsultancy').modal('show');
				} else {
					showErrorMessage(jArray[0].responseText,5000);
				}
			},
			error:function(){
				////////////alert("In Deallocate Error");
			}
		});
	} else {
		showErrorMessage('Seminar Topic has not been selected. Try again!', 5000);

	}

}


function addSeminarResponseAfterSubmit(response,postData){
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#seminartable").getGridParam("selrow");
	if(gr != null){
		$("#seminartable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#seminartable").jqGrid('setCell',gr,'employeeSeminarId',jObject["employeeSeminarId"]);
		$("#seminartable").jqGrid('setCell',gr,'documentCategoryId',jObject["documentCategoryId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}

function addMembershipResponseAfterSubmit(response,postData){
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#membershiptable").getGridParam("selrow");
	if(gr != null){
		$("#membershiptable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#membershiptable").jqGrid('setCell',gr,'employeeMembershipId',jObject["employeeMembershipId"]);
		$("#membershiptable").jqGrid('setCell',gr,'documentCategoryId',jObject["documentCategoryId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}



function addConsultancyResponseAfterSubmit(response,postData){
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#consultancytable").getGridParam("selrow");
	if(gr != null){
		$("#consultancytable").jqGrid('setCell',gr,'consultancyId',jObject["consultancyId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}


//************************************************************************************function to get document category ID 

function getDocumentCategoryBySeminar(employeeProfileId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryBySeminarId.json',
			data:'employeeProfileDetailId='+employeeProfileId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}

//************************************************************************************function to get document category ID 

function getDocumentCategoryByIndustrialLiaison(employeeProfileId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryByIndustrialLiaisonId.json',
			data:'employeeProfileDetailId='+employeeProfileId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}

//************************************************************************************function to get document category ID 

function getDocumentCategoryByConsultancy(employeeProfileId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryByConsultancyId.json',
			data:{employeeProfileDetailId:employeeProfileId},
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}

function getDocumentCategoryByConsultancyWithType(consultId,callback, type){
	try{
		$.ajax({
			url:'getDocumentCategoryByConsultancyIdWithType.json',
			data:{consultId:consultId, type:type},
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}
