	var sr = jQuery("#seminartable").getGridParam('selrow');

	//alert(" in sr row data" + sr);

	if(sr != null){
		rowdata = jQuery("#seminartable").getRowData(sr);
		MemberNames =  rowdata.Attended_With;
		//alert(MemberNames);
	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return MemberNames;
}


function getAllocatedMemberIds(){

	var allocatedMemberIds = '';

	var sr = jQuery("#seminartable").getGridParam('selrow');
	if(sr != null){
		rowdata = jQuery("#seminartable").getRowData(sr);
		allocatedMemberIds =  rowdata.attendedId;
		alert(allocatedMemberIds);
	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return allocatedMemberIds;
}	 


//******************************************************************************************************	 


function setEmployeeListGrid(){


	var allocatedMemberIdArr = allocatedMemberIds.split(', ');
	var allocatedMemberNameArr = allocatedMemberNames.split(', ');


	$('select#selectedEmployeeName').html('');
	$("input:not(:checked)").each(function(){
		/** find the closest 'tr' element and then find the third child 'td' element for getting the id of the employee from the employee list dialog box.
		 * @see jQuery.inArray(value,Array) returns index of the value in array. If not found returns -1
		 * @details index variable will be assigned by that index on which employee Id in the grid matches same in the 'allocatedTeacherIdArr' array
		 * */
		var index = jQuery.inArray($($(this).closest('tr')).children(':eq(2)').html(),allocatedMemberIdArr);
		if(index != -1){
			$(this).attr('checked','checked');
			$('select#selectedEmployeeName').append('<option value="'+allocatedMemberIdArr[index]+'">'+allocatedMemberNameArr[index]+'</option>');
		}
	});

} 




