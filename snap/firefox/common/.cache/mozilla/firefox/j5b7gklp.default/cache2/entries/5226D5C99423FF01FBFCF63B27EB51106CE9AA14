$(document).ready(function(){
	$(function(){
		$('#uploadSeminarDocumentDialog').modal({
			backdrop: 'static',
			keyboard: false,
			show: false
		}).css({
			width: '600px',
			'margin-left': function () {
				return -($(this).width() / 2);
			}
		});
	});
});





//***********************************************************************************Comman Functions



function pickdateSeminar(id)
{


	var frdt=id+"_startDate";
	$(function() {
		var dates = $( "#"+id+"_startDate,#"+id+"_endDate" ).datepicker({

			dateFormat:"M dd,yy",
			changeMonth: true,
			showWeek: true,
			changeYear: true,
			yearRange:"-60:+0",
			onSelect: function( selectedDate ) {
				var option = this.id == frdt ? "minDate" : "maxDate",
						instance = $( this ).data( "datepicker" ),
						date = $.datepicker.parseDate(
								instance.settings.dateFormat ||
								$.datepicker._defaults.dateFormat,
								selectedDate, instance.settings );
				dates.not( this ).datepicker( "option", option, date );
			},
		});
	});
}





function getAllocatedEmployeeNames(gridId){

	var MemberNames = '';
	var sr = jQuery("#"+gridId).getGridParam('selrow');			
	if(sr != null){
		rowdata = jQuery("#"+gridId).getRowData(sr);
		MemberNames =  rowdata.Attended_With;
		//alert(MemberNames);
	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return MemberNames;
}


function getAllocatedEmployeeIds(gridId){

	var allocatedMemberIds = '';
	//alert("Gride ID "+gridId );

	var sr = jQuery("#"+gridId).getGridParam('selrow');
	if(sr != null){
		rowdata = jQuery("#"+gridId).getRowData(sr);
		allocatedMemberIds =  rowdata.attendedId;
		//alert(allocatedMemberIds);

	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return allocatedMemberIds;
}



/**
 * @param categoryId
 * @param tabType e.g: seminar,training, workshop etc
 */
function openEmployeeProfileDocumentUploadDialog(categoryId,tabType,isViewOnly){ 
	//alert("categoryId: "+categoryId);
	$('#uploadSeminarDocumentDialog').modal('show');
	$('#divModalAddingNewBooksAndMonograph').modal('hide');
	$("#divModalAddingNewPaperPublication").modal('hide');
	$("#divModalAddingNewPatentsAndCopyRigts").modal('hide');
	$('#divModalAddingNewProfessionalSocietyMembership').modal('hide');
	$('#divModalAddingNewEventConferences').modal('hide');
	$('#divModalAddingNewSeminar').modal('hide');
	$('#divModalAddingNewWorkshop').modal('hide');
	$('#divModalAddingNewTraining').modal('hide');
	var uploadDocument = new UploadDocument();
	if(isViewOnly == undefined || isViewOnly == 'false'){
		try{
			uploadDocument.init({
				elementId:'employeeUploadedFile',
				categoryId:categoryId,
				fileSize:AllowedFileSize.DOCUMENT_UPLOAD,
			},
			function(docId){
				//alert("docId: "+docId);
				uploadDocument.getDocument(categoryId, function(jArray){
					$('#showDownloadedFiles').html('');
					uploadDocument.showDocument({
						elementId:'showDownloadedFiles',
						documentList:jArray,
						fileSize:AllowedFileSize.DOCUMENT_UPLOAD
					});

				});
			});
		} catch(e){
			jAlert(e);
		}
	}

	uploadDocument.getDocument(categoryId, function(jArray){
		//showUploadedEmployeeProfileDocument(jArray);
		$('#showDownloadedFiles').html('');
		uploadDocument.showDocument({
			elementId:'showDownloadedFiles',
			documentList:jArray
		});

	});
}
function openStudentProfileDocumentUploadDialog(categoryId,tabType,isViewOnly){ 
	$('#uploadSeminarDocumentDialog').modal('show');
	var uploadDocument = new UploadStudentDocument();
	if(isViewOnly == undefined || isViewOnly == 'false'){
		try{
			uploadDocument.init({
				elementId:'studentUploadedFile',
				categoryId:categoryId,
			},
			function(docId){
				uploadDocument.getDocument(categoryId, function(jArray){
					$('#showDownloadedFiles').html('');
					uploadDocument.showDocument({
						elementId:'showDownloadedFiles',
						documentList:jArray
					});

				});
			});
		} catch(e){
			jAlert(e);
		}
	}

	uploadDocument.getDocument(categoryId, function(jArray){
		//showUploadedEmployeeProfileDocument(jArray);
		$('#showDownloadedFiles').html('');
		uploadDocument.showDocument({
			elementId:'showDownloadedFiles',
			documentList:jArray
		});

	});
}


function showUploadedEmployeeProfileDocument(jArray){

	var boiler = '';
	boiler = '<div id="accordionDownloadAttachment">';
	for(var i=0;i<jArray.length;i++)
	{
		var eDocPathId = jArray[i].eDocPathId!=undefined?jArray[i].eDocPathId:jArray[i]["documentPathId"];//E-N-C
		var downloadLink = (juno.label['url.download.json'].setIndexedArgs(eDocPathId, "UPLOADED", 'NA'));
		
		var fileType = jArray[i]["extension"];
		var path = gFileTypeIconMapping[fileType];
		boiler += '<h6><a href="#"><span><img src = "'+path+'"></span>'+
		'<lable onclick = "window.location.replace(\''+downloadLink+'\')">'+jArray[i]["name"]+'</lable></a></h6>';
		'<div></div>';
	}
	boiler += '</div>';
	$('#showDownloadedFiles').html(boiler);
	$('#accordionDownloadAttachment').accordion({active:-1,autoHeight: true,collapsible: false});
}


