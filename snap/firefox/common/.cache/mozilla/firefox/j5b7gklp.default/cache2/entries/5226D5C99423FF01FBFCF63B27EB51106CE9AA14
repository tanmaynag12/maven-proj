	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());

	}

	if($('#txtOutsideInstiMember').val()!=undefined){

		addOther=$('#txtOutsideInstiMember').val();
	}
	if(employee.length == 0){
		employee='';
	}	

	

	$.ajax({
		url:'addmembertomembership.json?employee='+employee+'&employeeMembershipId='+employeeMembershipId+'&addOther='+addOther,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
