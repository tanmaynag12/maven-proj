	$('#instituteemp').hide();
	$('#showemplist').hide();

}
function showcombo() {
	$('#instituteemp').show();
	$('#showemplist').show();

}

function openseminardialog(gridId) {
	$('#txtAddOther').val('');
	$('#sourceGridId').val(gridId);
	$('#selectedEmployeeName').find('option').remove();
	$( "#MemberListDialog" ).modal( 'show' );

	$('#divModalAddingNewBooksAndMonograph').modal('hide');
	$("#divModalAddingNewPaperPublication").modal('hide');
	$("#divModalAddingNewPatentsAndCopyRigts").modal('hide');
	$('#divModalAddingNewProfessionalSocietyMembership').modal('hide');
	$('#divModalAddingNewEventConferences').modal('hide');
	$('#divModalAddingNewSeminar').modal('hide');
	$('#divModalAddingNewWorkshop').modal('hide');
	$('#divModalAddingNewTraining').modal('hide');
	$('#divModalAddingNewEventConsultancy').modal('hide');
	jQuery("#instituteEmployeeSubjectAllocationGrid").jqGrid('clearGridData');
	//populateInstiCourseList('cbInstiCourseId');	 
	fetchInstiCourseList('cbInstiCourseId');

}

$(function (){

	$('#myModal').hide();
	$('#otheremployeename').hide();

	$('#MemberListDialog').modal({
		backdrop: 'static',
		keyboard: false,
		show: false
	}).css({
		width: '900px',

		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});


	$('#employeeList').modal({
		backdrop: 'static',
		keyboard: false,
		show: false
	}).css({
		width: '700px',

		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});



	$('#addImpact').modal({
		backdrop: 'static',
		keyboard: false,
		show: false
	}).css({
		width: '900px',

		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});

	$('#isAuthorEditor').modal({
		backdrop: 'static',
		keyboard: false,
		show: false
	}).css({
		width: '900px',

		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});


});


function Showname(){
