function addBookMonographResponseAfterSubmit(response,postData) {
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#bookstable").getGridParam("selrow");
	if(gr != null){
		$("#bookstable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#bookstable").jqGrid('setCell',gr,'employeePublicationId',jObject["employeePublicationId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}



//******************************************************************************************Research Publication JS


function getResearchPublication(){

	var empId =0;
	var subcnt=1;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();


	$.ajax({
		url:'getBooksResearchPublicationDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var j=jArray.length;
			var button='';
			jQuery("#researchtable").jqGrid('clearGridData');
			for(var i=0;i<j;i++)
			{
				button = "<input type='button' value='Add Impact' class='btn btn-inverse btn-mini  ' onclick='showImpactFactor("+jArray[i]["employeeReasearchId"]+")'/>";

				var tempBoiler = jArray[i]["Attended_With"].split(",") ;
				console.log("tempBoiler..."+tempBoiler);
				 
				var datarow = {
						id:subcnt,
						issnNo:jArray[i]["issnNo"],
						paperName:jArray[i]["paperName"],
						publishDate:jArray[i]["publishDate"],
						journalName:jArray[i]["journalName"],
						typeName:jArray[i]["typeName"],
						statusOfPaper:jArray[i]["statusOfPaper"],
						remark:jArray[i]["remark"],
						employeeReasearchId:jArray[i]["employeeReasearchId"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						Addedby:jArray[i]["Addedby"],
						employee_id:jArray[i]["employee_id"],
						employeeName:jArray[i]["employeeName"],
						employeeProfileId:jArray[i]["employeeProfileDetailId"],
						addImpact:button,
						status:jArray[i]["status"],
				}; 
				jQuery("#researchtable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#researchtable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}

function ResearchPublicationMemberSave(typeOfAuthor,empReasearchId){
	var employee = new Array();

	var addOther='';
	var outsideInstiMember='';
	var id = $('#researchtable').jqGrid('getGridParam','selrow');
	//var empReasearchId = $('#researchtable').jqGrid('getRowData',id).employeeReasearchId;
	for(var j=0;j<$('#selectedEmployeeName option').length;j++)
	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());

	}
	/*alert("$('#outsideInsti').is(':checked')==="+$('#outsideInsti').is(':checked'));
	return;*/
	if ($('#outsideInsti').is(':checked')){
		outsideInstiMember= $('#txtOutsideInstiMember').val();
	
	}
	else if($('#txtAddOther').val()!=undefined){

		addOther=$('#txtAddOther').val();
	}
	if(employee.length == 0){
		employee='';
	}	

	$.ajax({
		url:'addMemberToResearchPublication.json?employee='+employee+'&employeeReasearchId='+empReasearchId+'&addOther='+addOther+'&typeOfAuthor='+typeOfAuthor+'&outsideInstiMember='+outsideInstiMember,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$('#MemberListDialog').modal('hide');
			$('#addOther').modal('hide');
			$('#divModalAddingNewPaperPublication').modal('show');
			//getResearchPublication();
			viewResearchPublicationNew();
		},
		error:function(){
			////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});		

}



function deAllocateMemberForResearchPublication(ReasearchId){
	var employee = new Array();
	var i=0;
	$(":checkbox:checked").each(function() {
		employee.push($(this).val());
		i++;
	}); 
	if(employee.length > 0){
		if(ReasearchId != undefined && ReasearchId != ''){
			$.ajax({
				url:'deAllocateMemberOfResearchPublication.json?employeeReasearchId='+ReasearchId+'&employee='+employee,	
				dataType:'json',
				success:function(jArray){
					if(jArray[0].responseFlag == '1'){
						showSuccessMessage(jArray[0].responseText,10000);
						getResearchPublication();
						$('#employeeList').modal('hide');
					} else {
						showErrorMessage(jArray[0].responseText,5000);
					}
				},
				error:function(){
				}
			});
		} else {
			showErrorMessage('Research Paper has not been selected. Try again!', 5000);
		}
	} else {
		$('#employeeList').modal('hide');
	}

}



