function populateInstiCourseList(cmbId) {

	//alert("in populateInstiCourseList"+cmbId);
	getPostType('cbSubjectAllocationPostType');
	$.ajax({
		url:'courseListByRoaster.json',
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jsonInstiCourseArray){
			var len = jsonInstiCourseArray.length;
			//alert("Len: "+len);
			var courseComboBoiler='<option value = "0">Select</option>';
			for(var i=0 ; i<len;i++){
				if(jsonInstiCourseArray[i]["instituteCourseObj"]!=undefined)
					courseComboBoiler +='<option value="'+jsonInstiCourseArray[i]["instituteCourseObj"]["instituteCourseId"]+'-'+jsonInstiCourseArray[i]["instituteCourseObj"]["courseObj"]["courseId"]+'-'+jsonInstiCourseArray[i]["instituteCourseObj"]["courseObj"]["duration"]+'">'+jsonInstiCourseArray[i]["instituteCourseObj"]['instituteObj']['instituteName']+'-'+jsonInstiCourseArray[i]["instituteCourseObj"]["courseObj"]["courseName"]+'</option>';
			}
			$('#'+cmbId).html(courseComboBoiler+'<option value = "Other">Other</option>');
		},
		error:function(){
			//////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});																	

}

//Nilam
function fetchInstiCourseList(cmbId) {
	
	getPostType('cbSubjectAllocationPostType');
	
	$.ajax({
		url:'courseListByCustomer.json',
		dataType:'json',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var courseComboBoiler='<option value = "0">Select</option>';
			for(var i=0 ; i<jArray.length;i++){
			//	if(jsonInstiCourseArray[i]["instituteCourseObj"]!=undefined)
					courseComboBoiler +='<option value="'+jArray[i]["instiCourseId"]+'-'+jArray[i]["courseId"]+'">'+jArray[i]['instituteName']+'-'+jArray[i]["courseName"]+'</option>';
			}
			$('#'+cmbId).html(courseComboBoiler+'<option value = "Other">Other</option>');
		},
		error:function(){
			//////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});																	

}

function getPostType(elementId){
	try{
		$.ajax({
			url:'getAllPostType.json', 
			dataType:'json',
			success:function(jArray){
				populatePostType(jArray,elementId);
			},
		});
	} catch(e){

	}
}

function getAllPost(chkId){
	var postArray = new Array();
	////alert(chkId);
	if($('#'+chkId).is(':checked') == true){
		$('#cbSubjectAllocationPostType option').each(function(){
			if($(this).html().toLowerCase() != 'select')
				postArray.push($(this).html());
		});
	}
	////alert(postArray);
	return postArray;
}

function populatePostType(jArray,elementId){

	//instituteEmployeeSubjectAllocationGrid();

	var boiler = '<option value = "0">Select</option>'
		;
	for ( var int = 0; int < jArray.length; int++) {
		boiler += '<option value = "'+jArray[int]["postTypeId"]+'">'+jArray[int]["postTypeName"]+'</option>';
	}
	$('#'+elementId).html(boiler);
}


function instituteEmployeeSubjectAllocationGrid(){
	if($('#cbInstiCourseId').val()=='Other'){
		$('#txtAddOther').val('');
		$("#addOther").modal('show'); 

		$("#MemberListDialog").modal('hide'); 

	}
	else{
		jQuery("#instituteEmployeeSubjectAllocationGrid").jqGrid({ 
			datatype: "local", 
			height: 280,
			width:600,
			colNames:['Employee ID','Name'],
			colModel:[ {name:'employeeId',index:'employeeId',width:90, editable:false, hidden:true},
			           {name:'name',index:'name',width:100,editable: false},
			           ], 
			           loadonce:false,
			           mtype: "GET",
			           rownumbers: true,
			           rownumWidth: 40,
			           gridview: true,
			           pager: '#instituteptoolbar',
			           sortname: 'name',
			           viewrecords: true,
			           sortorder: "asc",
			           multiselect: true,
			           caption: "Member List",
			           gridComplete: function() {
			        	   $(".cbox").shiftSelect();
			           },
		});
		jQuery("#instituteEmployeeSubjectAllocationGrid").jqGrid('navGrid','#instituteptoolbar',{edit:false,add:false,del:false,search:false,refresh:false});
		jQuery("#instituteEmployeeSubjectAllocationGrid").jqGrid('filterToolbar',{stringResult: true,searchOnEnter : false});
		var id = $('#cbInstiCourseId').val();
		instiCourseId = id.split('-')[0]; 			
		if(instiCourseId != undefined){
			showEmployeeList('institute-'+instiCourseId,'teaching');
			var gridId = $('#sourceGridId').val();
			allocatedMemberIds = getAllocatedEmployeeIds(gridId);
			allocatedMemberNames = getAllocatedEmployeeNames(gridId);
			setEmployeeListGrid();
			jQuery("#instituteEmployeeSubjectAllocationGrid").click( function(){ 
				var employeeArr = new Array();
				$('input:checked').each(function(){
					employeeArr.push($(this).closest('tr').attr('id'));
				});
				selectedEmployeeList2(employeeArr,'#instituteEmployeeSubjectAllocationGrid');
			});

			jQuery("#cb_instituteEmployeeSubjectAllocationGrid").click( function(){ 

				var employeeArr = new Array();
				$('input:checked').each(function(){
					employeeArr.push($(this).closest('tr').attr('id'));

				});
				selectedEmployeeList2(employeeArr,'#instituteEmployeeSubjectAllocationGrid');

			});

		} else {
			showErrorMessage('Institute Id not found!',5000);
		}

	}

	//alert(instiCourseId);
}	 



function removeSelectedEmployee(){
	$('#selectedEmployeeName option:selected').remove(); 
}



function showEmployeeList(domain,postTypes){

	if(postTypes == undefined || postTypes == '')
		postTypes = 'teaching';
	//alert(postTypes);
	$.ajax({
		url:'populateEmployeeListForSubjectAllocation.json?domain='+domain+'&postTypes='+postTypes,
		dataType:'json',
		tyrp:'GET',
		contentType:'application/json; charset=utf-8',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jsonTeachingEmployeeList){

			showEmployeeListInGrid(jsonTeachingEmployeeList);
		},
		error:function(){
			////////////alert("In showEmployeeList errorr");
		}
	});
}

function showEmployeeListInGrid(jsonTeachingEmployeeList){
	jQuery("#instituteEmployeeSubjectAllocationGrid").jqGrid('clearGridData');
	//alert(memberEmployees);
	for(var i=0;i<jsonTeachingEmployeeList.length;i++){
		if($.inArray(jsonTeachingEmployeeList[i]["Id"].toString(),memberEmployees) ==  -1){
			var mydata = {
					employeeId:jsonTeachingEmployeeList[i]["Id"],
					name:jsonTeachingEmployeeList[i]["Name"]
			};
			jQuery("#instituteEmployeeSubjectAllocationGrid").jqGrid('addRowData',i+1,mydata);
		}
	}

}



function getSelectedInstituteCourseId(contextSrc){
	var courseId ='0';
	if(contextSrc == 'student'){
		courseId = $('#cbInstiCourseId-tab-2').val();
	} else {
		courseId = $('#cbInstiCourseId').val();
	}
	if(courseId != '0'){
		courseId = courseId.split('-')[0];
	}
	return parseInt(courseId);
}


function selectedEmployeeList2(employeeArr,gridId){

	var blEmployee = '';
	for(var i=0;i<employeeArr.length;i++){
		employeeName = jQuery(gridId).jqGrid('getRowData',employeeArr[i]);
		if(employeeName.employeeId != undefined && employeeName.name != undefined){
			blEmployee += '<option value="'+employeeName.employeeId+'">'+employeeName.name+'</option>';
		}
	}
	$('select#selectedEmployeeName').html(blEmployee);

}






