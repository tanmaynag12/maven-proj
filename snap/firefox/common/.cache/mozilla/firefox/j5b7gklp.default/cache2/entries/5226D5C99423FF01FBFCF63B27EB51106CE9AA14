function addTrainingResponseAfterSubmit(response,postData) {
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#indTrainingtable").getGridParam("selrow");
	if(gr != null){
		$("#indTrainingtable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#indTrainingtable").jqGrid('setCell',gr,'employeeTrainingId',jObject["employeeTrainingId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}




//*********************************************************************************************Conference JS

function getConferenceDetails(){

	var empId =0;
	var subcnt=0;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	var excelButtonblr='<input class="btn btn-inverse btn-mini  " type="button" value="Export to Excel" onclick="employeeConferenceExcelSheet()">';
	$('#conferenceExcelButton').html(excelButtonblr);


	$.ajax({
		url:'getConferenceDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var j=jArray.length;
			jQuery("#conferencetable").jqGrid('clearGridData');
			for(var i=0;i<j;i++)
			{
				var datarow = {
						id:subcnt,
						name:jArray[i]["name"],
						venue:jArray[i]["venue"],
						fromDate:jArray[i]["fromDate"],
						toDate:jArray[i]["toDate"],
						attendingPurpose:jArray[i]["attendingPurpose"],
						attendingDetails:jArray[i]["attendingDetails"],
						accountingDetails:jArray[i]["accountingDetails"],
						sponsoredBy:jArray[i]["sponsoredBy"],
						conferenceId:jArray[i]["conferenceId"],
						typeName:jArray[i]["typeName"],
						categoryTypeName:jArray[i]["categoryTypeName"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						Addedby:jArray[i]["Addedby"],
						employee_id:jArray[i]["employee_id"],
						employeeName:jArray[i]["employeeName"],
						employeeProfileId:jArray[i]["employeeProfileDetailId"],
						eventType:jArray[i]["eventType"],
						status:jArray[i]["status"],
				}; 
				jQuery("#conferencetable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#conferencetable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}

function employeeConferenceExcelSheet()
{
	var empId =0;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	window.location.replace("getEmpConferenceExcelSheet.json?employeeId="+empId);
}

function getSelectedConferenceName(){

	var workshopName = '';

	var rowId = jQuery("#conferencetable").getGridParam("selrow");
	if(rowId != undefined && rowId != null){
		var tempObj = jQuery("#conferencetable").jqGrid('getRowData',rowId);
		workshopName = tempObj.Name;

	}


	return workshopName;
}

function getSelectedConferenceId(){

	var workshopId = '';

	var sr = jQuery("#conferencetable").getGridParam('selrow');
	if(sr != null){
		rowdata = jQuery("#conferencetable").getRowData(sr);
		workshopId =  rowdata.employeeWorkshopId; 

	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return workshopId;
}




function conferenceMemberSave(conferenceId){

	var employee = new Array();
	var addOther='';
	//var id = $('#conferencetable').jqGrid('getGridParam','selrow');
	//var conferenceId = $('#conferencetable').jqGrid('getRowData',id).conferenceId;
	for(var j=0;j<$('#selectedEmployeeName option').length;j++)
	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());

	}

	if($('#txtOutsideInstiMember').val()!=undefined){

		addOther=$('#txtOutsideInstiMember').val();
	}
	if(employee.length == 0){
		employee='';
	}	
	$.ajax({
		url:'addMemberToConference.json?employee='+employee+'&conferenceId='+conferenceId+'&addOther='+addOther,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$('#MemberListDialog').modal('hide');
			$('#addOther').modal('hide');
			$('#divModalAddingNewEventConferences').modal('show');
			//viewConference();
			viewNewEmpConference();

		},
		error:function(){
			////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});		

}




function deAllocateMemberForConference(conferenceId){

	var employee = new Array();
	var i=0;
	$(":checkbox:checked").each(function() {

		employee.push($(this).val());
		i++;
	}); 
	if(employee.length > 0){
		if(conferenceId != undefined && conferenceId != ''){
			$.ajax({
				url:'deAllocateMemberOfConference.json?conferenceId='+conferenceId+'&employee='+employee,	
				dataType:'json',
				success:function(jArray){
					if(jArray[0].responseFlag == '1'){
						showSuccessMessage(jArray[0].responseText,10000);
						getConferenceDetails();
						$('#employeeList').modal('hide');
					} else {
						showErrorMessage(jArray[0].responseText,5000);
					}
				},
				error:function(){
				}
			});
		} else {
			showErrorMessage('Conference Name has not been selected. Try again!', 5000);
		}
	} else {
		$('#employeeList').modal('hide');
	}

}


function getDocumentCategoryByConferenceId(employeeProfileDetailId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryByConferenceId.json',
			data:'employeeProfileDetailId='+employeeProfileDetailId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}






function addConferenceResponseAfterSubmit(response,postData){

	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#conferencetable").getGridParam("selrow");
	if(gr != null){
		$("#conferencetable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#conferencetable").jqGrid('setCell',gr,'conferenceId',jObject["conferenceId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}

}

//***********************************************************************************************Patent Js

function getPatentDetails(){

	var empId =0;
	var subcnt=1;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();

	$.ajax({
		url:'getPatentDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var j=jArray.length;
			var typeName="";
			jQuery("#patentstable").jqGrid('clearGridData');
			for(var i=0;i<j;i++)
			{
				if(jArray[i]["typeName"]=="select"){
					typeName="";
				}
				else{
					typeName=jArray[i]["typeName"];
				}

				var datarow = {
						id:subcnt,
						patentBody:jArray[i]["patentBody"],
						patentDetail:jArray[i]["patentDetail"],
						patentFees:jArray[i]["patentFees"],
						patentStartDate:jArray[i]["patentStartDate"],
						patentEndDate:jArray[i]["patentEndDate"],
						patentLevel:jArray[i]["patentLevel"],
						patentRemark:jArray[i]["patentRemark"],
						employeePatentId:jArray[i]["employeePatentId"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						categoryTypeName:jArray[i]["categoryTypeName"],
						typeName:typeName,
						Addedby:jArray[i]["Addedby"],
						employee_id:jArray[i]["employee_id"],
						employeeName:jArray[i]["employeeName"],
						employeeProfileId:jArray[i]["employeeProfileDetailId"],
						status:jArray[i]["status"]

				}; 
				jQuery("#patentstable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#patentstable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}

function getSelectedPatentName(){

	var patentName = '';

	var rowId = jQuery("#patentstable").getGridParam("selrow");
	if(rowId != undefined && rowId != null){
		var tempObj = jQuery("#patentstable").jqGrid('getRowData',rowId);
		patentName = tempObj.patentBody;

	}


	return patentName;
}

function getSelectedPatentId(){

	var patentId = '';

	var sr = jQuery("#patentstable").getGridParam('selrow');
	if(sr != null){
		rowdata = jQuery("#patentstable").getRowData(sr);
		patentId =  rowdata.employeePatentId; 

	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return patentId;
}

function patentMemberSave(empPatentId){

	var employee = new Array();

	var addOther='';
	//var id = $('#patentstable').jqGrid('getGridParam','selrow');
	//var empPatentId = $('#patentstable').jqGrid('getRowData',id).employeePatentId;
	for(var j=0;j<$('#selectedEmployeeName option').length;j++)
	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());

	}

	if($('#txtOutsideInstiMember').val()!=undefined){

		addOther=$('#txtOutsideInstiMember').val();
	}
	if(employee.length == 0){
		employee='';
	}	

	$.ajax({
		url:'addMemberToPatent.json?employee='+employee+'&employeePatentId='+empPatentId+'&addOther='+addOther,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$('#MemberListDialog').modal('hide');
			$('#addOther').modal('hide');
			$('#divModalAddingNewPatentsAndCopyRigts').modal('show');
			//getPatentDetails();
			viewNewEmpPatents();

		},
		error:function(){
			////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});		

}






function deAllocateMemberForPatent(patentId){
	var employee = new Array();
	var i=0;
	$(":checkbox:checked").each(function() {
		employee.push($(this).val());
		i++;
	}); 
	if(employee.length > 0){
		if(patentId != undefined && patentId != ''){
			$.ajax({
				url:'deAllocateMemberOfPatent.json?employeePatentId='+patentId+'&employee='+employee,	
				dataType:'json',
				success:function(jArray){
					if(jArray[0].responseFlag == '1'){
						showSuccessMessage(jArray[0].responseText,10000);
						getPatentDetails();
						$('#employeeList').modal('hide');
					} else {
						showErrorMessage(jArray[0].responseText,5000);
					}
				},
				error:function(){
				}
			});
		} else {
			showErrorMessage('Patent has not been selected. Try again!', 5000);
		}
	} else {
		$('#employeeList').modal('hide');
	}

}

function getDocumentCategoryByPatentId(employeeProfileDetailId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryByPatentId.json',
			data:'employeeProfileDetailId='+employeeProfileDetailId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}


function addPatentResponseAfterSubmit(response,postData){

	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#patentstable").getGridParam("selrow");
	if(gr != null){
		$("#patentstable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#patentstable").jqGrid('setCell',gr,'employeePatentId',jObject["employeePatentId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}

}


//*******************************************************************************Books And Monograph


function getBooks_Monograph(){

	var empId =0;
	var subcnt=1;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();


	$.ajax({
		url:'getBooksMonographDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			//startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			////alert("in  success");
			jQuery("#bookstable").jqGrid('clearGridData');
			var j=jArray.length;
			for(var i=0;i<j;i++)
			{
				var datarow = {
						id:subcnt,
						bookISBN:jArray[i]["bookISBN"],
						bookSubject:jArray[i]["bookSubject"],
						bookPublisher:jArray[i]["bookPublisher"],
						bookType:jArray[i]['bookType'],
						endDate:jArray[i]['seminarEndDate'],
						bookPublishYear:jArray[i]["bookPublishYear"],
						publicationType:jArray[i]["publicationType"],
						bookDocPath:jArray[i]["bookDocPath"],
						employeePublicationId:jArray[i]["employeePublicationId"],
						typeName:jArray[i]["typeName"],
						categoryTypeName:jArray[i]["categoryTypeName"],
						Attended_With:jArray[i]["Attended_With"],
						Edited_With:jArray[i]["Edited_With"],
						attendedId:jArray[i]["attendedId"],
						Addedby:jArray[i]["Addedby"],
						employee_id:jArray[i]["employee_id"],
						employeeName:jArray[i]["employeeName"],
						employeeProfileId:jArray[i]["employeeProfileDetailId"],
						status:jArray[i]["status"],

				}; 
				jQuery("#bookstable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#bookstable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}


