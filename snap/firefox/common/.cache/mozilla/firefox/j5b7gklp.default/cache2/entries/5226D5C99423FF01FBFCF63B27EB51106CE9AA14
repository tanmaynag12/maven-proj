//********************************************************************************************************


//********************************************************************************************************Workshop tab js

function getSelectedWorkshopName(){

	var workshopName = '';

	var rowId = jQuery("#workshoptable").getGridParam("selrow");
	if(rowId != undefined && rowId != null){
		var tempObj = jQuery("#workshoptable").jqGrid('getRowData',rowId);
		workshopName = tempObj.workshopTopic;
		alert(" WorkshopName is:--"+ workshopName);
	}


	return workshopName;
}

function getSelectedWorkshopId(){

	var workshopId = '';

	var sr = jQuery("#workshoptable").getGridParam('selrow');
	if(sr != null){
		rowdata = jQuery("#workshoptable").getRowData(sr);
		workshopId =  rowdata.employeeWorkshopId; 
		alert(" WorkshopId is:--"+ workshopId);
	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return workshopId;
}



function getWorkshopDetail(){

	var empId =0;
	var subcnt=1;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	var excelButtonblr='<input class="btn btn-inverse btn-mini  " type="button" value="Export to Excel" onclick="employeeWorkshopExcelSheet()">';
	$('#workshopsExcelButton').html(excelButtonblr);

	$.ajax({
		url:'getEmpWorkshopDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var j=jArray.length;
			jQuery("#workshoptable").jqGrid('clearGridData');
			for(var i=0;i<j;i++)
			{
				var datarow = {
						id:subcnt,
						employeeWorkshopId:jArray[i]["employeeWorkshopId"],
						workshopTopic:jArray[i]["workshopTopic"],
						workshopConductedBy:jArray[i]["workshopConductedBy"],
						workshopVenue:jArray[i]["workshopVenue"],
						workshopStartDate:jArray[i]["workshopStartDate"],
						workshopEndDate:jArray[i]["workshopEndDate"],
						typeName:jArray[i]["typeName"],
						workshopRemark:jArray[i]["workshopRemark"],
						employeeName:jArray[i]["employeeName"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						Addedby:jArray[i]["AddedBy"],
						categoryTypeName:jArray[i]["categoryTypeName"],
						employee_id:jArray[i]["employee_id"],
						employeeProfileId:jArray[i]["employeeProfileDetailId"],
						eventType:jArray[i]["eventType"],
						status:jArray[i]["status"],
				}; 
				jQuery("#workshoptable").jqGrid('addRowData',i+1,datarow); 

			}
			jQuery("#workshoptable").trigger('reloadGrid');

		},
		error:function(){
			////alert("In Training Error");
		}
	});

}

function employeeWorkshopExcelSheet()
{
	var empId =0;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	window.location.replace("getEmpWorkshopDetailsExcelSheet.json?employeeId="+empId);

}

function workshopMemberSave(employeeWorkshopId){

	var addOther='';
	var employee = new Array();
	//var id = $('#workshoptable').jqGrid('getGridParam','selrow');
	//var employeeWorkshopId = $('#workshoptable').jqGrid('getRowData',id).employeeWorkshopId;
	for(var j=0;j<$('#selectedEmployeeName option').length;j++)
	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());
	}
	if($('#txtOutsideInstiMember').val()!=undefined){

		addOther=$('#txtOutsideInstiMember').val();
	}
	if(employee.length == 0){
		employee='';
	}	

	$.ajax({
		url:'addmembertoworkshop.json?employee='+employee+'&employeeWorkshopId='+employeeWorkshopId+'&addOther='+addOther,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$('#MemberListDialog').modal('hide');
			$('#addOther').modal('hide');
			$('#divModalAddingNewWorkshop').modal('show');
			//getWorkshopDetail();
			viewEmpWorkShopNew();

		},
		error:function(){
			////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});		

}



function deAllocateMemberForWorkshop(WorkshopId){
	var employee = new Array();
	var i=0;
	$(":checkbox:checked").each(function() {
		employee.push($(this).val());
		i++;
	}); 
	if(employee.length > 0){
		if(WorkshopId != undefined && WorkshopId != ''){
			$.ajax({
				url:'deAllocateMemberOfWorkshop.json?WorkshopId='+WorkshopId+'&employee='+employee,	
				dataType:'json',
				success:function(jArray){
					if(jArray[0].responseFlag == '1'){
						showSuccessMessage(jArray[0].responseText,10000);
						getWorkshopDetail();
						$('#employeeList').modal('hide');
					} else {
						showErrorMessage(jArray[0].responseText,5000);
					}
				},
				error:function(){
					////////////alert("In Deallocate Error");
				}
			});
		} else {
			showErrorMessage('Workshop Topic has not been selected. Try again!', 5000);
		}
	} else {
		$('#employeeList').modal('hide');
	}

}


function openWorkshopDocumentUploadDialog(categoryId){
	//alert("categoryId: "+categoryId);
	$('#uploadWorkshopDocumentDialog').modal('show');
	var uploadDocument = new UploadDocument();
	try{
		uploadDocument.init({
			elementId:'speakerUploadedFile',
			categoryId:categoryId,
		},
		function(docId){
			//alert("docId: "+docId);
			uploadDocument.getDocument(categoryId, function(jArray){
				showUploadedWorkshopDocument(jArray);
			});
		});
	} catch(e){
		jAlert(e);
	}
	uploadDocument.getDocument(categoryId, function(jArray){
		showUploadedWorkshopDocument(jArray);
	});
}

function addWorkshopResponseAfterSubmit(response,postData){
	var jObject = eval('(' + response.responseText + ')');
	var gr = jQuery("#workshoptable").getGridParam("selrow");
	if(gr != null){
		$("#workshoptable").jqGrid('setCell',gr,'employeeProfileDetailId',jObject["employeeProfileDetailId"]);
		$("#workshoptable").jqGrid('setCell',gr,'employeeWorkshopId',jObject["employeeWorkshopId"]);
		return [true,"",gr];
	} else {
		return [false,"",gr];
	}
}




function getDocumentCategoryByWorkshop(employeeProfileId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryByWorkshopId.json',
			data:'employeeProfileDetailId='+employeeProfileId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}






//**********************************************************************************************************Training Tab	

function getTrainingDetails(){
	var empId =0;
	var subcnt=1;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	var excelButtonblr='<input class="btn btn-inverse btn-mini  " type="button" value="Export to Excel" onclick="employeeIndTrainningExcelSheet()">';
	$('#indTrainingExcelButton').html(excelButtonblr);

	$.ajax({
		url:'getEmpTraningDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var j=jArray.length;
			jQuery("#indTrainingtable").jqGrid('clearGridData');
			for(var i=0;i<j;i++)
			{
				var datarow = {
						id:subcnt,
						trainingTopic:jArray[i]["trainingTopic"],
						trainingConductedBy:jArray[i]["trainingConductedBy"],
						trainingVenue:jArray[i]["trainingVenue"],
						trainingStartDate:jArray[i]["trainingStartDate"],
						trainingEndDate:jArray[i]["trainingEndDate"],
						typeName:jArray[i]["typeName"],
						categoryTypeName:jArray[i]["categoryTypeName"],
						trainingRemark:jArray[i]["trainingRemark"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						employeeTrainingId:jArray[i]["employeeTrainingId"],
						employee_id:jArray[i]["employee_id"],
						Addedby:jArray[i]["Addedby"],
						employeeName:jArray[i]["employeeName"],
						employeeProfileId:jArray[i]["employeeProfileDetailId"],
						eventType:jArray[i]["eventType"],
						status:jArray[i]["status"]
				}; 
				jQuery("#indTrainingtable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#indTrainingtable").trigger('reloadGrid');
		},
		error:function(){
			////alert("In Training Error");
		}
	});

}

function employeeIndTrainningExcelSheet()
{
	var empId =0;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	window.location.replace("getEmpTrainingDetailsExcelSheet.json?employeeId="+empId);
}

function trainingMemberSave(employeeTrainingId){

	var employee = new Array();
	var addOther='';
	//var id = $('#indTrainingtable').jqGrid('getGridParam','selrow');
	//var employeeTrainingId = $('#indTrainingtable').jqGrid('getRowData',id).employeeTrainingId;
	for(var j=0;j<$('#selectedEmployeeName option').length;j++)
	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());

	}

	if($('#txtOutsideInstiMember').val()!=undefined){
		//addOther=$('#txtAddOther').val();
		addOther=$('#txtOutsideInstiMember').val();
	}
	if(employee.length == 0){
		employee='';
	}	


	//alert("In ajax save"+employeeTrainingId);
	$.ajax({
		url:'addMemberToTraining.json?employee='+employee+'&employeeTrainingId='+employeeTrainingId+'&addOther='+addOther,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$('#MemberListDialog').modal('hide');
			$('#addOther').modal('hide');
			$('#divModalAddingNewTraining').modal('show');
			//getTrainingDetails();
			viewEmpTrainingNew();
		},
		error:function(){
			////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});		

}



function deAllocateMemberForTraining(TraningId){
	var employee = new Array();
	var i=0;
	$(":checkbox:checked").each(function() {
		employee.push($(this).val());
		i++;
	}); 
	if(employee.length > 0){
		if(TraningId != undefined && TraningId != ''){
			$.ajax({
				url:'deAllocateMemberOfTraning.json?TraningId='+TraningId+'&employee='+employee,	
				dataType:'json',
				success:function(jArray){
					if(jArray[0].responseFlag == '1'){
						showSuccessMessage(jArray[0].responseText,10000);
						getTrainingDetails();
						$('#employeeList').modal('hide');
					} else {
						showErrorMessage(jArray[0].responseText,5000);
					}
				},
				error:function(){
				}
			});
		} else {
			showErrorMessage(' Traning Topic has not been selected. Try again!', 5000);
		}
	} else {
		$('#employeeList').modal('hide');
	}

}

