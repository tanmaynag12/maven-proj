function checkInsti(){
	
	var radioValue = $("input[name='withinOutsideRadio']:checked"). val();
	if(radioValue == "Within" ){
		$('#cbInstiCourseId').val(0);
		$('#trShowWithinInsti').show();
		$('#trShowOutsideInsti').hide();
		$('#gview_instituteEmployeeSubjectAllocationGrid').show();
		$('#instituteptoolbar').show();
	}else{
		
		$('#txtOutsideInstiMember').val('');
		$('#trShowWithinInsti').hide();
		$('#gview_instituteEmployeeSubjectAllocationGrid').hide();
		$('#instituteptoolbar').hide();
		$('#trShowOutsideInsti').show();
	}
	
}

function getDocumentCategoryByInsuranceDetail(type,employeeInsuranceDetailId,callback,empId){
  if(empId==undefined)
       empId="";
	try{
		$.ajax({
			url:'getDocumentCategoryByEmployeeInsuranceDetailId.json',
			data:{employeeInsuranceDetailId:employeeInsuranceDetailId,type:type,employeeId:empId},
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}

