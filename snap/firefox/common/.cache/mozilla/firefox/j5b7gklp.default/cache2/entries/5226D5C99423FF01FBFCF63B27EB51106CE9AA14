//***********************************************************************************Seminar JS



function getSelectedSeminarName(){

	var seminarName = '';

	var rowId = jQuery("#seminartable").getGridParam("selrow");
	if(rowId != undefined && rowId != null){
		var tempObj = jQuery("#seminartable").jqGrid('getRowData',rowId);
		seminarName = tempObj.seminarTopic;
		//alert(" seminarName is:--"+ seminarName);
	}


	return seminarName;
}


function getSelectedSeminarId(){

	var seminarId = '';

	var sr = jQuery("#seminartable").getGridParam('selrow');
	if(sr != null){
		rowdata = jQuery("#seminartable").getRowData(sr);
		seminarId =  rowdata.employeeSeminarId; 
		alert(" seminarId is:--"+ seminarId);
	} else {
		//showErrorMessage('Kindly select subject.', 5000);
	}

	return seminarId;
}



function getSeminarDetail(){

	var empId =0;
	var subcnt=1;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	var excelButtonblr='<input class="btn btn-inverse btn-mini  " type="button" value="Export to Excel" onclick="employeeSeminarExcelSheet()">';
	$('#seminarExcelButton').html(excelButtonblr);


	$.ajax({
		url:'getSeminarDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			//startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			jQuery("#seminartable").jqGrid('clearGridData');
			var j=jArray.length;
			for(var i=0;i<j;i++)
			{

				var datarow = {
						id:subcnt,
						seminarTopic:jArray[i]["seminarTopic"],
						conductedBy:jArray[i]["conductedBy"],
						venue:jArray[i]["venue"],
						startDate:jArray[i]['startDate'],
						endDate:jArray[i]['endDate'],
						typeName:jArray[i]["typeName"],
						employee_name:jArray[i]["employee_name"],
						employee_id:jArray[i]["employee_id"],
						categoryTypeName:jArray[i]["categoryTypeName"],
						employeeSeminarId:jArray[i]["employeeSeminarId"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],		
						AddedBy:jArray[i]["AddedBy"],
						employeeProfileDetailId:jArray[i]["employeeProfileDetailId"],
						employee_id:jArray[i]["employee_id"],
						eventType:jArray[i]["eventType"],
						status:jArray[i]["status"],
				}; 
				jQuery("#seminartable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#seminartable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}


function employeeSeminarExcelSheet()
{
	var empId =0;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();
	window.location.replace("getEmpSeminarDetailsExcelSheet.json?employeeId="+empId);

}


function getConsultancyDetail(){

	var empId =0;
	if($('#empName').val()!= undefined)
		empId =$('#empName').val();


	//	alert('in');
	$.ajax({
		url:'getConsultancyDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			jQuery("#consultancytable").jqGrid('clearGridData');
			var j=jArray.length;
			//	alert(j);
			for(var i=0;i<j;i++)
			{

				//////alert("empid==  "+jArray[i]["consultancyId"]);
				var datarow = {
						title:jArray[i]["title"],
						description:jArray[i]["description"],
						startDate:jArray[i]["startDate"],
						endDate:jArray[i]["endDate"],
						fundedAgency:jArray[i]["fundedAgency"],
						amountSanctioned:jArray[i]["amountSanctioned"],
						amountReceived:jArray[i]["amountReceived"],
						percentageCompletion:jArray[i]["percentageCompletion"],
						consultancyType:jArray[i]["consultancyType"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						consultancyId:jArray[i]["consultancyId"],
						consultancyAs:jArray[i]["consultancyAs"],
						employeeProfileDetailId:jArray[i]["employeeProfileDetailId"],
						status:jArray[i]["status"],
				}; 
				jQuery("#consultancytable").jqGrid('addRowData',i+1,datarow); 
			}
			jQuery("#consultancytable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}

