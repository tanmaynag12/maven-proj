	if($('#empName').val()!= undefined)
		empId =$('#empName').val();

	$.ajax({
		url:'getMembershipDetails.json?employeeId='+empId, 
		dataType:'json',
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			jQuery("#membershiptable").jqGrid('clearGridData');
			var j=jArray.length;
			for(var i=0;i<j;i++)
			{

				var datarow = {
						membershipBody:jArray[i]["membershipBody"],
						membershipDetail:jArray[i]["membershipDetail"],
						membershipFees:jArray[i]["membershipFees"],
						membershipStartDate:jArray[i]['membershipStartDate'],
						membershipEndDate:jArray[i]['membershipEndDate'],
						membershipLevel:jArray[i]["membershipLevel"],
						membershipRemark:jArray[i]["membershipRemark"],
						membershipDocPath:jArray[i]["membershipDocPath"],
						Attended_With:jArray[i]["Attended_With"],
						attendedId:jArray[i]["attendedId"],
						employeeMembershipId:jArray[i]["employeeMembershipId"],
						employeeProfileDetailId:jArray[i]["employeeProfileDetailId"],
						status:jArray[i]["status"],
				}; 
				jQuery("#membershiptable").jqGrid('addRowData',i+1,datarow); 
				subcnt++;
			}
			jQuery("#membershiptable").trigger('reloadGrid');

		},
		error:function(){
			////jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}



function seminarMemberSave(employeeSeminarId){

	var addOther='';
	var employee = new Array();
	//var id = $('#seminartable').jqGrid('getGridParam','selrow');
	//var employeeSeminarId = $('#seminartable').jqGrid('getRowData',id).employeeSeminarId;	
	for(var j=0;j<$('#selectedEmployeeName option').length;j++)
	{
		employee.push($('#selectedEmployeeName option:eq('+j+')').val());

	}

	if($('#txtOutsideInstiMember').val()!=undefined){

		addOther=$('#txtOutsideInstiMember').val();
	}
	if(employee.length == 0){
		employee='';
	}	


	$.ajax({
		url:'addmembertoseminar.json?employee='+employee+'&employeeSeminarId='+employeeSeminarId+'&addOther='+addOther,
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		async:false,
		success:function(jArray){
			$('#MemberListDialog').modal('hide');
			$('#addOther').modal('hide');
			$('#divModalAddingNewSeminar').modal('show');
			//getSeminarDetail();
			viewEmpSeminarNew();
		},
		error:function(){
			////////////jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}

