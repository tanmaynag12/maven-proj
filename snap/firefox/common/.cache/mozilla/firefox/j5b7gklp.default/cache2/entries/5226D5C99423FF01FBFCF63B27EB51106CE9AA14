function showEmployeeForSeminar(rmempSeminarId,callback){
	$('#divModalAddingNewSeminar').modal('hide');

	$.ajax({
		url:'getEmployeeProfileDetailForSeminar.json?seminarId='+rmempSeminarId,	
		dataType:'json',
		success:function(jArray){
			var htm= '<form name="frmTeachingEmployee" id="frmTeachingEmployee">'+
			'<table class = "smalltable">';	 

			for(var i = 0 ; i < jArray.length; i++){

				if(jArray[i]["employeeObj"]!=undefined){

					htm += '<tr><td>'+
					'<input type="checkbox" class = "chkEmployeeName" name = "chkEmployeeName" id="'+jArray[i]["employeeProfileDetailId"]+'" value="'+jArray[i]["employeeProfileDetailId"]+'" /></td><td><label>'+jArray[i]["employeeObj"]["firstName"]+' '+jArray[i]["employeeObj"]["lastName"]+'</label></td></tr>';
				}

				if(jArray[i]["other"]!=undefined){

					htm += '<tr><td>'+
					'<input type="checkbox" class = "chkEmployeeName" name = "chkEmployeeName" id="'+jArray[i]["employeeProfileDetailId"]+'" value="'+jArray[i]["employeeProfileDetailId"]+'" /></td><td><label>'+jArray[i]["other"]+'</label></td></tr>';
				}
			}

			htm += '<div class = "login"><input type = "button" id = "btnDeallocateMemberId" class = "btn btn-inverse btn-mini  " value  = "Delete"  onClick = ""></div></table><form>';
			$('#divDeleteMember').html(htm);
			$('#employeeList').modal('show');
			callback();
		},
	});


}


//Show Consultancy Employee


function showEmployeeForConsultancy(consultancyId,callback){

	$('#divModalAddingNewEventConsultancy').modal('hide');
	$.ajax({
		url:'getEmployeeProfileDetailForConsultancy.json?consultancyId='+consultancyId,	
		dataType:'json',
		success:function(jArray){
			var htm= '<form name="frmTeachingEmployee" id="frmTeachingEmployee">'+
			'<table class = "smalltable">';	 

			for(var i = 0 ; i < jArray.length; i++){

				if(jArray[i]["employeeObj"]!=undefined){

					htm += '<tr><td>'+
					'<input type="checkbox" class = "chkEmployeeName" name = "chkEmployeeName" id="'+jArray[i]["employeeProfileDetailId"]+'" value="'+jArray[i]["employeeProfileDetailId"]+'" /></td><td><label>'+jArray[i]["employeeObj"]["firstName"]+' '+jArray[i]["employeeObj"]["lastName"]+'</label></td></tr>';
				}

				if(jArray[i]["other"]!=undefined){

					htm += '<tr><td>'+
					'<input type="checkbox" class = "chkEmployeeName" name = "chkEmployeeName" id="'+jArray[i]["employeeProfileDetailId"]+'" value="'+jArray[i]["employeeProfileDetailId"]+'" /></td><td><label>'+jArray[i]["other"]+'</label></td></tr>';
				}
			}

			htm += '<div class = "login"><input type = "button" id = "btnDeallocateMemberId" class = "btn btn-inverse btn-mini  " value  = "Delete"  onClick = ""></div></table><form>';
			$('#divDeleteMember').html(htm);
			$('#employeeList').modal('show');
			callback();
		},
	});


}


