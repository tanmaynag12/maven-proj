function getDocumentCategoryByWorkExperienceMCI(mciWorkExperienceId,callback){
	try{
		$.ajax({
			url:'getDocumentCategoryByMCIWorkExperienceId.json',
			data:'mciWorkExperienceId='+mciWorkExperienceId,
			success:function(docId){
				callback(docId);
			}
		});
	} catch(e){

	}
}

