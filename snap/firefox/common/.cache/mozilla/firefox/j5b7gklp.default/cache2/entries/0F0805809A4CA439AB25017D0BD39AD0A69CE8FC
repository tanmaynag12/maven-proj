function showAllPostedToUser(alumniRingPostId) {
  $.ajax({
    url: 'showAllPostedToUser.json?__alumniRingPostId=' + alumniRingPostId,
    dataType: 'json',
    headers: {
      'security-helper-prop': true
    },
    beforeSend: function () {
      startCommonAjaxSpinner();
    },
    complete: function () {
      stopCommonAjaxSpinner();
    },
    success: function (jArray) {
      var modalBoiler = '			<div id="divSharedWithModal" class="modal hide fade">' + '				<div class="modal-header">' + '					<button type="button" class="close" data-dismiss="modal">x</button>' + '					<h3>You shared the post with...</h3>' + '				</div>' + '				<div class="modal-body">' + '					<div id="divSharedWithModalBody"></div>' + '				</div>' + '				<div class="modal-footer">' + '					<input type="button" class="btn btn-inverse btn-mini"' + '						data-dismiss="modal" value="Close" />' + '				</div>' + '			</div>';
      $('#sharedWithModalDiv').html(modalBoiler);
      var boiler = '<table class="table">';
      for (var i = 0; i < jArray.length; i++) {
        boiler += '<tr>';
        boiler += '<td><img src="' + jArray[i]["ImageURL"] + '"/></td>';
        boiler += '<td><strong>' + jArray[i]["name"] + '</strong></td>';
        boiler += '</tr>';
      }
      boiler += '</table>';
      $('#divSharedWithModalBody').html(boiler);
      $('#divSharedWithModal').modal('show');
    },
    error: function () {}
  });
}
function scrollToDom(idOrClass, timeout) {
  if (document.getElementById('tabResult') != null) {
    ReactDOM.unmountComponentAtNode(document.getElementById('tabResult'));
  }
  $([document.documentElement, document.body]).animate({
    scrollTop: $(idOrClass).offset().top - 50
  }, timeout != undefined ? timeout : 500);
