        }, "Remove  "), /*#__PURE__*/React.createElement("a", {
          onClick: e => this.removeAllPost(post.alumniRingPostDetailId, post.alumniRingPostId)
        }, "Remove All")), /*#__PURE__*/React.createElement("span", {
          style: {
            float: 'right'
          }
        }, post.date, " ", '   ', post.time)), Comments, (() => {
          let docArray = post.attachments;
          $('#attachment-' + post.alumniRingPostId).html("");
          if (docArray.length > 0) {
            let isCreator = false;
            if (post.selfPost) {
              isCreator = true;
            }
            var person = $('#hdnUserRole').val();
            if (person.toLowerCase() != 'student' && person != 'alumnistudent') {
              person = 'Employee';
            } else if (person == 'alumnistudent') {
              person = 'Student';
            }
            setTimeout(function () {
              showAluminiPostAttachment(docArray, 'attachment-' + post.alumniRingPostId, isCreator, person, "thumbnail");
            }, 100);
          }
          replyid++;
        })());
      }));
    }
  }
}
$(document).ready(function () {
  ReactDOM.render( /*#__PURE__*/React.createElement(AlumniPost, null), document.getElementById('divalumpost'));
});
function CommentBox(props) {
  let commentArr = props.comments;
  let postArr = props.posts;
  let commentLength = commentArr.length;
  let shown = commentLength > 3 ? "block" : "none";
  const ShowHideTable = () => /*#__PURE__*/React.createElement("table", {
    id: 'tableShowHideComment-' + postArr.alumniRingPostId,
    style: {
      marginLeft: '5%',
      display: shown,
      marginTop: '3px',
      width: '94%',
      backgroundColor: '#F3F3F3'
    },
    className: $('#showCommentHide').attr('showhide')
  }, /*#__PURE__*/React.createElement("tbody", null, /*#__PURE__*/React.createElement("tr", {
    id: 'tr_showComments-' + postArr.alumniRingPostId
  }, /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("a", {
    onClick: e => props.state.showHiddenComment(postArr.alumniRingPostId)
  }, "Show all comments(", commentArr.length, ")"))), /*#__PURE__*/React.createElement("tr", {
    id: 'tr_hideComments-' + postArr.alumniRingPostId,
    style: {
      display: 'none'
    }
  }, /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("a", {
    onClick: e => props.state.hideShownComment(postArr.alumniRingPostId)
  }, "Hide comments")))));
  return /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement(ShowHideTable, null), commentArr.map((comment, i) => {
    let Ownertbl;
    if (comment.isOwner) {
      Ownertbl = /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement("span", {
        id: "parentAction"
      }, /*#__PURE__*/React.createElement("span", {
        id: 'actionRemove-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("a", {
        href: "#linkDiv",
        title: "Remove",
        onClick: e => props.state.deleteCommentThread(comment.commentThreadId)
      }, /*#__PURE__*/React.createElement("i", {
        style: {
          float: 'right',
          marginLeft: '5px'
        }
      }, "remove"))), /*#__PURE__*/React.createElement("span", {
        id: 'actionEdit-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("a", {
        onClick: e => props.state.editComment(comment.commentThreadId)
      }, /*#__PURE__*/React.createElement("i", {
        style: {
          float: 'right',
          marginLeft: '5px'
        }
      }, "edit")))));
    }
    if (commentLength > 3 && i < commentLength - 3) {
      return /*#__PURE__*/React.createElement("table", {
        className: 'hiddenComment-' + postArr.alumniRingPostId,
        key: i,
        style: {
          display: 'none',
          marginLeft: '5%',
          marginTop: '3px',
          width: '94%',
          backgroundColor: '#F3F3F3'
        }
      }, /*#__PURE__*/React.createElement("tbody", {
        id: 'tr-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("tr", null, /*#__PURE__*/React.createElement("td", {
        rowSpan: "2",
        style: {
          verticalAlign: 'top',
          padding: '4px',
          width: '30px'
        }
      }, /*#__PURE__*/React.createElement("img", {
        src: comment.ImageURL,
        style: {
          height: '20px',
          width: '20px'
        }
      })), /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("p", {
        style: {
          display: 'inline'
        }
      }, /*#__PURE__*/React.createElement("b", null, comment.name, " : ")), /*#__PURE__*/React.createElement("p", {
        style: {
          display: 'inline',
          float: 'right'
        },
        className: "muted"
      }, moment(comment.commentDateTime).fromNow()))), /*#__PURE__*/React.createElement("tr", null, /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("p", {
        className: "muted"
      }, /*#__PURE__*/React.createElement("span", {
        id: 'commentData-' + comment.commentThreadId,
        key: comment.commentThreadId
      }, comment.comment), Ownertbl)))));
    } else {
      return /*#__PURE__*/React.createElement("table", {
        className: 'comment-' + postArr.alumniRingPostId,
        key: i,
        style: {
          marginLeft: '5%',
          marginTop: '3px',
          width: '94%',
          backgroundColor: '#F3F3F3',
          display: 'table'
        },
        className: $('#showCommentHide').attr('showhide')
      }, /*#__PURE__*/React.createElement("tbody", {
        id: 'tr-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("tr", null, /*#__PURE__*/React.createElement("td", {
        rowSpan: "2",
        style: {
          verticalAlign: 'top',
          padding: '4px',
          width: '30px'
        }
      }, /*#__PURE__*/React.createElement("img", {
        src: comment.ImageURL,
        style: {
          height: '20px',
          width: '20px'
        }
      })), /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("p", {
        style: {
          display: 'inline'
        }
      }, /*#__PURE__*/React.createElement("b", null, comment.name, "(", comment.rollNo, ") : ")), /*#__PURE__*/React.createElement("p", {
        style: {
          display: 'inline',
          float: 'right'
        },
        className: "muted"
      }, moment(comment.commentDateTime).fromNow()))), /*#__PURE__*/React.createElement("tr", null, /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("p", {
        className: "muted"
      }, /*#__PURE__*/React.createElement("span", {
        id: 'commentData-' + comment.commentThreadId,
        key: comment.commentThreadId
      }, comment.comment), Ownertbl)))));
    }
  }));
}
function dangerouslySetHtml(str) {
  return /*#__PURE__*/React.createElement("span", {
    dangerouslySetInnerHTML: {
      __html: '' + str
    }
  });
}
function showAllPostedToUser(alumniRingPostId) {
  $.ajax({
    url: 'showAllPostedToUser.json?__alumniRingPostId=' + alumniRingPostId,
    dataType: 'json',
    headers: {
      'security-helper-prop': true
    },
    beforeSend: function () {
      startCommonAjaxSpinner();
    },
    complete: function () {
      stopCommonAjaxSpinner();
    },
    success: function (jArray) {
      var modalBoiler = '			<div id="divSharedWithModal" class="modal hide fade">' + '				<div class="modal-header">' + '					<button type="button" class="close" data-dismiss="modal">x</button>' + '					<h3>You shared the post with...</h3>' + '				</div>' + '				<div class="modal-body">' + '					<div id="divSharedWithModalBody"></div>' + '				</div>' + '				<div class="modal-footer">' + '					<input type="button" class="btn btn-inverse btn-mini"' + '						data-dismiss="modal" value="Close" />' + '				</div>' + '			</div>';
      $('#sharedWithModalDiv').html(modalBoiler);
      var boiler = '<table class="table">';
      for (var i = 0; i < jArray.length; i++) {
        boiler += '<tr>';
        boiler += '<td><img src="' + jArray[i]["ImageURL"] + '"/></td>';
        boiler += '<td><strong>' + jArray[i]["name"] + '</strong></td>';
        boiler += '</tr>';
      }
      boiler += '</table>';
      $('#divSharedWithModalBody').html(boiler);
      $('#divSharedWithModal').modal('show');
    },
    error: function () {}
  });
}
function scrollToDom(idOrClass, timeout) {
  if (document.getElementById('tabResult') != null) {
    ReactDOM.unmountComponentAtNode(document.getElementById('tabResult'));
  }
  $([document.documentElement, document.body]).animate({
    scrollTop: $(idOrClass).offset().top - 50
  }, timeout != undefined ? timeout : 500);
