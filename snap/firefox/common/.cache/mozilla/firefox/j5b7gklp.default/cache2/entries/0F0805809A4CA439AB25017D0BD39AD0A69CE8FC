$('#post-alum').click(function () {
  globalPostType = $('.shareType').val();
  var alumniTextPost = '';
  let emailSubject = "";
  if (globalPostType == 'status') {
    try {
      alumniTextPost = encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData()); //$('#alumniTextPost').val();
      if (alumniTextPost == '') return jAlert("Please enter a message!");
    } catch (Exception) {
      alumniTextPost = "";
    }
  } else if (globalPostType == 'videos') {
    try {
      var textPost = encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData()); //$('#alumniTextPost').val();
    } catch (Exception) {
      alumniTextPost = "";
    }
    var url = $('div.liveurl div.inner div.url').html();
    var urlboiler = '<a href="' + url + '" target="_blank">' + url + '</a>';
    $('div.liveurl div.inner div.url').html(urlboiler);
    var title = $('div.liveurl div.inner div.title').html();
    var titleBoiler = '<b>' + title + '</b>';
    $('div.liveurl div.inner div.title').html(titleBoiler);
    //$('div.liveurl div.inner').html()
    alumniTextPost = textPost + encodeURIComponent('<div class="row-fluid">' + $('#alumniLinkPost').val() + '</div>');
  } else if (globalPostType == 'photos') {
    try {
      alumniTextPost = encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData()); //$('#alumniTextPost').val();
      if (alumniTextPost == '') return jAlert("Please enter some text or upload file!");
    } catch (Exception) {
      alumniTextPost = "";
    }
  } else if (globalPostType == 'location') {
    try {
      alumniTextPost = encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData()); //$('#alumniTextPost').val();
    } catch (Exception) {
      alumniTextPost = "";
    }
    var lng = $("input[name='lng']").val();
    var lat = $("input[name='lat']").val();
    if (lat != undefined && lng != undefined && lat != 0 && lng != 0 && lat != '' && lng != '') {
      alumniTextPost += '<br><strong>' + $('#geocomplete').val() + '</strong><br/>';
      alumniTextPost += '<img class="img-responsive" src="http://maps.googleapis.com/maps/api/staticmap?size=500x150&maptype=roadmap\&markers=size:mid%7Ccolor:red%7C' + lat + ',' + lng + '&zoom=15&sensor=false" />';
    }
  } else if (globalPostType == 'sms') {
    try {
      alumniTextPost = $('#alumniTextPost').val();
    } catch (Exception) {
      alumniTextPost = "";
    }
  } else if (globalPostType == 'email') {
    try {
      if (ckeditorFlag != undefined) ckeditorFlag = false;
      emailSubject = $("#emailSubject").val();
      alumniTextPost = encodeURIComponent(CKEDITOR.instances.alumniTextPost.getData()); //$('#alumniTextPost').val();
      $("#emailSubject").val('');
      if (alumniTextPost == '') return jAlert("Please enter email text!");
    } catch (Exception) {
      alumniTextPost = "";
    }
  }
  var alumUserId = '';
  var selfPostFlag = false;
  if ($('#postScope').val() != '' && $('#postScope').val() != undefined) {
    alumUserId = $('#postScope').val();
    $('#postScope').focus();
    selfPostFlag = true;
  } else {
    selfPostFlag = false;
  }
  var isProductionEnvironment = $('#txtIsProduction').val() != undefined ? $('#txtIsProduction').val() : false;

  /*var urlStr='addAlumniPost.json';
  if(isProductionEnvironment.toString()=='false')*/
  var urlStr = 'addAlumniPostNew.json';
  if (selfPostFlag) {
    $.ajax({
      url: urlStr,
      dataType: 'json',
      type: 'POST',
      data: {
        alumUserId: alumUserId,
        alumniTextPost: alumniTextPost,
        postType: globalPostType,
        emailSubject: emailSubject
      },
      async: false,
      beforeSend: function () {
        startCommonAjaxSpinner();
      },
      complete: function () {
        stopCommonAjaxSpinner();
        try {
          CKEDITOR.instances.alumniTextPost.setData("");
        } catch (Exception) {}
        //$('#alumniTextPost').val("");
        $('#alumniLinkPost').val("");
        $('#alumniDocumentPostText').val('');
        $('#showAluminiPostUploadDiv').html('');
        $('#postScope').tokenInput("clear");
        $('.liveurl').hide();
        //$('#alumniTextPost').focus();
        /*for(name in CKEDITOR.instances){
            CKEDITOR.instances[name].destroy(true);
        }*/
        $('#post-alum').focus();
        //changeMessageType(globalPostType);
      },
      success: function (jArray) {
        ReactDOM.render( /*#__PURE__*/React.createElement(AlumniPost, {
          isUpadationRequired: true
        }), document.getElementById('divalumpost'));
      },
      error: function () {}
    });
  }
});
class AlumniPost extends React.Component {
  constructor() {
    super();
    this.state = {
      isLoaded: false,
      isError: false,
      isUpdated: false,
      posts: [],
      shown: true,
      isUpadationRequired: false,
      limit: 5,
      customer: $('#customerHelper').val()
    };
    this.fetchPosts = this.fetchPosts.bind(this);
    this.deleteCommentThread = this.deleteCommentThread.bind(this);
    this.saveEditComment = this.saveEditComment.bind(this);
  }
  fetchPosts(limit) {
    var hdnUserRole = $('#showCommentHide').val();
    var url = 'getAlumniPost.json';
    if (hdnUserRole == "student" || hdnUserRole == "Student") {
      url = 'stu_getAlumniPost.json';
    }
    $.ajax({
      url: url,
      data: {
        limit: limit
      },
      dataType: 'json',
      headers: {
        'security-helper-prop': true
      },
      beforeSend: function () {
        if (limit > 5) {
          $('#divalumpost').append('<div style="text-align:center;" class="faSpinner"><i class="fa fa-spinner fa-pulse fa-fw fa-3x" style="color:#0b4971"></i></div>');
          scrollToDom($('#divalumpost .faSpinner'));
        }
      },
      complete: function () {
        $('#divalumpost .faSpinner').remove();
      },
      success: function (jArray) {
        $(".tComment").val("");
        this.setState({
          isLoaded: true,
          posts: jArray,
          isUpdated: true
        });
      }.bind(this),
      error: function () {}
    });
  }
  componentDidMount() {
    this.fetchPosts(this.state.limit);
  }
  static getDerivedStateFromProps(nextProps, prevState) {
    if (nextProps.isUpadationRequired !== prevState.isUpadationRequired) {
      return {
        isUpadationRequired: nextProps.isUpadationRequired,
        limit: nextProps.limit != undefined ? nextProps.limit : prevState.limit
      };
    } else if (nextProps.isUpadationRequired == prevState.isUpadationRequired) {
      return {
        isUpadationRequired: false,
        limit: nextProps.limit != undefined ? nextProps.limit : prevState.limit
      };
    } else {
      return null;
    }
  }
  shouldComponentUpdate(newProps, newState) {
    if (newState.isUpdated == true) {
      return true;
    }
    return false;
  }
  componentDidUpdate(prevProps, prevState) {
    if (prevProps.isUpadationRequired !== this.state.isUpadationRequired) {
      let postLimit = this.state.limit;
      this.fetchPosts(postLimit);
    }
    if (prevState.isUpdated == true) {
      return true;
    }
    return false;
  }
  showPostofUser(userId, e) {
    window.location.href = "alumniPost.htm?type=other&id=" + userId;
  }
  showHiddenComment(alumniRingPostId) {
    $('.hiddenComment-' + alumniRingPostId).show();
    $('#tr_showComments-' + alumniRingPostId).hide();
    $('#tr_hideComments-' + alumniRingPostId).show();
  }
  hideShownComment(alumniRingPostId) {
    $('.hiddenComment-' + alumniRingPostId).hide();
    $('#tr_showComments-' + alumniRingPostId).show();
    $('#tr_hideComments-' + alumniRingPostId).hide();
  }
  editComment(id) {
    let textVal = $('#actionEdit-' + id).text();
    if (textVal == "edit") {
      let data = $('#commentData-' + id).text();
      ReactDOM.render( /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement("span", {
        id: 'actionSave-' + id
      }, /*#__PURE__*/React.createElement("a", {
        onClick: e => this.saveComment(id)
      }, /*#__PURE__*/React.createElement("i", {
        style: {
          float: 'right',
          marginLeft: '5px'
        }
      }, "save"))), /*#__PURE__*/React.createElement("span", {
        id: 'actionCancle-' + id
      }, /*#__PURE__*/React.createElement("a", {
        onClick: e => this.cancleComment(id, data)
      }, /*#__PURE__*/React.createElement("i", {
        style: {
          float: 'right',
          marginLeft: '5px'
        }
      }, "cancel")))), document.getElementById('actionEdit-' + id));
      ReactDOM.render( /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement("input", {
        type: "text",
        style: {
          width: '80%',
          margin: '5px',
          borderRadius: '0px',
          display: 'inline'
        },
        id: 'txtCommentEdit-' + id,
        defaultValue: data
      })), document.getElementById('commentData-' + id));
      document.getElementById('txtCommentEdit-' + id).focus();
    }
  }
  cancleComment(id, data) {
    ReactDOM.render( /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement("span", {
      id: 'actionEdit-' + id
    }, /*#__PURE__*/React.createElement("a", {
      onClick: e => this.editComment(id)
    }, /*#__PURE__*/React.createElement("i", {
      style: {
        float: 'right',
        marginLeft: '5px'
      }
    }, "edit")))), document.getElementById('actionEdit-' + id));
    ReactDOM.render(data, document.getElementById('commentData-' + id));
  }
  deleteCommentThread(commentThreadId) {
    $.ajax({
      url: 'deleteCommentThread.json?__commentThreadId=' + commentThreadId,
      dataType: 'text',
      headers: {
        'security-helper-prop': true
      },
      async: false,
      beforeSend: function () {
        startCommonAjaxSpinner();
      },
      complete: function () {
        stopCommonAjaxSpinner();
      },
      success: function (text) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {
        alert("error in deleteCommentThread");
      }
    });
  }
  saveComment(id) {
    this.saveEditComment(id);
    var editedComment = $('#txtCommentEdit-' + id).val();
    var boiler = '' + editedComment;
    ReactDOM.render( /*#__PURE__*/React.createElement(React.Fragment, null, "boiler"), document.getElementById('commentData-' + id));
    ReactDOM.render( /*#__PURE__*/React.createElement("a", {
      onClick: e => this.editComment(id)
    }, /*#__PURE__*/React.createElement("i", {
      style: {
        float: 'right',
        marginLeft: '5px'
      }
    }, "edit")), document.getElementById('actionEdit-' + id));
  }
  saveEditComment(commentThreadId) {
    var comment = $("#txtCommentEdit-" + commentThreadId).val().trim();
    if (comment == "") {
      jAlert("Please Enter Comment");
      return;
    }
    $.ajax({
      url: 'editCommentThread.json',
      dataType: 'json',
      headers: {
        'security-helper-prop': true
      },
      data: {
        __commentThreadId: commentThreadId,
        comment: comment
      },
      async: false,
      beforeSend: function () {
        startCommonAjaxSpinner();
      },
      complete: function () {
        stopCommonAjaxSpinner();
      },
      success: function (commentjArray) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {
        jAlert("Error posting your comment, Please try after some time");
      }
    });
  }
  savePostComment(alumniRingPostId) {
    var comment = $("#txtNewCommen-" + alumniRingPostId).val().trim();
    if (comment == "") {
      jAlert("Please Enter Comment");
      return;
    }
    $.ajax({
      url: 'savePostCommentThread.json?comment=' + escape(comment) + '&__alumniRingPostId=' + alumniRingPostId,
      dataType: 'json',
      headers: {
        'security-helper-prop': true
      },
      async: false,
      beforeSend: function () {
        startCommonAjaxSpinner();
      },
      complete: function () {
        stopCommonAjaxSpinner();
      },
      success: function (commentjArray) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {
        jAlert("Error posting your comment, Please try after some time");
      }
    });
  }
  removePost(alumnipostDetailId) {
    $.ajax({
      url: 'removeAlumniPost.json?__alumnipostDetailId=' + alumnipostDetailId,
      headers: {
        'security-helper-prop': true
      },
      beforeSend: function () {},
      complete: function () {},
      success: function (jArray) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {}
    });
  }
  removeAllPost(alumnipostDetailId, alumniRingPostId) {
    $.ajax({
      url: 'removeAllAlumniPost.json?__alumnipostDetailId=' + alumnipostDetailId + '&__alumniRingPostId=' + alumniRingPostId,
      headers: {
        'security-helper-prop': true
      },
      beforeSend: function () {},
      complete: function () {},
      success: function (jArray) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {}
    });
  }
