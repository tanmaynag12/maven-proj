  saveEditComment(commentThreadId) {
    var comment = $("#txtCommentEdit-" + commentThreadId).val().trim();
    if (comment == "") {
      jAlert("Please Enter Comment");
      return;
    }
    $.ajax({
      url: 'editCommentThread.json',
      dataType: 'json',
      headers: {
        'security-helper-prop': true
      },
      data: {
        __commentThreadId: commentThreadId,
        comment: comment
      },
      async: false,
      beforeSend: function () {
        startCommonAjaxSpinner();
      },
      complete: function () {
        stopCommonAjaxSpinner();
      },
      success: function (commentjArray) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {
        jAlert("Error posting your comment, Please try after some time");
      }
    });
  }
  savePostComment(alumniRingPostId) {
    var comment = $("#txtNewCommen-" + alumniRingPostId).val().trim();
    if (comment == "") {
      jAlert("Please Enter Comment");
      return;
    }
    $.ajax({
      url: 'savePostCommentThread.json?comment=' + escape(comment) + '&__alumniRingPostId=' + alumniRingPostId,
      dataType: 'json',
      headers: {
        'security-helper-prop': true
      },
      async: false,
      beforeSend: function () {
        startCommonAjaxSpinner();
      },
      complete: function () {
        stopCommonAjaxSpinner();
      },
      success: function (commentjArray) {
        this.fetchPosts(this.state.limit);
      }.bind(this),
      error: function () {
        jAlert("Error posting your comment, Please try after some time");
      }
    });
  }
