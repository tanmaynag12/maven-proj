function CommentBox(props) {
  let commentArr = props.comments;
  let postArr = props.posts;
  let commentLength = commentArr.length;
  let shown = commentLength > 3 ? "block" : "none";
  const ShowHideTable = () => /*#__PURE__*/React.createElement("table", {
    id: 'tableShowHideComment-' + postArr.alumniRingPostId,
    style: {
      marginLeft: '5%',
      display: shown,
      marginTop: '3px',
      width: '94%',
      backgroundColor: '#F3F3F3'
    },
    className: $('#showCommentHide').attr('showhide')
  }, /*#__PURE__*/React.createElement("tbody", null, /*#__PURE__*/React.createElement("tr", {
    id: 'tr_showComments-' + postArr.alumniRingPostId
  }, /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("a", {
    onClick: e => props.state.showHiddenComment(postArr.alumniRingPostId)
  }, "Show all comments(", commentArr.length, ")"))), /*#__PURE__*/React.createElement("tr", {
    id: 'tr_hideComments-' + postArr.alumniRingPostId,
    style: {
      display: 'none'
    }
  }, /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("a", {
    onClick: e => props.state.hideShownComment(postArr.alumniRingPostId)
  }, "Hide comments")))));
  return /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement(ShowHideTable, null), commentArr.map((comment, i) => {
    let Ownertbl;
    if (comment.isOwner) {
      Ownertbl = /*#__PURE__*/React.createElement(React.Fragment, null, /*#__PURE__*/React.createElement("span", {
        id: "parentAction"
      }, /*#__PURE__*/React.createElement("span", {
        id: 'actionRemove-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("a", {
        href: "#linkDiv",
        title: "Remove",
        onClick: e => props.state.deleteCommentThread(comment.commentThreadId)
      }, /*#__PURE__*/React.createElement("i", {
        style: {
          float: 'right',
          marginLeft: '5px'
        }
      }, "remove"))), /*#__PURE__*/React.createElement("span", {
        id: 'actionEdit-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("a", {
        onClick: e => props.state.editComment(comment.commentThreadId)
      }, /*#__PURE__*/React.createElement("i", {
        style: {
          float: 'right',
          marginLeft: '5px'
        }
      }, "edit")))));
    }
    if (commentLength > 3 && i < commentLength - 3) {
      return /*#__PURE__*/React.createElement("table", {
        className: 'hiddenComment-' + postArr.alumniRingPostId,
        key: i,
        style: {
          display: 'none',
          marginLeft: '5%',
          marginTop: '3px',
          width: '94%',
          backgroundColor: '#F3F3F3'
        }
      }, /*#__PURE__*/React.createElement("tbody", {
        id: 'tr-' + comment.commentThreadId
      }, /*#__PURE__*/React.createElement("tr", null, /*#__PURE__*/React.createElement("td", {
        rowSpan: "2",
        style: {
          verticalAlign: 'top',
          padding: '4px',
          width: '30px'
        }
      }, /*#__PURE__*/React.createElement("img", {
        src: comment.ImageURL,
        style: {
          height: '20px',
          width: '20px'
        }
      })), /*#__PURE__*/React.createElement("td", null, /*#__PURE__*/React.createElement("p", {
        style: {
