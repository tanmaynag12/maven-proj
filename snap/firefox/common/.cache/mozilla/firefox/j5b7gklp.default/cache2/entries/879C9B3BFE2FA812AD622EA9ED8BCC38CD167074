var gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip','r'];
var gCategoryType = '';

var gjArrTextDocument = new Array();
var gjArrUploadedDocument= new Array();
var gNodeId='';
var gExtraData = {};
function UploadDocument(){
	this.elementId = '';
	this.categoryId = '';
	this.barcode = ''; //optional parameter
	this.tags = '';
	this.subjectName = '';
	var fileSize = AllowedFileSize.EXCELSHEET_UPLOAD;
	var fileType = gExtensionArray;
	var elementId = '';
	this.init = function(jObject,callback){
		if(jObject == undefined){
			jAlert('Object is undefined');
			return undefined;
		} else {
			try{
				this.elementId = jObject.elementId;
				elementId = jObject.elementId;
				this.categoryId = jObject.categoryId;
				this.barcode = jObject.barcode;
				this.tags = jObject.tags;
				this.subjectName = jObject.subjectName;
				if(this.elementId == undefined || this.elementId == '' || this.categoryId == undefined || this.categoryId == '' || this.categoryId == 'null' || this.categoryId == 'undefined'){
				    jAlert('Error: Undefined parameters categoryId and elementId. These parameters are mandatory');
					return null;
				}	
				
				if(jObject.fileSize == undefined || isNaN(jObject.fileSize)){
					fileSize = AllowedFileSize.EXCELSHEET_UPLOAD;
				} else {
					fileSize = jObject.fileSize;
				}
				
				if(jObject.caption == undefined || jObject.caption == ''){
					uploadButtonCaption = 'Upload';//Upload Document
				} else {
					uploadButtonCaption = jObject.caption;
				}
				
				if(jObject.fileType != undefined){
					if(typeof jObject.fileType != 'array' && typeof jObject.fileType != 'object' ){
						jAlert('File type parameter must be an Array');
						return null;
					} else
						fileType = jObject.fileType;
				}
				
			} catch(e){
				alert(e);
				return null;
			}
		}
		paintBoiler(this.elementId,this.categoryId,function(){
			$('.uploadButtonLabel').text(uploadButtonCaption);
			$(document).ready(function(){
				var fileElementId = 'txtUploadDocument-'+jObject.elementId+'-'+jObject.categoryId+'';
				new AjaxUpload(fileElementId,{
					action : 'uploadDocument.json',
					name : 'fileData',
					timeout:60000,
					size:fileSize,
					data:{},
					onSubmit : function(file, extension) {
						$('#'+fileElementId).attr('disabled','disabled');
						
						if ($.inArray(extension.toString().toLowerCase(),fileType) != -1 || fileType=='ALL') {
							$('#elementId').find('div.preview').addClass('loading');
							var arrayToSend = {};
							arrayToSend["categoryId"] = jObject.categoryId;
							arrayToSend["fileSize"] = fileSize;
							if(jObject.onUpload != undefined){
								if (false === jObject.onUpload.call(this)){
									return false;
								}
								for (var prop in gExtraData) {
					                if (gExtraData.hasOwnProperty(prop)){
					                	arrayToSend[prop] = gExtraData[prop];
					                }
							    }
							}
							
							this.setData(arrayToSend);
							return true;
						} else {
							jAlert('File type is not valid ! Please upload only <strong>&nbsp;'+fileType.join(', ')+'&nbsp;<strong> files');
							$('#'+fileElementId).removeAttr('disabled');
							return false;
						}
					},
					onComplete : function(file, response) {
						$('#'+fileElementId).removeAttr('disabled');
						try{
							response = eval('('+response+')');
							$('#elementId').find('div.preview').removeClass('loading');
							if(response[0].responseFlag == '1'){
								callback(response[1]["eDocId"]);
							} else {
								throw response[0].responseText;
							}
						} catch (e){
							jAlert(e);
						}
					}
				});
			});
		});

	};

	this.getDocument = function(categoryId,callback){
		return new Promise(function(resolve, reject){



			if(categoryId == undefined || categoryId == '' || categoryId=='null' || categoryId=='undefined'){
				//jAlert('Error: define category id');
				reject('Category not found!');
				return null;
			}		
			try{
				gNodeId=categoryId;
				$.ajax({
					url:'getUploadedDocumentAjax.json?categoryid='+categoryId,
					dataType:'json',
					async:callback==undefined,//false
					success:function(jArray){

						var jsonArray = new Array();
						for(var i = 0; i < jArray.length ; i++){
							var documentObj = new GemsDocument();
							documentObj.categoryId = jArray[i]['categoryId'];
							documentObj.eCatId = jArray[i]['eCatId'];
							documentObj.catName = jArray[i]['catName'];

							documentObj.documentName = jArray[i]['name'].gemsDecode();
							documentObj.name = jArray[i]['name'].gemsDecode();
							documentObj.hasProfileImage = jArray[i]['hasProfileImage'];
							documentObj.gender = jArray[i]['gender'];
							documentObj.sharedStatus = jArray[i]['sharedStatus'];
							documentObj.documentId = jArray[i]['documentPathId'];
							documentObj.eDocId = jArray[i]['eDocPathId'];
							documentObj.creationDate = jArray[i]['creationDate'];
							documentObj.ownerEmployeeId = jArray[i]['ownerEmployeeId'];
							documentObj.extension = jArray[i]['extension'];
							documentObj.type = jArray[i]['extension'];
							documentObj.isTemporary = false;
							documentObj.description = jArray[i]['description'];
							documentObj.documentBody = jArray[i]['documentBody'];
							documentObj.createdAt = jArray[i]['createdAt'];
							jsonArray.push(documentObj);
						}
						gjArrUploadedDocument=jArray;
						if(callback!=undefined)
							callback(jsonArray);
						resolve(jsonArray);
					},
					error:function(){

					}

				});
			} catch(e){
				alert(e);
				reject(e);
			}
		});
	};

	/** Showing document as in dms look*/

	this.showDocument = function(jsonObject,callback) {

		var gFileId = 1;
		if(jsonObject == undefined){
			jAlert('Error: Parameter expected!');
			return; 

		}  else {
			try{
				

				if(jsonObject.elementId == '' || jsonObject.documentList == undefined){
					if(jsonObject.elementId == ''){
						jAlert('Error: Required parameter "elementId" is mission');
						return null;
					}
					if(jsonObject.documentList == undefined) {
						jAlert('Error: Required parameter "documentJArray" is mission');
						return null;
					}
				} else {
					var jArray = jsonObject.documentList;
					if (jArray.length > 0) {
						var attachementBoiler = "<table class = 'table'>";
						for ( var i = 0; i < jArray.length; i++) {
							var attachmentFileName = jArray[i]["name"].toString();
							var documentId = jArray[i]["documentId"];
							var eDocId = jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];
							var isTemporary = jArray[i]["isTemporary"];

							var trId = 'trAttachmentFile-' + eDocId;

							var fileType = jArray[i]["type"];
							iconPath = gFileTypeIconMapping[fileType];
							if (!isTemporary) {
								var downloadLinkTmp = (juno.label['a.download.json'].setIndexedArgs((eDocId), "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
								attachementBoiler += '<tr id = "'
										+ trId
										+ '"><td><img src="'
										+ iconPath
										+ '" />&nbsp;&nbsp;&nbsp;&nbsp;'+downloadLinkTmp+'</td>';
								if (true) {
									attachementBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "deleteUploadedDMSDocument(this,\''
											+ eDocId + '\')" /></td>';
								}
								attachementBoiler += '</tr>';
							} else {
								attachementBoiler += '<tr id = "'
										+ trId
										+ '"><td><img src="'
										+ iconPath
										+ '" />&nbsp;&nbsp;&nbsp;&nbsp;<a href = "javascript:void(0);">'
										+ attachmentFileName
										+ '</a></td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "deleteUploadedCV(\''
										+ eDocId
										+ '\')" />&nbsp;&nbsp;&nbsp;&nbsp<img src = "img/unsaved.png" title = "Unsaved"/></td></tr>';
							}
						}
						attachementBoiler += "</table>";
						if (jsonObject.elementId == undefined)
							$('#uploadedDocList').html(attachementBoiler);
						else
							$('#' + jsonObject.elementId).html(attachementBoiler);
					} else {
						$('#' + jsonObject.elementId).html('No document uploaded!!!');
					}
			}	
			
			} catch(e){
				alert(e);
				return null;
			}
		}


	};
	
	this.setData= function(data){
    	gExtraData = data;
    };
    
    this.getAllUserRootFolder = function(typeArray,parentType,callback) {
