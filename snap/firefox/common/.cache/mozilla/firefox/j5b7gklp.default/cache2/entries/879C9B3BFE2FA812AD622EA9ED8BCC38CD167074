/**
 * @author vimal kr.
 * @since 13 Oct 2012
 */

var gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip','r'];
var gCategoryType = '';

var gjArrTextDocument = new Array();
var gjArrUploadedDocument= new Array();
var gNodeId='';
var gExtraData = {};
function UploadDocument(){
	this.elementId = '';
	this.categoryId = '';
	this.barcode = ''; //optional parameter
	this.tags = '';
	this.subjectName = '';
	var fileSize = AllowedFileSize.EXCELSHEET_UPLOAD;
	var fileType = gExtensionArray;
	var elementId = '';
	this.init = function(jObject,callback){
		if(jObject == undefined){
			jAlert('Object is undefined');
			return undefined;
		} else {
			try{
				this.elementId = jObject.elementId;
				elementId = jObject.elementId;
				this.categoryId = jObject.categoryId;
				this.barcode = jObject.barcode;
				this.tags = jObject.tags;
				this.subjectName = jObject.subjectName;
				if(this.elementId == undefined || this.elementId == '' || this.categoryId == undefined || this.categoryId == '' || this.categoryId == 'null' || this.categoryId == 'undefined'){
				    jAlert('Error: Undefined parameters categoryId and elementId. These parameters are mandatory');
					return null;
				}	
				
				if(jObject.fileSize == undefined || isNaN(jObject.fileSize)){
					fileSize = AllowedFileSize.EXCELSHEET_UPLOAD;
				} else {
					fileSize = jObject.fileSize;
				}
				
				if(jObject.caption == undefined || jObject.caption == ''){
					uploadButtonCaption = 'Upload';//Upload Document
				} else {
					uploadButtonCaption = jObject.caption;
				}
				
				if(jObject.fileType != undefined){
					if(typeof jObject.fileType != 'array' && typeof jObject.fileType != 'object' ){
						jAlert('File type parameter must be an Array');
						return null;
					} else
						fileType = jObject.fileType;
				}
				
			} catch(e){
				alert(e);
				return null;
			}
		}
		paintBoiler(this.elementId,this.categoryId,function(){
			$('.uploadButtonLabel').text(uploadButtonCaption);
			$(document).ready(function(){
				var fileElementId = 'txtUploadDocument-'+jObject.elementId+'-'+jObject.categoryId+'';
				new AjaxUpload(fileElementId,{
					action : 'uploadDocument.json',
					name : 'fileData',
					timeout:60000,
					size:fileSize,
					data:{},
					onSubmit : function(file, extension) {
						$('#'+fileElementId).attr('disabled','disabled');
						
						if ($.inArray(extension.toString().toLowerCase(),fileType) != -1 || fileType=='ALL') {
							$('#elementId').find('div.preview').addClass('loading');
							var arrayToSend = {};
							arrayToSend["categoryId"] = jObject.categoryId;
							arrayToSend["fileSize"] = fileSize;
							if(jObject.onUpload != undefined){
								if (false === jObject.onUpload.call(this)){
									return false;
								}
								for (var prop in gExtraData) {
					                if (gExtraData.hasOwnProperty(prop)){
					                	arrayToSend[prop] = gExtraData[prop];
					                }
							    }
							}
							
							this.setData(arrayToSend);
							return true;
						} else {
							jAlert('File type is not valid ! Please upload only <strong>&nbsp;'+fileType.join(', ')+'&nbsp;<strong> files');
							$('#'+fileElementId).removeAttr('disabled');
							return false;
						}
					},
					onComplete : function(file, response) {
						$('#'+fileElementId).removeAttr('disabled');
						try{
							response = eval('('+response+')');
							$('#elementId').find('div.preview').removeClass('loading');
							if(response[0].responseFlag == '1'){
								callback(response[1]["eDocId"]);
							} else {
								throw response[0].responseText;
							}
						} catch (e){
							jAlert(e);
						}
					}
				});
			});
		});

	};

	this.getDocument = function(categoryId,callback){
		return new Promise(function(resolve, reject){



			if(categoryId == undefined || categoryId == '' || categoryId=='null' || categoryId=='undefined'){
				//jAlert('Error: define category id');
				reject('Category not found!');
				return null;
			}		
			try{
				gNodeId=categoryId;
				$.ajax({
					url:'getUploadedDocumentAjax.json?categoryid='+categoryId,
					dataType:'json',
					async:callback==undefined,//false
					success:function(jArray){

						var jsonArray = new Array();
						for(var i = 0; i < jArray.length ; i++){
							var documentObj = new GemsDocument();
							documentObj.categoryId = jArray[i]['categoryId'];
							documentObj.eCatId = jArray[i]['eCatId'];
							documentObj.catName = jArray[i]['catName'];

							documentObj.documentName = jArray[i]['name'].gemsDecode();
							documentObj.name = jArray[i]['name'].gemsDecode();
							documentObj.hasProfileImage = jArray[i]['hasProfileImage'];
							documentObj.gender = jArray[i]['gender'];
							documentObj.sharedStatus = jArray[i]['sharedStatus'];
							documentObj.documentId = jArray[i]['documentPathId'];
							documentObj.eDocId = jArray[i]['eDocPathId'];
							documentObj.creationDate = jArray[i]['creationDate'];
							documentObj.ownerEmployeeId = jArray[i]['ownerEmployeeId'];
							documentObj.extension = jArray[i]['extension'];
							documentObj.type = jArray[i]['extension'];
							documentObj.isTemporary = false;
							documentObj.description = jArray[i]['description'];
							documentObj.documentBody = jArray[i]['documentBody'];
							documentObj.createdAt = jArray[i]['createdAt'];
							jsonArray.push(documentObj);
						}
						gjArrUploadedDocument=jArray;
						if(callback!=undefined)
							callback(jsonArray);
						resolve(jsonArray);
					},
					error:function(){

					}

				});
			} catch(e){
				alert(e);
				reject(e);
			}
		});
	};

	/** Showing document as in dms look*/

	this.showDocument = function(jsonObject,callback) {

		var gFileId = 1;
		if(jsonObject == undefined){
			jAlert('Error: Parameter expected!');
			return; 

		}  else {
			try{
				

				if(jsonObject.elementId == '' || jsonObject.documentList == undefined){
					if(jsonObject.elementId == ''){
						jAlert('Error: Required parameter "elementId" is mission');
						return null;
					}
					if(jsonObject.documentList == undefined) {
						jAlert('Error: Required parameter "documentJArray" is mission');
						return null;
					}
				} else {
					var jArray = jsonObject.documentList;
					if (jArray.length > 0) {
						var attachementBoiler = "<table class = 'table'>";
						for ( var i = 0; i < jArray.length; i++) {
							var attachmentFileName = jArray[i]["name"].toString();
							var documentId = jArray[i]["documentId"];
							var eDocId = jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];
							var isTemporary = jArray[i]["isTemporary"];

							var trId = 'trAttachmentFile-' + eDocId;

							var fileType = jArray[i]["type"];
							iconPath = gFileTypeIconMapping[fileType];
							if (!isTemporary) {
								var downloadLinkTmp = (juno.label['a.download.json'].setIndexedArgs((eDocId), "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
								attachementBoiler += '<tr id = "'
										+ trId
										+ '"><td><img src="'
										+ iconPath
										+ '" />&nbsp;&nbsp;&nbsp;&nbsp;'+downloadLinkTmp+'</td>';
								if (true) {
									attachementBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "deleteUploadedDMSDocument(this,\''
											+ eDocId + '\')" /></td>';
								}
								attachementBoiler += '</tr>';
							} else {
								attachementBoiler += '<tr id = "'
										+ trId
										+ '"><td><img src="'
										+ iconPath
										+ '" />&nbsp;&nbsp;&nbsp;&nbsp;<a href = "javascript:void(0);">'
										+ attachmentFileName
										+ '</a></td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "deleteUploadedCV(\''
										+ eDocId
										+ '\')" />&nbsp;&nbsp;&nbsp;&nbsp<img src = "img/unsaved.png" title = "Unsaved"/></td></tr>';
							}
						}
						attachementBoiler += "</table>";
						if (jsonObject.elementId == undefined)
							$('#uploadedDocList').html(attachementBoiler);
						else
							$('#' + jsonObject.elementId).html(attachementBoiler);
					} else {
						$('#' + jsonObject.elementId).html('No document uploaded!!!');
					}
			}	
			
			} catch(e){
				alert(e);
				return null;
			}
		}


	};
	
	this.setData= function(data){
    	gExtraData = data;
    };
    
    this.getAllUserRootFolder = function(typeArray,parentType,callback) {
    	$.ajax({
    		url:'getDocumentCategoriesForHome.json?categoryPath=undefined&typeArray=undefined&parentType='+parentType+'&root=undefined',
    		dataType:'json',
    		beforeSend: function() {
    			$('.ulDMSCourseFileAttachmentTree').html('<div class="preview-large"></div>');
    			$('.ulDMSCourseFileAttachmentTree').find('.preview-large').addClass('loading-large');
    		},
    		complete: function() {
    			$('.ulDMSCourseFileAttachmentTree').find('.preview-large').removeClass('loading-large');
    		},
    		success:function(jArray){
    			callback(jArray);
    		},
    		error:function(){
    		}
    	});
    };
    
    this.getAllUserRootFolderforHome = function(typeArray,parentType,callback) {
    	$.ajax({
    		url:'getDocumentCategoriesForHome.json?categoryPath=undefined&typeArray=undefined&parentType='+parentType+'&root=undefined',
    		dataType:'json',
    		beforeSend: function() {
    			$('.ulDMSCourseFileAttachmentTree').html('<div class="preview-large"></div>');
    			$('.ulDMSCourseFileAttachmentTree').find('.preview-large').addClass('loading-large');
    		},
    		complete: function() {
    			$('.ulDMSCourseFileAttachmentTree').find('.preview-large').removeClass('loading-large');
    		},
    		success:function(jArray){
    			callback(jArray);
    		},
    		error:function(){
    		}
    	});
    };
    
    this.getFilesForCourseFileAttachment = function(categoryId,callback){
    	$.ajax({
    		url:'getAllUserDocumentByCategoryId.json',
    		dataType:'json',
    		data:'categoryId='+categoryId,
    		beforeSend: function() {
    			$('.divUserFileList').html('<div class="preview-large"></div>');
    			$('.divUserFileList').find('.preview-large').addClass('loading-large');
    		},
    		complete: function() {
    			$('.divUserFileList').find('.preview-large').removeClass('loading-large');
    		},
    		success:function(jArray){
    			callback(jArray);
    		},
    		error:function(){
    		}
    	});
    };
    
    this.getCategoryPathWithId = function(documentCategoryId,callback){
    	$.ajax({
    		url:'getCategoryPathWithIdAjax.json?categoryId='+documentCategoryId,
    		dataType:'json',
    		async:false,
    		success:function(jArray){
    			try{
    				callback(jArray);
    			}catch(e){
    			}
    		},
    		error:function(){

    		}
    	});
    };
    
    this.getTextDocumentWithAttachment = function(documentId,callback){
    	$.ajax({
    		url:'getTextDocumentAjax.json?d__documentId='+documentId,
    		beforeSend: function() {
    			startCommonAjaxSpinner();
    		},
    		complete: function() {
    			stopCommonAjaxSpinner();
    		},
    		dataType:'json',
    		async:false,
    		success:function(jArray){
    			if(jArray.length > 0){
    				try{
    					jArray[0].documentBody = jArray[0].documentBody!=undefined?jArray[0].documentBody.gemsDecode():'';
    					jArray[0].description = jArray[0].description!=undefined?jArray[0].description.gemsDecode():'';
    				}catch(e){console.log(e);}
    				callback(jArray[0]);
    			} else {
    				jAlert('No Data Found!!!');
    			}
    		},
    		error:function(){

    		}
    	});
    };
    
    this.getAttachmentListAjax = function(documentId,callback) {
    	try{
    	$.ajax({
    		url:'getAttachmentList.json?d__documentId='+documentId,
    		async:false,
    		dataType:'json',
    		beforeSend: function() {
    			$('#tblAttachementList').html('<div class = "preview-large"></div>');
    			$('.preview-large').addClass('loading-large');
    		},
    		complete: function() {
    			$('.preview-large').removeClass('loading-large');
    		},
    		success:function(jArray){
    			callback(jArray);
    		},
    		error:function(){

    		}
    	});
    	} catch(e){
    		
    	}
    };
    this.showDocumentAttachmentList = function(jArray,tableId,documentId,isOwner){
    	var attachementBoiler = "";
    	if(jArray.length > 0){
    		for(var i = 0 ; i < jArray.length; i++){
    			var attachmentFileName = jArray[i]["name"].toString(); 
    			var trId = 'trAttachmentFile-'+documentId+'-'+i;
    			var downloadLinkTmp = (juno.label['a.download.json'].setIndexedArgs((documentId), "TEXT", encodeURIComponent(attachmentFileName), attachmentFileName));
    			var downloadLinkTmpIcon = (juno.label['a.download.json'].setIndexedArgs((documentId), "TEXT", encodeURIComponent(attachmentFileName), '<i class = "icon-download-alt"></i>'));
    			var fileType = jArray[i]["type"];
    			iconPath = gFileTypeIconMapping[fileType];
    			if(isOwner == undefined || isOwner){
    				attachementBoiler += 
    					'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+downloadLinkTmp+'</td><td><i style = "cursor:pointer" title = "remove" class = "icon-remove-sign" onclick = "removeAttachmentAjax(\''+trId+'\',\''+attachmentFileName+'\')" ></i>&nbsp;&nbsp;&nbsp;&nbsp'+downloadLinkTmpIcon+'</td></tr>';
    			} else {
    				attachementBoiler += 
    					'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+downloadLinkTmp+'</td><td>&nbsp;&nbsp;&nbsp;&nbsp'+downloadLinkTmpIcon+'</td></tr>';			
    			}

    		}
    		if(tableId == undefined)
    			$('#tblAttachementList').html(attachementBoiler);
    		else
    			$('#'+tableId).html(attachementBoiler);		
    	} else {
    		$('#'+tableId).html('<div class="alert alert-info" style="padding:2px"><strong style="color:red">No attachments!!!</strong></div>');
    	}
    };
}

function  paintBoiler(elementId,categoryId,callback){
	var boiler = 
		'<form id = "uploadDocument" action="uploadDocument.json" method="POST" enctype="multipart/form-data">'+
		'<table class = "table tblUploadDoc">'+
		'<tr>'+
		
		'<td>'+
		'<div class="span6">'+
		'<div class="span2">'+
    	'<div class="preview"></div></div>'+
		'<div class="span4">'+
		'<div class="btn btn-info" style = "width:150px;height:20px;">'+//style = "width:165px;height:20px"
			'<span class="uploadButtonLabel"> Upload file</span>'+
			'<input  type="file" style = "margin-top:-22px;margin-left:5px;opacity:0;" name="fileData" id="txtUploadDocument-'+elementId+'-'+categoryId+'"></div>'+//style = "margin-top:-40px;margin-left:-10px;opacity:0;"
			'</div>'+
		'</div>'+
		'</td>'+
		
		'</tr>'+
		'</table>'+
		'</form>';

	$('#'+elementId).html(boiler);
	$('#uploadDocument').reset();
	try{
		callback();
	} catch(e){
		alert(e);
	}
};



/**
 * @type Document
 * @author Vimal Kr.
 */
function GemsDocument() {
	this.documentId;
	this.categoryId;
	this.catName;
	this.documentBody;
	this.documentName;
	this.hasProfileImage;
	this.gender;
	this.shareStatus;
	this.creationDate;
	this.description;
	this.extension;
	this.type;
	this.isTemporary;
	this.ownerEmployeeId;
	this.name;
}



function removeUploadedDocumentById(docId,callback){
	jConfirm("Do you want to Remove Document ?","Confirm",function(r)
	{
		if(r ==true)
		{	
			$.ajax({
				url:'removeUploadedDocumentById.json?d__docId='+docId,
				dataType:'json',
				success:function(jObject){
					callback(jObject);
				}
			});
		}
	});
}


function getUploadedDocumentById(docId,callback) {
	$.ajax({
		url:'getUploadedDocumentById.json?d__docId='+docId,
		dataType:'json',
		success:function(jObject){
			callback(jObject);
		}
	});
}



/*********************************************Temporary Code- It has to be implement for he dmsStudent.js *************888*//*
function UploadStudentDocument(){
	this.elementId = '';
	this.categoryId = '';
	this.barcode = ''; //optional parameter

	fileSize = AllowedFileSize.DEFAULT;

	this.init = function(jObject,callback){
		if(jObject == undefined){
			jAlert('Object is undefined');
			return undefined;
		} else {
			try{
				this.elementId = jObject.elementId;
				this.categoryId = jObject.categoryId;
				if(this.elementId == '' || this.categoryId == undefined || this.categoryId == '' || isNaN(parseInt(this.categoryId))){
					jAlert('Error: undefined parameters');
					return null;
				}


				if(jObject.fileSize == undefined || isNaN(jObject.fileSize)){
					fileSize = AllowedFileSize.DEFAULT;
				} else {
					fileSize = jObject.fileSize;
				}
			} catch(e){
				alert(e);
				return null;
			}
		}
		paintBoiler(this.elementId,this.categoryId,function(){
			$(document).ready(function(){
				new AjaxUpload('txtUploadDocument-'+jObject.elementId+'-'+jObject.categoryId+'',{
					action : 'uploadDocumentStudent.json',
					name : 'fileData',
					timeout:60000,
					size:AllowedFileSize.DOCUMENT_UPLOAD,
					data:{},
					onSubmit : function(file, extension) {
						if ($.inArray(extension.toString().toLowerCase(),gExtensionArray) != -1) {
							$('div.preview').addClass('loading');
							this.setData({
								'categoryId':jObject.categoryId,
								'fileSize':fileSize,
							});
							return true;
						} else {
							jAlert('File type is not valid!');
							return false;
						}
					},
					onComplete : function(file, response) {
						try{
							response = eval('('+response+')');
							$('div.preview').removeClass('loading');
							if(response[0].responseFlag == '1'){
								callback(response[1]["DocumentId"]);
							} else {
								throw response[0].responseText;
							}
						} catch (e){
							jAlert(e);
						}
					}
				});
			});
		});



	};

	this.getDocument = function(categoryId,callback){
		if(categoryId == undefined || categoryId == '' || isNaN(parseInt(categoryId))){
			jAlert('Error: define category id');
			return null;
		}		
		try{
			$.ajax({
				url:'getUploadedDocumentAjaxStudent.json?categoryid='+categoryId,
				dataType:'json',
				beforeSend: function() {
				},
				complete: function() {
				},
				async:false,
				success:function(jArray){
					gjArrUploadedDocument=jArray;
					var jsonArray = new Array();
					for(var i = 0; i < jArray.length ; i++) {
						var documentObj = new GemsDocument();
						documentObj.categoryId = jArray[i]['categoryId'];
						documentObj.catName = jArray[i]['catName'];

						documentObj.documentName = jArray[i]['name'];
						documentObj.name = jArray[i]['name'];
						documentObj.hasProfileImage = jArray[i]['hasProfileImage'];
						documentObj.gender = jArray[i]['gender'];
						documentObj.sharedStatus = jArray[i]['sharedStatus'];
						documentObj.documentId = jArray[i]['documentId'];
						documentObj.creationDate = jArray[i]['creationDate'];
						documentObj.ownerEmployeeId = jArray[i]['ownerStudentId'];
						documentObj.extension = jArray[i]['extension'];
						documentObj.type = jArray[i]['extension'];
						documentObj.isTemporary = false;
						documentObj.description = jArray[i]['description'];
						documentObj.documentBody = jArray[i]['documentBody'];
						jsonArray.push(documentObj);
					}
					callback(jsonArray);
				},
				error:function(){

				}

			});
		} catch(e){
			alert(e);
		}

	};

	*//**
	 * student placement id is only required for getting the owner information of the document
	 *//*
	this.getDocumentByStudentPlacementId = function(categoryId,studentPlacementId,callback){
		if(categoryId == undefined || categoryId == '' || isNaN(parseInt(categoryId))){
			jAlert('Error: define category id');
			return null;
		}		
		try{
			$.ajax({
				url:'getUploadedDocumentAjaxStudentForTPO.json?categoryid='+categoryId+'&studentPlacementId='+studentPlacementId,
				dataType:'json',
				beforeSend: function() {
				},
				complete: function() {
				},
				async:false,
				success:function(jArray){
					gjArrUploadedDocument=jArray;
					var jsonArray = new Array();
					for(var i = 0; i < jArray.length ; i++) {
						var documentObj = new GemsDocument();
						documentObj.categoryId = jArray[i]['categoryId'];
						documentObj.catName = jArray[i]['catName'];
						documentObj.documentName = jArray[i]['name'];
						documentObj.gender = jArray[i]['gender'];
						documentObj.sharedStatus = jArray[i]['sharedStatus'];
						documentObj.documentId = jArray[i]['documentId'];
						documentObj.creationDate = jArray[i]['creationDate'];
						documentObj.extension = jArray[i]['extension'];
						documentObj.description = jArray[i]['description'];
						documentObj.documentBody = jArray[i]['documentBody'];
						jsonArray.push(documentObj);
					}
					callback(jsonArray);
				},
				error:function(){

				}

			});
		} catch(e){
			alert(e);
		}

	};

	*//** Showing document as in dms look*//*

	this.showDocument = function(jsonObject,callback) {

		var gFileId = 1;
		if(jsonObject == undefined){
			jAlert('Error: Parameter expected!');
			return; 

		}  else {
			try{
				gjArrTextDocument=jsonObject;

				if(jsonObject.elementId == '' || jsonObject.documentList == undefined){
					if(jsonObject.elementId == ''){
						jAlert('Error: Required parameter "elementId" is mission');
						return null;
					}
					if(jsonObject.documentList == undefined) {
						jAlert('Error: Required parameter "documentJArray" is mission');
						return null;
					}
				} else {
					var link = "";
					for(var i = 0 ; i < jsonObject.documentList.length ; i++){

						var downloadLink = '#';
						var iconPath = 'css/img/add_attachment_icon.png';
						if(jsonObject.documentList[i]["documentBody"] != undefined){
							downloadLink = (juno.label['url.download.json'].setIndexedArgs(jsonObject.documentList[i]["documentId"], "UPLOADED", encodeURIComponent(attachmentFileName)));
						}
						var fileType = jsonObject.documentList[i]["extension"].toLowerCase();
						iconPath = gFileTypeIconMapping[fileType];

						link += '<tr><td  class="ui-state-default" width="40px"><input type = "checkbox" id = "chkFileListIcon-uploaded-'+jsonObject.documentList[i]["documentId"]+'"  class = "chkFileListIcon" name = "chkFileListIcon" ></td><td class="context-menu-one tooltiplink ui-state-default" id = "tdFileListIcon-uploaded-'+jsonObject.documentList[i]["documentPathId"]+'" onmouseover = "showFileDetail(\'tdFileListIcon-uploaded-'+jsonObject.documentList[i]["documentId"]+'\')" style =  "cursor:pointer"><a class="ui-state-default" href = "'+downloadLink+'" ><span><image src = "'+iconPath+'" /></span>&nbsp;&nbsp;'+jsonObject.documentList[i]["documentName"]+'</a></td>';

						if(jsonObject.documentList[i]["sharedStatus"]==1)
						{
							link += '<td width="20px;" class="ui-state-default"><a href="javascript:void(0);" onclick="#" title="view Shared Employees"><img src="img/share.png" alt="Shared" /></a></td>';
						}
						else
						{
							link += '<td width="20px;" class = "ui-state-default"></td>';
						}
						link += '<td valign="top" width="70px">'+
						'<a href="#" class="btn btn-inverse btn-mini  ">Share</a></td>';

						if(jsonObject.documentList[i]["sharedStatus"]==1)
						{
							link += '<td valign="top" width="70px;"><ul class="jbar">'+
							'<li><a href="javascript:void(0);" class="has_down_arrow trigger first" onclick="showComment('+gFileId+','+jsonObject.documentList[i]["documentId"]+',\'uploaded\','+i+')"><span class="link_text">Comment</span></a></li>'+
							'</td></tr>'+
							'<tr id="trshowComment'+gFileId+'" style="display:none;"><td colspan="5"><table style="margin-left:95%"><tr><th align="right"><a href="#" onclick="javascript:$(\'#trshowComment'+gFileId+'\').hide();"><img src="img/remove.png" /></a></th></tr></table>'+
							'<div id="showComment'+gFileId+'">'+
							'<div class="commentPanel" id="commentPanel-first-'+gFileId+'" align="left">'+
							'<div class="commentBox" align="right" id="commentBox-'+gFileId+'">';
							if(jsonObject.documentList[i]["hasProfileImage"] == undefined) {

								if(jsonObject.documentList[i]["ownerGender"] != undefined){
									if(jsonObject.documentList[i]["ownerGender"].toString().toLowerCase() == 'male') {
										link+='<img src="img/boy.gif" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
										gProfileImgPath='img/boy.gif';
									} else if(jsonObject.documentList[i]["ownerGender"].toString().toLowerCase() == 'male') {
										link+='<img src="img/girl.gif" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
										gProfileImgPath='img/girl.gif';
									}
								} else {
									link+='<img src="img/girl.gif" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';
									gProfileImgPath='img/girl.gif';
								}
							} else {

								link += '<img src="getStudentProfileImage.json?id='+jsonObject.documentList[i]["ownerStudentId"]+'" style="float:left; padding-right:6px;" width="37" height="37" border="0" alt="" class="CommentImg" />';

							}
							link+='<input type="hidden" id="owner" value="" />'+
							'<label id="record-">'+
							'<textarea class="commentMark" id="commentMark-first-'+gFileId+'" onblur="if (this.value==\'\') this.value = \'Write a comment\'" onfocus="if (this.value==\'Write a comment\') this.value = \'\'" onKeyPress="return SubmitComment(this,event,\'uploaded\','+jsonObject.documentList[i]["documentId"]+','+gFileId+');" wrap="hard" name="commentMark" style=" background-color:#fff; overflow: hidden;" cols="60">Write a comment</textarea>'+
							'</label>'+
							'<br clear="all" />'+
							'</div>'+
							'</div>'+
							'</div>'+
							'</div></td>';
						}
						else
							link+='<td></td>';

						link+='</tr>'+
						'<tr id="trshowStudentShare'+gFileId+'" style="display:none;"><td colspan="6"><div id="showStudentShare'+gFileId+'"></div></td></tr>'+
						'<tr id="trshowEmployeeShare'+gFileId+'" style="display:none;"><td colspan="6"><div id="showEmployeeShare'+gFileId+'"></div></td></tr>'+
						'<tr id="trshowPrivateComment'+gFileId+'" style="display:none;"><td colspan="6"><div id="showPrivateComment'+gFileId+'"></div></td></tr>';
						gFileId++;
					}
					$('#'+jsonObject.elementId).append(link);
