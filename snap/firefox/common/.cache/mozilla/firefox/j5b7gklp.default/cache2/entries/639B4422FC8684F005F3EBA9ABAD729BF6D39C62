function cleanAttendance()
{
	var sembatchId=$('#cbBatchId').val().split('-')[2];
	//alert("sembatchId=="+sembatchId);

	$.ajax({
		url:'cleanAttendance.json?sembatchId='+sembatchId,
		dataType:'json',
		async:false,
		success:function(jArray){

			jAlert("clean successfully.","Message");


		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});


}





function studentManagementGridResponse(response,postData){
	var jArray = eval('(' + response.responseText + ')');
	//var sr = jQuery("#student-management-grid").getGridParam("selrow");
	var sr=	$("#student-management-grid").jqGrid('getGridParam','selrow');

	if(jArray[0].errorCode == '0'){
		/*var rowdata = jQuery("#student-management-grid").getRowData(sr);
		rowId = rowdata.id;*/
		//alert(growId);
		showSuccessMessage(jArray[0].responseText,10000);
		//alert("studDeptPromotionId==="+jArray[1]["deptPromotionId"]+"==studParentInstituteCourseId==="+jArray[1]["parentInstituteCourseId"]);
		$("#student-management-grid").jqGrid('setCell',growId,'deptPromotionId',jArray[1]["deptPromotionId"]);
		$("#student-management-grid").jqGrid('setCell',growId,'parentInstituteCourseId',jArray[1]["parentInstituteCourseId"]);

		return [true,"",sr];
	} else {
		if(jArray[0].errorCode == '1001'){
			alert(jArray[0].responseText+" Please try with different roll no.");
		}
		return [false,"",sr];
	}
}

