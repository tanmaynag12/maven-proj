				'<tr><td><b>14)Year in which studying and since<br> when: </b></td><td><input type="text" name="yearofStuding1" placeholder="Enter studying year & since when" id="yearofStuding" value="'+yearinstudyingandsince+'"></td></tr>'+
				'<tr><td><b>15)Reasons for leaving junior<br> College : </b></td><td><input type="text" name="reason1" placeholder="Enter reason" id="reason" value="'+reason+'" ></td></tr>'+
				'<tr><td ><b>16)Remarks : </b></td><td><input type="text" name="remarks1" placeholder="Enter remarks" id="remarks" value="'+remarks+'"></td></tr>'+
				'<tr><td ><b>Date Of Declaration: </b></td><td><input type="text" name="declarationDate"  id="declarationDate" value="'+dateOfDeclaration+'"></td></tr>'+
				'<tr><td ><b>Copy Type: </b></td><td><input type="Radio" value="ORIGINAL" name="copyType" class="originalCls">Original<br><input type="Radio" value="DUPLICATE" name="copyType" class="duplicateCls">Duplicate</tr>'+
				'<tr><td ><b>LC Print Date: </b></td><td><input type="text" name="lcPrintdate"  id="lcPrintdate" value="'+lcPrintDate+'"></td></tr>'+
				'</tbody></table></div>'+
				'</div>'+
				'<div class="modal-footer"><div class="row-fluid"><div class="span3"></div>'+
				'<div class="span6"><input type="button" class="btn btn-mini btn-info" style="" value="Save And Print" onclick="saveLeavingCerificateDetails(\''+studId+'\')" >'+
			 	'<input type="button" class="btn btn-mini btn-warning" value="Close" data-dismiss="modal" />'+
			 	'</div><div class="span3"></div></div>'+
			 	'</div>';
				
			$('#leavingcertificateDIV').html(boiler);
			$('#aadharCardNoId').numeric();
			$('#motherTongueId').val(motherTongue);
			$( "input[name='copyType'][value='"+typeofCopy+"']" ).attr("checked",true);

			$("#datefLeaving").datepicker({
				dateFormat:"M dd,yy",
				});
			
			$("#birthDate").datepicker({
				dateFormat:"M dd,yy",
				showWeek: true,
				changeMonth: true,
				changeYear: true,
				yearRange:"-60:+10",
				maxDate:0
			});
			$("#admissionDate").datepicker({
				dateFormat:"M dd,yy",
				showWeek: true,
				changeMonth: true,
				changeYear: true,
				yearRange:"-60:+10",
				maxDate:0
			});
			$("#declarationDate").datepicker({
				dateFormat:"M dd,yy",
				
			});
			$("#lcPrintdate").datepicker({
				dateFormat:"M dd,yy",
				
			});
			
			$('#leavingcertificate').modal('show');	
			getStateRelNationalityCatCityCountryForLC(studId,nationality,religion);
			getCaste(studId,castName);
			
			
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
	
}

function saveLeavingCerificateDetails(studId)
{
			var studentName=$('#pupilNameAsPerTenthMarksheet').val();
			var regNo=$('#regNo').val();
			var studntId=$('#studntId').val();
			var aadharCardNo=$('#aadharCardNoId').val();
			var progress=$('#progress').val();
			var conduct=$('#conduct').val();
			var dateOfLeaving=$('#datefLeaving').val();
			var reason=$('#reason').val();
			var remarks=$('#remarks').val();
			var mo␀␀␀therName=$('#motherName').val();
			var castName=$('#cast').val();
			var SubCast=$('#subCast').val();
			var nationality=$('#nationality').val();
			var motherTongue=$('#motherTongueId').val();
			var religion=$('#studreligion').val();
			var placeofBirth=$('#placeofBirth').val();
			var birthDate=$('#birthDate').val();
			var admissionDate=$('#admissionDate').val();
			var lastSecSchool=$('#lastSecSchool').val();
			var yearinstudyingandsince=$('#yearofStuding').val();
			var dateOfDeclaration=$('#declarationDate').val();
			var placeOfBirthDistrict=$('#placeofBirthDistrict').val();
			var placeOfBirthTaluka=$('#placeofBirthTaluka').val();
			var printType=$("input[name='copyType']:checked").val();
			var lcPrintdate=$('#lcPrintdate').val();
			var religionNameDes = $('#studreligionNew').val();
			
			if(studentName=="")
			{
				jAlert("Please enter the pupil Name");
			}
			else if(regNo=="")
			{
				jAlert("Please enter register number of the pupil");
			}	
			else if(studntId=="")
			{
				jAlert("Please enter the Student ID");
			}
			else if(aadharCardNo=="")
			{
				jAlert("Please enter the Aadhar Card No");
			}	
			else if(progress=="")
			{
				jAlert("Please enter the progress");
			}	
			
			else if(conduct=="")
			{
				jAlert("Please enter the conduct");
			}	
			
			else if(dateOfLeaving=="")
			{
				jAlert("Please select date of leaving");
			}	
			
			else if(reason=="")
			{
				jAlert("Please enter the reason");
			}	
			
			else if(remarks=="")
			{
				jAlert("Please enter the remarks");
			}	
			else if(motherName=="")
			{
				jAlert("Please enter the mother name");
			}	
			else if(religion=="")
			{
				jAlert("Please select religion");
			}	
			/* else if(castName=="")
			{
				jAlert("please select the cast");
			}	*/
			
			/*else if(SubCast=="")
			{
				jAlert("Please enter the sub cast");
			}	*/
			
			else if(nationality=="")
			{	
				jAlert("Please select the nationality");
			}	
			else if(motherTongue=="")
			{
				jAlert("Please select mother tongue");
			}
			else if(placeofBirth=="")
			{
				jAlert("Please enter the place of birth");
			}
			else if(placeOfBirthDistrict=="")
			{
				jAlert("Please enter the place of birth District");
			}
			else if(placeOfBirthTaluka=="")
			{
				jAlert("Please enter the place of birth Taluka");
			}
			else if(birthDate=="")
			{
				jAlert("Please select the date of birth");
			}	
		
			else if(admissionDate=="")
			{
				jAlert("Please select the admission date");
			}	
			
			else if(lastSecSchool=="")
			{
				jAlert("Please enter the last secondary school/junior college attended");
			}	
			else if(yearinstudyingandsince=="")
			{
				jAlert("Please enter year in which studying & since when");
			}
			else if(dateOfDeclaration=="")
			{
				jAlert("Please select date of declaration");
			}	
			else if(printType=="" || printType==undefined)
			{
				jAlert("Please select Type of copy");
			}	
			else
			{	
			$.ajax({ 
				url:'saveLCDetails.json?progress='+progress+'&conduct='+conduct+'&dateOfLeaving='+dateOfLeaving+'&reason='+reason+'&remarks='+remarks+'&studId='+studId+'&regNo='+regNo+'&motherName='+motherName+'&castName='+castName+'&SubCast='+SubCast+'&nationality='+nationality+'&placeofBirth='+placeofBirth+
				'&birthDate='+birthDate+'&admissionDate='+admissionDate+'&lastSecSchool='+lastSecSchool+'&yearinstudyingandsince='+yearinstudyingandsince+'&dateOfDeclaration='+dateOfDeclaration+'&aadharCardNo='+aadharCardNo+'&religion='+religion+'&motherTongue='+motherTongue+'&placeOfBirthDistrict='+placeOfBirthDistrict+
				'&placeOfBirthTaluka='+placeOfBirthTaluka+'&studntId='+studntId+'&studentName='+studentName+'&printType='+printType+'&lcPrintdate='+lcPrintdate+'&religionNameDes='+religionNameDes,
				dataType :'text',
				type:'GET',
				async : false,                 
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					jConfirm("Details saved successfully ! Do you want print Leaving Certificate ?","Confirm",function(ans) {
						if(ans) {
							$('#leavingcertificate').modal('hide');
							generatePDFOfLC(studId,"SAVE");
						}
					});
				},
				error:function(e){
					getCustomAlertMessage("alert_error");
				}
			});

			}
}

function getStateRelNationalityCatCityCountryForLC(studId,nationalityid,religionid) {
	
	$.ajax({
		url : 'getAllStateRelNationalityCatCityCountry.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			// AllNationalities();
			var html3 = '<option value="0">-</option>';
			var len = data[2].length;
			for ( var i = 0; i < len; i++)
				html3 += '<option value="' + data[2][i]["nationalityId"] + '">'
				+ data[2][i]["nationalityName"] + '</option>';

			$('#nationality').html(html3);
			if(nationalityid !=undefined)
				$('#nationality').val(nationalityid);
			
			var html2 = '<option value="0">Select</option>';
			var len = data[1].length;
			for ( var i = 0; i < len; i++)
				html2 += '<option value="' + data[1][i]["relId"] + '">'
				+ data[1][i]["relName"] + '</option>';

			$('#studreligion').html(html2);
			if(religionid !=undefined)
				$('#studreligion').val(religionid);
			
			//getCaste(studId,cast);
		},
	});
}
function getCaste(studId,castId)
{
	$.ajax({
		url:'getAllCaste.json', 
		dataType:'json',
		async:false,
		success:function(jArray){
			var html = '<option value="0">Select</option>';
			var len = jArray.length;
			for(var i=0; i<len; i++){
				html += '<option value="'+ jArray[i]["casteId"] + '">'+ jArray[i]["casteName"] + '</option>';
				casteListForSearching.push({
					name : jArray[i]["casteName"],
					to : jArray[i]["casteName"]
				});
			}
		

setTimeout(function() {
	$('#cast').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {
					$("#cast").autocomplete(casteListForSearching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("+ term + ")", "gi"),"<strong>$1</strong>");
								},
								formatResult : function(row) {
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {

				}

			});
}, 1000);
		},

	});	
	//jAlert("ERROR");
}		


function generatePDFOfLC(studId,callFrom)
{
	var customerIdentificationObj = $('#customerHelper').val();
	
	
	var studentIdArray = new Array();
	if(callFrom=="SAVE")
	{
		studentIdArray.push(studId);
	}
	else
	{
		$('.selectedStudent:checked').each(function(){
			studentIdArray.push($(this).val());	
		});
	}
	if(studentIdArray.length > 0)
	{
		if(customerIdentificationObj !='DYPATIL')
		{
			window.open('printPDFforGenerateLeavingCertificate.json?studentIdArray='+studentIdArray);
		}
		else
		{
			window.open('printPDFforGenerateTransferCertificateForMultipleStudent.json?studentIdArray='+studentIdArray);
		}
		setTimeout(function(){
		getPrintlcCount(studentIdArray);
		}, 10000);
	}	
	else
	jAlert("please select at least one student");	
		
}
function selectAllChk()
{
	 if($('.selectAllChk').is(':checked'))
	  {
		$('.selectedStudent').attr('checked',true); 
	  }	
	  else if(!$('.selectAllChk').is(':checked'))
		  {
		   $('.selectedStudent').attr('checked',false);  
		  }
}
function getAllStudentList()
{
	var studentIdArrayForDate = new Array();
	$('.selectedStudent:checked').each(function(){
		studentIdArrayForDate.push($(this).val());	
	});
	var dateOfLeavingJuniorClg=$('#dateofLeavingJu').val();
	
	$.ajax({
		url:'savedateofleavingJunioClg.json',
		dataType:'text',
		data:{studentIdArrayForDate:studentIdArrayForDate.toString(),dateOfLeavingJuniorClg:dateOfLeavingJuniorClg.toString()},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function()
		{
			getCustomAlertMessage("alert_create");
		},
		error:function(){
			getCustomAlertMessage("alert_error");
			
		}
	});
}
function getDeclarationDateForAllStud()
{
	var studentIdArrayForDeclaDate = new Array();
	$('.selectedStudent:checked').each(function(){
		studentIdArrayForDeclaDate.push($(this).val());	
	});
	var dateOfDeclaration=$('#dateofdeclaration').val();
	
	$.ajax({
		url:'saveDeclarationdateofleavingJunioClg.json',
		dataType:'text',
		data:{studentIdArrayForDeclaDate:studentIdArrayForDeclaDate.toString(),dateOfDeclaration:dateOfDeclaration.toString()},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function()
		{
			getCustomAlertMessage("alert_create");

		},
		error:function(){
			getCustomAlertMessage("alert_error");

		}
	});
}
//////Transfer Certificate
function getTransferCertificate(studId)
{
	//var currentdate=new Date();
///	var curYear=currentdate.getFullYear();
	$.ajax({
		url :'GenerateTransferCertificate.json?studId='+studId,
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jsonObj) {
			var fullName='';
			var degreeName='';
			var PassYear='';
			var Examination='';
			var ExamYear='';
			var passFail='';
			var juneYear='';
			var octYear='';
			var novYear='';
			var marchYear='';
			var stdClass='';
			var dob='';
			var mediumOfInstForCourse='';
			
			if(jsonObj.fullName!=undefined)
			{
				fullName=jsonObj.fullName;
			 
			}
			if(jsonObj.degreeName!=undefined)
			{
				degreeName=jsonObj.degreeName;
			}
			if(jsonObj.PassYear !=undefined)
			{
				PassYear=jsonObj.PassYear;
			}
			/*if(jsonObj.juneYear !=undefined)
			{
				juneYear=jsonObj.juneYear;
			}
			if(jsonObj.octYear !=undefined)
			{
				octYear=jsonObj.octYear;
			}
			if(jsonObj.novYear !=undefined)
			{
				novYear=jsonObj.novYear;
			}
			if(jsonObj.marchYear !=undefined)
			{
				marchYear=jsonObj.marchYear;
			}*/
			if(jsonObj.Examination !=undefined)
			{
				Examination=jsonObj.Examination;
			}
			if(jsonObj.ExamYear !=undefined)
			{
				ExamYear=jsonObj.ExamYear;
			}
			if(jsonObj.passFail !=undefined)
			{
				passFail=jsonObj.passFail;
			}
			if(jsonObj.stdClass !=undefined)
			{
				stdClass=jsonObj.stdClass;
			}
			if(jsonObj.dob !=undefined)
			{
				dob=jsonObj.dob;
			}
			if(jsonObj.mediumOfInstForCourse !=undefined)
			{
				mediumOfInstForCourse=jsonObj.mediumOfInstForCourse;
			}
				var boiler='';
				boiler+='<div id = "transferCrtificate" class = "modal fade hide">'+
				'<div class="modal-header">Transfer Certificate: '+
				'<button type="button" class="close" data-dismiss="modal">x</button>'+
			  	'</div>'+
			  	
			 	'<div class="modal-body" >'+
				'<div><table class="table table-bordered"><tbody>'+
				
				'<tr><td><b>Student Name in full : </b></td><td><input type="text" name="Namestud" readonly="readonly" placeholder="Enter student name " id="studName" value="'+fullName+'"></td></tr>'+
				'<tr><td><b>Course Name : </b></td><td><input type="text" name="Namecourse" placeholder="Enter course name" id="courseName" value="'+degreeName+'" ></td></tr>'+
				'<tr><td><b>Pass/Fail Year : </b></td><td><input type="text" name="Nameyear" id="yearId" value="'+PassYear+'" ></td></tr>'+
				//'<tr><td><b>Last Term From -June : </b></td><td><input type="text" name="NameJune" placeholder="Enter june year" id="juneYear" value="'+juneYear+'" ></td></tr>'+
				//'<tr><td><b>Last Term To-October : </b></td><td><input type="text" name="NameOctober" placeholder="Enter october year" id="octoberYear" value="'+octYear+'" ></td></tr>'+
				//'<tr><td><b>Last Term From-November : </b></td><td><input type="text" name="NameNovember" placeholder="Enter november year" id="NovemberYear" value="'+novYear+'" ></td></tr>'+
				//'<tr><td><b>Last Term To-March : </b></td><td><input type="text" name="NameMarch" placeholder="Enter march year" id="marchYear" value="'+marchYear+'" ></td></tr>'+
				'<tr><td><b>Program Name : </b></td><td><input type="text" name="NameExamin" placeholder="Enter program" id="examination" value="'+Examination+'" ></td></tr>'+
				'<tr><td><b> Passing Year : </b></td><td><input type="text" name="year" id="examYear" value="'+ExamYear+'" ></td></tr>'+
				'<tr><td><b>Result Passed/Failed/Grade : </b></td><td><input type="text" name="passFail" id="passfail" value="'+passFail+'" ></td></tr>'+
				'<tr><td><b>Class : </b></td><td><input type="text" name="class" id="stdCls" value="'+stdClass+'" ></td></tr>'+
				'<tr><td><b>DOB: </b></td><td><input type="text" name="Namedob" id="dobId" value="'+dob+'" ></td></tr>'+
				'<tr><td><b>Medium Of Instruction For Course : </b></td><td><input type="text" placeholder="Enter Medium of Instruction for Course was"  name="mediInst" id="Medium" value="'+mediumOfInstForCourse+'" ></td></tr>'+
				'</tbody></table></div>'+
				'</div>'+
				'<div class="modal-footer"><div class="row-fluid"><div class="span3"></div>'+
				'<div class="span6"><input type="button" class="btn btn-mini btn-info" style="" value="Save And Print" onclick="saveTransferCertificateInfo(\''+studId+'\')" >'+
			 	'<input type="button" class="btn btn-mini btn-warning" value="Close" data-dismiss="modal" />'+
			 	'</div><div class="span3"></div></div>'+
			 	'</div>';
				
			$('#transfercertificateDIV').html(boiler);
			
			$('#yearId').datepicker({
				changeMonth: true,
			    changeYear: true,
			    showButtonPanel: true,
			    dateFormat: 'MM yy',
			});
			
			$("#juneYear").datepicker({
				changeMonth: true,
			    changeYear: true,
			    showButtonPanel: true,
			    dateFormat: 'MM yy',
			});
			
			$('#octoberYear').datepicker({
				changeMonth: true,
			    changeYear: true,
			    showButtonPanel: true,
			    dateFormat: 'MM yy',
			});
			$('#NovemberYear').datepicker({
				changeMonth: true,
			    changeYear: true,
			    showButtonPanel: true,
			    dateFormat: 'MM yy',
			});
			$('#marchYear').datepicker({
				changeMonth: true,
			    changeYear: true,
			    showButtonPanel: true,
			    dateFormat: 'MM yy',
			});
			$('#examYear').datepicker({
				dateFormat:"M dd,yy",
				showWeek: true,
				changeMonth: true,
				changeYear: true,
				yearRange:"-60:+10",
				maxDate:0
				});
			$('#dobId').datepicker({
			dateFormat:"M dd,yy",
			showWeek: true,
			changeMonth: true,
			changeYear: true,
			yearRange:"-60:+10",
			maxDate:0
			});
			
			$('#transferCrtificate').modal('show');	
			
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
	
	
}
function saveTransferCertificateInfo(studId)
{
			var studName=$('#studName').val();
			var courseName=$('#courseName').val();
			var passYear=$('#yearId').val();
			var juneYear=$('#juneYear').val();
			var octoberYear=$('#octoberYear').val();
			var NovemberYear=$('#NovemberYear').val();
			var marchYear=$('#marchYear').val();
			var examination=$('#examination').val();
			var examYear=$('#examYear').val();
			var passfail=$('#passfail').val();
			var stdCls=$('#stdCls').val();
			var dobId=$('#dobId').val();
			var Medium=$('#Medium').val();
			/*
			if(studName=="")
			{
				jAlert("Please enter Student Name");
			}	
			
			else if(courseName=="")
			{
				jAlert("Please enter Course Name");
			}	
			
				*/
			
			if(passYear=="")
			{
				jAlert("Please select the Passing Year");
			}
			/*else if(juneYear=="")
			{
				jAlert("Please select june Year");
			}	
			else if(octoberYear=="")
			{
				jAlert("Please select october Year");
			}	
			else if(NovemberYear=="")
			{
				jAlert("Please select november Year");
			}	
			else if(marchYear=="")
			{
				jAlert("Please select march Year");
			}	*/
			else if(examination=="")
			{
				jAlert("Please enter examination");
			}	
			else if(examYear=="")
			{
				jAlert("Please select exam Year");
			}	
			else if(passfail=="")
			{
				jAlert("Please enter passed/Failed Result");
			}	
			
			else if(stdCls=="")
			{
				jAlert("Please enter Std Class");
			}
			else if(dobId=="")
			{
				jAlert("Please select Dob");
			}
			else if(Medium=="")
			{
				jAlert("Please enter Medium Of Instruction for Course was");
			}
			
			else
			{	
			$.ajax({ 
				url:'saveTransferenceCertificateDetails.json?studId='+studId+'&studName='+studName+'&courseName='+courseName+'&passYear='+passYear+'&juneYear='+juneYear+'&octoberYear='+octoberYear+'&NovemberYear='+NovemberYear+
				'&marchYear='+marchYear+'&examination='+examination+'&examYear='+examYear+'&passfail='+passfail+'&stdCls='+stdCls+'&dobId='+dobId+'&Medium='+Medium,
				dataType :'text',
				type:'GET',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					//jConfirm("Details saved successfully ! Do you want print Transfer Certificate ?","Confirm",function(ans) {
						//if(ans) {
							$('#transferCrtificate').modal('hide');
							printTransferCerificateDetails(studId,"SAVE");
						//}
					//});
				},
				error:function(e){
					getCustomAlertMessage("alert_error");
				}
			});

			}
}
function printTransferCerificateDetails(studId,callFromNew)
{
	var CourseName=$('#cbInstiCourseId option:selected').text();
	var customerIdentificationObj = $('#customerHelper').val();
	
	
	var studentIdArray = new Array();
	if(callFromNew=="SAVE")
	{
		studentIdArray.push(studId);
	}
	else
	{
		$('.selectedStudent:checked').each(function(){
			studentIdArray.push($(this).val());	
		});
	}
	if(studentIdArray.length > 0)
	{
		if(customerIdentificationObj != 'DYPATIL'){
			window.open('printPDFforGenerateTransferCertificate.json?studentIdArray='+studentIdArray.toString()+"&CourseName="+CourseName);
		}
		else
		{
			window.open('printPDFforGenerateTransferCertificateForMultipleStudent.json?studentIdArray='+studentIdArray.toString()+"&CourseName="+CourseName);
		}
		
	}	
	else
	jAlert("please select at least one student");	
	
	
}
//Indira Transfer Certificate
var casteListForSearching = new Array();
function getLeavingCertificateOfIndira(studId)
{
	$.ajax({
		url :'GenerateTransferCertificateOfIndira.json?studId='+studId,
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jsonObj) {
			var tenthName='';
			var studFullName='';
			
			var motherName='';
			var motherMiddleName='';
			var motherLastName='';
			
			var fatherFirstName='';
			var fatherMiddleName='';
			var fatherLastName='';
			
			var guardianFirstName='';
			var guardianMiddleName='';
			var guardianLastName='';
			
			var nationality='';
			var placeofBirth='';
			var scheduleCasteTribe='';
			var admissionDate='';
			var dob='';
			var lastStudiedClass='';
			var lastStudiedClassWords='';
			var annualExamination='';
			var failedOncesTwice='';
			var promotionToHigherClass='';
			var promotionToHigherClassWords='';
			var monthOfSchoolDues='';
			var feeConcessionAvailed='';
			var totalNOOfWorkingDays='';
			var totalNOOfWorkingDaysPresent='';
			var nccCadetBoyScoutGirl='';
			var gamesExtraCurricularAct='';
			var conduct='';
			var applicationDateOfCertificate='';
			var dateOfIssueOfCerti='';
			var reason='';
			var remarks='';
			var aadharCardNo='';
			var previousSchool='';
			var grBookNo='';
			var motherTongue='';
			var religion='';
			var castName='';
			var SubCast='';
			//var schoolApprovalNo='';
			//var udiseNo='';
	
			if(jsonObj.tenthName!=undefined && jsonObj.tenthName!="")
			{
				tenthName=jsonObj.tenthName;
			}else{
				tenthName=jsonObj.fullName;
			}
			
			if(jsonObj.motherName!=undefined)
			{
				motherName=jsonObj.motherName;
			}
			if(jsonObj.motherMiddleName!=undefined)
			{
				motherMiddleName=jsonObj.motherMiddleName;
			}
			if(jsonObj.motherLastName!=undefined)
			{
				motherLastName=jsonObj.motherLastName;
			}
			
			
			if(jsonObj.fatherFirstName!=undefined)
			{
				fatherFirstName=jsonObj.fatherFirstName;
			}
			if(jsonObj.fatherMiddleName!=undefined)
			{
				fatherMiddleName=jsonObj.fatherMiddleName;
			}
			if(jsonObj.fatherLastName!=undefined)
			{
				fatherLastName=jsonObj.fatherLastName;
			}
			
			if(jsonObj.guardianFirstName!=undefined)
			{
				guardianFirstName=jsonObj.guardianFirstName;
			}
			if(jsonObj.guardianMiddleName!=undefined)
			{
				guardianMiddleName=jsonObj.guardianMiddleName;
			}
			if(jsonObj.guardianLastName!=undefined)
			{
				guardianLastName=jsonObj.guardianLastName;
			}
			
			if(jsonObj.nationality!=undefined)
			{
				nationality=jsonObj.nationality;
			 
			}
			if(jsonObj.placeofBirth!=undefined)
			{
				placeofBirth=jsonObj.placeofBirth;
			}
			if(jsonObj.scheduleCasteTribe!=undefined)
			{
				scheduleCasteTribe=jsonObj.scheduleCasteTribe;
			}
			if(jsonObj.admissionDate !=undefined)
			{
				admissionDate=jsonObj.admissionDate;
			}
			if(jsonObj.dob !=undefined)
			{
				dob=jsonObj.dob;
			}
			if(jsonObj.lastStudiedClass !=undefined)
			{
				lastStudiedClass=jsonObj.lastStudiedClass;
			}
			if(jsonObj.lastStudiedClassWords !=undefined)
			{
				lastStudiedClassWords=jsonObj.lastStudiedClassWords;
			}
			if(jsonObj.annualExamination !=undefined)
			{
				annualExamination=jsonObj.annualExamination;
			}
			if(jsonObj.failedOncesTwice !=undefined)
			{
				failedOncesTwice=jsonObj.failedOncesTwice;
			}
			if(jsonObj.promotionToHigherClass !=undefined)
			{
				promotionToHigherClass=jsonObj.promotionToHigherClass;
			}
			if(jsonObj.promotionToHigherClassWords !=undefined)
			{
				promotionToHigherClassWords=jsonObj.promotionToHigherClassWords;
			}
			if(jsonObj.monthOfSchoolDues !=undefined)
			{
				monthOfSchoolDues=jsonObj.monthOfSchoolDues;
			}
			if(jsonObj.feeConcessionAvailed !=undefined)
			{
				feeConcessionAvailed=jsonObj.feeConcessionAvailed;
			}
			if(jsonObj.totalNOOfWorkingDays !=undefined)
			{
				totalNOOfWorkingDays=jsonObj.totalNOOfWorkingDays;
			}
			if(jsonObj.totalNOOfWorkingDaysPresent !=undefined)
			{
				totalNOOfWorkingDaysPresent=jsonObj.totalNOOfWorkingDaysPresent;
			}
			
			if(jsonObj.nccCadetBoyScoutGirl !=undefined)
			{
				nccCadetBoyScoutGirl=jsonObj.nccCadetBoyScoutGirl;
			}
			if(jsonObj.gamesExtraCurricularAct !=undefined)
			{
				gamesExtraCurricularAct=jsonObj.gamesExtraCurricularAct;
			}
			if(jsonObj.conduct !=undefined)
			{
				conduct=jsonObj.conduct;
			}
			if(jsonObj.applicationDateOfCertificate !=undefined)
			{
				applicationDateOfCertificate=jsonObj.applicationDateOfCertificate;
			}
			if(jsonObj.dateOfIssueOfCerti !=undefined)
			{
				dateOfIssueOfCerti=jsonObj.dateOfIssueOfCerti;
			}
			if(jsonObj.reason !=undefined)
			{
				reason=jsonObj.reason;
			}
			if(jsonObj.remarks !=undefined)
			{
				remarks=jsonObj.remarks;
			}
			if(jsonObj.aadharCardNo !=undefined)
			{
				aadharCardNo=jsonObj.aadharCardNo;
			}
			/*if(jsonObj.schoolApprovalNo !=undefined)
			{
				schoolApprovalNo=jsonObj.schoolApprovalNo;
			}
			if(jsonObj.udiseNo !=undefined)
			{
				udiseNo=jsonObj.udiseNo;
			}*/
			if(jsonObj.previousSchool!=undefined)
			{
				previousSchool=jsonObj.previousSchool;
			}
			if(jsonObj.grBookNo!=undefined)
			{
				grBookNo=jsonObj.grBookNo;
			}
			if(jsonObj.motherTongue!=undefined)
			{
				motherTongue=jsonObj.motherTongue;
			}
			if(jsonObj.religion!=undefined)
			{
				religion=jsonObj.religion;
			}
			if(jsonObj.castName!=undefined)
			{
				castName=jsonObj.castName;
			}
			if(jsonObj.SubCast!=undefined)
			{
				SubCast=jsonObj.SubCast;
			}
			
				var boiler='';
				boiler+='<div id = "transferCertificateOfIndira" class = "modal fade hide">'+
				'<div class="modal-header">Transfer Certificate: '+
				'<button type="button" class="close" data-dismiss="modal">x</button>'+
			  	'</div>'+
			  	
			 	'<div class="modal-body" >'+
				'<div><table class="table table-bordered"><tbody>'+
			
				'<tr><td><b>1)Name of the pupil: </b></td><td><input type="text" placeholder="" id="pupilNameInd" value="'+tenthName+'"></td></tr>'+
				
				'<tr><td><b>2)Mother'+"'"+'s Name/Father'+"'"+'s Name/Guardian'+"'"+'s Name : </b></td><td><input type="text" placeholder="Enter Mother First Name" id="motherNameInd" value="'+motherName+'" ></td></tr>'+
				'<tr><td><b>Mother Middle Name : </b></td><td><input type="text" placeholder="Enter Mother Middle Name" id="motherMiddleNameInd" value="'+motherMiddleName+'" ></td></tr>'+
				'<tr><td><b>Mother Last Name : </b></td><td><input type="text" placeholder="Enter Mother Last Name" id="motherLastNameInd" value="'+motherLastName+'" ></td></tr>'+
				
				'<tr><td><b>Father First Name : </b></td><td><input type="text" placeholder="Enter Father First Name" id="fatherFirstNameInd" value="'+fatherFirstName+'" ></td></tr>'+
				'<tr><td><b>Father Middle Name : </b></td><td><input type="text" placeholder="Enter Father Middle Name" id="fatherMiddleNameInd" value="'+fatherMiddleName+'" ></td></tr>'+
				'<tr><td><b>Father Last Name : </b></td><td><input type="text" placeholder="Enter Father Last Name" id="fatherNameLastNameInd" value="'+fatherLastName+'" ></td></tr>'+
				
				'<tr><td><b>Guardian First Name : </b></td><td><input type="text" placeholder="Enter Guardian First Name" id="guardianFirstNameInd" value="'+guardianFirstName+'" ></td></tr>'+
				'<tr><td><b>Guardian Middle Name : </b></td><td><input type="text" placeholder="Enter Guardian Middle Name" id="guardianMiddleNameInd" value="'+guardianMiddleName+'" ></td></tr>'+
				'<tr><td><b>Guardian Last Name : </b></td><td><input type="text" placeholder="Enter Guardian Last Name" id="guardianNameLastNameInd" value="'+guardianLastName+'" ></td></tr>'+
				
				'<tr><td><b>3)Nationality : </b></td><td><select  placeholder="" id="nationalityInd" ><option value="0" label="Select" /></select></td></tr>'+
				'<tr><td><b>4)Aadhar Card No : </b></td><td><input type="text" id="aadharCardNoInd" value="'+aadharCardNo+'" maxlength="12"></td></tr>'+
				//'<tr><td><b>5)School Approval No : </b></td><td><input type="text" id="schoolApprovalNoInd" value="'+schoolApprovalNo+'"></td></tr>'+
				//'<tr><td><b>6)UDISE No : </b></td><td><input type="text" id="udiseNoInd" value="'+udiseNo+'"></td></tr>'+
				
				'<tr><td><b>5)Place of birth: </b></td><td><input type="text" placeholder="" id="placeofBirthInd" value="'+placeofBirth+'"></td></tr>'+
				//'<tr><td><b>6)Whether the candidate belongs to Schedule Caste or Schedule Tribe: </b></td><td><input type="text" placeholder="" id="scheduleCasteTribeInd" value="'+scheduleCasteTribe+'"></td></tr>'+
				'<tr><td><b>6)Religion : </b></td><td><select name="religion1" placeholder="Enter Religion" id="studreligion" data="'+religion+'" ><option value="0" label="Select" /></select></td></tr>'+
				'<tr><td><b>7)Caste and Sub-Caste: </b></td><td><input type="text" placeholder="Enter caste" id="cast" value="'+castName+'"  class="validate[required,custom[onlyLetter]] text-input" /><br><br><input type="text" name="castAndSubCast1" placeholder="Enter subcaste " id="subCast"  value="'+SubCast+'" ></td></tr>'+
				
				'<tr><td><b>8)Date of first admission in the school with class & std : </b></td><td><input type="text" placeholder="" id="admissionDateInd" value="'+admissionDate+'" readonly></td></tr>'+
				'<tr><td><b>9)Date of birth<br>according to Admission register: </b></td><td><input type="text" placeholder="" id="birthDateInd" value="'+dob+'" readonly></td></tr>'+
				'<tr><td><b>10)Privious School: </b></td><td><input type="text" placeholder="" id="previousSchoolId" value="'+previousSchool+'"></td></tr>'+
				
				'<tr><td><b>11)Class in which the pupil last studied(in figure): </b></td><td><input type="text" id="pupilClassLastStudInd" value="'+lastStudiedClass+'">'+
				'<br><br><input type="text" placeholder="Enter class in words" id="LastStudClassWordsInd" value="'+lastStudiedClassWords+'" onfocus="istextOnly(event)"></td></tr>'+
				
				'<tr><td><b>12)School / Board Annual examination last taken with result: </b></td><td><input type="text"  placeholder="" id="annualExamInd" value="'+annualExamination+'"></td></tr>'+
				'<tr><td><b>13)Whether failed, if so once / twice in the same class: </b></td><td><input type="text" placeholder="" id="whetherFailedInd" value="'+failedOncesTwice+'"></td></tr>'+
				'<tr><td><b>14)Whether qualified for promotion to the higher class if so,to which class(in figure.): </b></td><td><input type="text"  id="promoForHighClassInd" value="'+promotionToHigherClass+'">'+
				'<br><br><input type="text" placeholder="Enter class in words" id="promHighClassWordsInd" value="'+promotionToHigherClassWords+'" onfocus="istextOnly(event)"></td></tr>'+
				
				'<tr><td><b>15)Month upto which the <br>(pupil has said)school dues: </b></td><td><input type="text" placeholder="" id="schoolDuesMonthInd" value="'+monthOfSchoolDues+'" readonly></td></tr>'+
				'<tr><td><b>16)Any fee concession availed of:if so,the nature of such concession : </b></td><td><input type="text" placeholder="" id="feeConcessionAvailedInd" value="'+feeConcessionAvailed+'"></td></tr>'+
				'<tr><td><b>17)Total No. of working days: </b></td><td><input type="text" placeholder="" id="noOfWorkDayInd" value="'+totalNOOfWorkingDays+'"></td></tr>'+
				'<tr><td><b>18)Total No. of working days present: </b></td><td><input type="text" placeholder="" id="noOfWorkDayPresentInd" value="'+totalNOOfWorkingDaysPresent+'"></td></tr>'+
				'<tr><td><b>19)Whether NCC Cadet/Boy Scout/Girl Guide(details may be given): </b></td><td><input type="text" placeholder="" id="NCCGuideInd" value="'+nccCadetBoyScoutGirl+'"></td></tr>'+
				'<tr><td><b>20)Games played of extra-curricular activities in which the pupil usually took part(mention achievement level there in): </b></td><td><input type="text" placeholder="" id="extraCurrActInd" value="'+gamesExtraCurricularAct+'"></td></tr>'+
				'<tr><td><b>21)General Conduct: </b></td><td><input type="text" placeholder="" id="genConductInd" value="'+conduct+'"></td></tr>'+
				'<tr><td><b>22)Date of application for certificate: </b></td><td><input type="text" placeholder="" id="appliDateInd" value="'+applicationDateOfCertificate+'" readonly></td></tr>'+
				'<tr><td><b>23)Date of issue of certificate: </b></td><td><input type="text" placeholder="" id="dateOfIssueInd" value="'+dateOfIssueOfCerti+'" readonly></td></tr>'+
				'<tr><td><b>24)Reasons for leaving the school: </b></td><td><input type="text" placeholder="" id="reasonofLeavSchoolInd" value="'+reason+'"></td></tr>'+
				'<tr><td><b>25)Any others remark: </b></td><td><input type="text" placeholder="" id="otherRemarkInd" value="'+remarks+'"></td></tr>'+
				'<tr><td><b>26)Gr Book No: </b></td><td><input type="text" placeholder="" id="grBookNoId" value="'+grBookNo+'"></td></tr>'+
				'<tr><td><b>27)Mother Tounge : </b></td><td><select name="motherTongue1" placeholder="Enter Mother Tongue" id="motherTongueId" data="'+motherTongue+'" >'+
				'<option value="0">Select</option>'+
				'<option value="Assamese (Asamiya)">Assamese(Asamiya)</option>'+
				'<option value="Bengali">Bengali</option>'+
				'<option value="Bodo">Bodo</option>'+
				'<option value="Dogri">Dogri</option>'+
				'<option value="English">English</option>'+
				'<option value="French">French</option>'+
				'<option value="Gujarati">Gujarati</option>'+
				'<option value="Hindi">Hindi</option>'+
				'<option value="Kannada">Kannada</option>'+
				'<option value="Kashmiri">Kashmiri</option>'+
				'<option value="Kokborok">Kokborok</option>'+
