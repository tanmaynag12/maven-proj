	$('#editStudentRecordDialog').find('#firstName').val(studObj.firstName);
	$('#editStudentRecordDialog').find('#middleName').val(studObj.middleName);
	$('#editStudentRecordDialog').find('#lastName').val(studObj.lastName);
	$('#editStudentRecordDialog').find('#gender').val(studObj.gender);
	$('#editStudentRecordDialog').find('#phoneNo').val(studObj.phoneNo);
	$('#editStudentRecordDialog').find('#rollNo').val(studObj.rollNo);
	$('#editStudentRecordDialog').find('#subbatch').val(studObj.subbatch);
	$('#editStudentRecordDialog').find('#studentIdSrvr').val(studObj.hdnStudentIdClnt);
	$('#editStudentRecordDialog').find('#hdnStudentBatchIdSrvr').val(studObj.hdnStudentBatchIdClnt);
	$('#editStudentRecordDialog').find('#majorOpenElectiveSubjectId').val(studObj.majorOpenElectiveSubjectId);
	$('#editStudentRecordDialog').find('#minorOpenElectiveSubjectId').val(studObj.minorOpenElectiveSubjectId);
	var deptPromotionId = studObj.deptPromotionId;
	var parentInstituteCourseId = studObj.parentInstituteCourseId;
	var speliId = studObj.speli;

	var name = $(trObj).find('.name').text();
	$('#editStudentRecordDialog').find('.studentName').text(name);
	$('#editStudentRecordDialog').find('#speli').html(gSpecilizationSelectBoiler);
	$('#editStudentRecordDialog').find('#parentDepartmentId').html(gDepartmentListSelectBoiler);


	$('#editStudentRecordDialog').find('#speli').val(speliId);
	$('#editStudentRecordDialog').find('#parentDepartmentId').val(parentInstituteCourseId+'-'+deptPromotionId);
	$('#editStudentRecordDialog').find('#admissionDate').datepicker({
		dateFormat:"M dd, yy",
	});
	$('#editStudentRecordDialog').modal('show');





	$('#editStudentRecordDialog').find('li').removeClass('active');
	$('#editStudentRecordDialog').find('li').first().addClass('active');

	$('#editStudentRecordDialog').find('.tab-content').find('div.tab-pane').removeClass('active');
	$('#editStudentRecordDialog').find('.tab-content').find('div.tab-pane').first().addClass('active');

	$('.btnStudentEdit').show();
	$('.btnEditUserName').hide();

	$('.btnStudentEdit').unbind();
	$('.btnStudentEdit').bind('click',function(){
		doEditStudentRecord(trObj);
	});

	if($('#customerIdentificationObj').val() == 'IMT_NAGPUR' || $('#customerIdentificationObj').val() == 'IMT_DUBAI'){
		   $('.btnStudentEdit').show();
		   $('.btnEditUserName').hide();
		   $('.btn-createUser').hide();
	}
	
	if($('#customerIdentificationObj').val() == 'IMT_NAGPUR' || $('#customerIdentificationObj').val() == 'IMT_DUBAI'){
