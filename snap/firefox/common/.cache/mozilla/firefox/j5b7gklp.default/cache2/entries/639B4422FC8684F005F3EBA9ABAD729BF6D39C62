function cleanAttendance()
{
	var sembatchId=$('#cbBatchId').val().split('-')[2];
	//alert("sembatchId=="+sembatchId);

	$.ajax({
		url:'cleanAttendance.json?sembatchId='+sembatchId,
		dataType:'json',
		async:false,
		success:function(jArray){

			jAlert("clean successfully.","Message");


		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});


}





function studentManagementGridResponse(response,postData){
	var jArray = eval('(' + response.responseText + ')');
	//var sr = jQuery("#student-management-grid").getGridParam("selrow");
	var sr=	$("#student-management-grid").jqGrid('getGridParam','selrow');

	if(jArray[0].errorCode == '0'){
		/*var rowdata = jQuery("#student-management-grid").getRowData(sr);
		rowId = rowdata.id;*/
		//alert(growId);
		showSuccessMessage(jArray[0].responseText,10000);
		//alert("studDeptPromotionId==="+jArray[1]["deptPromotionId"]+"==studParentInstituteCourseId==="+jArray[1]["parentInstituteCourseId"]);
		$("#student-management-grid").jqGrid('setCell',growId,'deptPromotionId',jArray[1]["deptPromotionId"]);
		$("#student-management-grid").jqGrid('setCell',growId,'parentInstituteCourseId',jArray[1]["parentInstituteCourseId"]);

		return [true,"",sr];
	} else {
		if(jArray[0].errorCode == '1001'){
			alert(jArray[0].responseText+" Please try with different roll no.");
		}
		return [false,"",sr];
	}
}

function openAddStudentDialog(){
	$('#frmAddStudent').reset();
	$('#dialogAddStudent').modal('show');
}

function closeAddStudentDialog(){
	$('#dialogAddStudent').modal('hide');
}

function showNRITR()
{
	if($("#chkNRI").is(":checked")){
		$('#NRITD1').show();
		$('#NRITD2').show();
		$('#NRITD3').show();
	}
	else
	{
		$('#NRITD1').hide();
		$('#NRITD2').hide();
		$('#NRITD3').hide();
	}
}

function deleteStudent(studentBatchIds){
	//alert(studentBatchIds);
	try{
		$.ajax({
			url:'deleteStudent.json',
			data:'studentBatchId='+studentBatchIds,
			dataType:'json',
			type:'POST',
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success:function(responseObj){
				var flag= responseObj.responseFlag;
				if(flag == "1"){
					showStudentListByCourseSemeserAndDivision();
					jAlert('Student(s) deleted successfully',20000);
				} else {
					var notDeletedStudents = responseObj.notDeletedStudentArray;
					var boiler = '<div class="alert alert-error">';

					if(studentBatchIds.length != responseObj.unDeleteCount){
						boiler +=
							'<u><strong>Following '+responseObj.unDeleteCount+' student(s) could NOT be deleted due to specified reasons:-</strong></u><br>';
					} else {
						boiler += '<u><strong>Deletion failed!!! Following '+responseObj.unDeleteCount+' student(s) could NOT be deleted due to specified reasons:-</strong></u><br>';
					}
					for(var i = 0 ; i < notDeletedStudents.length ; i++){
						boiler += '&nbsp;&nbsp;<i class="icon-hand-right"></i>&nbsp;Roll: <b>'+notDeletedStudents[i].rollNo+'</b>&nbsp;&nbsp;Name: <b>'+notDeletedStudents[i].name+'</b>&nbsp;&nbsp;<b>Reason:-</b>&nbsp;'+notDeletedStudents[i].error+'<br>';
					}
					jAlert(boiler,'Error');
				}
			},error:function(e){
				getCustomAlertMessage("alert_error");
			}

		});
	} catch(e){
		//alert(e);
	}
}


function showStudentDetail(trId) {
	//return '&nbsp;<img id = "studentImg-'+rowObject.hdnStudentIdClnt+'" src="getStudentProfileImageById.json?id='+rowObject.hdnStudentIdClnt+'" height = "40px" width = "40px" onclick = "" class = "pointer"/>';
}

