				'<option value="NA">NA</option>'+
				'<option value="-">-</option>'+
				'</select></td></tr>'+
				
				'<tr><td><b>7)Place of birth: </b></td><td><input type="text" name="placeofBirth1" placeholder="Enter place of birth" id="placeofBirth" value="'+placeofBirth+'"></td></tr>'+
				'<tr><td><b>Place of birth District: </b></td><td><input type="text" name="placeofBirthDistrict" placeholder="Enter place of birth District" id="placeofBirthDistrict" value="'+placeofBirthDistrict+'"></td></tr>'+
				'<tr><td><b>Place of birth Taluka: </b></td><td><input type="text" name="placeofBirthTaluka" placeholder="Enter place of birth Taluka" id="placeofBirthTaluka" value="'+placeofBirthTaluka+'"></td></tr>'+
				'<tr><td><b>8)Date of birth,month and year<br>both in words and figures : </b></td><td><input type="text" name="birthDate1" placeholder=" Select DOB" id="birthDate" value="'+dob+'"></td></tr>'+
				'<tr><td><b>9)Last secondary School std X/XI : </b></td><td><input type="text" name="lastSecSchool1" placeholder="Enter 10th school name"id="lastSecSchool" value="'+tenthSchoolName+'"></td></tr>'+
				'<tr><td><b>10)Date of admission: </b></td><td><input type="text" name="admissionDate1" placeholder="Select admission date" id="admissionDate" value="'+admissionDate+'"></td></tr>'+
