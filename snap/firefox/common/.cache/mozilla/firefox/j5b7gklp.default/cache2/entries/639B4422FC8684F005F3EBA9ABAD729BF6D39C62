function doEditStudentRecord(trObj){
		var studentBatchIds = getSemesterBatchId();

		var majorOpenElectiveSubjectId=$("#majorOpenElectiveSubjectId").val();
		var minorOpenElectiveSubjectId=$("#minorOpenElectiveSubjectId").val();

		if(majorOpenElectiveSubjectId == 0 && minorOpenElectiveSubjectId != 0)
			{
				jAlert("Select Major Course");
				return false;
			}


		$.ajax({
			url:'addEditStudent.json?studentBatchId='+studentBatchIds,
			data:$('#frmStudentEdit').serialize(),
			dataType:'json',
			type:'POST',
			beforeSend : function() {
				$('#editStudentRecordDialog').find('.btnStudentEdit').attr('disabled','disabled');
				$('#editStudentRecordDialog').find('.preview').addClass('loading');
			},
			complete : function() {
				$('#editStudentRecordDialog').find('.btnStudentEdit').removeAttr('disabled');
				$('#editStudentRecordDialog').find('.preview').removeClass('loading');
			},
			success:function(jArray){
				
				/*if(jArray[1]["majorOpenElectiveSubjectId"] == 0 && jArray[1]["minorOpenElectiveSubjectId"] != 0)
				{
					jAlert("Select Major Subject");
					return false;
				}
				else
					return true;*/


				if(jArray[0].errorCode == '0'){
					jAlert("Changes saved successfully",'Success');
					// var trObj = $('#'+trId);
					var promotionalName = $('#editStudentRecordDialog').find('#parentDepartmentId option:selected').text();
					if($('#customerIdentificationObj').val() != 'IMT_NAGPUR'){
					var subbatch = $('#editStudentRecordDialog').find('#subbatch').val().trim();
					}
					 $(trObj).find('.parentInstituteCourseId').text(jArray[1]["parentInstituteCourseId"]);
					 $(trObj).find('.deptPromotionId').text(jArray[1]["deptPromotionId"]);
					 $(trObj).find('.firstName').text(jArray[1]["firstName"]);
					 $(trObj).find('.middleName').text(jArray[1]["middleName"]);
					 $(trObj).find('.lastName').text(jArray[1]["lastName"]);
					 $(trObj).find('.name').text(jArray[1]["firstName"]+' '+jArray[1]["middleName"]+' '+jArray[1]["lastName"]);
					 $(trObj).find('.parentDepartmentId').text(promotionalName);
					 $(trObj).find('.rollNo').text(jArray[1]["rollNo"]);
					 $(trObj).find('.phoneNo').text(jArray[1]["alternetContactNumber"]);
					 $(trObj).find('.subbatch').text(subbatch);
					 $(trObj).find('.gender').text(jArray[1]["gender"]);
					 $(trObj).find('.admissionDate').text(jArray[1]["admissionDate"]);
					 $(trObj).find('.majorOpenElectiveSubjectId').text(jArray[1]["majorOpenElectiveSubjectId"]);
					 $(trObj).find('.minorOpenElectiveSubjectId').text(jArray[1]["minorOpenElectiveSubjectId"]);

					 
					 $('#editStudentRecordDialog').modal('hide');
					 showStudentListByCourseSemeserAndDivision();
					// showStudentListInGrid(intNamingOrderValue);
					//showSuccessMessage(jArray[0].responseText,10000);
					//alert("studDeptPromotionId==="+jArray[1]["deptPromotionId"]+"==studParentInstituteCourseId==="+jArray[1]["parentInstituteCourseId"]);
					//$("#student-management-grid").jqGrid('setCell',growId,'deptPromotionId',jArray[1]["deptPromotionId"]);
					//$("#student-management-grid").jqGrid('setCell',growId,'parentInstituteCourseId',jArray[1]["parentInstituteCourseId"]);
					//return [true,"",sr];
				} else {
					if(jArray[0].errorCode == '1001'){
						jAlert(jArray[0].responseText+" Please try with different roll no.",'Error');
					}
				}
			},error:function(e){
				getCustomAlertMessage("alert_error");
			}
		});
}

function resetSubjectChoice(){
	var studentArray = new Array();
	$('.selectedStudent:checked').each(function(){
		var id=$(this).attr('id');
		studentArray.push(id);
	});
	var instituteCourseStrengthId = $('#cbAcademicBatch').val().split('-')[0];
	$.ajax({
		url:'resetStudentSubjectChoice.json?studentArray='+studentArray+'&instituteCourseStrengthId='+instituteCourseStrengthId,
		dataType:'text',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(text){
			$('#successMsg').show('slow');
			getCustomAlertMessage("alert_update");

		},error:function(e){
			getCustomAlertMessage("alert_error");
		}
	});
}

function showUserNameExample(){

	var first = $('#first').val().trim();
	var second = $('#second').val().trim();
	var third = $('#third').val().trim();
	var firstSeperator = $('#firstSeperator').val().trim();
	var secondSeperator = $('#secondSeperator').val().trim();

	var firstSuffix = $('#firstSuffix').val().trim();
	var secondSuffix = $('#secondSuffix').val().trim();
	
	var exUserName = first != 'blank' ? first : '';
	exUserName += firstSeperator;
	exUserName += second != 'blank' ? second : '';
	exUserName += secondSeperator;
	exUserName += third != 'blank' ? third : '';
	exUserName += firstSuffix+'@';
	exUserName += secondSuffix;
	$('#divCustomiseUsernameExample').html('<b>Ex. '+exUserName+'</b>');
}

function resetCustomUserNameFields(){
	$('#first').val('blank');
	$('#second').val('blank');
	$('#third').val('blank');
	$('#firstSeperator').val('');
	$('#secondSeperator').val('');

	$('#firstSuffix').val('');
	$('#secondSuffix').val('');
	$('#divCustomiseUsernameExample').html('');
	var studentIds = new Array();//getSelectedStudentId();
	$('.selectedStudent:checked').each(function(){
		studentIds.push($(this).attr("value"));
	});
	if(studentIds.length > 0){
		$('#generateUsername').modal('show');
	}
	else{
		jAlert("Please select atlest one student","Message");
	}
	
}

function getStudentAndSujectMapping(studentId)
{
	var patternId = $('#cbAcademicBatch').val().split('-')[1];
	var semesterId = $('#cbSemesterId').val();

	$.ajax({
		url : 'getForeignExchangeSubjectsMappingDetails.json',
		data : {studentId:studentId,patternId:patternId,semesterId:semesterId},
		dataType :'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(finalObject) {

			var boiler='';

			if(finalObject != undefined || finalObject != 'undefined'){
				if(finalObject.subjectMappingSubjectArray.length > 0 ){
					boiler+='<table id="displaySubject" class="table table-bordered">'+
					'<thead>'+
					'<tr><th colspan="4">Student Course Allocation List</th></tr>'+
					'<tr><th>Sr No.</th><th>Course Code</th><th>Course Name</th><th>Action</th></tr></thead><tbody>';

					for(var i=0;i<finalObject.subjectMappingSubjectArray.length;i++)
					{
						boiler+='<tr>'+
						'<td style="text-align:center">'+eval(i+1)+'</td>'+
						'<td class="subjectCode">'+finalObject.subjectMappingSubjectArray[i].subjectCode+'</td>'+
						'<td class="subjectName">'+decodeURIComponent(finalObject.subjectMappingSubjectArray[i].subjectName.gemsDecode())+'</td>'+
						'<td><label id="" value="">'+
						'<i id="" class="icon-trash removeAc" title="Delete" onclick="deleteSubjectDetails('+finalObject.subjectMappingSubjectArray[i].subjectWiseStudentId+','+studentId+')" style="cursor:pointer;"></i>'+
						'</label></td>'+
						'</tr>';
					}

					boiler+='</tbody></table>';
					
				}
				
				if(finalObject.subjectArray.length > 0 ){
					boiler+='<table id="" class="table table-bordered">'+
					'<thead>'+
					'<tr><th colspan="8">Foreign Exchange Course List</th></tr>'+
					'<tr><th></th><th>Course Code</th><th>Course Name</th><th></th><th>Course Code</th><th>Course Name</th></tr></thead><tbody>';
					
					var j=0;
					for(var i=0;i<finalObject.subjectArray.length;i++){
						if(j%2==0){
							boiler+='<tr>';
						}
						if(finalObject.subjectArray[i].subjectwiseBatchId > 0){
						boiler+='<td style="align-text:center"><input class="ckhForgienExchangeSubjectName" value="'+finalObject.subjectArray[i].subjectId+'" type="checkbox" ></td>';
						}
						else{
							boiler+='<td></td>';
						}
						boiler+='<td>'+finalObject.subjectArray[i].subjectCode+'</td>'+
						'<td>'+decodeURIComponent(finalObject.subjectArray[i].subjectName.gemsDecode())+'</td>';

						j++;
						if(j/2==0)
						{
							boiler+='</tr>';
							j==0;
						}
					}
					boiler+='</tbody></table>';
					if(finalObject.subjectArray.length > 0){
						boiler+='<div><input type="button" class="btn btn-mini btn-inverse" onclick="assignForeignExchangeSubjectToStudentMapping('+studentId+');" value="Save"/>';
					}
				}
				$("#getForeignExchangeSubject").html(boiler);
				//getForeignExchangeSubjectList(studentId);
			}else{
				var tempboiler ='<table><tr><td>No Data Found</td></tr></table>';
				$("#getForeignExchangeSubject").html(tempboiler);
			}
			
			
		},

	});
}

function deleteSubjectDetails(subjectWiseStudentId,studentId)
{

	jConfirm("Do you want to delete Course?","Confirm",function(r)
			{
		if(r ==true)
		{
			$.ajax({
				url:'deleteSubjectMappedToStudent.json?subjectWiseStudentId='+subjectWiseStudentId,
				dataType:'json',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();

				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(){

					getCustomAlertMessage("alert_remove");
					//getForeignExchangeSubjectList(studentId);
					getStudentAndSujectMapping(studentId);
					
				},

			});
		}
	});
}

function assignForeignExchangeSubjectToStudentMapping(studentId)
{
	var studentIdArr = new Array();
	var foreignExhangeSubjectIdArr = new Array();

	$('.selectedStudent:checked').each(function(){
		studentIdArr.push($(this).val());
	});

	$('.ckhForgienExchangeSubjectName:checked').each(function(){
		foreignExhangeSubjectIdArr.push($(this).val());
	});

	$.ajax({
		url : 'outBoundstudentSubjectMapping.json',
		dataType :'json',
		data:{studentIdArray:studentId,subjectIdArray:foreignExhangeSubjectIdArr.toString()},
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			
			jAlert("Successfully done", "Message");
			getStudentAndSujectMapping(studentId);
			
		},

	});
}

function editStudentOutBoundMapping(studentOutboundId,studentId)
{
	var outBoundUniversityId = $('#cmbUniversityName_'+studentId).val();
	var semesterBatchId = $('#cbBatchId').val().split('-')[2];
	
	$.ajax({
		url : 'editStudentOutBoundMapping.json',
		dataType :'json',
		type:'POST',
		data:{studentId:studentId,outBoundUniversityId:outBoundUniversityId,semesterBatchId:semesterBatchId,studentOutboundId:studentOutboundId},
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			
			jAlert("Successfully updated", "Message");
			
		},

	});
}
var casteListForSearching = new Array();
function getLeavingCertificate(studId)
{
	
	//alert("batchName="+batchName);
	$.ajax({
		url :'GenerateLeavingCertificate.json?studId='+studId,
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jsonObj) {
			var tenthName='';
			var castName='';
			var nationality='';
			var motherTongue='';
			var religion='';
			var motherName='';
			var placeofBirth='';
			var placeofBirthDistrict='';
			var placeofBirthTaluka='';
			var dob='';
			var SubCast='';
			var admissionDate='';
			var tenthSchoolName='';
			var nameofJuniorCollege='';
			var regNo='';
			var studntId='';
			var aadharCardNo='';
			var progress='Satisfactory';
			var conduct='Good';
			var dateOfLeaving='';
			var reason='Sent up for HSC Board Exam held in Feb.2016';
			var remarks='Passed';
			//var yearinstudyingandsince='';
			var yearinstudyingandsince = $('#cbAcademicBatch option:selected').text(); 
			var typeofCopy='';
			var religionToDisplay='';
	
			if(jsonObj.tenthName!=undefined)
			{
				tenthName=jsonObj.tenthName;
			}
			if(jsonObj.religion!=undefined)
			{
				religion=jsonObj.religion;
			}
			if(jsonObj.castName!=undefined)
			{
			 castName=jsonObj.castName;
			 
			}
			if(jsonObj.nationality!=undefined)
			{
				nationality=jsonObj.nationality;
			}
			if(jsonObj.motherTongue!=undefined)
			{
				motherTongue=jsonObj.motherTongue;
			}
			
			if(jsonObj.motherName !=undefined)
			{
				motherName=jsonObj.motherName;
			}
			if(jsonObj.placeofBirth !=undefined)
			{
				placeofBirth=jsonObj.placeofBirth;
			}
			if(jsonObj.placeofBirthDistrict !=undefined)
			{
				placeofBirthDistrict=jsonObj.placeofBirthDistrict;
			}
			if(jsonObj.placeofBirthTaluka !=undefined)
			{
				placeofBirthTaluka=jsonObj.placeofBirthTaluka;
			}
			if(jsonObj.dob !=undefined)
			{
				dob=jsonObj.dob;
			}
			if(jsonObj.SubCast !=undefined)
			{
				SubCast=jsonObj.SubCast;
			}
			if(jsonObj.admissionDate !=undefined)
			{
				admissionDate=jsonObj.admissionDate;
			}
			if(jsonObj.tenthSchoolName !=undefined)
			{
				tenthSchoolName=jsonObj.tenthSchoolName;
			}
			if(jsonObj.nameofJuniorCollege !=undefined)
			{
				nameofJuniorCollege=jsonObj.nameofJuniorCollege;
			}
			
			if(jsonObj.regNo !=undefined)
			{
				regNo=jsonObj.regNo;
			}
			if(jsonObj.studntId !=undefined)
			{
				studntId=jsonObj.studntId;
			}
			if(jsonObj.aadharCardNo !=undefined)
			{
				aadharCardNo=jsonObj.aadharCardNo;
			}
			if(jsonObj.progress !=undefined)
			{
				progress=jsonObj.progress;
			}
			if(jsonObj.conduct !=undefined)
			{
				conduct=jsonObj.conduct;
			}
			if(jsonObj.dateOfLeaving !=undefined)
			{
				dateOfLeaving=jsonObj.dateOfLeaving;
			}
			if(jsonObj.reason !=undefined)
			{
				reason=jsonObj.reason;
			}
			if(jsonObj.remarks !=undefined)
			{
				remarks=jsonObj.remarks;
			}
			if(jsonObj.yearinstudyingandsince !=undefined)
			{
				yearinstudyingandsince=jsonObj.yearinstudyingandsince;
			}
			if(jsonObj.dateOfDeclaration !=undefined)
			{
				dateOfDeclaration=jsonObj.dateOfDeclaration;
			}
			if(jsonObj.typeofCopy !=undefined)
			{
				typeofCopy=jsonObj.typeofCopy;
			}
			if(jsonObj.lcPrintDate !=undefined)
			{
				lcPrintDate=jsonObj.lcPrintDate;
			}
			if(jsonObj.religionToDisplay !=undefined)
			{
				religionToDisplay=jsonObj.religionToDisplay;
			}
				var boiler='';
				boiler+='<div id = "leavingcertificate" class = "modal fade hide">'+
				'<div class="modal-header">Leaving Certificate: '+
				'<button type="button" class="close" data-dismiss="modal">x</button>'+
			  	'</div>'+
			  	
			 	'<div class="modal-body" >'+
				'<div><table class="table table-bordered"><tbody><tr><td><b>General Register Number : </b></td><td><input type="text" name="regNo1" placeholder="Enter register no " id="regNo" value="'+regNo+'"></td></tr>'+
				'<tr><td><b>Student ID (Saral Id) : </b></td><td><input type="text" name="studId" placeholder="Enter student Id " id="studntId" value="'+studntId+'"></td></tr>'+
				'<tr><td><b>Aadhar Card No: </b></td><td><input type="text" name="uId" placeholder="Enter Aadhar Card No" id="aadharCardNoId" value="'+aadharCardNo+'" maxlength="12"></td></tr>'+
				//'<tr><td><b>Name of the Junior College : </b></td><td><input type="text" name="nameOfJuniorClg1" placeholder="Enter name of junior college" id="nameOfJuniorClg" value="'+nameofJuniorCollege+'"></td></tr>'+
				//'<tr><td><b>1)Name of the pupil in full : </b></td><td><input type="text" name="pupilName1" placeholder="Enter full name of pupil" id="pupilName" value='+jsonObj.firstName+'&nbsp;'+jsonObj.middleName+'&nbsp;'+jsonObj.lastName+'></td></tr>'+//
				'<tr><td><b>1)Name of the pupil in full : </b></td><td><input type="text" name="pupilName1" placeholder="Enter full name of pupil" id="pupilNameAsPerTenthMarksheet" value="'+tenthName+'"></td></tr>'+//
				
				'<tr><td><b>2)Mother Name : </b></td><td><input type="text" name="motherName1" placeholder="Enter mother name" id="motherName" value="'+motherName+'" ></td></tr>'+
				'<tr><td><b>3)Nationality : </b></td><td><select name="nationality1" placeholder="Enter nationality" id="nationality" data="'+nationality+'" ><option value="0" label="Select" /></select></td></tr>';
				//'<tr><td><b>3)Cast and Sub-Cast in the case<br>of pupils belonging to backward<br>classes & category among Back-<br>ward classes(e.g. SC/ST/VJ/NT) : </b></td><td><input type="text" placeholder="Enter cast" id="cast" value="'+castName+'"  class="validate[required,custom[onlyLetter]] text-input" /><br><br><input type="text" name="castAndSubCast1" placeholder="Enter subcast " id="subCast"  value="'+SubCast+'" ></td></tr>'+
				if($('#customerHelper').val()=='DES')
				{
					boiler+='<tr id="addNewReligionTxt"><td><b>4)Religion : </b></td><td><input type="text" name="religionNew" placeholder="Enter Religion" id="studreligionNew" value="'+religionToDisplay+'"></td></tr>';

				}else{
					boiler+='<tr id="ReligionDesTxt"><td><b>4)Religion : </b></td><td><select name="religion1" placeholder="Enter Religion" id="studreligion" data="'+religion+'" ><option value="0" label="Select" /></select></td></tr>';
				}
				boiler+='<tr><td><b>5)Caste and Sub-Caste: </b></td><td><input type="text" placeholder="Enter caste" id="cast" value="'+castName+'"  class="validate[required,custom[onlyLetter]] text-input" /><br><br><input type="text" name="castAndSubCast1" placeholder="Enter subcaste " id="subCast"  value="'+SubCast+'" ></td></tr>'+
				//'<tr><td><b>5)Nationality : </b></td><td><select name="nationality1" placeholder="Enter nationality" id="nationality" data="'+nationality+'" ><option value="0" label="Select" /></select></td></tr>'+
				
				'<tr><td><b>6)Mother Tounge : </b></td><td><select name="motherTongue1" placeholder="Enter Mother Tongue" id="motherTongueId" data="'+motherTongue+'" >'+
				'<option value="0">Select</option>'+
