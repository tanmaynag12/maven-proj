				}
				boiler+='<tr><td><b>5)Caste and Sub-Caste: </b></td><td><input type="text" placeholder="Enter caste" id="cast" value="'+castName+'"  class="validate[required,custom[onlyLetter]] text-input" /><br><br><input type="text" name="castAndSubCast1" placeholder="Enter subcaste " id="subCast"  value="'+SubCast+'" ></td></tr>'+
				//'<tr><td><b>5)Nationality : </b></td><td><select name="nationality1" placeholder="Enter nationality" id="nationality" data="'+nationality+'" ><option value="0" label="Select" /></select></td></tr>'+
				
				'<tr><td><b>6)Mother Tounge : </b></td><td><select name="motherTongue1" placeholder="Enter Mother Tongue" id="motherTongueId" data="'+motherTongue+'" >'+
