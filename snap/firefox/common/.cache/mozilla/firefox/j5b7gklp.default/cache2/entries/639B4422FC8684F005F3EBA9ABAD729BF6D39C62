function showNRITR()
{
	if($("#chkNRI").is(":checked")){
		$('#NRITD1').show();
		$('#NRITD2').show();
		$('#NRITD3').show();
	}
	else
	{
		$('#NRITD1').hide();
		$('#NRITD2').hide();
		$('#NRITD3').hide();
	}
}

function deleteStudent(studentBatchIds){
	//alert(studentBatchIds);
	try{
		$.ajax({
			url:'deleteStudent.json',
			data:'studentBatchId='+studentBatchIds,
			dataType:'json',
			type:'POST',
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success:function(responseObj){
				var flag= responseObj.responseFlag;
				if(flag == "1"){
					showStudentListByCourseSemeserAndDivision();
					jAlert('Student(s) deleted successfully',20000);
				} else {
					var notDeletedStudents = responseObj.notDeletedStudentArray;
					var boiler = '<div class="alert alert-error">';

					if(studentBatchIds.length != responseObj.unDeleteCount){
						boiler +=
							'<u><strong>Following '+responseObj.unDeleteCount+' student(s) could NOT be deleted due to specified reasons:-</strong></u><br>';
					} else {
						boiler += '<u><strong>Deletion failed!!! Following '+responseObj.unDeleteCount+' student(s) could NOT be deleted due to specified reasons:-</strong></u><br>';
					}
					for(var i = 0 ; i < notDeletedStudents.length ; i++){
						boiler += '&nbsp;&nbsp;<i class="icon-hand-right"></i>&nbsp;Roll: <b>'+notDeletedStudents[i].rollNo+'</b>&nbsp;&nbsp;Name: <b>'+notDeletedStudents[i].name+'</b>&nbsp;&nbsp;<b>Reason:-</b>&nbsp;'+notDeletedStudents[i].error+'<br>';
					}
					jAlert(boiler,'Error');
				}
			},error:function(e){
				getCustomAlertMessage("alert_error");
			}

		});
	} catch(e){
		//alert(e);
	}
}


