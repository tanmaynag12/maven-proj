			showStudentCurrentInfoDialog(studObj.hdnStudentIdClnt);
		//	gUserNameFetchFlag = false;
		//}
		$('.btnStudentEdit').hide();
		$('.btnEditUserName').show();
	});
	$('#editStudentRecordLink').unbind();
	$('#editStudentRecordLink').bind('click',function(){
		$('.btnStudentEdit').show();
		$('.btnEditUserName').hide();
		 //$('.btn-createUser').hide();
	});


	$('#resetPasswordLink').unbind();
	$('#resetPasswordLink').bind('click',function(){
		$('.btnStudentEdit').hide();
		$('.btnEditUserName').hide();
		if($('#customerIdentificationObj').val() == 'IMT_NAGPUR' || $('#customerIdentificationObj').val() == 'IMT_DUBAI'){
			   /*$('.btnStudentEdit').hide();
			   $('.btnEditUserName').show();*/
			   $('.btn-createUser').hide();
		}
		$('.btn-resetpassword').unbind();
		$('.btn-resetpassword').bind('click',function(){
			resetStudentUserPassword(studObj.hdnStudentIdClnt,name);
		});
	});
	
	var isForeignExchangeStudent = $(trObj).find('.isForeignExchangeStudent').attr('isForeignExchangeStudent');
	if(isForeignExchangeStudent == "true"){
