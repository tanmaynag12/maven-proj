				'<option value="Maithili">Maithili</option>'+
				'<option value="Malayalam">Malayalam</option>'+
				'<option value="Meiteilon(Manipuri)">Meiteilon(Manipuri)</option>'+
				'<option value="Marathi">Marathi</option>'+
				'<option value="Mizo">Mizo</option>'+
				'<option value="Nepali">Nepali</option>'+
				'<option value="Oriya">Oriya</option>'+
				'<option value="Punjabi">Punjabi</option>'+
				'<option value="Sanskrit">Sanskrit</option>'+
				'<option value="Santali">Santali</option>'+
				'<option value="Sindhi">Sindhi</option>'+
				'<option value="Tamil">Tamil</option>'+
				'<option value="Telugu">Telugu</option>'+
				'<option value="Urdu">Urdu</option>'+
				'<option value="Arabic">Arabic</option>'+
				'<option value="Mauritian">Mauritian</option>'+
				'<option value="Spanish">Spanish</option>'+
				'<option value="NA">NA</option>'+
				'<option value="-">-</option>'+
				'</select></td></tr>'+

				
				'</tbody></table></div>'+
				'</div>'+
				'<div class="modal-footer"><div class="row-fluid"><div class="span3"></div>'+
				'<div class="span6"><input type="button" class="btn btn-mini btn-inverse" style="" value="Save And Print" onclick="saveTransferCerificateDetailsOfIndira(\''+studId+'\')" >'+
			 	'<input type="button" class="btn btn-mini btn-danger" value="Close" data-dismiss="modal" />'+
			 	'</div><div class="span3"></div></div>'+
			 	'</div>';
				
			$('#leavingcertificateDIV').html(boiler);
			$('#motherTongueId').val(motherTongue);
			$('#noOfWorkDayInd').numeric();
			$('#noOfWorkDayPresentInd').numeric();
			
			$('#aadharCardNoInd').numeric();
			$('#udiseNoInd').numeric();
			$('#grBookNoId').numeric();
			

			$("#admissionDateInd").datepicker({
				dateFormat:"M dd,yy",
				});
			
			$("#birthDateInd").datepicker({
				dateFormat:"M dd,yy",
			});
			$("#schoolDuesMonthInd").datepicker({
				dateFormat:"M dd,yy",
			});
			$("#appliDateInd").datepicker({
				dateFormat:"M dd,yy",
			});
			
			$("#dateOfIssueInd").datepicker({
				dateFormat:"M dd,yy",
			});
			
			$('#transferCertificateOfIndira').modal('show');	
			getNationalityForTCIndi(studId,nationality,religion);
			getCaste(studId,castName);
					
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
	
}
function istextOnly(evt) {
	$('#LastStudClassWordsInd').keydown(function(e) {
		var key = e.keyCode;
		if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
			e.preventDefault();
		}
	}); 
	
	$('#promHighClassWordsInd').keydown(function(e) {
		var key = e.keyCode;
		if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
			e.preventDefault();
		}
	}); 
	$('#annualExamInd').keydown(function(e) {
		var key = e.keyCode;
		if (!((key == 9) || (key == 8) || (key == 32) || (key == 46) || (key >= 35 && key <= 40) || (key >= 65 && key <= 90))) {
			e.preventDefault();
		}
	}); 
}
function getNationalityForTCIndi(studId,nationalityid,religionid) {
	
	$.ajax({
		url : 'getAllStateRelNationalityCatCityCountry.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			// AllNationalities();
			var html3 = '<option value="0">-</option>';
			var len = data[2].length;
			for ( var i = 0; i < len; i++)
				html3 += '<option value="' + data[2][i]["nationalityId"] + '">'
				+ data[2][i]["nationalityName"] + '</option>';

			$('#nationalityInd').html(html3);
			if(nationalityid !=undefined)
				$('#nationalityInd').val(nationalityid);
			
			var html2 = '<option value="0">Select</option>';
			var len = data[1].length;
			for ( var i = 0; i < len; i++)
				html2 += '<option value="' + data[1][i]["relId"] + '">'
				+ data[1][i]["relName"] + '</option>';

			$('#studreligion').html(html2);
			if(religionid !=undefined)
				$('#studreligion').val(religionid);
		
		},
	});
}
 
function saveTransferCerificateDetailsOfIndira(studId){

	var studentName=$('#pupilNameInd').val();
	var previousSchool=$('#previousSchoolId').val();
	var grBookNo=$('#grBookNoId').val();
	var motherTongue=$('#motherTongueId option:selected').val();
	var religion=$('#studreligion').val();
	var caste=$('#cast').val();
	var subCaste=$('#subCast').val();
	
	var motherName=$('#motherNameInd').val();
	var motherMiddleName=$('#motherMiddleNameInd').val();
	var motherLastName=$('#motherLastNameInd').val();
	
	var fatherFirstName=$('#fatherFirstNameInd').val();
	var fatherMiddleName=$('#fatherMiddleNameInd').val();
	var fatherLastName=$('#fatherNameLastNameInd').val();
	
	var guardianFirstName=$('#guardianFirstNameInd').val();
	var guardianMiddleName=$('#guardianMiddleNameInd').val();
	var guardianLastName=$('#guardianNameLastNameInd').val();
	
	
	var nationality=$('#nationalityInd').val();
	var placeofBirth=$('#placeofBirthInd').val();
	var scheduleCasteTribe=$('#scheduleCasteTribeInd').val();
	var admissionDate=$('#admissionDateInd').val();
	var birthDate=$('#birthDateInd').val();
	
	var pupilClassLastStud=$('#pupilClassLastStudInd').val();
	var LastStudClassWords=$('#LastStudClassWordsInd').val();
	
	var annualExamResult=$('#annualExamInd').val();
	var whetherFailed=$('#whetherFailedInd').val();
	var promoForHighClass=$('#promoForHighClassInd').val();
	
	var promHighClassWords=$('#promHighClassWordsInd').val();
	
	
	var monthUptoTheSchoolDues=$('#schoolDuesMonthInd').val();
	var anyFeeConcessionAvailed=$('#feeConcessionAvailedInd').val();
	var noOfWorkDay=$('#noOfWorkDayInd').val();
	var noOfWorkDayPresent=$('#noOfWorkDayPresentInd').val();
	var NCCGuide=$('#NCCGuideInd').val();
	var extraCurrAct=$('#extraCurrActInd').val();
	var genConduct=$('#genConductInd').val();
	var appliDateOfCerti=$('#appliDateInd').val();
	var issueDateOfCerti=$('#dateOfIssueInd').val();
	var reason=$('#reasonofLeavSchoolInd').val();
	var remarks=$('#otherRemarkInd').val();
	
	var aadharCardNo=$('#aadharCardNoInd').val();
	//var schoolApprovalNo=$('#schoolApprovalNoInd').val();
	//var udiseNo=$('#udiseNoInd').val();
	
	if(studentName=="")
	{
		jAlert("Please enter the pupil Name");
		$('#pupilNameInd').css("border", "1px solid red").focus();
	}
	/*else if(motherName=="")
	{
		jAlert("Please enter mother Name");
	}	*/
	else if(nationality=="")
	{
		jAlert("Please enter Nationality");
		$('#nationalityInd').css("border", "1px solid red").focus();
	}
	else if(placeofBirth=="")
	{
		jAlert("Please enter Place of birth");
		$('#placeofBirthInd').css("border", "1px solid red").focus();
	}	
	else if(scheduleCasteTribe=="")
	{
		jAlert("Please enter Whether the candidate belongs to Schedule Caste or Schedule Tribe");
		$('#scheduleCasteTribeInd').css("border", "1px solid red").focus();
	}	

	else if(admissionDate=="")
	{
		jAlert("Please enter Date of first admission in the school with class & std");
		$('#admissionDateInd').css("border", "1px solid red").focus();
	}	

	else if(birthDate=="")
	{
		jAlert("Please enter Date of birth according to Admission register");
		$('#birthDateInd').css("border", "1px solid red").focus();
	}	

	else if(pupilClassLastStud=="")
	{
		jAlert("Please enter Class in which the pupil last studied(in figure)");
		$('#pupilClassLastStudInd').css("border", "1px solid red").focus();
	}	
	else if(LastStudClassWords=="")
	{
		jAlert("Please enter Class in which the pupil last studied(in words)");
		$('#LastStudClassWordsInd').css("border", "1px solid red").focus();
	}	
	
	else if(annualExamResult=="")
	{
		jAlert("Please enter School / Board Annual examination last taken with result");
		$('#annualExamInd').css("border", "1px solid red").focus();
	}	

	else if(whetherFailed=="")
	{
		jAlert("Please enter Whether failed, if so once / twice in the same class");
		$('#whetherFailedInd').css("border", "1px solid red").focus();
	}	
	else if(promoForHighClass=="")
	{
		jAlert("Please enter Whether qualified for promotion to the higher class if so,to which class(in figures.)");
		$('#promoForHighClassInd').css("border", "1px solid red").focus();
	}	
	else if(promHighClassWords=="")
	{
		jAlert("Please enter Whether qualified for promotion to the higher class if so,to which class(in words.)");
		$('#promHighClassWordsInd').css("border", "1px solid red").focus();
	}
	else if(monthUptoTheSchoolDues=="")
	{
		jAlert("Please enter Month upto which the (pupil has said)school dues");
		$('#schoolDuesMonthInd').css("border", "1px solid red").focus();
	}	
	else if(anyFeeConcessionAvailed=="")
	{	
		jAlert("Please enter Any fee concession availed of if so,the nature of such concession");
		$('#feeConcessionAvailedInd').css("border", "1px solid red").focus();
	}	
	else if(noOfWorkDay=="")
	{
		jAlert("Please enter Total No. of working days");
		$('#noOfWorkDayInd').css("border", "1px solid red").focus();
	}
	else if(noOfWorkDayPresent=="")
	{
		jAlert("Please enter Total No. of working days present");
		$('#noOfWorkDayPresentInd').css("border", "1px solid red").focus();
	}
	else if(NCCGuide=="")
	{
		jAlert("Please enter Whether NCC Cadet/Boy Scout/Girl Guide(details may be given)");
		$('#NCCGuideInd').css("border", "1px solid red").focus();
	}
	else if(extraCurrAct=="")
	{
		jAlert("Please enter Games played of extra-curricular activities in which the pupil usually took part(mention achievement level there in)");
		$('#extraCurrActInd').css("border", "1px solid red").focus();
	}
	else if(genConduct=="")
	{
		jAlert("Please enter General Conduct");
		$('#genConductInd').css("border", "1px solid red").focus();
	}	

	else if(appliDateOfCerti=="")
	{
		jAlert("Please select Date of application for certificate");
		$('#appliDateInd').css("border", "1px solid red").focus();
	}	

	else if(issueDateOfCerti=="")
	{
		jAlert("Please select Date of issue of certificate");
		$('#dateOfIssueInd').css("border", "1px solid red").focus();
	}	
	else if(reason=="")
	{
		jAlert("Please enter Reasons for leaving the school");
		$('#reasonofLeavSchoolInd').css("border", "1px solid red").focus();
	}
	else if(remarks=="")
	{
		jAlert("Please enter Any others remark");
		$('#otherRemarkInd').css("border", "1px solid red").focus();
	}	
	else
	{	
		jConfirm("Details saved successfully ! Do you want print Transfer Certificate ?","Confirm",function(ans) {
			if(ans) {
				$.ajax({ 
					url:'saveTransferCertificateOfIndiDetails.json?studId='+studId+'&studentName='+studentName+'&motherName='+motherName+'&nationality='+nationality+'&placeofBirth='+placeofBirth+'&scheduleCasteTribe='+scheduleCasteTribe+'&admissionDate='+admissionDate+
					'&birthDate='+birthDate+'&annualExamResult='+annualExamResult+'&whetherFailed='+whetherFailed+'&promoForHighClass='+promoForHighClass+'&monthUptoTheSchoolDues='+monthUptoTheSchoolDues+'&anyFeeConcessionAvailed='+anyFeeConcessionAvailed+'&noOfWorkDay='+noOfWorkDay+
					'&noOfWorkDayPresent='+noOfWorkDayPresent+'&NCCGuide='+NCCGuide+'&extraCurrAct='+extraCurrAct+'&genConduct='+genConduct+'&appliDateOfCerti='+appliDateOfCerti+'&issueDateOfCerti='+issueDateOfCerti+'&reason='+reason+'&remarks='+remarks+
					'&motherMiddleName='+motherMiddleName+'&motherLastName='+motherLastName+'&fatherFirstName='+fatherFirstName+'&fatherMiddleName='+fatherMiddleName+'&fatherLastName='+fatherLastName+'&guardianFirstName='+guardianFirstName+'&guardianMiddleName='+guardianMiddleName+'&guardianLastName='+guardianLastName+
					'&aadharCardNo='+aadharCardNo+'&pupilClassLastStud='+pupilClassLastStud+'&LastStudClassWords='+LastStudClassWords+'&promHighClassWords='+promHighClassWords+'&previousSchool='+previousSchool+'&grBookNo='+grBookNo+'&motherTongue='+motherTongue+'&religion='+religion+'&caste='+caste+'&subCaste='+subCaste,//+'&schoolApprovalNo='+schoolApprovalNo+'&udiseNo='+udiseNo
					dataType :'text',
					type:'GET',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(jArray) {
						$('#transferCertificateOfIndira').modal('hide');
						generatePDFOfTransferCertificateOfIndi(studId,"SAVE");

					},
					error:function(e){
						getCustomAlertMessage("alert_error");
					}
				});
			}
		});

	}

}
function generatePDFOfTransferCertificateOfIndi(studId,callFrom)
{
	var customerIdentificationObj = $('#customerHelper').val();
	
	
	var studentIdArray = new Array();
	if(callFrom=="SAVE")
	{
		studentIdArray.push(studId);
	}
	else
	{
		$('.selectedStudent:checked').each(function(){
			studentIdArray.push($(this).val());	
		});
	}
	if(studentIdArray.length > 0)
	{
		if(customerIdentificationObj =='INDIRA_PUNE')
		{
			window.open('printPDFforGenerateTransferCertificateOfIndira.json?studentIdArray='+studentIdArray);
		}
	}	
	else
	jAlert("please select at least one student");	
		
}

function getPrintlcCount(studentIdArray){
	$.ajax({ 
		url:'getLeavingCertificateCountDetails.json?studentIdArray='+studentIdArray,
		dataType :'json',
		type:'GET',
		async : false,
		success : function(jArray) {
			var len = jArray.length;
			for(var i=0; i<len; i++){
			if(jArray[i]["lcPrintCount"]>0){
				$('#tdIssued-'+jArray[i]["studentId"]).html('<label class="badge badge-success">Issued '+jArray[i]["lcPrintCount"]+' time(s)</label>');   
				}
				else{
					$('#tdIssued-'+jArray[i]["studentId"]).html('<label><b>Pending<b></label>');
				}
			}
		},
		error:function(e){
			getCustomAlertMessage("alert_error");
		}
	});
}
