				url:'outBoundUnivAll.json',
				dataType:'json',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();

				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){
					
					var html = '<option value="0">Select</option>';
					var len = jArray.length;
					for ( var i = 0; i < len; i++)
						html += '<option value="'+jArray[i]["outBoundUniversityId"]+'">'+jArray[i]["outBoundUniName"]+ '</option>';

					$('#cmbUniversityName_'+studentId).html(html);
					$('#cmbUniversityName_'+studentId).val(outBoundUniversityId);
				},
			});
	});
	/*var subBatchFetchFlag = true;
