				'<input type="text" class="validnum" name="guardianPinCodeModal" id="studguardianpincodeModal" maxLength="7" />'+
			'</td>'+
		'</tr>';
		
		
			boilerGuardModal+='<tr class="newGuradianDetailsModal">'+
			'<td colspan="4" style="text-align: center;"><input type="submit" class="btn btn-inverse btn-mini   btn_medium  btnColorGreen" value="Save" onclick="saveGuradianDetailsForStudentNew()"/>'+
			'</td>'+
		'</tr>';
		
			boilerGuardModal+='</table>';

		 
		 
		 
			boilerGuardModal+='</div>'+
		 '<div class="modal-footer">'+
	    '<a href="#" class="btn btn-inverse btn-mini  " data-dismiss="modal">Close</a>'+
	  '</div>'+
	'</div>';

			$('#addquestionModal').modal('show');
}

