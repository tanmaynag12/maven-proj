			'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtsgpa'+ i + '" value="" class="span1 forNum" /></td>'+
			'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtcgpa'	+ i	+ '" value="" class="span1 forNum"  /></td>'+
			'</tr>';
		}

