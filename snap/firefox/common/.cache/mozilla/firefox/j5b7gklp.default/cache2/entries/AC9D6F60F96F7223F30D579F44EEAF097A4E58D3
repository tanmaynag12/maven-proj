	physicallyHandicappedDetailsArray.push('_' + $('#txtPhysicallyHandicappedOther').val());
	var physicallyHandicapped = $('input[name = "physicallyHandicapped"]:checked').val();
	
	//htpu condition 
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		var addressLine1 = $('input[name = "currentaddressLine1_1"]').val();
