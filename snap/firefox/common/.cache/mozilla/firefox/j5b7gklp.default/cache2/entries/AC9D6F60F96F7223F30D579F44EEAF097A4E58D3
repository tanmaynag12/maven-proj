        var result = !(keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || (keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64) || (keyCode>=91 && keyCode<=96) || (keyCode>=123 && keyCode<=126));
        return result;
    });
	
