		if ((gArray[i]["isCheckedText"] == "neckOtherFinding") && ($('input[name = "neckOtherFinding"]')!=undefined) && (($('input[name = "neckOtherFinding"]').val())!=null) &&  ($.trim($('input[name = "neckOtherFinding"]').val())!="") && ($('input[name = "neckOtherFinding"]').val().length>0) ){
			$('input[name = "neckOtherFinding"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "upperNail") && ($('input[name = "upperNail"]')!=undefined) && (($('input[name = "upperNail"]').val())!=null) &&  ($.trim($('input[name = "upperNail"]').val())!="") && ($('input[name = "upperNail"]').val().length>0)) {
			$('input[name = "upperNail"]').attr("disabled", true);
		}

		if ((gArray[i]["isCheckedText"] == "lowerNail")&& ($('input[name = "lowerNail"]')!=undefined) && (($('input[name = "lowerNail"]').val())!=null) &&  ($.trim($('input[name = "lowerNail"]').val())!="") && ($('input[name = "lowerNail"]').val().length>0)) {
			$('input[name = "lowerNail"]').attr("disabled", true);
		}

