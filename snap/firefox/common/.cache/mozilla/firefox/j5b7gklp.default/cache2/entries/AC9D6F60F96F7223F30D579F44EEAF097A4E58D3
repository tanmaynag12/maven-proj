function getOnlineButton(){
	
	$.ajax({
		url : 'fetchEligibilityRegistrationFeeDetail.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			$('div.preview').addClass('loading');
			$('div.preview').show();
		},
		complete : function() {
			$('div.preview').removeClass('loading');
			$('div.preview').hide();
		},
		success : function(jArray) {
			var paymentStatus = "";
			var bankId = 0;
			if(jArray.length>0){
				$('#DetailsDiv').show();
			var boiler = '<br><br><table class="table table-bordered table-condensed">'
				+ '<tr><th>Sr.No</th><th>Student Name</th><th>Eligibility Registration \n Payment Status</th><th>Transaction Id</th><th>Amount</th></tr>';
			for(var i =0;i<jArray.length; i++){
				boiler+='<tr><td>'+eval(i+1)+'</td><td>'+jArray[i]["studentName"]+'</td><td>'+((jArray[i]["paymentStatus"]!=true)?"Pending":"Done")+'</td><td>'+((jArray[i]["voucherNo"]!=true)?jArray[i]["voucherNo"]:"Not Available")+'</td><td>'+(jArray[i]["amount"]!=undefined?jArray[i]["amount"]:"")+'</td></tr>';
				paymentStatus = jArray[i]["paymentStatus"];
				bankId = jArray[i]["bankId"];
			}
			 boiler +='</table>';
			
			 $('#DetailsDiv').html(boiler);
			 if(bankId<=0){
				 jAlert("Bank Details Not Found");
				 return false;
			 }
			if(paymentStatus == false){
			//	$('#btnPayment').show();
//				$('#paymentPluginsDivEli').show();
				$('#onlinePaymentButtonSpan').junoPymt({
					sPC : 'EFR',
					iFBIDs : bankId,
				});			
			}else if(paymentStatus == true){
//				$('#pdfDiv').show();
				//$('#btnPayment').hide();
				$('#PaymentBtnDiv').hide();
				$('#paymentPluginsDivEli').hide();
			}
			}
		},
		error:function(){
			//jAlert("Error for yeardown fee","Error");
			getCustomAlertMessage("alert_error");
		}
	});
}
function getStudentContactForStudentDetailsTab(gStudentId){


	getCountry();
	try {
		var hdnUserRole = $('#hdnUserRole').val();
		var url = 'getStudentContactDetails.json?id='+gStudentId;
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_getStudentContactDetails.json';
		}
		$.ajax({
			url : url,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
				
			},
			success : function(jArray) {
				for ( var i = 0; i < jArray.length-1; i++) {
					if (jArray[i]["permanentOrCurrent"] == "current") {
						if(!jArray[2]["is_icard_confirmed"])
						{
							var cntryId = jArray[i]["countryId"];
							$('#studentCountryCombo_1 option[value='+ cntryId + ']').attr("selected", "selected");
							getStudentStates('studentCountryCombo_1','studentState_1');
							var stateId = jArray[i]["stateId"];
							$('#studentState_1 option[value=' + stateId + ']').attr("selected", "selected");
							getStudentCity('studentState_1', 'studentCity_1');
							var cityId = jArray[i]["cityId"];
							$("#studAddLine1_1").val(jArray[i]["addressLine1"]);
							$("#studAddLine2_1").val(jArray[i]["addressLine2"]);
							$('#studentCity_1').val(cityId);
							$("#studPincode_1").val(jArray[i]["pinCode"]);

							$('#CurrAddressId_1').val(jArray[i].studentAddressId);
							$('#txtStudentDistrict_1').val(jArray[i].studentDistrict);
							$('#txtStudentTaluka_1').val(jArray[i].studentTaluka);

							if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')
							$('#CurrAddressId_1').val(jArray[i].studentAddressId);
						}
						else{

							var cntryId = jArray[i]["countryId"];
							$('#studentCountryCombo_1 option[value='+ cntryId + ']').attr("selected", "selected");
							$('#studentCountryCombo_1').attr('disabled', true);
							getStudentStates('studentCountryCombo_1','studentState_1');
							var stateId = jArray[i]["stateId"];
							$('#studentState_1 option[value=' + stateId + ']').attr("selected", "selected");
							$('#studentState_1').attr('disabled', true);
							getStudentCity('studentState_1', 'studentCity_1');
							var cityId = jArray[i]["cityId"];
							$("#studAddLine1_1").val(jArray[i]["addressLine1"]);
							$('#studAddLine1_1').attr('disabled', true);
							$("#studAddLine2_1").val(jArray[i]["addressLine2"]);
							$('#studAddLine2_1').attr('disabled', true);
							$('#studentCity_1').val(cityId);
							
							$('#studentCity_1').attr('disabled', true);
							$("#studPincode_1").val(jArray[i]["pinCode"]);
							$('#studPincode_1').attr('disabled', true);

							if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')
							$('#CurrAddressId_1').val(jArray[i].studentAddressId);
							$('#CurrAddressId_1').attr('disabled', true);
							
							$('#txtStudentDistrict_1').val(jArray[i].studentDistrict);
							$('#txtStudentDistrict_1').attr('disabled', true);
							$('#txtStudentTaluka_1').val(jArray[i].studentTaluka);
							$('#txtStudentTaluka_1').attr('disabled', true);
						} 
					}
					 else {
						if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')	{
						var cntryId = jArray[i]["countryId"];
						$('#permstudentCountryCombo1_1 option[value='+ cntryId + ']').attr("selected", "selected");
						getStudentStates('permstudentCountryCombo1_1','permstudentState_1');
						var stateId = jArray[i]["stateId"];
						$('#permstudentState_1 option[value='+ stateId + ']').attr("selected", "selected");
						getStudentCity('permstudentState_1','permstudentCity_1');
						var cityId = jArray[i]["cityId"];
						$("#permstudAddLine1_1").val(jArray[i]["addressLine1"]);
						$("#permstudAddLine2_1").val(jArray[i]["addressLine2"]);
						$('#permstudentCity_1').val(cityId);
						$("#permstudPincode_1").val(jArray[i]["pinCode"]);
						$('#txtStudentPermDistrict_1').val(jArray[i]["studentDistrict"]);
						$('#txtStudentPermTaluka_1').val(jArray[i]["studentTaluka"]);
						$('#PermaAddressId_1').val(jArray[i].studentAddressId);
						}
					 }
					}
				if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
					if(jArray[0]["studentPaymnetForRergistartion"]!=undefined && jArray[0]["studentPaymnetForRergistartion"] == "true"){
						//alert("ddd");
						$('#studentPayemntDetails').show();
						
					}else{
						$('#studentPayemntDetails').hide();
					}
					
				}
				
				getstudProfileConfigurationDetailsCheckingNames();
			},
			error : function() {
				alert("In Student Contact error");
			}
		});
	} catch (e) {
	}

}

function CopyCurrToPermanentAddForStudDetails() {

	if ($('#addrcheckStudDetail').is(':checked')) {
		$("#permstudAddLine1_1").val($("#studAddLine1_1").val());
		$("#permstudAddLine2_1").val($("#studAddLine2_1").val());
		$("#permstudPincode_1").val($("#studPincode_1").val());
		$("#txtStudentPermDistrict_1").val($("#txtStudentDistrict_1").val());
		$("#txtStudentPermTaluka_1").val($("#txtStudentTaluka_1").val());
		var cntryId = $("#studentCountryCombo_1").val();
		var stateId = $("#studentState_1").val();
		getStudentStates('studentCountryCombo_1', 'permstudentState_1');
		getStudentCity('studentState_1', 'permstudentCity_1');

		var cityId = $("#studentCity_1").val();
		$('#permstudentCountryCombo1_1 option[value=' + cntryId + ']').attr(
				"selected", "selected");
		$('#permstudentState_1 option[value=' + stateId + ']').attr("selected",
		"selected");
		$('#permstudentCity_1 option[value=' + cityId + ']').attr("selected",
		"selected");
	} else {
		$("#permstudAddLine1_1").val('');
		$("#permstudAddLine2_1").val('');
		$("#permstudPincode_1").val('');
		$('#permstudentCountryCombo1_1 option[value=0]').attr("selected",
		"selected");
		$('#permstudentState_1 option[value=0]').attr("selected", "selected");
		$('#permstudentCity_1 option[value=0]').attr("selected", "selected");
		$("#txtStudentPermDistrict_1").val('');
		$("#txtStudentPermTaluka_1").val('');
	}
}

function addStudentAddressForStudDetailsTab()
{

	
	var addressLine1 = $('input[name = "currentaddressLine1_1"]').val();
	var addressLine2 = $('input[name = "currentaddressLine2_1"]').val();
	var cityId = $('#studentCity_1').val();
	var stateId = $('#studentState_1').val();
	var countryId = $('#studentCountryCombo_1').val();
	var pinCode = $('input[name = "currentpinCodeNumber_1"]').val();
	var CurrAddressId = $('#CurrAddressId_1').val();
	var currDistrict = $('#txtStudentDistrict_1').val();
	var currTaluka = $('#txtStudentTaluka_1').val();
	
	var permaaddressLine1 = $('#permstudAddLine1_1').val();
	var permaaddressLine2 = $('#permstudAddLine2_1').val();
	var permstudentCity = $('#permstudentCity_1').val();
	var permstudentState = $('#permstudentState_1').val();
	var permstudentCountryCombo1 = $('#permstudentCountryCombo1_1').val();
	var permstudPincode = $('#permstudPincode_1').val();
	var PermaAddressId = $('#PermaAddressId_1').val();
	var permDistrict = $('#txtStudentPermDistrict_1').val();
	var permTaluka = $('#txtStudentPermTaluka_1').val();

	
		$.ajax({
			url : 'studentAddress.json',
			data : {
				PermaAddressId : PermaAddressId,
				CurrAddressId : CurrAddressId,
				addressLine1 : addressLine1,
				addressLine2 : addressLine2,
				countryId : countryId,
				stateId : stateId,
				cityId : cityId,
				pinCode : pinCode,
				permaaddressLine1 : permaaddressLine1,
				permaaddressLine2 : permaaddressLine2,
				permstudentCity : permstudentCity,
				permstudentState : permstudentState,
				permstudentCountryCombo1 : permstudentCountryCombo1,
				currDistrict:currDistrict,
				currTaluka:currTaluka,
				permDistrict:permDistrict,
				permTaluka:permTaluka,
				permstudPincode : permstudPincode
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				if (jArray[0].members.responseFlag.value == '1')

					$('#CurrAddressId_1').val(jArray[1][0].studentAddressId);
				$('#PermaAddressId_1').val(jArray[1][1].studentAddressId);
				//getStudentContactForStudentDetailsTab();
				getCustomAlertMessage("alert_create");
			},
			error : function() {
			}
		});
	

}

function showPaymentBtn(){
	$('#PaymentBtnDiv').show();
	$('#paymentPluginsDivEli').show();
}
function translateToHindi(fromId,ToId)
{
	 google.language.translate($('#'+fromId).val(), 'en', google.elements.transliteration.LanguageCode.HINDI, function(result) {
         $('#'+ToId).val(result.translation);
     });
}
function printStudentProfile()
{
	var subCatId=$('#studSubCategory option:selected').text();
	
	var category=$('#txtstudcategory option:selected').text();
	window.location.replace('getstudentProfilePdf.json?&category='+category+'&subCatId='+subCatId);
}
function showFreezeBtn()
{
	if(g_freeze!=undefined && g_freeze)
	{
		console.log('sgd');
		$('#declareChkBox').attr('checked','checked');
		$('#freezeBtn').hide();
		$('#printBtn').show();
	}
	else
		{
	if($('#declareChkBox').is(':checked'))
	{
		$('#freezeBtn').show();
	}
	else
	{
		$('#freezeBtn').hide();
	}
		}
}
function freezeStudentProfile()
{
	$.ajax({
		url : 'freezeRegistredStudentProfileHptu.json',
		dataType : 'text',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(result) {
			if(result=="1"){
				jAlert("Profile Freezed Successfully");
			$('#freezeBtn').hide();
			$('#printBtn').show();
			}
			else
				{
				jAlert("Profile Freezing Unsuccessfull");
				$('#declareChkBox').removeAttr('checked');
				$('#freezeBtn').show();
				$('#printBtn').hide();
				}
		},
		error : function() {
		}
	});
}

function initUploadCourseExemptionDocuments(){
	
	new AjaxUpload('uploadCertificated', {
		action : 'uploadEligibilityCriteriaFormDocuments.json',
		name : 'fileData',
		size:AllowedFileSize.DOCUMENT_UPLOAD,
		fileType:[ 'pdf', 'jpeg','jpg', 'bmp', 'png' ],
		onSubmit : function(file, extension) {
			
			if(extension.toString().toLowerCase() == 'pdf' || extension.toString().toLowerCase() == 'jpeg' || extension.toString().toLowerCase() == 'jpg' ||extension.toString().toLowerCase() == 'bmp' ||extension.toString().toLowerCase() == 'png'){
				this.setData({
					'fileSize':AllowedFileSize.DOCUMENT_UPLOAD
				});
			} else {
				jAlert('Use only "pdf" format file!');
				return false;
			}
		},
		onComplete : function(file, response) {
			 try{
				response = eval('('+response+')');

				if(response[0].responseFlag == '1'){
					jAlert(response[0].responseText);
					var docId = response[1]["DocumentId"];
					var attachmentFileName = response[1]["name"].toString(); 
					var documentId = response[1]["eDocId"]!=undefined ? response[1]["eDocId"] : response[1]["documentId"];
					var fileType = response[1]["extension"];
					var downloadLink = (juno.label['a.download.json'].setIndexedArgs(documentId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
					iconPath = gFileTypeIconMapping[fileType];
					jAlert("Document Uploaded Successfully..!")

						
				} else {
					throw response[0].responseText;
				} 
			} catch (e){
				jAlert(e);
			}					
		}
	}); 
}


function deleteAuthorDetailsExceptPublication(employeeProfileDetailId,ele,eleId,elePrefix,employeeId)
{

	jConfirm("Are you sure you want to delete record ??","Confirm",function(r){
		if(r) { 
			if(employeeProfileDetailId == '0'){
				if(ele!=undefined){
					$(ele).closest('li').remove();
					var index = employeeNames.indexOf(employeeId);
					if (index > -1) {
						employeeNames.splice(index, 1);
					}
				}
				if(eleId!=undefined && eleId!=''){
						$('#'+elePrefix+'_WithinInstituteAuthorName2 option[value="'+eleId+'"]').remove();
						$('#'+elePrefix+'_otherInstituteAuthorName2  option[value="'+eleId+'"]').remove();
						$('#'+elePrefix+'_WithinInstituteStudentName2 option[value="'+eleId+'"]').remove();
						$('#'+elePrefix+'_AuthorNameListwithinInstitute li[del="'+eleId+'"]').remove();
				}
				return;
			}
			$.ajax({
				url:'deleteWithinInstituteEmp.json',
				data:{employeeProfileDetailId:employeeProfileDetailId},
				dataType:'text',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function() {
					getCustomAlertMessage("alert_delete");
					if(ele!=undefined){
						$(ele).closest('li').remove();
					}
					//getAllEmployeeListWihtinInstitute('withinInstitute');			
				},
				error:function(){
					jAlert("Something went wrong..","Warning");
				}
			});
		}
	});
}

function getApplicationFormOnPersonalTab()
{
	var studentId = $('#studId').val();
		$.ajax({
			url:'restApi/adm_setApplicantAutoLoginFlag.json', 
			data:{
				appStudId:studentId
			},
			type:'POST',
			dataType:'json',
			junoSpinner:true,
			success:function(jObj)
			{
				if(jObj.isError==false){
					if(jObj.shoNewApplForm){
						window.open('anon_applAdmissionForm.htm','_self');
					}else if(jObj.autoLogin){
						$(`<form method="post" action="${jObj.website}/anon_authenticatingCredentials.htm" target="_blank">
								<input type="hidden" name="log_credencials" value="${jObj.id}">
						</form>`).appendTo('body').submit().remove();
					}/*else{
						//Work as it is if autologin false
						getPersonalTab_call(studentId,status);
					}*/
				}else{
					jAlert('Something went wrong! Please try again!');
				}
			}
		});
}

