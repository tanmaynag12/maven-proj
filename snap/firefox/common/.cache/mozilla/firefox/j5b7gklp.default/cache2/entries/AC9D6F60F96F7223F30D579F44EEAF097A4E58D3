				$('#studMotherProfession').css("border","1px solid #CCCCCC");
			}
		 	
		}*/
		
		 
		
		if(fatherFirstName != ""){
			if(parentAnnualIncome == "" && $('#customerHelper').val()!="MEGHE" && $('#customerHelper').val()!="MAHINDRA_UNIVERSITY"  && $('#customerHelper').val()!="MANDSAUR")
			{
				jAlert("Please Fill Father's Annual Income");
				$('#studparentannulaincome').css("border","1px solid red");
				return false;
			}else{
				$('#studparentannulaincome').css("border","1px solid #CCCCCC");
			}
		}
		if(motherFirstName != "" && $('#customerHelper').val()!="MEGHE"  && $('#customerHelper').val()!="MAHINDRA_UNIVERSITY"  && $('#customerHelper').val()!="MANDSAUR"){
			if(motherAnnualIncome == "")
			{
				jAlert("Please Fill Mother's Annual Income");
				$('#motherannulaincome').css("border","1px solid red");
				return false;
			}else{
				$('#motherannulaincome').css("border","1px solid #CCCCCC");
			}
	  		
		 }
			if(parentDistrict!="" )
			{
				if(!regExp.test(parentDistrict))
				{
					$('#studparentdistrict').css("border","1px solid red");
					return false;
				}else{
					$('#studparentdistrict').css("border","1px solid #CCCCCC");
				}
			}else{
				$('#studparentdistrict').css("border","1px solid #CCCCCC");
			}	
			 if(parentVillage!="")
			{
				 if(!regExp.test(parentVillage))
					{
						$('#studParentvillage').css("border","1px solid red");
						return false;
					}else{
						$('#studParentvillage').css("border","1px solid #CCCCCC");
					} 
			}else{
				//$('#studParentvillage').css("border","1px solid #CCCCCC");
			}	
			 if(parentTaluka!="" )
			 {
				 if(!regExp.test(parentTaluka))
				 {
					 $('#studParenttaluka').css("border","1px solid red");
					 return false;
				 }else{
					 $('#studParenttaluka').css("border","1px solid #CCCCCC");
				 }
			 }else{
				// $('#studParenttaluka').css("border","1px solid #CCCCCC");
			 }
			 if(parentPinCode!="")
			 {
				 if(isNaN(parentPinCode))
					{
						$('#studparentpincode').css("border","1px solid red");
						return false;
					}else{
						$('#studparentpincode').css("border","1px solid #CCCCCC");
					}
			 }else{
				 $('#studparentpincode').css("border","1px solid #CCCCCC");
			 }
			 
			var filter = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
			if($('#customerHelper').val()!="REVA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR") {//not required
				if(fatherAlternetEmailID != "")
				{
					if (!filter.test(fatherAlternetEmailID))
					{
						$('#studFatherEmail').css("border","1px solid red");
						return false;
					}else{
						$('#studFatherEmail').css("border","1px solid #CCCCCC");
					}
				}else{
					jAlert("Please enter email ");
					$('#studFatherEmail').css("border","1px solid #CCCCCC");
					return false;
				}
				if(motherAlternetEmailID != "")
				{
					if (!filter.test(motherAlternetEmailID))
					{
						$('#studMotherEmail').css("border","1px solid red");
						return false;
					}else{
						$('#studMotherEmail').css("border","1px solid #CCCCCC");
					}
				}else{
					jAlert("Please enter email ");
					$('#studMotherEmail').css("border","1px solid #CCCCCC");
					return false;
				}
	}else {
		if(fatherAlternetEmailID != "" && $('#customerHelper').val()!="MANDSAUR")
		{
			if (!filter.test(fatherAlternetEmailID))
			{
				$('#studFatherEmail').css("border","1px solid red");
				return false;
			}else{
				$('#studFatherEmail').css("border","1px solid #CCCCCC");
			}
		}
	if(motherAlternetEmailID != "")
		{
			if (!filter.test(motherAlternetEmailID) && $('#customerHelper').val()!="MANDSAUR")
			{
				$('#studMotherEmail').css("border","1px solid red");
				return false;
			}else{
				$('#studMotherEmail').css("border","1px solid #CCCCCC");
			}
		}	
	}
}
		if($('#customerHelper').val()=="BLM_MUNJAL"){
		jConfirm("After pressing 'Save' button, if you click 'OK' then the tab will be locked. If you press 'Cancel' then the tab will not be locked. \nIn both cases, data will be saved.","Confirm",function(r)
				{
			if(r ==true)
			{
				var parentStatus =true;
				$.ajax({
					url:'studentParentInfoForFaculty.json?studId='+studId, 
					data:{fatherFirstName:fatherFirstName,fatherMiddleName:fatherMiddleName,fatherLastName:fatherLastName,motherFirstName:motherFirstName,motherAlternetEmailID:motherAlternetEmailID,motherMobileNo:motherMobileNo
						,motherMiddleName:motherMiddleName,motherLastName:motherLastName,fatherAlternetEmailID:fatherAlternetEmailID,parentStd:parentStd,parentPhoneNo:parentPhoneNo,
						fatherMobileNo:fatherMobileNo,parentFax:parentFax,parentAddressResi:parentAddressResi,fatherOfficeAddress:fatherOfficeAddress,motherOfficeAddress:motherOfficeAddress,
						parentTaluka:parentTaluka,parentDistrict:parentDistrict,parentVillage:parentVillage,
						parentTaluka:parentTaluka,parentPinCode:parentPinCode,fatherProfession:fatherProfession,parentAnnualIncome:parentAnnualIncome,parentCreamyLayer:parentCreamyLayer,motherProfession:motherProfession,
						motherOfficeStd:motherOfficeStd,motherOfficePhnoneNo:motherOfficePhnoneNo,fatherHomeStd:fatherHomeStd,fatherHomephoneNo:fatherHomephoneNo,motherHomeStd:motherHomeStd,motherHomePhnoneNo:motherHomePhnoneNo,
						motherAnnualIncome:motherAnnualIncome,fatherOrganization:fatherOrganization,motherOrganization:motherOrganization,fatherDesignation:fatherDesignation,
						motherDesignation:motherDesignation,fatherState:fatherState,motherState:motherState,parentStatus:parentStatus,fatherQualification:fatherQualification,motherQualification:motherQualification},
						dataType:'json',
						type:'POST',
						success:function(jArray){
							getCustomAlertMessage("alert_create");
							getStudParentDetailsProfile();
						},
						error:function(){
							getCustomAlertMessage("alert_error");
						}
				});
			}else{
				$.ajax({
					url:'studentParentInfoForFaculty.json?studId='+studId, 
					data:{fatherFirstName:fatherFirstName,fatherMiddleName:fatherMiddleName,fatherLastName:fatherLastName,motherFirstName:motherFirstName,motherAlternetEmailID:motherAlternetEmailID,motherMobileNo:motherMobileNo
						,motherMiddleName:motherMiddleName,motherLastName:motherLastName,fatherAlternetEmailID:fatherAlternetEmailID,parentStd:parentStd,parentPhoneNo:parentPhoneNo,
						fatherMobileNo:fatherMobileNo,parentFax:parentFax,parentAddressResi:parentAddressResi,fatherOfficeAddress:fatherOfficeAddress,motherOfficeAddress:motherOfficeAddress,
						parentTaluka:parentTaluka,parentDistrict:parentDistrict,parentVillage:parentVillage,
						parentTaluka:parentTaluka,parentPinCode:parentPinCode,fatherProfession:fatherProfession,parentAnnualIncome:parentAnnualIncome,parentCreamyLayer:parentCreamyLayer,motherProfession:motherProfession,
						motherOfficeStd:motherOfficeStd,motherOfficePhnoneNo:motherOfficePhnoneNo,fatherHomeStd:fatherHomeStd,fatherHomephoneNo:fatherHomephoneNo,motherHomeStd:motherHomeStd,motherHomePhnoneNo:motherHomePhnoneNo,
						motherAnnualIncome:motherAnnualIncome,fatherOrganization:fatherOrganization,motherOrganization:motherOrganization,fatherDesignation:fatherDesignation,
						motherDesignation:motherDesignation,fatherState:fatherState,motherState:motherState,fatherQualification:fatherQualification,motherQualification:motherQualification},
						dataType:'json',
						type:'POST',
						success:function(jArray){
							getCustomAlertMessage("alert_create");
							getStudParentDetailsProfile();
						},
						error:function(){
							getCustomAlertMessage("alert_error");
						}
				});
			}
				});
}else{
	$.ajax({
		url:'studentParentInfoForFaculty.json?studId='+studId, 
		data:{fatherFirstName:fatherFirstName,fatherMiddleName:fatherMiddleName,fatherLastName:fatherLastName,motherFirstName:motherFirstName,motherAlternetEmailID:motherAlternetEmailID,motherMobileNo:motherMobileNo
			,motherMiddleName:motherMiddleName,motherLastName:motherLastName,fatherAlternetEmailID:fatherAlternetEmailID,parentStd:parentStd,parentPhoneNo:parentPhoneNo,
			fatherMobileNo:fatherMobileNo,parentFax:parentFax,parentAddressResi:parentAddressResi,fatherOfficeAddress:fatherOfficeAddress,motherOfficeAddress:motherOfficeAddress,
			parentTaluka:parentTaluka,parentDistrict:parentDistrict,parentVillage:parentVillage,
			parentTaluka:parentTaluka,parentPinCode:parentPinCode,fatherProfession:fatherProfession,parentAnnualIncome:parentAnnualIncome,parentCreamyLayer:parentCreamyLayer,motherProfession:motherProfession,
			motherOfficeStd:motherOfficeStd,motherOfficePhnoneNo:motherOfficePhnoneNo,fatherHomeStd:fatherHomeStd,fatherHomephoneNo:fatherHomephoneNo,motherHomeStd:motherHomeStd,motherHomePhnoneNo:motherHomePhnoneNo,
			motherAnnualIncome:motherAnnualIncome,fatherOrganization:fatherOrganization,motherOrganization:motherOrganization,fatherDesignation:fatherDesignation,
			motherDesignation:motherDesignation,fatherState:fatherState,motherState:motherState,fatherQualification:fatherQualification,motherQualification:motherQualification,fatherAadhaarNO:fatherAadhaarNO,motherAadhaarNO:motherAadhaarNO},
			dataType:'json',
			type:'POST',
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				getStudParentDetailsProfile();
			},
			error:function(){
				getCustomAlertMessage("alert_error");
			}
	});
}
	
}
function uploadStudParentDocument(parentId){
	if(parentId != '' && parentId != 'null' && parentId != undefined){
		getDegreeCategoryId(gTabDegreeName.TabName_PARENTDETAILS,parentId);
} else
		jAlert('Student Parent record not found!');
}
function getStudParentDetailsProfile()
{
		var studId= $('#studentId').val();
		var hdnUserRole = $('#hdnUserRole').val();
		var url = 'getParentDetailsProfile.json?studId='+studId;
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_getParentDetailsProfile.json';
		}
		$.ajax({
			url:url, 
			dataType:'json',
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray)
			{

				$('#saveParentDetailsBtnDiv').show();
				if(jArray.length>0)
				{
					var faxStdCode="";
					var faxNumber="";
					if(jArray[0]["parentFax"]!=undefined)
					{
						faxStdCode=jArray[0]["parentFax"].split("-")[0];
						faxNumber=jArray[0]["parentFax"].split("-")[1];
					}
					$('#studParentfirstname').val(jArray[0]["fatherFirstName"]);
					$('#studParentmiddlename').val(jArray[0]["fatherMiddleName"]);
					$('#studParentlastname').val(jArray[0]["fatherLastName"]);
					$('#parentAddressResi').val(jArray[0]["parentAddress"]);
					$('#studFatherno').val(jArray[0]["fatherMobileNo"]);
					$('#parentstudphnumber').val(jArray[0]["parentPhoneNo"]);
					$('#parentfaxstdcode').val(faxStdCode);
					$('#applstudparentfaxno').val(faxNumber);
					$('#studFatherEmail').val(jArray[0]["fatherAlternetEmailID"]);
					$('#studParentprofession').val(jArray[0]["fatherProfession"]);

					if(jArray[0]["parentAnnualIncome"]=="" || jArray[0]["parentAnnualIncome"]==undefined)
						$('#studparentannulaincome').val("");
					else
						$('#studparentannulaincome').val(jArray[0]["parentAnnualIncome"]);
					
					$('#studParenttaluka').val(jArray[0]["parentTaluka"]);
					$('#studparentdistrict').val(jArray[0]["parentDistrict"]);
					$('#studparentpincode').val(jArray[0]["parentPinCode"]);
					$('#studParentvillage').val(jArray[0]["parentVillage"]);
					
					
					if(jArray[0]["parentDetailsStatus"]==true || jArray[0]["parentDetailsStatus"] == 'true'){
						$('#saveParentDetailsBtnDiv').hide();
					}else{
						$('#saveParentDetailsBtnDiv').show();
					}

					$('#studmotherfirstname').val(jArray[0]["motherFirstName"]);
					$('#studmothermiddlename').val(jArray[0]["motherMiddleName"]);
					$('#studmotherlastname').val(jArray[0]["motherLastName"]);
					$('#studmotherMobile').val(jArray[0]["motherMobileNo"]);
					$('#studMotherEmail').val(jArray[0]["motherAlternetEmailID"]);
					$('#studMotherProfession').val(jArray[0]["motherProfession"]);
					
					$('#txtFatherQualification').val(jArray[0]["fatherQualification"]);
					$('#txtMotherQualification').val(jArray[0]["motherQualification"]);
					
					$('#txtFatherOfficeAddress').val(jArray[0]["fatherOfficeAddress"]);
					$('#txtMotherOfficeAddress').val(jArray[0]["motherOfficeAddress"]);
					
					$('#motherstdcode').val(jArray[0]["motherOfficeStd"]);
					$('#motherphnumber').val(jArray[0]["motherOfficePhnoneNo"]);
					$('#fatherHomestd').val(jArray[0]["fatherHomeStd"]);
					$('#fatherStudPhnumber').val(jArray[0]["fatherHomephoneNo"]);
					$('#motherHomestdcode').val(jArray[0]["motherHomeStd"]);
					$('#motherHomePhnumber').val(jArray[0]["motherHomePhnoneNo"]);
					$("#studeparentstdcode").val(jArray[0]["parentStd"]);
					
					if(jArray[0]["motherAnnualIncome"]=="" || jArray[0]["motherAnnualIncome"]==undefined)
						$('#motherannulaincome').val("");
					else
						$('#motherannulaincome').val(jArray[0]["motherAnnualIncome"]);
					
					$('#fatherOrganization').val(jArray[0]["fatherOrganization"]);
					$('#motherOrganization').val(jArray[0]["motherOrganization"]);
					$('#fatherDesignation').val(jArray[0]["fatherDesignation"]);
					$('#motherDesignation').val(jArray[0]["motherDesignation"]);
					$('#txtFatherState').val(jArray[0]["fatherState"]);
					$('#txtMotherState').val(jArray[0]["motherState"]);
					
					if(jArray[0]["fatherFirstName"] !=""){
						     $('#fatherMobNo').html('<b style="color:red;font-size:16px;">*</b>');
		                        if($('#customerHelper').val()!="MAHINDRA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR")
						      $('#fatheroccupationId').html('<b style="color:red;font-size:16px;">*</b>');

	                            if($('#customerHelper').val()!="MAHINDRA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR"&& $('#customerHelper').val()!="MEGHE")
						     $('#fatherIncomeId').html('<b style="color:red;font-size:16px;">*</b>');
						}else{
							 $('#fatherMobNo').html('');
						     $('#fatheroccupationId').html('');
						     $('#fatherIncomeId').html('');
						}
						
					if( jArray[0]["motherFirstName"] !=""){
					
						    $('#motherMobNo').html('<b style="color:red;font-size:16px;">*</b>');
			                 if($('#customerHelper').val()!="MAHINDRA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR")
						     $('#motheroccupationId').html('<b style="color:red;font-size:16px;">*</b>');
	
			                   if($('#customerHelper').val()!="MAHINDRA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR"&& $('#customerHelper').val()!="MEGHE")
						    $('#motherIncomeId').html('<b style="color:red;font-size:16px;">*</b>');
					     }else{
					    	 $('#motherMobNo').html('');
					  	    $('#motheroccupationId').html('');
					  	    $('#motherIncomeId').html('');
					     }
					$('#studParentfirstname,#studParentmiddlename,#studParentlastname').keyup(function(){
                              $(this).val($(this).val().toUpperCase());
                           });

                        $('#studmotherfirstname,#studmothermiddlename,#studmotherlastname').keyup(function(){
                                $(this).val($(this).val().toUpperCase());
                             });
					$('#fatherOrganization').val(jArray[0]["fatherOrganizationName"]);//
					$('#motherOrganization').val(jArray[0]["motherOrganizationName"]);
					$('#fatherDesignation').val(jArray[0]["fatherDesignationName"]);
					$('#motherDesignation').val(jArray[0]["motherDesignationName"]);
					$('#txtFatherOfficeAddress').val(jArray[0]["fatherOfficeAddressName"]);
					$('#txtMotherOfficeAddress').val(jArray[0]["motherOfficeAddressName"]);
					$('#motherphnumber').val(jArray[0]["officemotherphonenumber"]);
					$('#fatherStudPhnumber').val(jArray[0]["fatherStudentPhnumber"]);
					$('#motherHomePhnumber').val(jArray[0]["motherHomePhnumber"]);
					$('#studParentAadhaarNo').val(jArray[0]["fatherAadhaarNO"]);
					$('#studMotherAadhaarNo').val(jArray[0]["motherAadhaarNO"]);
					//$('#fatherOrganization').val();
					/*if(jArray[0]["fatherMobileNo"] !="" && jArray[0]["fatherMobileNo"]!=undefined){
						$('#studFatherno').val(jArray[0]["fatherMobileNo"]);
						$('#studFatherno').attr('disabled',true);
					}else{
						$('#studFatherno').val(jArray[0]["fatherMobileNo"]);
						$('#studFatherno').attr('disabled',false);
					}
					
					if(jArray[0]["motherMobileNo"] !="" && jArray[0]["motherMobileNo"]!=undefined ){
						$('#studmotherMobile').val(jArray[0]["motherMobileNo"]);
						$('#studmotherMobile').attr('disabled',true);
					}else{
						$('#studmotherMobile').val(jArray[0]["motherMobileNo"]);
						$('#studmotherMobile').attr('disabled',false);
					}
					
					if(jArray[0]["fatherHomephoneNo"] !="" && jArray[0]["fatherHomephoneNo"] !=undefined){
						$('#fatherStudPhnumber').val(jArray[0]["fatherHomephoneNo"]);
						$('#fatherStudPhnumber').attr('disabled',true);
					}else{
						$('#fatherStudPhnumber').val(jArray[0]["fatherHomephoneNo"]);
						$('#fatherStudPhnumber').attr('disabled',false);
					}
					
					if(jArray[0]["fatherHomeStd"] !="" && jArray[0]["fatherHomeStd"] !=undefined){
						$('#fatherHomestd').val(jArray[0]["fatherHomeStd"]);
						$('#fatherHomestd').attr('disabled',true);
					}else{
						$('#fatherHomestd').val(jArray[0]["fatherHomeStd"]);
						$('#fatherHomestd').attr('disabled',false);
					}
					
					if(jArray[0]["motherHomeStd"] !="" && jArray[0]["motherHomeStd"] !=undefined){
						$('#motherHomestdcode').val(jArray[0]["motherHomeStd"]);
						$('#motherHomestdcode').attr('disabled',true);
					}else{
						$('#motherHomestdcode').val(jArray[0]["motherHomeStd"]);
						$('#motherHomestdcode').attr('disabled',false);
					}
					
					if(jArray[0]["motherHomePhnoneNo"] !="" && jArray[0]["motherHomePhnoneNo"] !=undefined){
						$('#motherHomePhnumber').val(jArray[0]["motherHomePhnoneNo"]);
						$('#motherHomePhnumber').attr('disabled',true);
					}else{
						$('#motherHomePhnumber').val(jArray[0]["motherHomePhnoneNo"]);
						$('#motherHomePhnumber').attr('disabled',false);
					}
					
					if(jArray[0]["parentPhoneNo"] !="" && jArray[0]["parentPhoneNo"] !=undefined){
						$('#parentstudphnumber').val(jArray[0]["parentPhoneNo"]);
						$('#parentstudphnumber').attr('disabled',true);
					}else{
						$('#parentstudphnumber').val(jArray[0]["parentPhoneNo"]);
						$('#parentstudphnumber').attr('disabled',false);
					}
					
					if(jArray[0]["motherOfficeStd"] !="" && jArray[0]["motherOfficeStd"] !=undefined){
						$('#motherstdcode').val(jArray[0]["motherOfficeStd"]);
						$('#motherstdcode').attr('disabled',true);
					}else{
						$('#motherstdcode').val(jArray[0]["motherOfficeStd"]);
						$('#motherstdcode').attr('disabled',false);
					}
					
					if(jArray[0]["motherOfficePhnoneNo"] !="" && jArray[0]["motherOfficePhnoneNo"] !=undefined){
						$('#motherphnumber').val(jArray[0]["motherOfficePhnoneNo"]);
						$('#motherphnumber').attr('disabled',true);
					}else{
						$('#motherphnumber').val(jArray[0]["motherOfficePhnoneNo"]);
						$('#motherphnumber').attr('disabled',false);
					}
					
					if(jArray[0]["parentStd"] !="" && jArray[0]["parentStd"] !=undefined){
						$("#studeparentstdcode").val(jArray[0]["parentStd"]);
						$('#studeparentstdcode').attr('disabled',true);
					}else{
						$('#studeparentstdcode').val(jArray[0]["motherOfficePhnoneNo"]);
						$('#studeparentstdcode').attr('disabled',false);
					}*/
					var parentId = 0;
					if(jArray[0]["parentId"]!=undefined)
						parentId=jArray[0]["parentId"];
					$('#uploadStudParentDoc').bind('click', function(){
						uploadStudParentDocument(parentId);
					});
				}
				getFatherMotherProfileImagesStatus();
				getstudProfileConfigurationDetailsCheckingNames();
			},
		});	
}	

function addStudentGuardianDetails()
{
	var studId= 0;
	var gaurdinName = $('#gaurdiName').val();
	var guardianMiddleName = $('#studguardianmiddlename').val();
	var guardianLastName = $('input[name = "guardianLastName"]').val();
	var guardianRelation = $('#guardianRelation').val();
	var gaurdianEmailID =$('#gaurdiEmail').val();
	var gaurdianAddress = $('#guardiAddress').val();
	var cityId = $('#cityCombo').val();
	var guardianTaluka = $('input[name = "guardianTaluka"]').val();
	var guardianVillage = $('input[name = "guardianVillage"]').val();

	var guardianPinCode = $('input[name = "guardianPinCode"]').val();
	var guardianStd = $('input[name = "guardianStd"]').val();
	var guardianPhoneNo = $('#guardianstudphnumber').val();
	var guardianFax = $('input[name = "guardianFax"]').val();
	var gaurdianMobileNo = $('#studguardiancellno').val();
	var studGuardianId = $('#studGuardianId').val();
	
	var regExp = /^[A-Za-z. ]+$/;
	if($('#customerHelper').val()!="MANDSAUR" && (gaurdinName == "" || !regExp.test(gaurdinName)))
	{
		$('#gaurdiName').css("border","1px solid red");
		return false;
	}else{
		$('#gaurdiName').css("border","1px solid #CCCCCC");
	}
	if(guardianMiddleName != "")
	{
		if(!regExp.test(guardianMiddleName))
		{	
			$('#studguardianmiddlename').css("border","1px solid red");
			return false;
		}else{
			$('#studguardianmiddlename').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#studguardianmiddlename').css("border","1px solid #CCCCCC");
	}
	if($('#customerHelper').val()!="MANDSAUR" && (guardianLastName == ""  || !regExp.test(guardianLastName)))
	{
		$('#studguardianlastname').css("border","1px solid red");
		return false;
	}else{
		$('#studguardianlastname').css("border","1px solid #CCCCCC");
	}
	
	if($('#customerHelper').val()!="MANDSAUR" && guardianRelation == 0)
	{
		$('#guardianRelation').css("border","1px solid red");
		return false;
	}else{
		$('#guardianRelation').css("border","1px solid #CCCCCC");
	}
	if($('#customerHelper').val()!="MANDSAUR" && (gaurdianMobileNo == "" || isNaN(gaurdianMobileNo)))
	{
		$('#studguardiancellno').css("border","1px solid red");
		return false;
	}
	else if(gaurdianMobileNo != "" && ((gaurdianMobileNo.length < 10)))
	{
		jAlert("Mobile Number must be of minimum 10 Digits.");
		$('#studguardiancellno').css("border","1px solid red");
		return false;
	}else{
		$('#studguardiancellno').css("border","1px solid #CCCCCC");
	}
	if(guardianPhoneNo != "")
	{
		if(guardianStd=="" || isNaN(guardianStd))
		{
			$('#studeguardianstdcode').css("border","1px solid red");
			return false;
		}else{
			$('#studeguardianstdcode').css("border","1px solid #CCCCCC");
		}	
		if(isNaN(guardianPhoneNo))
		{
			$('#guardianstudphnumber').css("border","1px solid red");
			return false;
		}
		else{
			$('#guardianstudphnumber').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#guardianstudphnumber').css("border","1px solid #CCCCCC");
		$('#studeguardianstdcode').css("border","1px solid #CCCCCC");
	}
	if($('#customerHelper').val()!="MANDSAUR" && ( gaurdianAddress == ""))
	{
		$('#guardiAddress').css("border","1px solid red");
		return false;
	}else{
		$('#guardiAddress').css("border","1px solid #CCCCCC");
	}
	
	if(guardianTaluka !="" )
	{
		if(!regExp.test(guardianTaluka))
		{
			$('#studguardiantaluka').css("border","1px solid red");
			return false;
		}else{
			$('#studguardiantaluka').css("border","1px solid #CCCCCC");
		}
		
	}else{
		$('#studguardiantaluka').css("border","1px solid #CCCCCC");
	}
	
	if(guardianVillage != "")
	{
		if(!regExp.test(guardianVillage))
		{
			$('#studguardianVillage').css("border","1px solid red");
			return false;
		}else{
			$('#studguardianVillage').css("border","1px solid #CCCCCC");
		}
	}
	else{
		$('#studguardianVillage').css("border","1px solid #CCCCCC");
	}
	
	if($('#customerHelper').val()!="MANDSAUR" && (guardianPinCode=="" || isNaN(guardianPinCode)))
	{
		$('#studguardianpincode').css("border","1px solid red");
		return false;
	}else{
		$('#studguardianpincode').css("border","1px solid #CCCCCC");
	}
	
	var filter = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	
	if(gaurdianEmailID != "")
	{
		if (!filter.test(gaurdianEmailID))
		{
			$('#gaurdiEmail').css("border","1px solid red");
			return false;
		}else{
			$('#gaurdiEmail').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#gaurdiEmail').css("border","1px solid #CCCCCC");
	}
	
	$.ajax({
		url:'studentGuardianInfoForFaculty.json?studId='+studId,  
		data:{cityId:cityId,gaurdinName:gaurdinName,guardianMiddleName:guardianMiddleName,guardianLastName:guardianLastName,gaurdianEmailID:gaurdianEmailID,guardianRelation:guardianRelation,gaurdianAddress:gaurdianAddress,guardianTaluka:guardianTaluka,guardianVillage:guardianVillage,guardianPinCode:guardianPinCode,guardianStd:guardianStd,guardianPhoneNo:guardianPhoneNo,guardianFax:guardianFax,gaurdianMobileNo:gaurdianMobileNo,studGuardianId:studGuardianId},
		dataType:'json',
		type:'POST',
		success:function(jArray){
			getCustomAlertMessage("alert_create");
			getstudentGaurdianinfo();
		},
		error:function(){
			//jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}


