//				'</td>';
//				}
			tempSemBoiler +='<td style="text-align: right;">'	+ temp + '</td>';

//					if (jArray[0]["creditRegistered2"+ temp + ""] != undefined)
//						tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsReg(this);" id="txtCreditRegistered'+ temp+'" readonly="readonly" value="'+ jArray[0]["creditRegistered2"+ temp + ""]+'"  class="span1 credRegistCls forNum" /></td>';
//					else
						tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsReg(this);" id="txtCreditRegistered'	+ temp+ '" readonly="readonly" value="0" class="span1 credRegistCls forNum" /></td>';

//					if (jArray[0]["creditEarned2" + temp+ ""] != undefined)
//						tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsEar(this);" id="txtCreditEarned'+ temp+ '" readonly="readonly" value="'+ jArray[0]["creditEarned2"+ temp + ""]+ '" class="span1 credEarCls forNum" /></td>';
//					else
						tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsEar(this);" id="txtCreditEarned'	+ temp+ '" readonly="readonly" value="0" class="span1 credEarCls forNum" /></td>';
					
//					if (jArray[0]["totMarksPerc2"+ temp + ""] !="" && jArray[0]["totMarksPerc2"+ temp + ""] != undefined && jArray[0]["totMarksPerc2"+ temp + ""]!="undefined" && jArray[0]["totMarksPerc2" + temp + ""] !="0")
//						tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="calcultateCgpaSgpa(this);" id="txttotalMarks'+ temp+ '" readonly="readonly" value="'+ jArray[0]["totMarksPerc2"+ temp + ""]	+ '" class="span1 totPtsCls forNum" /></td>';
//					else
						tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="calcultateCgpaSgpa(this);" id="txttotalMarks'+ temp+ '" readonly="readonly" value="0" class="span1 totPtsCls forNum" /></td>';
					
