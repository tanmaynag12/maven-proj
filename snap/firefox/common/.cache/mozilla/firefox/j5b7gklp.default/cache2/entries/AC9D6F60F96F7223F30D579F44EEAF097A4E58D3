function fetchApplicantQualificationForRefreshingFlags1(){
	var instituteCourseStrengthId = $('#saveBtn').attr('institutecoursestrengthid');
	if(instituteCourseStrengthId=='undefined' || instituteCourseStrengthId==undefined){
		instituteCourseStrengthId = $('#cbAcademicBatch').val().split("-")[0];
	}
	
	$.ajax({
				url : 'fetchApplicantQualification.json?instituteCourseStrengthId='+ instituteCourseStrengthId,
				dataType : 'json',
				type:'POST',
				async : false,
				success : function(json) {
/*					if(json!=undefined){*/
					var jArray = json["courseEligibilityList"];
					if(jArray!=undefined && jArray!=null){
					callForAdmissionFromDate = jArray["callForAdmissionFromDate"];
					callForAdmissionToDate = jArray["callForAdmissionToDate"];
					gCallForAdmissionFromDate = jArray["callForAdmissionFromDate"];		
					isSubjectChoice = json["isSubjectChoice"];
					isWorkExperience = json["isWorkExperience"];
					isAllowDocumentDetails = json["isAllowDocumentDetails"];
					isAdmissionQuota = json["isAllowAdmissionQuota"];
					isPhSportsArmyOrExServiceman = json["isPhSportsArmyOrExServiceman"];
					isProfileImage = json["isProfileImage"];
					isAllowEntranceExamHallTicket = json["allowEntranceExamHallTicket"];
					allowEntranceExamDetails = json["allowEntranceExamDetails"];
					allowGraduationTotal = json["allowGraduationTotal"];
					campusPreferences = json["campusPreferences"];
					allowAdmissionShiftPreferences = json["allowAdmissionShiftPreferences"];
					allowSingleAdmissionShiftPreferences = json["allowSingleAdmissionShiftPreferences"];
					allowmediumOfCourse = json["allowmediumOfCourse"];
					allowChoiceOfSpecialSubject = json["allowChoiceOfSpecialSubject"];
					allowCastDetails = json["allowCastDetails"];
					allowMinorityChoice = json["allowMinorityChoice"]; 
					allowSourseOfBrand = json["allowSourseOfBrand"];
					allowEligibilityPDF = json["allowEligibilityPDF"];
					allowMathematicSubjectYesNo = json["allowMathematicMarksYesNo"];
					isSponsoredCandidate = json["isSponsoredCandidate"];
					allowParentDtls = json["allowParentDtls"];
					allowGuardianDtls = json["allowGuardianDtls"];
					mariitalStatusCls = json["mariitalStatusCls"];
					placeOfBirthCls = json["placeOfBirthCls"];
					isOnlineAdmission = json["isOnlineAdmission"];
					isBankAdmission = json["isBankAdmission"];
					isAllowHamiptraPdf = json["isAllowHamiptraPdf"];
					isAllowOtherDetails = json["isAllowOtherDetails"];
					isAllowMedicalInformation = json["isAllowMedicalInformation"];
					allowOnlyForReligious = json["allowOnlyForReligious"];
					isShowNursingCouncil = json["isShowNursingCouncil"];
					professionalAssociation=json["professionalAssociation"];
					sonOfStaff=json["sonOfStaff"];
					allowMeritNumberDetails = json["allowMeritNumberDetails"];
					allowUniversityNo = json["allowUniversityNo"];
					isAllowFamilyBackground = json["isAllowFamilyBackground"];
					isOnlineAdmissionFromProvisional = json["isOnlineAdmissionFromProvisional"];
					isAllowCourseEligibility = json["allowCourseEligibility"];
					isAllowMedicalQualificationDetails = json["isAllowMedicalQualificationDetails"];
					isAllowSpecialPdfFormat = json["isAllowSpecialPdfFormat"];
					isAllowRuralService = json["isAllowRuralService"];
					isAllowExtraCarricularActivities = json["isAllowExtraCarricularActivities"];
					isAllowOtherParticulars = json["isAllowOtherParticulars"];
					isAllowApplicationFormFooter=json["isAllowApplicationFormFooter"];
					allowIsForeigner=json["allowIsForeigner"];
					isBloodGroupOptional = json["isBloodGroupOptional"];
					isTalukaOptional = json["isTalukaOptional"];
					isAadhaarOptional = json["isAadhaarOptional"];
					isAadhaarUnEditable = json["isAadhaarUnEditable"];
					isStateOfDomicileOptional = json["isStateOfDomicileOptional"]
					minimumAge = json["minimumAge"];
					allowParentsDetailsInApplicationForm = json["allowParentsDetailsInApplicationForm"];
					allowGuardiansDetailsInApplicationForm = json["allowGuardiansDetailsInApplicationForm"];
					isAllowTwoReferance = json["isAllowTwoReferance"];
					isAllowStatementOfPurpose = json["isAllowStatementOfPurpose"];
					allowWithoutPayment = json["allowWithoutPayment"];
					allowAgriculturalDetails = true;
					allowPhdGuideDtls = json["allowPhdGuideDtls"];
					allowParentPhoto = json["allowParentPhoto"];
					isAllowEntranceExamHallTicketForSelectedStudent = json["isAllowEntranceExamHallTicketForSelectedStudent"];
					allowParentsWorkDetailsInApplicationForm =json["allowParentsWorkDetailsInApplicationForm"];
					isHallTicketShow = json["isHallTicketShow"];
					
					//allowAgriculturalDetails = true;
					allowPhdGuideDtls = json["allowPhdGuideDtls"];
//					allowParentPhoto = json["allowParentPhoto"];
					
					for ( var i = 0; i < jArray.length; i++) {
						if (jArray[i]["name"] == 'SSC') {
							isSSCDetailStatus = jArray[i]["SSCDetailStatus"];
							isAllowCountryStateCityOptionSSC   = jArray[i]["isAllowCountryStateCityOption"];
							}
						else if (jArray[i]["name"] == 'HSC') {
							isHscDetailStatus = jArray[i]["hscDetailStatus"];
							isAllowCountryStateCityOptionHSC   = jArray[i]["isAllowCountryStateCityOption"];
						}
						else if (jArray[i]["name"] == 'Graduation' || jArray[i]["name"] == 'Any Graduation') 
							globalEligibilityCriteriaUniverseId = jArray[i]["eligibilityCriteriaUniverseId"];
						else if (jArray[i]["name"] == 'Specific Graduation' || jArray[i]["name"] == 'Any Graduation') 
							isAllowedDegreeDetails = jArray[i]["isAllowedDegreeDetails"];
						else if (jArray[i]["name"] == 'Specific Post Graduation') 
							isAllowedDegreeDetailsPG = jArray[i]["isAllowedDegreeDetailsPG"];
					}
				}else{
					//jAlert("Admission round not created.");
				}
			},
				error : function() {
					jAlert("In Flag error");
				}

			});
	}


function GenrateAdmissionPDF() {
	var showParentDetails = false;
	var allowGuardianDtlsForPdf = false;
	var isAllowRuralService1 = false; 
	var isAllowExtraCarricularActivities1 = false;
	var isAllowOtherParticulars1 = false; 
	var allowMedicalInformationForPdf = false; 
	var isAllowCourseEligibilityForPdf=false;
	var allowOnlyForReligiousForPdf = false;
	var isShowNursingCouncilForPdf = false;
	var isShowCampusPreference = false;
	
	if(isAllowCourseEligibility){
		isAllowCourseEligibilityForPdf=true;
	}
	
	
	if (allowAddressSplit){
		allowAddressSplitForPdf=true;
	}
	
	var professionalAssociationForPdf = false;
	if(professionalAssociation){
		professionalAssociationForPdf=true;
	}
	

	var sonOfStaffForPdf = false;
	if(sonOfStaff){
		sonOfStaffForPdf=true;
	}
	
	if(isAllowFamilyBackground)
		showParentDetails = true;
	
	if(allowOnlyForReligious){
		allowOnlyForReligiousForPdf = true;
	}
	
	
	if(isShowNursingCouncil){
		isShowNursingCouncilForPdf = true;
	}
	
	if(isAllowMedicalInformation){
		allowMedicalInformationForPdf = true;
	}
	
	if(campusPreferences){
		isShowCampusPreference = true;
	}
	
	 
	
	if (gCallForAdmissionFromDate != "" && gCallForAdmissionToDate != ""){
		if(allowParentDtls)
			showParentDetails = true;
		if(allowGuardianDtls)
			allowGuardianDtlsForPdf = true;
		
		if(isAllowRuralService)
			isAllowRuralService1 = true;
		if(isAllowExtraCarricularActivities)
			isAllowExtraCarricularActivities1 = true;
		if(isAllowOtherParticulars)
			isAllowOtherParticulars1 = true; 
		
		
	}else{
		if(allowParentsDetailsInApplicationForm){
			showParentDetails = true;
		}
		if(allowGuardiansDetailsInApplicationForm){
			allowGuardianDtlsForPdf = true;
		}
	} 
	
	
	var instituteCourseStrengthId = $('#saveBtn').attr('institutecoursestrengthid');
	
	fetchApplicantQualificationForRefreshingFlags1(instituteCourseStrengthId);
	
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RAMAIAH" && isAllowSpecialPdfFormat){
			window.location.replace('genrateRAMAIAHAdmissionPDF.json?instituteCourseStrengthId='+ instituteCourseStrengthId + '&isHscDetailStatus='+isHscDetailStatus +
					'&isSSCDetailStatus='+ isSSCDetailStatus + '&isAllowDocumentDetails='+ isAllowDocumentDetails + + '&isAllowStudentCorrespondenceCertificateDetails='+ isAllowStudentCorrespondenceCertificateDetails +
					+ '&isSscMarksEnglishDetails='+ isSscMarksEnglishDetails
					+ '&isSscMarksSecondLanguageDetails='+ isSscMarksSecondLanguageDetails+ '&isSscMarksScienceDetails='+ isSscMarksScienceDetails
					+ '&isSscMarksSocialStudiesDetails='+ isSscMarksSocialStudiesDetails +
					'&isPhSportsArmyOrExServiceman='+ isPhSportsArmyOrExServiceman + '&isAdmissionQuota='+ isAdmissionQuota + 
					'&allowmediumOfCourse='+ allowmediumOfCourse + '&allowChoiceOfSpecialSubject='+ allowChoiceOfSpecialSubject+ 
					'&allowCastDetails='+ allowCastDetails+
