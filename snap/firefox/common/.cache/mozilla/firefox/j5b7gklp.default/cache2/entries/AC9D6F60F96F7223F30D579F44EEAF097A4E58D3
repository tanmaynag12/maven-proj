							Hobbies : Hobbies,
							motherTongue : motherTongue,
							otherLanguagesKnown:otherLanguagesKnown,
							religionId : religionId,
							nationalityId : nationalityId,
							studbloodgrp : studbloodgrp,
							studHostelite : studHostelite,
							categoryId : categoryId,
							PRN : PRN,
							domacileId : domacileId,
							fName : fName,
							mName : mName,
							lName : lName,
							contactHours : contactHours,
							motherName : motherName,
							fatherName:fatherName,
							tenthappearName : tenthappearName,
							licenceNo:licenceNo,
							Caste:Caste,
							isScollership:isScollership,
							adharCardNo:adharCardNo,
							subCaste:subCaste,
							physicallyHandicapped:physicallyHandicapped,
							sports:sports,
							armyOrExserviceman:armyOrExserviceman,
							admissionQuotaType:admissionQuotaType,
							admissionCategory:admissionCategory,
							strengthId:strengthId,
							isForiegner:isForiegner,
							placeOfBirth:placeOfBirth,
							enrollmentNo:enrollmentNo,
							eligibilityNo:eligibilityNo,
							vehicleNo:vehicleNo,
							musmino:musmino,
							CoOfConduct:CoOfConduct,
							AnRaggReg:AnRaggReg,
							othermino:othermino,
							studentPanNo:studentPanNo,
							personalEmailID:personalEmailID,
							studplacemtmob1 : studplacemtmob1,
							studplacemtmob2 : studplacemtmob2,
							applicationId : applicationId,
							utrNo:utrNo,
							skypeId:skypeId,
							voterIdNo:voterIdNo,
							marital_Status:marital_Status,
							abcAccountId:abcAccountId
						},
						dataType : 'json',
						type : 'POST',
						async : false,
						beforeSend : function() {
							startCommonAjaxSpinner();
						},
						complete : function() {
							stopCommonAjaxSpinner();
						},
						success : function(jArray) {
							if (jArray[0].responseFlag == '1')
								gIsForiegner = jArray[0].isForiegner;
							/*if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="INDIRA_PUNE")
							{
								$('a[href="#passportAndVisaDetailsDetails"]').show();
							}
							else{*/
							if(jArray[0].isForiegner)
							{
								$('a[href="#passportAndVisaDetailsDetails"]').show();
							}else{
								$('a[href="#passportAndVisaDetailsDetails"]').hide();
							}
							//}

							getCustomAlertMessage("alert_create");
							getstudentPersonalinfo();
							if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
								getStudentContactForStudentDetailsTab();
							}
						},
						error : function() {
						}
					});
						
				}
					});
				
		}
		else if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY')
			{

			$.ajax({
				url : 'studentProfilePost.json', 
				data : {
					txtReg : txtReg,
