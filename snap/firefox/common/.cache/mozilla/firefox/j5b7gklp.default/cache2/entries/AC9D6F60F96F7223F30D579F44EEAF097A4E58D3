function studentResumeDocument(){

	var strengthId=$('#strengthId').val();
	$.ajax({
		url:'checkCvUploadDeadline.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var isButton=false;
			var cvTypeArr=jArray[1];
//			'<li><a href="javascript:void(0);" class="expenseDoc"><i class="icon-folder-open "></i> Documents</a></li>'+
			var boiler = '<table class="table table-bordered"><tr><td><b>Attached Docs:</b></td>'+
			'<td colspan="3" class = "UploadedResumeFilesList"><div id="studentResumeList"></div></td></tr>'+
			'<tr><td><b>Upload</b></td><td colspan="3" class="divEmployeeLeaveUploadedFilesUploadBar">'+
			'<div class="preview"></div>';
			/***Condition to show button only for gaziabad 2nd year**/
			if(jArray[0]["flag"]==true){
				//Hide CV Upload Button
			}else{
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="GOKHALE_INSTITUTE"){
					if(cvTypeArr.length>0){
					isButton=true;
					boiler+='<table class="table table-bordered">';
					for(obj of cvTypeArr){
						boiler+='<tr><td>'+obj.cvType+'</td><td>';
						boiler+='<div class="btn btn-info" style = "width:165px;height:20px">Upload CV'+
						'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="studentResumeUploadBtn_'+obj.cvTypeId+'" onclick="initResumeFileUploadPan(\''+obj.cvTypeId+'\',\''+obj.cvType+'\');"></div><br><br>';
						boiler+='</td></tr>';
					}
					boiler+='</table>';
				}
				}
				else if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD"){
					isButton=true;
					//boiler+='<div><input type="button" class="uploadResumeDocument btn btn-info" value="Upload Document"></div><br><br>';
					boiler+='<div class="btn btn-info" style = "width:165px;height:20px">Upload CV'+
					'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="studentResumeUploadBtn" onclick="initResumeFileUploadPan();"></div><br><br>';
				}else{
					isButton=true;
					boiler+='<div class="btn btn-info" style = "width:165px;height:20px">Upload CV'+
					'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="studentResumeUploadBtn" onclick="initResumeFileUploadPan();"></div><br><br>';
				}
			}
			boiler+='<div class = "alert alert-info" style="color:black;font-size:13px;">';
			if(jArray[0]["flag"]==true){
				if(jArray[0]["fromDate"]!='' && jArray[0]["toDate"]!=''){
					boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;<b style="color:red;">Upload button will be visible from '+jArray[0]["fromDate"]+' - '+jArray[0]["toDate"]+'</b><br/>';
				}else{
					boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;<b style="color:red;">CV Upload schedule not defined by TPO</b><br/>';
				}
			}else{
				boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;<b style="color:red;">Upload button will be visible from '+jArray[0]["fromDate"]+' - '+jArray[0]["toDate"]+'</b><br/>';
			}
			boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;Click on Upload Document Button to Upload Your Document <br />'+
			'<i class = "icon-hand-right"></i>&nbsp;&nbsp;.pdf files only<br />'+
			'<i class = "icon-hand-right"></i>&nbsp;&nbsp;Document once uploaded cannot be removed, it will be replaced. <br />'+
			'<i class = "icon-hand-right"></i>&nbsp;&nbsp;Once uploaded, file will get renamed by your roll no. and first name.<br />'+
			'</div>'+
			'</td></tr>';
			if(jArray[0]["flag"]!=true){
				boiler +='<tr><td><b>Upload Video Resume</b></td><td colspan="3" class="divEmployeeLeaveUploadedFilesUploadBar">';
				boiler +='<div style="display: none;" class="video-prev" class="pull-right">'+
			       '<video height="200" width="300" id="videoPre" class="video-preview" controls/>'+
			    	   '</div>';
				boiler +='<div class="btn btn-info" style = "width:165px;height:20px">Upload Video Resume'+
				'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData"  id="videoUpload" accept="video"></div><br><br>';
					boiler +='<div style="display:none;" id="uploadedVideoResumeDiv"></div>';
				boiler +='<div class = "alert alert-info" style="color:black;font-size:13px;">';
				boiler +='<i class = "icon-hand-right"></i>&nbsp;&nbsp;Click on Upload Document Button to Upload Your Video Resume <br />'+
				'<i class = "icon-hand-right"></i>&nbsp;&nbsp;.mp4 files only<br />'+
				'<i class = "icon-hand-right"></i>&nbsp;&nbsp;Document once uploaded cannot be removed, it will be replaced. <br />'+
				'<i class = "icon-hand-right"></i>&nbsp;&nbsp;Once uploaded, file will get renamed by your roll no. and first name.<br />'+
				'</div>';
				
					boiler +='</td></tr>';
				
				
			}
			boiler +='</table>';
			$('#otherResumeDocument').html(boiler);
			
			
			$('#videoUpload').on('change', function(){
			      if (isVideo($(this).val())){
			    	  uploadVideoResume();
			    	  var element=document.getElementById('videoUpload');
			    	  var file=element.files[0];
			    	  var $source=$('.video-preview');
			    	  console.log(file)
			    	 $('.video-preview').attr('src',URL.createObjectURL(file));
			        $('.video-prev').show();
			        
			           
			      }
			      else
			      {
			        $('.upload-video-file').val('');
			        $('.video-prev').hide();
			        jAlert("Only video files are allowed to upload.")
			      }
			    });	
			    
			    var divBoiler=``;
			for(obj of cvTypeArr){
					divBoiler+=`<div id="studentResumeList_${obj.cvTypeId}"></div>`;
					if(isButton==true)
						initResumeFileUploadPan(obj.cvTypeId,obj.cvType);
				}console.log(divBoiler);
				$('.UploadedResumeFilesList').html(divBoiler);
				getStudentResume();		
			if(isButton==true){
				uploadVideoResume();
				initResumeFileUploadPan();
				getStudentVideoResume();
			}
			
		},
		error:function(){
			////alert("in Error stream CMB");
		}
	});
}
function isVideo(filename) {
    var ext = getExtension(filename);
    switch (ext.toLowerCase()) {
    case 'mp4':

        return true;
    }
    return false;
}

function getExtension(filename) {
    var parts = filename.split('.');
    return parts[parts.length - 1];
}
function uploadVideoResume()
{
	
	$('document').ready(function() {
		var elementId ='videoUpload';
		var index=1;
		try {
		new AjaxUpload(elementId,{
			action :'uploadStudentPlacementvideo.json',
			data : {},
			name : 'fileData',
			size:AllowedFileSize.DOCUMENT_UPLOAD,
			onSubmit : function(file, extension) {
				/*	startLoading();*/
				startCommonAjaxSpinner();
				this.setData({
					'index':index,
					'fileSize':5000000
				});


			},
			onComplete : function(file, response) {

				stopCommonAjaxSpinner();

				getStudentVideoResume();
				getCustomAlertMessage("alert_create");

			}
		});
		 } catch (e) {}
	});	

}
//var IsResumeUploaded=false;
function getStudentResume()
{
	getStudentResumeByCategoryId(function(catIdArr){
		for(var obj of catIdArr){
			var {categoryId,cvTypId}=obj;
			var uploadDocument = new UploadDocument();
			uploadDocument.getDocument(categoryId, function(jArray)	{
				/*if(jArray.length!=''){
					IsResumeUploaded=true;
				}*/
				showStudentResumeDoc(jArray,cvTypId,true);
			});
		}
	});
}

function getStudentResumeByCategoryId(callback)
{
	$.ajax({
		url:'getStudentResumeCategoryIdsByStudentId.json',
		dataType:'json',
		//type:'POST',
		name : 'fileData',
		success:function(jArray){
			callback(jArray);
		}
	});
}
//Video Start
function getStudentVideoResume()
{
	getStudentVideoResumeByCategoryId(function(categoryId){
		var uploadDocument = new UploadDocument();
		uploadDocument.getDocument(categoryId, function(jArray)
				{
			
			showStudentVideoResumeDoc(jArray,'uploadedVideoResumeDiv',true);
				});
	});
}

function getStudentVideoResumeByCategoryId(callback)
{
	$.ajax({
		url:'getStudentVideoResumeByCategoryId.json',
		dataType:'text',
		//type:'POST',
		name : 'fileData',
		success:function(categoryId){
			callback(categoryId);
		}
	});
}

function showStudentVideoResumeDoc(jArray,tableId,isCreator){
	var StudentOtherFileBoiler = "<table class = 'table table-bordered' id='showStudentVideoResumetbl'>";
	//for(var i = 0 ; i < jArray.length; i++){
	if(jArray.length>0){
		var attachmentFileName = jArray[0]["name"].toString(); 
		var documentId = jArray[0]["documentId"];
		var eDocId =  jArray[0]["eDocId"]!=undefined?jArray[0]["eDocId"]:jArray[0]["documentId"];
		var isTemporary =jArray[0]["isTemporary"];///false;// 
		var trId1 = 'trAttachmentFile-'+documentId;
		var fileType = jArray[0]["type"];
		iconPath = gFileTypeIconMapping[fileType];
		if(!isTemporary){
			if(fileType=='mp4'){
			StudentOtherFileBoiler += 
				'<tr id = "'+trId1+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+(juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName))+'</td>';
			/*if(isCreator){
				StudentOtherFileBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeResumeAttachmentAjaxPan(\''+trId1+'\',\''+documentId+'\',\'saved\')" /></td>'; 
			}*/
			$('.video-prev').show();
			$('#videoPre').attr('src',(juno.label['url.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName))));
			StudentOtherFileBoiler +=  '</tr>';
			}
		}
	}
	StudentOtherFileBoiler += "</table>";
	
	$('#uploadedVideoResumeDiv').html(StudentOtherFileBoiler);   
	$('#uploadedVideoResumeDiv').show();
}

//Video ENd
function getStudentPlacementUploadedImage(){
	$.ajax({
		url:'checkStudentPlacementImage.json',
		dataType:'text',
		success:function(text) {
			if(text == 'no'){
				$('img#studentPlacementImage').attr('src','img/boy.gif');
			}
		}
	});
}

function studentPlacementImage(){

	$.ajax({
		url:'checkPlacementImageUploadDeadline.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){

			if(jArray[0]["flag"]==true){
				//Hide Image Upload Button
				$('#placementImageUploadBtn').hide();
				
			}else{
				$('#placementImageUploadBtn').show();
			}
			
			var boiler='<div class = "alert alert-info" style="color:black;font-size:13px;">';
			if(jArray[0]["flag"]){
				if(jArray[0]["fromDate"]!='' && jArray[0]["toDate"]!=''){
					boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;<b style="color:red;">Upload button will be visible from '+jArray[0]["fromDate"]+' - '+jArray[0]["toDate"]+'</b><br/>';
				}else{
					boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;<b style="color:red;">Placement Image Upload schedule not defined by TPO</b><br/>';
				}
			}else{
				boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;<b style="color:red;">Upload button will be visible from '+jArray[0]["fromDate"]+' - '+jArray[0]["toDate"]+'</b><br/>';
			}
			boiler+='<i class = "icon-hand-right"></i>&nbsp;&nbsp;Max image size 340 KB.<br />'+
			'<i class = "icon-hand-right"></i>&nbsp;&nbsp;.jpg, .jpeg files only<br />'+
			'<i class = "icon-hand-right"></i>&nbsp;&nbsp;Your image will automatically be cropped to 120*120 pixels. <br />';
			'</div>';
			
			$('#instructionsPlacementImgDiv').html(boiler);
			
		},
		error:function(){
			////alert("in Error stream CMB");
		}
	});
	
}

function uploadStudentplacementImage(){
	var elementId ='placementPictureUploadBtn';
	new AjaxUpload(elementId, {
		action : 'uploadStudentPlacementImage.json',
		name : 'fileData',
		size:AllowedFileSize.PROFILE_PHOTO,
		onSubmit : function(file, extension) {
			if(extension.toString().toLowerCase() == 'jpg' || extension.toString().toLowerCase() == 'jpeg' || extension.toString().toLowerCase() == 'mp4'){
				$('.placementImageUpload').find('.preview').addClass('loading');
				this.setData({
					'fileSize':AllowedFileSize.PROFILE_PHOTO
				});
			} else {
				jAlert('Use .jpg or .jpeg image file!');
				return false;
			}
		},
		onComplete : function(file, response) {
			$('.placementImageUpload').find('.preview').removeClass('loading');
			if(isNaN(response)){
				showErrorMessage('Image size is larger than specified size!',10000);
			} else {
			
			 	var imageBoiler = 
					'<img src="getStudentPlacementImage.json?id='+response+'&type=profile" alt="Photo" id = "profilePictureUpload" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" /></td>';
					
				$('#studentPlacementImage').html(imageBoiler);	
				jAlert('Image uploaded successfully !! ',10000);
			}
		}
	});
}
/*
function initResumeFileUploadPan()
{
	var elementId ='studentResumeUploadBtn';
	  var index=1;
		new AjaxUpload(elementId,{
			action :'setStudentResumeInSessionIndiraFlow.json',
			data : {},
			name : 'fileData',
			size:AllowedFileSize.DOCUMENT_UPLOAD,
			onSubmit : function(file, extension) {
				if(extension.toString().toLowerCase() == 'doc'
					|| extension.toString().toLowerCase() == 'docx' || extension.toString().toLowerCase() == 'pdf' ){
				
					startCommonAjaxSpinner();
					this.setData({
						'index':index,
						'fileSize':AllowedFileSize.DOCUMENT_UPLOAD
					});
				} else {
					jAlert('File Format not supported ! \n Please use .doc, .docx,pdf files only');
					return false;
				}

			},
			onComplete : function(file, response) {
				
				stopCommonAjaxSpinner();
				
				
				getCustomAlertMessage("alert_create");
				flushSessionstudentResume();
				getStudentResume();
			}
		});
}
*/


function initResumeFileUploadPan(cvTypeId,cvType)
{
	if(cvTypeId!=null && cvTypeId!=undefined && cvType!=null && cvType!=undefined){
	var elementId ='studentResumeUploadBtn_'+cvTypeId;
	  var index=1;
		new AjaxUpload(elementId,{
			action :'setStudentResumeInSession.json',
			data : {},
			name : 'fileData',
			size:AllowedFileSize.DOCUMENT_UPLOAD,
			onSubmit : function(file, extension) {
				if(extension.toString().toLowerCase() == 'pdf'){
				/*	startLoading();*/
					startCommonAjaxSpinner();
					this.setData({
						'index':index,
						'fileSize':AllowedFileSize.DOCUMENT_UPLOAD,
						'cvTypeId':cvTypeId,
						'cvType':cvType
					});
				} else {
					jAlert('File Format not supported ! \n Please use .pdf files only');
					return false;
				}

			},
			onComplete : function(file, response) {
				/*stopLoading();*/
				stopCommonAjaxSpinner();
				/*var jArray = eval('(' + response + ')');*/
				
				getCustomAlertMessage("alert_create");
				flushSessionstudentResume();
				getStudentResume();
			}
		});	
	}
	
}

function showStudentResumeDoc(jArray,tableId,isCreator){
	var StudentOtherFileBoiler = "<table class = 'table table-bordered' id='showStudentResumetbl'>";
	
	//for(var i = 0 ; i < jArray.length; i++){
	if(jArray.length>0){
		var attachmentFileName = jArray[0]["name"].toString(); 
		var documentId = jArray[0]["documentId"];
		var eDocId =  jArray[0]["eDocId"]!=undefined?jArray[0]["eDocId"]:jArray[0]["documentId"];
		var isTemporary =jArray[0]["isTemporary"];///false;// 
		var trId1 = 'trAttachmentFile-'+documentId;
		var fileType = jArray[0]["type"];
		iconPath = gFileTypeIconMapping[fileType];
		if(!isTemporary){
			if(fileType=='doc' || fileType=='docx'|| fileType=='pdf'){
			StudentOtherFileBoiler += 
				'<tr id = "'+trId1+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+(juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName))+'</td>';
			/*if(isCreator){
				StudentOtherFileBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeResumeAttachmentAjaxPan(\''+trId1+'\',\''+documentId+'\',\'saved\')" /></td>'; 
			}*/
			StudentOtherFileBoiler +=  '</tr>';
			}
		}
	}
	StudentOtherFileBoiler += "</table>";
	$('#studentResumeList_'+tableId).html(StudentOtherFileBoiler);   
}

var gEventJArry = null;
function showEventDetails(){
	var boiler = '<div id="maintable"><div id="eventTable"><table class="table table-bordered">'+
	'<tr><th>Event Name</th><td><input type="text" id="EvcentName"></td></tr>'+
	'<tr><th>Date</th><td><input type="text" id="EvcentDate"></td></tr>'+
	'<tr><th>Venue </th><td><input type="text" id="eventVenue"></td></tr>'+
	'<tr><th>Participant/Winner </th><td><input type="radio" id="cRadio" name="eventStatus"  value="Winner"  />&nbsp;<span class="label label-success"> Winner </span>&nbsp;&nbsp;'+
	'<input type="radio" checked="checked" name="eventStatus"  value="Participate" onclick=""/>&nbsp; <span class="label label-warning"> Participant </span>&nbsp;&nbsp;</td></tr>'+
	'<tr><th>Prize</th><td class=""><input type="text" id="eventprice"></td></tr>'+
	'<tr><th>Topic </th><td><input type="text" id="EnterTopic"></td></tr>'+
	'<tr><th>Guided By </th><td><select id="emplist" onchange="" class="eventEmplist"><option value="0">Select</option></select></td></tr>'+
	'<tr><td colspan="2" style="text-align: center;"><input type="button" value="Save" id="btnSave" class="btn btn-miini btn-inverse" onclick="saveStudentEventdata()">&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;<input type="button" value="Reset" class="btn btn-miini btn-inverse" onclick="resetallDetails()">'+
	'</td>'+
	'</tr>'+
	'</table></div>'+
	'<div id="studentEventDetails"></div></div>';
	
	$('#EventDetails').html(boiler);
	$('#EvcentDate').datepicker({
		dateFormat:"M dd,yy",				inline : true,
		changeMonth : true,
		changeYear : true,
		maxDate:new Date(),				yearRange : "-80:+0",
		firstDay : 1,
	});
	
	var instituteCourseId = $('#instCourseId').val();
	$.ajax({
		url:'populateEmployeeListForStudentEvents.json?instituteCourseId='+instituteCourseId,
		dataType:'json',
		type:'GET',
		async:false,
		beforeSend:function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(!jArray.error)
			{
				var boiler = '<option value="0">Select</option>';
				for(var i=0;i<jArray.length;i++)
					boiler += '<option value="'+jArray[i].Id+'">'+jArray[i].Name+'</option>';

				$('.eventEmplist').html(boiler);
				convertComboToSearchable('.eventEmplist');
			}
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
	
	displaystudeventReport();
	
}
function editEventDetails(i){  
	
	var studEventId = gEventJArry[i]["studentEventId"];
	$('#btnSave').data('studEventId',studEventId);
	$('#EvcentName').val(gEventJArry[i].eventName);
	$('#EvcentDate').val(gEventJArry[i].date);
	$('#eventVenue').val(gEventJArry[i].venue);
	$('#eventprice').val(gEventJArry[i].price);
	$('#EnterTopic').val(gEventJArry[i].topic);
	
	if(gEventJArry[i].participant == "true" && gEventJArry[i].winner == "false")
	$("input[name=eventStatus][value='Participate']").prop('checked', true);	
	else if(gEventJArry[i].winner == "true" && gEventJArry[i].participant == "true")
		$("input[name=eventStatus][value='Winner']").prop('checked', true);	
	fetchValuesOfEvent(); 
	$('#emplist option[value="'+gEventJArry[i].empId+'"]').attr('selected',true);
}

function saveStudentEventdata(){									   
	var eventtname=$('#EvcentName').val();
	var EventDate=$('#EvcentDate').val();
	var eventprice=$('#eventprice').val();
	var eventVenue=$('#eventVenue').val();
	var EventTopic=$('#EnterTopic').val();
	var participation = $("input[name='eventStatus']:checked").val();
	var empId = $('#emplist option:selected').val();
	var studId = $('#studId').val();
	
	var studEventId=0;
    if($('#btnSave').data('studEventId')!=undefined){
    	
    	studEventId=$('#btnSave').data('studEventId');
    }
	
	 if(eventtname=="")
	{
		jAlert("Please Enter Event Name");
		return false;
	}
	else if(EventDate==""){
		jAlert("Please Enter Event Date");
		return false;
	}
	else if(eventprice==""){
		jAlert("Please Enter Event Prize");
		return false;
	}
	else if(eventVenue==""){
		jAlert("Please Enter Event Venue");
		return false;
	}
	else if(EventTopic==""){
		jAlert("Please Enter Event Topic");
		return false;
	}
	else if(participation==""){
		jAlert("Please Enter Event Status");
		return false;
	} 
	else if(empId=="0"){
		jAlert("Please Select Employee");
		return false;
	} 
	 
	
	
	$.ajax({
		url:'saveEventDetailsFromStudentSide.json',
		dataType:'text',
		type: 'POST',
		data:{eventtname:eventtname,
			EventDate:EventDate,
			eventprice:eventprice,
			eventVenue:eventVenue,
			EventTopic:EventTopic,
			participation:participation,
			empId:empId,
			studId:studId,
			studEventId:studEventId
			
			},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},

		success:function(jArray){
			jAlert("Saved Successfully");
			resetallDetails();
			displaystudeventReport();
			
		},

		error:function(){
			alert("Error while Saving");
		}

	});


	}

function fetchValuesOfEvent(){
	var instituteCourseId = $('#instCourseId').val();
	$.ajax({
		url:'populateEmployeeListForStudentEvents.json?instituteCourseId='+instituteCourseId,
		dataType:'json',
		type:'GET',
		async:false,
		beforeSend:function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(!jArray.error)
			{
				var boiler = '<option value="0">Select</option>';
				for(var i=0;i<jArray.length;i++)
					boiler += '<option value="'+jArray[i].Id+'">'+jArray[i].Name+'</option>';

				$('.eventEmplist').html(boiler);
				convertComboToSearchable('.eventEmplist');
			}
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
}

function resetallDetails(){
	$('#EvcentName').val('');
	$('#EvcentDate').val('');
	$('#eventprice').val('');
	$('#eventVenue').val('');
	$('#EnterTopic').val('');
	$('input[name=eventStatus]').attr('checked',false);
	$('#emplist option[value="'+0+'"]').attr('selected',true);
	convertComboToSearchable('.eventEmplist');
	
}
	

function displaystudeventReport()
{
var studId = $('#studId').val();
$.ajax({
	url:'fetchEventDetailsFromStudentSide.json',
	dataType:'json',
	data:{
		studId:studId
		},
	async:false,
	beforeSend: function() {
		startCommonAjaxSpinner();

	},
	complete: function() {
		stopCommonAjaxSpinner();
	},

	success:function(jArray){

		if(jArray.length>0){
			var boiler=''+
			'<table class="table table-bordered" id="" style="width:100%;">'+
			'<thead>'+
			'<tr>'+
				'<th style="text-align: center">Sr. No</th>'+
				'<th style="text-align: center">Event Name</th>'+
				'<th style="text-align: center">Date</th>'+
				'<th style="text-align: center">Venue</th>'+
				'<th style="text-align: center">Prize</th>'+
				'<th style="text-align: center">Topic</th>'+
				'<th style="text-align: center">Guided By</th>'+
				'<th style="text-align: center">Participant/Winner</th>'+
				'<th></th>'+
			'</tr></thead><tbody>';
			
			var size=jArray.length;
			for(var i=0;i<size;i++)
			 {
				boiler+='<tr>'+
						    '<td style="text-align: right">'+(i+1)+'</td>';
				boiler+='<td style="text-align: center">'+jArray[i]["eventName"]+'</td>'+
							'<td style="text-align: left">'+jArray[i]["date"]+'</td>'+
							'<td style="text-align: left">'+jArray[i]["venue"]+'</td>'+
							'<td style="text-align: right">'+jArray[i]["price"]+'</td>'+
							'<td style="text-align: left">'+jArray[i]["topic"]+'</td>'+
							'<td style="text-align: left">'+jArray[i]["empName"]+'</td>';
							if(jArray[i]["participant"]=="true" && jArray[i]["winner"]!="true")
								boiler+='<td><span class="label label-warning"> Participant </span></td>';
							if(jArray[i]["winner"]=="true" && jArray[i]["participant"]=="true")
								boiler+='<td><span class="label label-success"> Winner </span></td>'; 
							if(jArray[i]["status"]!="Approved")
							boiler+='<td id="edit_'+jArray[i]["studentEventId"]+'"><input type="button" onclick="editEventDetails('+i+')"  value="Edit"></td>';
							else
							boiler+='<td><span class="label label-success">Approved</span></td>';
							boiler+='</tr>';
			 }
			
			boiler+='</tbody></table>';
			$('#studentEventDetails').html(boiler);
			
			gEventJArry = jArray;
			
		}
		else
        {
            $('#studentEventDetails').html('<label>*No Records Found </label>');
        }
	},

});
}	

function getstudentBankDetails() {

	$.ajax({
		url : 'getStudentBankDetailsOfApplicant.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			if(jArray.length > 0){
				
				
				
				if($('#customerHelper').val()=="BITS_MESRA"){
					
					$('#beneficiary').val(jArray[0]["relation"]);
					$('#BankAccountNoBit').val(jArray[0]["BankAccountNo"]);
					}

				$('#ifscCode').val(jArray[0]["IfscCode"]);
				$('#NameAsPerAcc').val(jArray[0]["NameAsPerAcc"]);
				$('#studName').val(jArray[0]["NameAsPerAdhaarCard"]);
				$('#nameOfBank').val(jArray[0]["NameOfTheBank"]);
				$('#BankAccountNo').val(jArray[0]["BankAccountNo"]);
				$('#panCardNo').val(jArray[0]["PanCardNumber"]);
				$('#branchName').val(jArray[0]["BranchName"]);

				if(jArray[0]["AadharCardLinkedWithBank"]=='Yes'){
					$("#LinkedWithAadharYes").attr("checked", true);
				}else{
					$("#LinkedWithAadharNo").attr("checked", true);
				}
				if(jArray[0]["AccountType"]=='Saving'){
					$("#SavingYes").attr("checked", true);
				}else{
					$("#CurrentYes").attr("checked", true);
				}


			}else{


			}},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

function addStudentBankDetails(){ 
	var studName = $('#studName').val();
	var LinkedWithAadhar = $('input[name = "LinkedWithAadhar"]:checked').val();
	var bankName = $('#nameOfBank').val();
	var BankAccountNumber =  $('#BankAccountNo').val();
	var branchName = $('#branchName').val();
	var ifscCode = $('#ifscCode').val();
	var NameAsPerAcc = $('#NameAsPerAcc').val();
	var AccType = $('input[name = "SavingCurrent"]:checked').val();
	var panCardNo = $('#panCardNo').val();
	
	let verifBankAccNo ="";
	let beneficiary ="";
	
	if($('#customerHelper').val()=="BITS_MESRA"){
		
		beneficiary = $('#beneficiary :selected').text();
		
		if(beneficiary=="" || beneficiary =="Select"){
			jAlert("Please select Beneficiary Name");
			return false;
		}
		
		verifBankAccNo = $('#BankAccountNoBit').val();
		if(verifBankAccNo=="" || BankAccountNumber=="") {
		jAlert("Please Enter Account Number..!!");
		return;
	}
	if(verifBankAccNo != BankAccountNumber) {
		jAlert("Account Number Mismatch...!!");
		return;
		}
	}else{
		if(BankAccountNumber=="") {
		jAlert("Please Enter Account Number..!!");
		return;
	}
	}
	 
	if(studName=="") {
		jAlert("Please Enter Name As Per Aadhar..!!");
		return;
	}
	if(bankName=="") {
		jAlert("Please Enter Bank Name..!!");
		return;
	}
	
	if( !$('#customerHelper').val()=="KURUKSHETRA_UNIVERSITY")
	{
	
		if(panCardNo=="") {
		jAlert("Please Enter PAN Card Number..!!");
		return;
	}
	}
	if(branchName=="") {
		jAlert("Please Enter Branch Name..!!");
		return;
	}
	if(ifscCode=="") {
		jAlert("Please Enter IFSC Code..!!");
		return;
	}
	if(NameAsPerAcc=="") {
		jAlert("Please Enter Name As Per Account..!!");
		return;
	}
	$.ajax({
		//url:'saveStudentBankDetail.json',
		url:'saveStudentBankDetailSpXML.json',
		dataType:'text',
		type:'POST',
		data:{
			studName:studName,
			LinkedWithAadhar:LinkedWithAadhar,
			bankName:bankName,
			branchName:branchName,
			ifscCode:ifscCode,NameAsPerAcc:NameAsPerAcc,
			AccType:AccType,panCardNo:panCardNo,BankAccountNumber:BankAccountNumber,
			beneficiary:beneficiary
		},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){

			getCustomAlertMessage("alert_create");
		},
		error:function(){
			////alert("in Error stream CMB");
		}
	});

}

//Guardian Image Code

function setGuardianPhoto(){

	$.ajax({
		url:'checkGuardianProfileImage.json',
		data : {photoOf:'Guardian'},
		dataType:'text',
		success:function(text) {
			$('#guardianHidden').val(text);
			if(text == 'no'){
				$('img#guardianProfilePhoto').attr('src','img/boy.gif');
			}
		}
	});

	new AjaxUpload('guardianPictureUpload', {
		action : 'uploadGuardianProfileImage.json',
		name : 'fileData',
		size:AllowedFileSize.PROFILE_PHOTO,
		onSubmit : function(file, extension) {
			if(extension.toString().toLowerCase() == 'jpeg' || extension.toString().toLowerCase() == 'jpg'){
				$('.guardianImageUpload').find('.preview').addClass('loading');
				this.setData({
					'fileSize':AllowedFileSize.PROFILE_PHOTO, 
					'photoOf' :'Guardian'
				});
			} else {
				jAlert('Use jpg image file!');
				return false;
			}
		},
		onComplete : function(file, response) {
			$('.guardianImageUpload').find('.preview').removeClass('loading');
			if(isNaN(response)){
				showErrorMessage('Image size is larger than specified size!',10000);
			} else {

				var imageBoiler = 
					'<img src="getGuardianProfileImage.json?studentId=0&id='+response+'&type=profile&photoOf=Guardian" alt="Photo" id = "guardianProfilePhoto" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" /></td>';

				$('#guardianProfileImage').html(imageBoiler);	
				jAlert("Guardians photo uploaded. ","Message");
			}
		}
	});
}

// Guardian Signature Code

function uploadGuardianPhotoSign(){
	$.ajax({
		url:'checkGuardianSignatureImage.json',
		data  :{photoOf:'Guardian'},
		dataType:'text',
		success:function(response) {
			$('#GuardianSigHidden').val(response);
			if(response == 'no'){
				initEmployeeSignatureUploaded('Guardian');
				$('#GuardianDeleteSignature').hide();
			} else {
				var imageBoiler = 
					'<img src="getSignatureImage.json?studentId=0&date='+new Date()+'&photoOf=Guardian " alt="Sign" name="Sign" width="80px" height="40px" border="1" align="right" style="background-color: #FFFFFF" /></td>';
				$('#GuardianSignatureUpload').html(imageBoiler);
				$('#GuardianDeleteSignature').html('<img src="css/images/not.png"  style="margin-top:70%;margin-left:25%">');
				$('#GuardianDeleteSignature').show();
				
				$('#GuardianDeleteSignature').unbind();
				$('#GuardianDeleteSignature').bind('click',function(){
					deleteEmployeeSignature('Guardian', function(){
					});
				});
			}
		}
	});
}

function initEmployeeSignatureUploaded(targetElementId){
	var boiler = '<div class="span12">'+
	'<div class="span2">'+
	'<div class="preview"></div></div>'+
	'<div class="span4">'+
		'<div class="btn btn-info" style = "width:165px;height:20px">Upload Your Signature'+
			'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="btn'+targetElementId+'SignatureUpload">'+
		'</div><br>'+
		 '<p style="font-size:9px; width:250px;" >(Signature image should be 10-20 KB)</p>'+
	'</div>'+
	'</div>';
	$('#'+targetElementId+'SignatureUpload').html(boiler);
	
	var targetElementId = "Guardian";
	new AjaxUpload('btn'+targetElementId+'SignatureUpload', {
		action : 'uploadGuardianSignatureImage.json',
		name : 'fileData',
		size:AllowedFileSize.PROFILE_SIGNATURE,
		minSize:AllowedFileSize.SIGNATURE_MIN_SIZE,
		onSubmit : function(file, extension) {
			if(extension.toString().toLowerCase() == 'png' || extension.toString().toLowerCase() == 'jpg'){
				$('#btn'+targetElementId+'SignatureUpload').find('.preview').addClass('loading');
				this.setData({
					'fileSize':AllowedFileSize.PROFILE_SIGNATURE,
					'photoOf' : targetElementId
				});
			} else {
				jAlert('Use only "png","jpg" image format file!');
				return false;
			}
		},
		onComplete : function(file, response) {
			$('#btn'+targetElementId+'SignatureUpload').find('.preview').removeClass('loading');
			if(isNaN(response)){
				jAlert("File size is larger than specified !!");
			} else {
		 	var imageBoiler = 
				'<img src="getSignatureImage.json?studentId=0&date='+new Date()+'&photoOf='+targetElementId+'" alt="Sign" name="Sign" width="80px" height="40px" border="1" align="left" style="background-color: #FFFFFF" /></td>';
			}
				$('#'+targetElementId+'SignatureUpload').html(imageBoiler);
			$('#'+targetElementId+'DeleteSignature').html('<img src="css/images/not.png"  style="margin-top:70%;margin-left:25%">');
			$('#'+targetElementId+'DeleteSignature').show();
			$('#'+targetElementId+'DeleteSignature').unbind();
			$('#'+targetElementId+'DeleteSignature').bind('click',function(){
				deleteEmployeeSignature(targetElementId, function(){
					
				});
			});
		}
	});
}

function deleteEmployeeSignature(targetElementId){
	$.ajax({
		url:'deleteGuardianSignature.json',
		data :{photoOf:targetElementId},
		dataType:'json',
		type:'POST',
		beforeSend: function() {
			$('#'+targetElementId+'DeleteSignature').html('<div class="preview" style="margin-top:10px;margin-left:2px"></div>');
			$('#'+targetElementId+'DeleteSignature').find('.preview').addClass('loading');
		},
		complete: function() {
			$('#'+targetElementId+'DeleteSignature').find('.preview').removeClass('loading');
		},
		success:function(response) {
			if(response.responseFlag == 1){
				initEmployeeSignatureUploaded(targetElementId);
				$('#'+targetElementId+'DeleteSignature').hide();
				$('#'+targetElementId+'SigHidden').val('no');
			}
		}
	});
}

function clearNewGuardianDetailFeilds(){
	$('#gaurdiNameModal').val('');
	$('#studguardianmiddlenameModal').val('');
	$('#studguardianlastnameModal').val('');
	$('#gaurdiEmailModal').val('');
	$('#guardianRelationModal').val('');
	$('#studguardiancellnoModal').val('');
	$('#guardianstudphnumberModal').val('');
	$('#guardiAddressModal').val('');
	$('#studguardiantalukaModal').val('');
	$('#studguardianVillageModal').val('');
	$('#studguardianpincodeModal').val('');
}

function  fetchAdmissionPDF(){

	var studentId =$('#hidenStudentId').val();
	var boiler = '<table class="table table-bordered"><tr><td><b>Admission Form PDF</b></td>'+
	'<td colspan="3" class = "UploadedFilesList"><input type="button" class="uploadDocument btn btn-info" onclick="GenrateAdmissionPDF()" value="Download Admission Form PDF"></td></tr>';
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="MGMU"){
	boiler +='<tr><td><b>Admission Eligibility PDF</b></td>'+
	'<td colspan="3" class = "UploadedFilesList"><input type="button" class="uploadDocument btn btn-info" onclick="GenrateApplicantEligibilityPDF(\''+studentId+'\')" value="Download Eligibility Form PDF"></td></tr>';
	}
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="MANDSAUR"){
		boiler +='<tr><td><b>Admission Certificate PDF</b></td>'+
	'<td colspan="3" class = "UploadedFilesList"><input type="button" class="uploadDocument btn btn-info" onclick="GenrateApplicantUndertakingPDF(\''+studentId+'\')" value="Admission Certificate PDF"></td>'+
	'</tr>';
	}else{
		boiler +='<tr><td><b>Admission Undertaking PDF</b></td>'+
	'<td colspan="3" class = "UploadedFilesList"><input type="button" class="uploadDocument btn btn-info" onclick="GenrateApplicantUndertakingPDF(\''+studentId+'\')" value="Download Undertaking Form PDF"></td>'+
	'</tr>';
	}
	
	boiler +='</table>';
	$('#AdmissionDocDetail').html(boiler);

}


