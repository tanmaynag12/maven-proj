			if ( (gArray[i]["isCheckedText"] == "siblingContNo") && ($('input[name = "siblingContNo"]')!='') && ($('input[name = "siblingContNo"]')!=undefined) && (($('input[name = "siblingContNo"]').val())!=null) &&  ($.trim($('input[name = "siblingContNo"]').val())!="") && ($('input[name = "siblingContNo"]').val().length>0)) {
				$('input[name = "siblingContNo"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "siblingContNo") && ($('input[name = "siblingContNo"]')=='') || ($('input[name = "siblingContNo"]')==undefined) || (($('input[name = "siblingContNo"]').val())==null) ||  ($.trim($('input[name = "siblingContNo"]').val())=="") || ($('input[name = "siblingContNo"]').val().length==0)){
				$('input[name = "siblingContNo"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "siblingEmail") && ($('input[name = "siblingEmail"]')!='') && ($('input[name = "siblingEmail"]')!=undefined) && (($('input[name = "siblingEmail"]').val())!=null) &&  ($.trim($('input[name = "siblingEmail"]').val())!="") && ($('input[name = "siblingEmail"]').val().length>0)) {
				$('input[name = "siblingEmail"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "siblingEmail") && ($('input[name = "siblingEmail"]')=='') || ($('input[name = "siblingEmail"]')==undefined) || (($('input[name = "siblingEmail"]').val())==null) ||  ($.trim($('input[name = "siblingEmail"]').val())=="") || ($('input[name = "siblingEmail"]').val().length==0)){
				$('input[name = "siblingEmail"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "isSibllingWorking") && ($('input[name = "isSibllingWorking"]')!='') && ($('input[name = "isSibllingWorking"]')!=undefined) && (($('input[name = "isSibllingWorking"]').val())!=null) &&  ($.trim($('input[name = "isSibllingWorking"]').val())!="") && ($('input[name = "isSibllingWorking"]').val().length>0)) {
				$('input[name = "isSibllingWorking"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "isSibllingWorking") && ($('input[name = "isSibllingWorking"]')=='') || ($('input[name = "isSibllingWorking"]')==undefined) || (($('input[name = "isSibllingWorking"]').val())==null) ||  ($.trim($('input[name = "isSibllingWorking"]').val())=="") || ($('input[name = "isSibllingWorking"]').val().length==0)){
				$('input[name = "isSibllingWorking"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "isExSiblling") && ($('input[name = "isExSiblling"]')!='') && ($('input[name = "isExSiblling"]')!=undefined) && (($('input[name = "isExSiblling"]').val())!=null) &&  ($.trim($('input[name = "isExSiblling"]').val())!="") && ($('input[name = "isExSiblling"]').val().length>0)) {
				$('input[name = "isExSiblling"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "isExSiblling") && ($('input[name = "isExSiblling"]')=='') || ($('input[name = "isExSiblling"]')==undefined) || (($('input[name = "isExSiblling"]').val())==null) ||  ($.trim($('input[name = "isExSiblling"]').val())=="") || ($('input[name = "isExSiblling"]').val().length==0)){
				$('input[name = "isExSiblling"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "isStudyingInBatch") && ($('input[name = "isStudyingInBatch"]')!='') && ($('input[name = "isStudyingInBatch"]')!=undefined) && (($('input[name = "isStudyingInBatch"]').val())!=null) &&  ($.trim($('input[name = "isStudyingInBatch"]').val())!="") && ($('input[name = "isStudyingInBatch"]').val().length>0)) {
				$('input[name = "isStudyingInBatch"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "isStudyingInBatch") && ($('input[name = "isStudyingInBatch"]')=='') || ($('input[name = "isStudyingInBatch"]')==undefined) || (($('input[name = "isStudyingInBatch"]').val())==null) ||  ($.trim($('input[name = "isStudyingInBatch"]').val())=="") || ($('input[name = "isStudyingInBatch"]').val().length==0)){
				$('input[name = "isStudyingInBatch"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "isStudiedInBatch") && ($('input[name = "isStudiedInBatch"]')!='') && ($('input[name = "isStudiedInBatch"]')!=undefined) && (($('input[name = "isStudiedInBatch"]').val())!=null) &&  ($.trim($('input[name = "isStudiedInBatch"]').val())!="") && ($('input[name = "isStudiedInBatch"]').val().length>0)) {
				$('input[name = "isStudiedInBatch"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "isStudiedInBatch") && ($('input[name = "isStudiedInBatch"]')=='') || ($('input[name = "isStudiedInBatch"]')==undefined) || (($('input[name = "isStudiedInBatch"]').val())==null) ||  ($.trim($('input[name = "isStudiedInBatch"]').val())=="") || ($('input[name = "isStudiedInBatch"]').val().length==0)){
				$('input[name = "isStudiedInBatch"]').attr("disabled", false);
			}
		
		}
		
	}
	
	 /*if($('#customerHelper').val()=="BLM_MUNJAL") { 
		   $('input').attr('disabled',true);
		   $('button').attr('disabled',true);
		   $('div').attr('disabled',true);
		   $('select').attr('disabled',true).trigger("liszt:updated");
		   $('textArea').attr('disabled',true);
		   $('img').attr('disabled',true);
		   $('.freezeProfile').attr('disabled',true);
		   $('.freezeProfile').off('click').attr('onclick','').css('cursor','not-allowed');
		  
	 }*/
}

function AddformOpenOtherEntanceExam(status){

	if(status=="NewEntrance"){
	
		$('#othExamNametxt').val("0");
		$('#othApplicationNotxt').val("");
		$('#othRollNotxt').val("");
		$('#othPercentiletxt').val("");
		$('#othScoretxt').val("");
		
		$('#othExamNametxt').prop("disabled", false);
		$('#othApplicationNotxt').prop("disabled", false);
		$('#othRollNotxt').prop("disabled", false);
		$('#othPercentiletxt').prop("disabled", false);
		$('#othScoretxt').prop("disabled", false);
	$('#othOutOf').html('');
	}
	
	var StudentEntranceExamId = 0;
	$('#showTblOtherEntranceExam').hide();
	$('#addNewOtherEntranceExam').show();
	getEntranceExamList();
	
	var boiler ='<input  class=" btn btn-inverse btn-mini btnColorGreen" type="button" value="Save"  onclick="saveOtherStudentEntranceExam(\'new\',\''+StudentEntranceExamId+'\')">&nbsp&nbsp'+
	'<input class=" btn btn-inverse btn-mini btnColorRed " type="button" value="Cancel" onclick ="getOtherStudentEntranceExam();" >';
	$('#entranceExamSaveEditTd').html(boiler);
	
	
}
var outof=0;
function getEntranceExamList(){
	var instituteCourseStrengthId = $('#saveBtn').attr('institutecoursestrengthid');
	$.ajax({
		url:'getEntranceExamListByinstituteCourseStrengthId.json?instituteCourseStrengthId='+instituteCourseStrengthId,
		dataType:'json',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
		
			var boiler='<option value="0">Select</option>';
			for(var i=0;i<jArray.length;i++){
				 outof=jArray[i]["Outof"];
				boiler +='<option marks="'+jArray[i]["Outof"]+'"value="'+jArray[i]["courcseExaminationID"]+'">'+jArray[i]["ExamName"]+'</option>';
			
			}
		//	convertComboToSearchable('#othExamNametxt');
			$('#othExamNametxt').html(boiler);
			
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
		});
}

function saveOtherStudentEntranceExam(inserttype,StudentEntranceExamId){
	var othEntranceExam =  $('#othExamNametxt option:selected').val();
	var othApplicationNo = $('#othApplicationNotxt').val();
	var othRollNo = $('#othRollNotxt').val();
	var othPercentile = $('#othPercentiletxt').val();
	var othScore = $('#othScoretxt').val();
	var studentId = 0;
	
	if(othEntranceExam=="0"){
		jAlert("Select Entrance Exam.");
		$('#othExamNametxt').focus();
		return false;
	}
	if(othRollNo==""){
		jAlert("Enter Roll No.");
		$('#othRollNotxt').focus();
		return false;
	}
	if(othPercentile==""){
		jAlert("Enter Percentile.");
		$('#othPercentiletxt').focus();
		return false;
	}
	if(othScore==""){
		jAlert("Enter Score.");
		$('#othScoretxt').focus();
		return false;
	}
	$.ajax({
		url:'saveOtherStudentEntranceExamFromStudentProfile.json',
		data : {
			othEntranceExam:othEntranceExam,
			othApplicationNo : othApplicationNo,
			othRollNo : othRollNo,
			othPercentile : othPercentile,
			othScore : othScore,
			StudentEntranceExamId : StudentEntranceExamId,
			studentId : studentId
		},
		dataType:'json',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(){
			
			$('#addNewOtherEntranceExam').hide();
			$('#showTblOtherEntranceExam').show();
			if(inserttype=='new'){
				getCustomAlertMessage("alert_create");
			}else{
				getCustomAlertMessage("alert_update");
			}
			getOtherStudentEntranceExam();
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
}


function setOutofMarksForEntranceExam(){
	var outof=$('#othExamNametxt option:selected').attr('marks');
	$('#othOutOf').html(outof);
	
}
function getOtherStudentEntranceExam(){
	var boiler ="";
	var studentId = 0;
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getOtherStudentEntranceExamFromStudentProfile.json?studentId='+studentId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getOtherStudentEntranceExamFromStudentProfile.json';
	}
	$.ajax({
		url:url,
		dataType:'json',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			
				boiler+='<table class = "table table-bordered" id="tblUserRuleAuth">'+
     			'<thead><th>Sr.No</th><th>Entrance Exam</th><th>Roll No</th><th>Score</th><th>Out of</th><th>Percentile</th><th>Action</th></thead><tbody>';
				for(var i=0; i<jArray.length;i++){
				
					boiler+='<tr>'+
							'<td style="text-align:right">'+eval(i+1)+'</td>'+
							'<td class="EntranceName" style="text-align:left">'+jArray[i]["EntranceName"]+'</td>'+
							'<td class="RollNo" style="text-align:right">'+jArray[i]["RollNo"]+'</td>'+
							'<td class="Score" style="text-align:right">'+formatAmount(parseFloat(jArray[i]["Score"]).toFixed(2))+'</td>'+
							'<td class="Score" style="text-align:right">'+jArray[i]["outOf"]+'</td>'+
							'<td class="Percentile" style="text-align:right">'+formatAmount(parseFloat(jArray[i]["Percentile"]).toFixed(2))+'</td>'+
							'<td class="action" style="text-align:left"><a class="freezeProfile hideSaveSubmitButton" onclick="editOtherEntranceExam('+jArray[i]["StudEntranceExamId"]+',this);getstudProfileConfigurationDetailsCheckingNames();"><i class="fa fa-pencil-square-o  iconColorBlack freezeProfile hideSaveSubmitButton" class="different" data-toggle="toltip" data-placement="bottom" title="Edit"></i></a>&nbsp&nbsp'+
							'<a  class="freezeProfile hideSaveSubmitButton" onclick="deleteOtherEntranceExam('+jArray[i]["StudEntranceExamId"]+');"><i class="fa fa-trash iconColorBlack  freezeProfile hideSaveSubmitButton" class="different" data-toggle="toltip" data-placement="bottom" title="Delete"></i></a></td>'+
							'<td class="applicationNo" style="display:none;">'+jArray[i]["applicationNo"]+'</td>'+
							'<td class="EntranceExamId" style="display:none;">'+jArray[i]["EntranceNameId"]+'</td>'+
							'</tr>';
				}
			boiler+='</tbody></table>';
			$('#showTblOtherEntranceExam').html(boiler);
			$('#showTblOtherEntranceExam').show();
			$('#addNewOtherEntranceExam').hide();
			/*$('#tblUserRuleAuth').dataTable({
				"sDom": "<'row'<'margin-left-22'l><'pull-right'f>r>t<'row'<'margin-left-22'i><'pull-right'p>>",
				"sPaginationType": "bootstrap",
			
				"bSort":false,
				"bInfo":true,
				"bAutoWidth":false,
				"aLengthMenu":[[10, 25, 50, 100, 200, -1], [10, 25, 50, 100, 200, "All"]],
				"iDisplayLength" : -1,
				"oLanguage": {
					"sLengthMenu": "_MENU_ records per page"
				},
				"sWrapper": "dataTables_wrapper form-inline",
			});*/
			
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
		
	 /*if($('#customerHelper').val()=="BLM_MUNJAL") { 
		   $('input').attr('disabled',true);
		   $('button').attr('disabled',true);
		   $('div').attr('disabled',true);
		   $('select').attr('disabled',true).trigger("liszt:updated");
		   $('textArea').attr('disabled',true);
		   $('img').attr('disabled',true);
		   $('.freezeProfile').attr('disabled',true);
		   $('.freezeProfile').off('click').attr('onclick','').css('cursor','not-allowed');
	 }*/
}

/** Edit Student Entrance Exam  **/
function editOtherEntranceExam(StudentEntranceExamId, src){
	
	AddformOpenOtherEntanceExam('EditEntrance');
	var EntranceName = $(src).closest('tr').find('.EntranceExamId').html();
	var RollNo = $(src).closest('tr').find('.RollNo').html();
	var Percentile = $(src).closest('tr').find('.Percentile').html();
	var Score = $(src).closest('tr').find('.Score').html();
	var applicationNo = $(src).closest('tr').find('.applicationNo').html();

	$("#othExamNametxt option[value="+EntranceName+"]").attr("selected", "selected");
	$('#othRollNotxt').val(RollNo);
	$('#othPercentiletxt').val(Percentile);
	$('#othApplicationNotxt').val(applicationNo);
	$('#othScoretxt').val(Score);
	getstudProfileConfigurationDetailsCheckingNames();
	setOutofMarksForEntranceExam();
	var boiler ='<input  class=" btn btn-inverse btn-mini btnColorGreen " type="button" value="Update"  onclick="saveOtherStudentEntranceExam(\'edit\',\''+StudentEntranceExamId+'\')">&nbsp&nbsp'+
	'<input class=" btn btn-inverse btn-mini btnColorRed " type="button" value="Cancel" onclick ="getOtherStudentEntranceExam();" >';
	$('#entranceExamSaveEditTd').html(boiler);
	
}

/** delete Student Entrance Exam  **/
function deleteOtherEntranceExam(StudentEntranceExamId){
	jConfirm("Do you want to Delete ?","Confirm",function(spc){
		if(spc ==true)
		{
			$.ajax({
				url:'deleteOtherEntranceExamByStudentEntranceExamId.json?StudentEntranceExamId='+StudentEntranceExamId,
				dataType:'json',
				type:'POST',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){
					getOtherStudentEntranceExam();
					getCustomAlertMessage("alert_delete");
				},
				error:function(){
					getCustomAlertMessage("alert_error");
				}
			});
		}
	});
		
}
/**** Upload father photo *****/
function uploadFatherPhotoForStudentProfile(){
	var studentId =$('#hidenStudentId').val();
	var fileElementId='FatherPhoto';
	var fileType=['png','jpg','jpeg'];
	
	new AjaxUpload('FatherprofilePictureUpload',{
		action : 'uploadparentFatherProfilePhotoForSchool.json',
		name : 'fileData',
		timeout:60000,
		size:AllowedFileSize.DOCUMENT_UPLOAD_2MB,
		data:{},
		onSubmit : function(file, extension) {
			$('div.preview2').addClass('loading');
			$('#FatherprofilePictureUpload').attr('disabled','disabled');
			if ($.inArray(extension.toString().toLowerCase(),fileType) != -1) {
				//$('#'+jObject.elementId).find('.preview').addClass('loading');
				this.setData({
					'fileSize':AllowedFileSize.DOCUMENT_UPLOAD_2MB,
				});
				return true;
			} else {
				jAlert('File type is not valid ! Please upload only <strong>&nbsp;'+fileType.join(', ')+'&nbsp;<strong> files');
				$('#FatherprofilePictureUpload').removeAttr('disabled');
				return false;
			}
		},
		onComplete : function(file, response) {
			getCustomAlertMessage("alert_upload");
			var imageBoiler = 
				'<img src="getApplStudentFatherImage.json?studentId='+studentId+'&type=profile&someparam='+response+'" alt="Photo" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" />';
			fatherImageStatus = true;
			$('div.preview2').removeClass('loading');
			$('#fatherProfileImage').html(imageBoiler);
		}
	});
}

/**** Upload Mother photo *****/
function uploadMotherPhotoForStudentProfile(){
	var studentId =$('#hidenStudentId').val();
	var fileElementId='MotherPhoto';
	var fileType=['png','jpg','jpeg'];
	
	new AjaxUpload('MotherprofilePictureUpload',{
		action : 'uploadparentMotherProfilePhotoForSchool.json',
		name : 'fileData',
		timeout:60000,
		size:AllowedFileSize.DOCUMENT_UPLOAD_2MB,
		data:{},
		onSubmit : function(file, extension) {
			$('div.preview2').addClass('loading');
			$('#MotherprofilePictureUpload').attr('disabled','disabled');
			if ($.inArray(extension.toString().toLowerCase(),fileType) != -1) {
				//$('#'+jObject.elementId).find('.preview').addClass('loading');
				this.setData({
					'fileSize':AllowedFileSize.DOCUMENT_UPLOAD_2MB,
				});
				return true;
			} else {
				jAlert('File type is not valid ! Please upload only <strong>&nbsp;'+fileType.join(', ')+'&nbsp;<strong> files');
				$('#MotherprofilePictureUpload').removeAttr('disabled');
				return false;
			}
		},
		onComplete : function(file, response) {
			getCustomAlertMessage("alert_upload");
			var imageBoiler = 
				'<img src="getApplStudentMotherImage.json?studentId='+studentId+'&type=profile&someparam='+response+'" alt="Photo" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" />';
			fatherImageStatus = true;
			$('div.preview2').removeClass('loading');
			$('#motherProfileImage').html(imageBoiler);
		}
	});
}

function getStudentInfoFromGrNo(element)
{
	
	var lastName= $('#siblastName').val()!=undefined?$('#siblastName').val():"";
	var grNo=$(element).val();
		$.ajax({
			url:'validateGRNumber.json',
			dataType:'json',
			type:'GET',
			data:{
				GrNo:grNo,
				lastName:lastName
			},
			beforeSend:function(){

			},
			complete:function(){

			},
			success:function(jArray){
				if(!jArray.error)
				{
					if(jArray.isRecord)
						flagGrNo = true;
					else
						flagGrNo = false;

					if(flagGrNo)
					{
						$('#siblingName').val(jArray.fname.toUpperCase());
						$('#sibmiddleName').val(jArray.mname.toUpperCase());
						$('#siblastName').val(jArray.lname.toUpperCase());
					}
					else
					{
						jAlert("Invalid GR.No. . Please enter valid GR.NO.");
						$(element).val('');
						$('#siblingName').val('');
						$('#classAndStandard option:first').attr('selected' , 'selected');
						return false;
					}
				}
			},
			error:function(){

				getCustomAlertMessage("alert_error");
			}
		});
	}

function addStudentSibling(status){

	if(status=="New"){

		$('#siblingName').val('');
		$('#sibmiddleName').val('');
		$('#siblastName').val('');

		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SPIT"){

			$("input[name=isSibllingStudying][value=No]").prop('checked', true);
			$("input[name=isExSiblling][value=No]").prop('checked', true);
			$("input[name=isSibllingWorking][value=No]").prop('checked', true);	

			var isSibllingStudying=$("input[name='isSibllingStudying']:checked").val();
			var isExSiblling=$("input[name='isExSiblling']:checked").val();

			$('#siblingEmailId').val('');
			$('#siblingContactNo').val('');
			$('#isStudyBatch').val('');
			$('#isExStudyBatch').val('');

			if(isSibllingStudying=="Yes"){
				$('#studyTr').show();
				$('#isStudyBatch').show();

			}else if(isExSiblling=="Yes"){
				$('#exStudyTr').show();
				$('#isExStudyBatch').show();

			}else{
				$('#studyTr').hide();
				$('#isStudyBatch').hide();
				$('#exStudyTr').hide();
				$('#isExStudyBatch').hide();
			}
		}
	}
	getstudProfileConfigurationDetailsCheckingNames();
	var StudentSiblingId =0;
	$('#addStudentSiblingDetails').show();
	$('#showStudentSiblingDetails').hide();
	var boiler ='<input  class=" btn btn-inverse btn-mini btnColorGreen" type="button" value="Save"  onclick="saveStudentSiblingDetails(\'new\',\''+StudentSiblingId+'\')">&nbsp&nbsp'+
	'<input class=" btn btn-inverse btn-mini btnColorRed " type="button" value="Cancel" onclick ="getStudentSiblingDetails();" >';
	$('#studentSublingSaveEditTd').html(boiler);

}

function saveStudentSiblingDetails(inserttype,studentSiblingId){

//	var studentGRNO =$('#studentGRNO').val();
	var siblingName = $('#siblingName').val();
	var sibmiddleName = $('#sibmiddleName').val();
	var siblastName = $('#siblastName').val();

	var studentId = 0;
	if(siblingName==""){
		jAlert("Enter sibling first name.");
		$('#siblingName').focus();
		return false;
	}
	if(sibmiddleName==""){
		jAlert("Enter sibling middle name.");
		$('#sibmiddleName').focus();
		return false;
	}
	if(siblastName==""){
		jAlert("Enter sibling last name.");
		$('#siblastName').focus();
		return false;
	}

	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SPIT"){
		
		var exSiblingStudyingInBatch="";
		var siblingStudyingInBatch="";
		
		var isSiblingWorking=$( "input[name='isSibllingWorking']:checked" ).val();
		var siblingEmail=$('#siblingEmailId').val();
		var siblingContactNo=$('#siblingContactNo').val();

		var isSiblingStudying=$( "input[name='isSibllingStudying']:checked" ).val();
		if(isSiblingStudying=="Yes"){
			siblingStudyingInBatch=$('#isStudyBatch').val();
		}else{ 
			siblingStudyingInBatch="";
		}
		
		var isExsibling=$( "input[name='isExSiblling']:checked" ).val();
		if(isExsibling=="Yes"){
			exSiblingStudyingInBatch=$('#isExStudyBatch').val();
		}else{
			exSiblingStudyingInBatch="";
		}
		
	}

	$.ajax({
		url:'saveStudentSiblingsDetails.json',
		data : {
			studentId :studentId,
			siblingName:siblingName,
			sibmiddleName : sibmiddleName,
			siblastName : siblastName,
			studentSiblingId :studentSiblingId,
			isSiblingStudying: isSiblingStudying,
			isSiblingWorking : isSiblingWorking,
			siblingEmail : siblingEmail,
			siblingContactNo : siblingContactNo,
			isExsibling:isExsibling,
			siblingStudyingInBatch:siblingStudyingInBatch,
			exSiblingStudyingInBatch:exSiblingStudyingInBatch

		},
		dataType:'text',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			getCustomAlertMessage("alert_create");
			getStudentSiblingDetails();

		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
}

function getStudentSiblingDetails(){

	var boiler ="";
	var studentId = 0;
	$.ajax({
		url:'getStudentSiblingsDetails.json?studentId='+studentId,
		dataType:'json',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){

			boiler+='<table class = "table table-bordered" id="tblsiblingDetails">'+
			'<thead><th>Sr.No</th><th>First Name</th><th>Middle Name</th><th>Last Name</th>';
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SPIT"){
				boiler+='<th>Email</th><th>Contact No</th><th>Is your Sibling Studying in SPIT?</th><th>Sibling Studying In Batch</th><th>Is your Sibling Ex Student of SPIT?</th><th>Ex Sibling Studied In Batch</th><th>Is your Sibling working</th>';
			}
			boiler+='<th>Action</th></thead><tbody>';
			for(var i=0; i<jArray.length;i++){

				boiler+='<tr>'+
				'<td style="text-align:right">'+eval(i+1)+'</td>'+
				'<td class="sibFirst" style="text-align:left">'+jArray[i]["siblingFirstName"]+'</td>'+
				'<td class="sibMiddle" style="text-align:left">'+jArray[i]["siblingMiddleName"]+'</td>'+
				'<td class="sibLast" style="text-align:left">'+jArray[i]["siblinglastName"]+'</td>';

				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SPIT"){
					boiler+='<td class="emailCls">'+jArray[i]["emailId"]+'</td>'+
					'<td class="mobNoCls">'+jArray[i]["mobileNo"]+'</td>'+
					'<td class="isStudyCls">'+jArray[i]["isStudying"]+'</td>'+
					'<td class="isStudyBatchCls">'+jArray[i]["siblingStudyingInBatch"]+'</td>'+
					'<td class="isExSibCls">'+jArray[i]["isExsibling"]+'</td>'+
					'<td class="isExSibBatchCls">'+jArray[i]["exSiblingStudyingInBatch"]+'</td>'+
					'<td class="isWorkCls">'+jArray[i]["isWorking"]+'</td>';
				}
				boiler+='<td class="action" style="text-align:left"><a onclick="editStudentSibling('+jArray[i]["studentSiblingId"]+',this);"><i class="fa fa-pencil-square-o  iconColorBlack " class="different" data-toggle="toltip" data-placement="bottom" title="Edit"></i></a>&nbsp&nbsp'+
				'<a onclick="deleteStudentSiblingsDetails('+jArray[i]["studentSiblingId"]+');"><i class="fa fa-trash iconColorBlack " class="different" data-toggle="toltip" data-placement="bottom" title="Delete"></i></a></td>'+
				'</tr>';
			}
			boiler+='</tbody></table>';
			$('#showStudentSiblingDetails').html(boiler);
			$('#showStudentSiblingDetails').show();
			$('#addStudentSiblingDetails').hide();
			$('#tblsiblingDetails').dataTable({
				"sDom": "<'row'<'margin-left-22'l><'pull-right'f>r>t<'row'<'margin-left-22'i><'pull-right'p>>",
				"sPaginationType": "bootstrap",

				"bSort":false,
				"bInfo":true,
				"bAutoWidth":false,
				"aLengthMenu":[[10, 25, 50, 100, 200, -1], [10, 25, 50, 100, 200, "All"]],
				"iDisplayLength" : -1,
				"oLanguage": {
					"sLengthMenu": "_MENU_ records per page"
				},
				"sWrapper": "dataTables_wrapper form-inline",
			});

		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});


	$('#addStudentSiblingDetails').hide();
}

function editStudentSibling(StudentSiblingId, src){

	addStudentSibling('Edit');
	var sibFirst = $(src).closest('tr').find('.sibFirst').html();
	var sibMiddle = $(src).closest('tr').find('.sibMiddle').html();
	var sibLast = $(src).closest('tr').find('.sibLast').html();

	$('#siblingName').val(sibFirst);
	$('#sibmiddleName').val(sibMiddle);
	$('#siblastName').val(sibLast);

	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SPIT"){
		var isSiblingStudying=$(src).closest('tr').find('.isStudyCls').html();
		var isSiblingWorking=$(src).closest('tr').find('.isWorkCls').html();
		var siblingEmail=$(src).closest('tr').find('.emailCls').html();
		var siblingContactNo=$(src).closest('tr').find('.mobNoCls').html();
		var isExSibling = $(src).closest('tr').find('.isExSibCls').html();
		var sibStudyingInBatch = $(src).closest('tr').find('.isStudyBatchCls').html();
		var exSibStudiedInBatch = $(src).closest('tr').find('.isExSibBatchCls').html();

		if(isSiblingStudying=="Yes"){
			$( "input[name='isSibllingStudying'][value='"+isSiblingStudying+"']" ).attr("checked",true).trigger('click');
			$('#isStudyBatch').show();
			$('#isStudyBatch').val(sibStudyingInBatch);
		}else{
		//	$( "input[name='isSibllingStudying'][value='"+isSiblingStudying+"']" ).attr("checked",false);
			$('#isStudyBatch').val('');
			$('#isStudyBatch').hide();
			$('#studyTr').hide();
		}

		if(isExSibling=="Yes"){
			$( "input[name='isExSiblling'][value='"+isExSibling+"']" ).attr("checked",true).trigger('click');
			$('#isExStudyBatch').show();
			$('#isExStudyBatch').val(exSibStudiedInBatch);
		}else{
		//	$( "input[name='isExSiblling'][value='"+isExSibling+"']" ).attr("checked",false);
			$('#isExStudyBatch').val('');
			$('#isExStudyBatch').hide();
			$('#exStudyTr').hide();
		}

		$("input[name='isSibllingWorking'][value='"+isSiblingWorking+"']" ).attr("checked",true);
		$('#siblingEmailId').val(siblingEmail);
		$('#siblingContactNo').val(siblingContactNo);

	}

	getstudProfileConfigurationDetailsCheckingNames();
	var boiler ='<input  class=" btn btn-inverse btn-mini btnColorGreen " type="button" value="Update"  onclick="saveStudentSiblingDetails(\'edit\',\''+StudentSiblingId+'\')">&nbsp&nbsp'+
	'<input class=" btn btn-inverse btn-mini btnColorRed " type="button" value="Cancel" onclick ="getStudentSiblingDetails();" >';
	$('#studentSublingSaveEditTd').html(boiler);

}

function deleteStudentSiblingsDetails(studentSiblingId){
	jConfirm("Do you want to Delete ?","Confirm",function(spc){
		if(spc ==true)
		{
			$.ajax({
				url:'deleteStudentSiblingsDetails.json?studentSiblingId='+studentSiblingId,
				dataType:'text',
				type:'POST',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){
					getStudentSiblingDetails();
					getCustomAlertMessage("alert_delete");
				},
				error:function(){
					getCustomAlertMessage("alert_error");
				}
			});
		}
	});
		
}
var studentFatherImage = false ;
var studentMotherImage = false ;
function getFatherMotherProfileImagesStatus(){
	$.ajax({
		url:'getFatherMotherProfileImagesStatus.json',
		dataType:'json',
		type:'POST',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(jArray.length>0){
				studentFatherImage = jArray[0]["studentFatherImage"];
				studentMotherImage = jArray[0]["studentMotherImage"];
			}
			
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
	});
}

