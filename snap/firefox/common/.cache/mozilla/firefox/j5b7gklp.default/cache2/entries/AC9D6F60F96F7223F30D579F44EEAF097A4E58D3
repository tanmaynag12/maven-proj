//				if (jArray[0]["isExamStatus" + temp + ""])
//					tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'	+ temp+ '" value="'+ jArray[0]["sgpa" + temp + ""]+ '" class="span1 sgpaPrCls forNum"  disabled="disabled"  /></td>';
//				else
//					tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'	+ temp+ '" value="'+ jArray[0]["sgpa" + temp + ""]+ '" class="span1 sgpaPrCls forNum"  /></td>';
//			}else
				tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'+ temp + '" value="0" class="span1 sgpaPrCls" /></td>';

//			if (jArray[0]["cgpa" + temp + ""] != undefined){
//				if (jArray[0]["isExamStatus" + temp + ""])
//					tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtcgpa'+ temp + '" value="'+ jArray[0]["cgpa" + temp+ ""]+ '" class="span1 cgpaPrCls forNum" disabled="disabled"  /></td>';
//				else
//					tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtcgpa'+ temp + '" value="'+ jArray[0]["cgpa" + temp+ ""]+ '" class="span1 cgpaPrCls forNum"  /></td>';
//			}else
				tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtcgpa'	+ temp+ '" value="0" class="span1 cgpaPrCls forNum"  /></td>';

			tempSemBoiler += '</tr>';
		}
		//jArray[0]["currTotSem"]
		for ( var i = temp; i <=totSem ; i++) {

			tempSemBoiler += '<tr>'+
			'<td style="text-align: right;">'+ i+ '</td>'+
			/*'<td><input type="text" style="text-align: right" Readonly="readonly" id="txttotalMarks'+ i	+ '" value="" class="span1" /></td>'+
							'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtCreditRegistered'+ i + '" value="" class="span1" /></td>'+
							'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtCreditEarned'+ i+ '" value="" class="span1" /></td>'*/
			'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtsgpa'+ i + '" value="" class="span1 forNum" /></td>'+
			'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtcgpa'	+ i	+ '" value="" class="span1 forNum"  /></td>'+
			'</tr>';
		}

		var currQualiLiveATKT = 0; 
//			jArray[0]["currQualiLiveATKT"] == undefined ? "0"
//				: jArray[0]["currQualiLiveATKT"];
		var currQualiDeadATKT = 0;
//		jArray[0]["currQualiDeadATKT"] == undefined ? "0"
//				: jArray[0]["currQualiDeadATKT"];

		tempSemBoiler += '</table>'
			+ '</td>'
			+ '</tr>'
			+ '</table>'
			+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
			+ '<tr id="atktTypeTr">'
			+ '<td>'
			+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
			+ '<tr>';
			if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
				tempSemBoiler+= '<th>No of Live Backlogs</th>';
			}else{
				tempSemBoiler+= '<th>No of Live ATKT</th>';
			}
			tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeLiveATKT" value="'
			+ currQualiLiveATKT
			+ '"  class="input-small forNum"></label></td>'
			+ '</tr>'
			+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeDeadATKT" value="'
			+ currQualiDeadATKT
			+ '"  class="input-small forNum"></label></td>'
			+ '</tr>'
			+ '<tr><td colspan="2">'
			+ '<input type="button" id="btnSaveMarksDetailsGrade" onclick="saveCurrentQualiGradeForSinhgad('
			+ totSem
		+ ')" class="btn btn-inverse btn-mini btnColorGreen " value="Save">'
		+ '<input type="button" onclick="viewPGDetails(\''
		+ degreeType
		+ '\')" class="btn btn-inverse btn-mini btnColorRed " value="Back" style="margin-left:10px;" >'
		+ '</table>' + '</td>' + '</tr>'
		+ '</table>';
		
		
		
		}
	
	///-------------------------------- For Sinhgad End----------------------------------------------///
	
	
	$('#tdSem').html(tempSemBoiler);
	$('.validnum').numeric();
	
	
}


function checkNextSemAndAddDetails(currntSem) {
	var type = $('input[name = "percGradPointr"]:checked').val();

	if (type == "Percent")
		addSemWisePercent();
	else if (type == "Grade")
		addSemWiseGrade();
	else if (type == "Pointer")
		addSemWisePointer();
}

function saveCurrentQualiPercent(totSem) {
	var chk = 0;
	for ( var j = 1; j < totSem; j++) {
		if ($('#txtPercentSysemTotMarks' + j + '').val() != ''
			&& $('#txtPercentSysemMarksOutOf' + j + '').val() == '') {
			chk = 1;
			break;
		} else if ($('#txtPercentSysemTotMarks' + j + '').val() == ''
			&& $('#txtPercentSysemMarksOutOf' + j + '').val() != '') {
			chk = 1;
			break;
		}
	}
	/*
	 * if(totSem==""){ jAlert("Please add details"); }
	 */
	if (chk == 1) {
		jAlert("Please add details");
	} else {
		var semArray = new Array();
		var percentArray = new Array();
		var totMarksArray = new Array();
		var outOfMarksArray = new Array();

		var percLiveAtktArray = $("#txtPercntLiveATKT").val();
		var percDeadAtktArray = $("#txtPercentDeadATKT").val();
		if (percLiveAtktArray == '') {
			percLiveAtktArray = 0;
		}

		if (parseInt(percLiveAtktArray) > 99
				|| parseInt(percDeadAtktArray) > 99) {
			jAlert("ATKT cannot be greater than 99");
			return;
		}
		if (percDeadAtktArray == '') {
			percDeadAtktArray = 0;
		}

		var sem = 1;

		for ( var j = 1; j <= totSem; j++) {
			if ($('#txtPercentSysemPercnt' + j + '').val() == "") {
				//break;
			} else {
				sem++;
				semArray.push(j);
				percentArray.push($('#txtPercentSysemPercnt' + j + '').val());
				totMarksArray.push($('#txtPercentSysemTotMarks' + j + '').val());
				outOfMarksArray.push($('#txtPercentSysemMarksOutOf' + j + '').val());
			}
		}
		if (semArray != "") {
			$.ajax({
				url : 'saveCurrentQuali.json?semArray=' + semArray
				+ '&percentArray=' + percentArray
				+ '&totMarksArray=' + totMarksArray
				+ '&outOfMarksArray=' + outOfMarksArray
				+ '&currentSem=' + sem + '&percLiveAtktArray='
				+ percLiveAtktArray + '&percDeadAtktArray='
				+ percDeadAtktArray,
				dataType : 'text',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(txt) {
					if (txt == "successful") {
						jAlert("Current Qualification Details have been saved Successfully","Success");
						getStudentCurrentQualification(degType);
					}
				},
				error : function() {
					// alert("in Error stream CMB");
				}
			});
		} else {
			jAlert("Please add details");
		}
	}
}

//for saving Grade system
function saveCurrentQualiGrade(totSem,degreeType,type,StudGraduation) {
	var chk = 0;
	for ( var j = 1; j < sem; j++) {
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DYPATIL"){
			if ($('#txtsgpa' + j + '').val() == "" && $('#txtsgpa' + j + '').val() == undefined && $('#txtcgpa' + j + '').val() == "" 
				&& $('#txtcgpa' + j + '').val() == undefined) {
				chk = 1;
			}
		}else{
			if ($('#txtsgpa' + j + '').val() == "" || $('#txtsgpa' + j + '').val() == undefined || $('#txtcgpa' + j + '').val() == "" 
				|| $('#txtcgpa' + j + '').val() == undefined) {
				chk = 1;
			}
		}
	}
	if (chk == 1) {
		jAlert("Please add details");
	} else {
		var semArray = new Array();

		//var totalMarks = new Array();
		//var creditRegistered = new Array();
		//var creditEarned = new Array();
		var sgpa = new Array();
		var cgpa = new Array();
		var sgpaCgpa = new Array();

		var percLiveAtktArray = $("#txtGradeLiveATKT").val();
		var percDeadAtktArray = $("#txtGradeDeadATKT").val();
		var sem = 1;
		if($('#customerHelper').val()!=null && ($('#customerHelper').val() =='CMR_UNIVERSITY'|| $('#customerHelper').val() =='CMR_IT')){
			var tempCount = [];
			var tempSgpaCount = 0;
			$('.tempSgpaCls').each(function(){
				tempSgpaCount++;
				if(!tempCount.includes(tempSgpaCount)){
					tempCount.push(tempSgpaCount);
				}
			});
			
			var tempCgpaCount = 0;
			$('.tempCgpaCls').each(function(){
				tempCgpaCount++;
				if(!tempCount.includes(tempCgpaCount)){
					tempCount.push(tempCgpaCount);
				}
			});
			
			for ( var j = 1; j <= tempCount.length; j++) {
				if ($('#txtsgpa' + j + '').val() == "" || $('#txtcgpa' + j + '').val() == "") {
					
				} else {
					semArray.push(j);
					sem++;
					sgpa.push($('#txtsgpa' + j + '').val());
					cgpa.push($('#txtcgpa' + j + '').val());
					sgpaCgpa.push($('#txtsgpa' + j + '').val()+'$'+$('#txtcgpa' + j + '').val());
				}
			}
		}else{
			for ( var j = 1; j <= totSem; j++) {
				if ($('#customerHelper').val() == "DYPATIL") {
					if ($('#txtsgpa' + j + '').val() == "" && $('#txtcgpa' + j + '').val() == "") {
					
					}else{
						semArray.push(j);
						sem++;
						sgpa.push($('#txtsgpa' + j + '').val());
						cgpa.push($('#txtcgpa' + j + '').val());
						sgpaCgpa.push($('#txtsgpa' + j + '').val()+'$'+$('#txtcgpa' + j + '').val());
					}
				}else{
					if ($('#txtsgpa' + j + '').val() == "" || $('#txtcgpa' + j + '').val() == "") {
				
					}else{
						semArray.push(j);
						sem++;
						sgpa.push($('#txtsgpa' + j + '').val());
						cgpa.push($('#txtcgpa' + j + '').val());
						sgpaCgpa.push($('#txtsgpa' + j + '').val()+'$'+$('#txtcgpa' + j + '').val());
					}
				}
			}
		}
		if (semArray != "") {
			if(degreeType==undefined){	
				$.ajax({
					url : 'saveCurrentQualiForGradeSystem.json',
					data : {
						sgpa : sgpa.toString(),
						cgpa:cgpa.toString(), 
						semArray : semArray.toString(),
						percLiveAtktArray : percLiveAtktArray,
						percDeadAtktArray : percDeadAtktArray,
						currentSem : sem
					},
					dataType : 'text',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(txt) {
						if (txt == "successful") {
							getCustomAlertMessage("alert_create");
							getStudentCurrentQualification(degType);
						}
					},
					error : function() {}
				});
			}else{
				$.ajax({
					url : 'saveCGPASGPAGraduationQualiForGradeSystem.json',
					data : {
						sgpa : sgpaCgpa.toString(),
						cgpa:cgpa.toString(), 
						semArray : semArray.toString(),
						percLiveAtktArray : percLiveAtktArray,
						percDeadAtktArray : percDeadAtktArray,
						currentSem : sem,
						type:type,
						universityId:$('#university option:selected').val(),
						institute:$('#txtInstInStudProUgPg').val(),
						stream:$('#stream option:selected').val(),
						degree:$('#degName :selected').val(),
						status:$('input[name = "qualifying"]:checked').val(),
						passingYear : $('#yearPassing :selected').val(),
						PRNNumber : $('#PRNNumber').val(),
						liveATKT : $('#atktLive').val(),
						deadATKT : $('#atktDead').val(),
						merit : $('#merit').val(),
						pgPercentGradeFlag : $('input[name = "pgPercentGradeFlag"]:checked').val(),
						studGraduationId:StudGraduation,
						degreeType:degreeType
					},
					dataType : 'text',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(txt) {
						if (txt == "successful") {
							getCustomAlertMessage("alert_create");
							UgFun();
						}
					},
					error : function() {}
				});
			}
		} else {
			jAlert("Please add details");
		}
	}
}

function saveCurrentQualiGradeForSinhgad(totSem){
	
	
	var studentCreditDeatiArray = new Array();	
	var sem = 1;
	$('.semesterChkBoxCls').each(function(){
		if($(this).is(':checked'))
		{
		var studUniExMarId = $(this).val();//('studUniExMarId');
		if(studUniExMarId==undefined || studUniExMarId=="undefined"){
			studUniExMarId = 0;
		}
		var totPts = $(this).closest('tr').find('.totPtsCls').val() != "" ? $(this).closest('tr').find('.totPtsCls').val() : 0;
		var regCredit = $(this).closest('tr').find('.credRegistCls').val() !="" ? $(this).closest('tr').find('.credRegistCls').val() : 0;
		var earCredit = $(this).closest('tr').find('.credEarCls').val() !="" ? $(this).closest('tr').find('.credEarCls').val() : 0;
		
		var tempTrNo = eval($(this).closest('tr').attr('sem'));
		sem = tempTrNo;
			if(eval(tempTrNo) == 1)
				{
					/*var cgpa = eval(totPts)/eval(regCredit);
					var sgpa = eval(totPts)/eval(regCredit);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));*/
				var cgpa = 0;
				var sgpa = 0;
				if(totPts != "0" && regCredit != "0")
				{
					cgpa = eval(totPts)/eval(regCredit);
					sgpa = eval(totPts)/eval(regCredit);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
					
				}
				else 
				{
					cgpa = 0;
					sgpa = 0;
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
					
				}
					var creditDetails = regCredit+"~~"+earCredit+"~~"+tempTrNo+"~~"+sgpa.toFixed(2)+"~~"+cgpa.toFixed(2)+'~~'+studUniExMarId+'~~'+totPts;	
				}
			else if(eval(tempTrNo) > 1)
				{
				var sgpa = 0;
				
				if(totPts != "0" && regCredit != "0")
				{
				sgpa = eval(totPts)/eval(regCredit);
				if(eval(sgpa)!="" && !isNaN(sgpa))
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
				}
				
				var totRegCredits = 0;
				var totPtsTotal = 0;
				for (var temp = 1; temp <=tempTrNo ; temp++) {
					totRegCredits+= eval($('.tempTrCls_'+temp).find('.credRegistCls').val() !="" ? $('.tempTrCls_'+temp).closest('tr').find('.credRegistCls').val() : 0);
					totPtsTotal+= eval($('.tempTrCls_'+temp).find('.totPtsCls').val() !="" ? $('.tempTrCls_'+temp).find('.totPtsCls').val() : 0);
					sem = temp;
			}
				
				var cgpa = 0;
				if(totPtsTotal != "0" && totRegCredits != "0")
				{
					cgpa = eval(totPtsTotal)/eval(totRegCredits);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
				}
				else 
				{
					cgpa = 0;
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
				}
//				var cgpa = eval(totPtsTotal)/eval(totRegCredits);
				var creditDetails = regCredit+"~~"+earCredit+"~~"+tempTrNo+"~~"+sgpa.toFixed(2)+"~~"+cgpa.toFixed(2)+'~~'+studUniExMarId+'~~'+totPts;
				
				if(eval(cgpa)!="" && !isNaN(cgpa))
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
			}
			studentCreditDeatiArray.push(creditDetails);
		}
	});
	
	
	var percLiveAtkt = $("#txtGradeLiveATKT").val();
	var percDeadAtkt = $("#txtGradeDeadATKT").val();
	
	if (studentCreditDeatiArray.length > 0) {
		$.ajax({
			url : 'saveCurrentQualiForGradeSystemForSinhgad.json',
			data : {
				//totalMarks : totalMarks.toString(),
				//creditRegistered : creditRegistered.toString(),
				//creditEarned : creditEarned.toString(),
				studentCreditDeatiArray : studentCreditDeatiArray.toString(),
				percLiveAtkt : percLiveAtkt,
				percDeadAtkt : percDeadAtkt,
				currentSem : sem
			},
			dataType : 'text',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(txt) {
				if (txt == "successful") {
					getCustomAlertMessage("alert_create");
					getStudentCurrentQualification(degType);
				}
			},
			error : function() {
				// alert("in Error stream CMB");
			}
		});
	} else {
		getCustomAlertMessage("Please Enter Details..!");
	}
	
	/*var chk = 0;
	for ( var j = 1; j < sem; j++) {
		if ($('#txtsgpa' + j + '').val() == "" || $('#txtsgpa' + j + '').val() == undefined || $('#txtcgpa' + j + '').val() == "" 
			|| $('#txtcgpa' + j + '').val() == undefined) {
			chk = 1;
		}
	}
	if (chk == 1) {
		jAlert("Please add details");
	} else {
