			$('input[name = "mrdNumber"]').attr("disabled", true);
		}
		
//Entrance Details......................................................................................................................................................................................................................................................
		if ((gArray[i]["isCheckedText"] == "othEntranceExam") && ($('select[name = "othEntranceExam"]')!=undefined) && ( ( $('select[name = "othEntranceExam"]').val())!=null) && ($('select[name = "othEntranceExam"]').val() !="0")) {
