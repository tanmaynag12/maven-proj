		}

		if ( (gArray[i]["isCheckedText"] == "othApplicationNo") && ($('input[name = "othApplicationNo"]')!='') && ($('input[name = "othApplicationNo"]')!=undefined) && (($('input[name = "othApplicationNo"]').val())!=null) &&  ($.trim($('input[name = "othApplicationNo"]').val())!="") && ($('input[name = "othApplicationNo"]').val().length>0)) {
