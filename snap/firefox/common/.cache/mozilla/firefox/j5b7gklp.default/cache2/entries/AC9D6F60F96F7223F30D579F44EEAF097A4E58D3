function guardianPictureUploadEdit(){
	new AjaxUpload('guardianPictureUpload', {
		action : 'uploadGuardianProfileImage.json',
		name : 'fileData',
		size:AllowedFileSize.PROFILE_PHOTO,
		onSubmit : function(file, extension) {
			if(extension.toString().toLowerCase() == 'jpeg' || extension.toString().toLowerCase() == 'jpg'){
				$('.guardianImageUpload').find('.preview').addClass('loading');
				this.setData({
					'fileSize':AllowedFileSize.PROFILE_PHOTO, 
					'photoOf' :'Guardian'
				});
			} else {
				jAlert('Use jpg image file!');
				return false;
			}
		},
		onComplete : function(file, response) {
			$('.guardianImageUpload').find('.preview').removeClass('loading');
			if(isNaN(response)){
				showErrorMessage('Image size is larger than specified size!',10000);
			} else {

				var imageBoiler = 
					'<img src="getGuardianProfileImage.json?studentId=0&id='+response+'&type=profile&photoOf=Guardian" alt="Photo" id = "guardianProfilePhoto" name="Photo" width="120px" height="120px" border="1" align="right" style="background-color: #FFFFFF" /></td>';

				$('#guardianProfileImage').html(imageBoiler);	
				jAlert("Guardians photo uploaded. ","Message");
			}
		}
	});
}
var globalPhdArr=[];
function getStudentPhdDetails(studentId){
	$.ajax({
		url:'stu_getStudentPhdDetails.json',
		dataType:'json',
		type:'POST',		
		async:false,
		beforeSend: function(){ 
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			globalPhdArr=[];
			if(jArray.length>0){
				var boiler='';
				boiler+='<table class="table table-bordered table-condensed">'+
							'<thead><tr>'+
								'<th>Degree Name</th>'+
								'<th>Institute Name</th>'+
								'<th>University Name</th>'+
								'<th>Current Designation</th>'+
								'<th>PRN/Registration Number</th>'+
								'<th>Specialization</th>'+
								'<th>Action</th>'+
							'</tr>'+
							'<thead><tbody>';
						for (var i = 0; i < jArray.length; i++) {
							globalPhdArr.push(jArray[i]);
							boiler += '<tr>'+
							'<td style="text-align: center;"><label>' + jArray[i].degreeName+ '</label></td>' +
							'<td style="text-align: center;"><label>' + jArray[i].instituteName + '</label></td>'+
							'<td style="text-align: center;"><label>' + jArray[i].universityName + '</label></td>' +
							'<td style="text-align: center;"><label>' + jArray[i].currentDesig + '</label></td>' +
							'<td style="text-align: center;"><label>' + jArray[i].prnNumber + '</label></td>' +
							'<td style="text-align: center;"><label>' + jArray[i].specialization + '</label></td>' +
							'<td colspan="2" style="width:8%"><a onclick="editStudentPhdDetails(\''+i+'\');" class="pointer" href="javascript:void(0)" ><i class="fa fa-pencil-square-o iconColorBlack "></i> Edit &nbsp;&nbsp;<a onclick="deleteStudentPhdDetails(\''+jArray[i].studentPhdDetailId+'\',\''+studentId+'\')" class="pointer" href="javascript:void(0)" ><i class="fa fa-trash iconColorBlack"></i> Delete </td>' +
							'</tr>';
					}
				boiler+='</tbody></table>';
				$('#getPhdDetailsId').html(boiler);
				$('#phdDetailtbl').hide();
				$('#addDetailbtn').show();
				$('#getPhdDetailsId').show();
			}else{
				$('#addDetailbtn').hide();
				$('#getPhdDetailsId').html('');
				$('#phdDetailtbl').show();
				$('#phdDetailtbl').find(':input:not(:radio)').val('');
			}
		},
		error:function(){
		}
	});
}
function addDetails(){		
			$('#getPhdDetailsId').hide();
			$('#phdDetailtbl').show();
			$('#addDetailbtn').hide();
			$('#phdDetailtbl').find(':input:not(:radio)').val('');
		}
function addStudentPhdDetails(studPhdDId){
	var studentId =$('#hidenStudentId').val();
	var nameOfStudent = $('#nameOfCandidatesPhd').val();
	var universityBoardId = $('#universityBoardName option:selected').val();
	var instituteName = $('#instituteName').val();
	var degreeNameId = $('#degreeNamePhd option:selected').val();
	var stateId = $('#stateName option:selected').val();
	var prnRegistrationNo = $('#prnRegNoPhd').val();
	var facultyStfAd = $('input[name="facultyStf"]:checked').val();
	var designation = $('#currentDesignation').val();
	var qualifiedBasics = $('#qualifiedBasics option:selected').val();
	var transcriptCert = $('#transcriptCert').val();
	var specialization = $('#txtSpecializationPhd').val();
	
	if(nameOfStudent == ''){
		$('#nameOfCandidatesPhd').focus();
		$('#nameOfCandError').show('slow');
		return false;
	}else if(universityBoardId == '' || universityBoardId == "0" || universityBoardId == 0 || universityBoardId == 'undefined' || universityBoardId == undefined){
		$('#universityBoardName').focus();
		$('#universityBoardError').show('slow');
		return false;
	}
	if(instituteName==''){
		$('#instituteName').focus();
		$('#instituteNameError').show('slow');
		return false;
	}
	if(degreeNameId == '' || degreeNameId == '0' || degreeNameId == 0 || degreeNameId == 'undefined' || degreeNameId == undefined){
		$('#degreeNamePhd').focus();
		$('#degreeNamePhdError').show('slow');
		return false;
	}
	if(stateId == '' || stateId == '0' || stateId == 0 || stateId == 'undefined' || stateId == undefined){
		$('#stateName').focus();
		$('#stateNameError').show('slow');
		return false;
	}
	if(facultyStfAd == ''){
		$('#facultyYesError').show('slow');
		return false;
	}
	if(designation == ''){
		$('#currentDesignation').focus();
		$('#currentDesignationError').show('slow');
		return false;
	}
	if(qualifiedBasics == '' || qualifiedBasics == "0" || qualifiedBasics == 0 || qualifiedBasics == 'undefined' || qualifiedBasics == undefined){
		$('#qualifiedBasics').focus();
		$('#qualiBasicError').show('slow');
		return false;
	}
	if(transcriptCert == ''){
		$('#transcriptCert').focus();
		$('#transcriptCertError').show('slow');
		return false;
	}
	
	 $.ajax({
		url:'stu_addStudentPhdDetails.json',
		data:{  
				nameOfStudent : nameOfStudent,
				universityBoardId : universityBoardId,
				instituteName : instituteName ,
				degreeNameId : degreeNameId ,
				stateId : stateId ,
				prnRegistrationNo : prnRegistrationNo ,
				facultyStfAd : facultyStfAd ,
				d␀␀␀esignation : designation ,
				qualifiedBasics : qualifiedBasics ,
				transcriptCert : transcriptCert ,
				specialization : specialization ,
				studPhdDId : studPhdDId
			},
		dataType:'text',
		type:'GET',		
		async:false,
		beforeSend: function(){ 
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(msg){
			if(msg == "Success"){
				jAlert("Record Saved Successfully");
				getStudentPhdDetails(studentId);
				$('#addDetailbtn').show();
				$('#getPhdDetailsId').show();
				$('#phdDetailtbl').hide();
				$('#savePhdDetBtnId').attr('onclick','addStudentPhdDetails(0)');
			}else{
				jAlert("Error!!!");
				return false;
			}
		},
		error:function(){
		}
	});
}
function editStudentPhdDetails(index){
	$('#phdDetailtbl').show();
	$('#nameOfCandidatesPhd').val(globalPhdArr[index]["studentName"]);
	$('#universityBoardName').val(globalPhdArr[index]["otherUniversityId"]);
	$('#instituteName').val(globalPhdArr[index]["instituteName"]);
	$('#dStreamId').val(globalPhdArr[index]["degreeStream"]);
	getDegreeNameNew();
	$('#degreeNamePhd').val(globalPhdArr[index]["otherPlacementQualiId"]);
	$('#stateName').val(globalPhdArr[index]["stateId"]);
	$('#prnRegNoPhd').val(globalPhdArr[index]["prnNumber"]);
	$('#currentDesignation').val(globalPhdArr[index]["currentDesig"]);
	$("input[name=facultyStf][value="+(globalPhdArr[index]["facultyStfM"])+"]").prop('checked',true);
	$('#qualifiedBasics').val(globalPhdArr[index]["qualifiedBasic"]);
	$('#transcriptCert').val(globalPhdArr[index]["transcriptCert"]);
	$('#txtSpecializationPhd').val(globalPhdArr[index]["specialization"]);
	$('#getPhdDetailsId').hide();
	$('#addDetailbtn').hide();
	$('#savePhdDetBtnId').attr('onclick','addStudentPhdDetails('+globalPhdArr[index]["studentPhdDetailId"]+')');
}
function deleteStudentPhdDetails(phdStudentId,studentId){
	$.ajax({
		url:'stu_deleteStudentPhdDetails.json',
		data:{phdStudentId : phdStudentId},
		dataType:'text',
		type:'POST',		
		async:false,
		beforeSend: function(){ 
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(msg){
			if(msg == "Success"){
				jAlert("Record Deleted Successfully");
				getStudentPhdDetails(studentId);
			}else{
				jAlert("Error!!!");
				return false;
			}
		},
		error:function(){
		}
	});
}

function showScholarshipDetailsNew() {
    var Boiler = '';

    Boiler += '<table class="table table-bordered table-condensed">' +
                '<tbody>' +
                '<tr>' +
                '<th colspan="2" style="color: #0088CC"><h4>Scholarship / Aid Details </h4></th>' +
                '</tr>' +
                '<tr>' +
                '<td><b>Scholarship / Aid :</b></td>' +
                '<td>' +
                    '<input name="stateScholarship" type="checkbox" id="stateScholarship" value="receivedScholarshipFromStateGovernment" />  Received Scholarship/Aid From State Government<br>' +
                    '<input name="centralScholarship" type="checkbox" id="centralScholarship" value="receivedScholarshipFromCentralGovernment" />  Received Scholarship/Aid From Central Government<br>' +
                    '<input name="NGOScholarship" type="checkbox" id="NGOScholarship" value="receivedScholarshipFromNGO" />  Received Scholarship/Aid From NGO' +
                '</td>' +
                '</tr>' +
                '<tr>' +
                '<td></td>' +
                '<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen" id="btnAddNewStudProj" onclick="saveStudentScholarshipReceivedDetails()"/>' + 
                '</td>' + 
                '</tr>' +
                '</tbody>' +
                '</table>';

    $('#scholarship').html(Boiler);

    $.ajax({
        url: 'getStudentScholarshipReceivedDetails.json',
        type: 'POST',
        dataType: 'json',
        success: function(response) {
            if (response && response.length > 0) {
                var scholarshipDetails = response[0]; 

                if (scholarshipDetails.stateScholarship == "receivedScholarshipFromStateGovernment") {
                    $('#stateScholarship').prop('checked', true);
                }
                if (scholarshipDetails.centralScholarship == "receivedScholarshipFromCentralGovernment") {
                    $('#centralScholarship').prop('checked', true);
                }
                if (scholarshipDetails.NGOScholarship == "receivedScholarshipFromNGO") {
                    $('#NGOScholarship').prop('checked', true);
                }
                $('#btnAddNewStudProj').attr('onclick', 'saveStudentScholarshipReceivedDetails(' + scholarshipDetails.studentScholarshipReceivedDetailId+')');
            }
        },
        error: function() {
            console.error("Error retrieving scholarship details");
        }
    });
}


function saveStudentScholarshipReceivedDetails(studentScholarshipReceivedDetailId) {
    var stateScholarship = $('input[name="stateScholarship"]:checked').val();
    var centralScholarship = $('input[name="centralScholarship"]:checked').val();
    var NGOScholarship = $('input[name="NGOScholarship"]:checked').val();

    $.ajax({
        url: 'saveStudentScholarshipReceivedDetails.json',
        dataType: 'text',
        data: {
            stateScholarship: stateScholarship,
            centralScholarship: centralScholarship,
            NGOScholarship: NGOScholarship,
            studentScholarshipReceivedDetailId: studentScholarshipReceivedDetailId
        },
        type: 'POST',
        beforeSend: function() {
            startCommonAjaxSpinner();
        },
        complete: function() {
            stopCommonAjaxSpinner();
        },
        success: function(msg) {
            if (msg === "Success") {
                jAlert("Record Saved Successfully");
                showScholarshipDetailsNew();
            } else {
                jAlert("Error saving record.");
            }
        },
        error: function() {
            console.error("Error saving scholarship details");
        }
    });
}

function getAllVoterConstituencys() {
	$.ajax({
		url : 'getVoterConstituencyList.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {"others"
			var html = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++)
				html += '<option value="' + data[i]["voterConstituencyId"] + '">'
				+ data[i]["voterConstituency"] + '</option>';
			convertComboToSearchable('#voterConstituency');
			$('#voterConstituency').html(html);
		},

	});
}
