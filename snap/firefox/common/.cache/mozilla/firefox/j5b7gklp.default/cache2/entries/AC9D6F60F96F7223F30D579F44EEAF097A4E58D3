		var stateId = $('#studentState_1').val();
		var countryId = $('#studentCountryCombo_1').val();
		var pinCode = $('input[name = "currentpinCodeNumber_1"]').val();
		var CurrAddressId = $('#CurrAddressId_1').val();
		var currDistrict = $('#txtStudentDistrict_1').val();
		var currTaluka = $('#txtStudentTaluka_1').val();
		
		var permaaddressLine1 = $('#permstudAddLine1_1').val();
		var permaaddressLine2 = $('#permstudAddLine2_1').val();
		var permstudentCity = $('#permstudentCity_1').val();
		var permstudentState = $('#permstudentState_1').val();
		var permstudentCountryCombo1 = $('#permstudentCountryCombo1_1').val();
		var permstudPincode = $('#permstudPincode_1').val();
		var PermaAddressId = $('#PermaAddressId_1').val();
		var permDistrict = $('#txtStudentPermDistrict_1').val();
		var permTaluka = $('#txtStudentPermTaluka_1').val();
		var studNameHindi=$('#firstNameHindi').val();
		var subCatId=$('#studSubCategory option:selected').val();
		var categoryValue=subCategory[subCatId];
		var admissionType=$('#studAdmissionType option:selected').val();
		//subCategory[]
		
		if($('#customerHelper').val()!="MANDSAUR" && (subCatId=="-1"))
			{
			jAlert("Please Select Sub-Category");
			return false;
			}
		 if((adharCardNo == "" || adharCardNo.length < 12)){
			jAlert("Please enter Aadhaar Card No.");
			return false;
		}
		 if(studNameHindi=="")
			 {
			 jAlert("Please Enter Name In Hindi.");
				return false;
			 }
		 else if(fatherNameHindi=="")
			 {
			 jAlert("Please Enter Father Name In Hindi.");
				return false;
			 }
		 else if(motherNameHindi=="")
			 {
			 jAlert("Please Enter Mother Name In Hindi.");
				return false;
			 }
		
		if (addressLine1 == '' || addressLine1 == undefined) {
			jAlert("Please enter address line 1 of current address");
			return false;
		} else if (countryId == 0 || countryId == '0' || countryId == undefined) {
			
			jAlert("Please select country of current address");
			return false;
		} else if (stateId == 0) {
			jAlert("Please select state of current address");
			return false;
		} else if (cityId == 0) {
			jAlert("Please select city of current address");
			return false;
		} else if (pinCode == '' || isNaN(pinCode)) {
			jAlert("Please enter valid pin code of current address");
			return false;
		} else if (permaaddressLine1 == '') {
			jAlert("Please enter address line 1 of permanent address");
			return false;
		} else if (permstudentCountryCombo1 == 0) {
			jAlert("Please select country of permanent address");
			return false;
		} else if (permstudentState == 0) {
			jAlert("Please select state of permanent address");
			return false;
		} else if (permstudentCity == 0) {
			jAlert("Please select city of permanent address");
			return false;
		} else if (permstudPincode == '' || isNaN(permstudPincode)) {
			jAlert("Please enter valid pin code of permanent address");
			return false;
		}
		
		if(regex_nameValidation.test(fName)==false){
			jAlert('Please enter valid name. <br> Dont use special characters.');
			return false;
		}
		else if(regex_nameValidation.test(mName)==false){
			jAlert('Please enter valid middle name.<br> Dont use special characters.');
			return false;
		}
		else if(regex_nameValidation.test(lName)==false){
			jAlert('Please enter valid last name.<br> Dont use special characters.');
			return false;
		}
		else if(regex_nameValidation.test(motherName)==false){
			jAlert('Please enter valid mother name.<br> Dont use special characters.');
			return false;
		}
		else if(regex_nameValidation.test(fatherName)==false){
			jAlert('Please enter valid father name.<br> Dont use special characters.');
			return false;
		}
		else if(regex_nameValidation.test(tenthappearName)==false){
			jAlert('Please enter valid name as on tenth marksheet.<br> Dont use special characters.');
			return false;
		}
		else if($('#customerHelper').val()!="MANDSAUR" && (Gender==undefined || Gender==""))
			{
			jAlert("Please Select Gender");
			return false;
			}
	}
	var regexp =/^[a-zA-Z`. ]*$/;

	if(regexp.test(fName)==false)
	{
		jAlert("Please Enter valid first Name.");
		return false;
	}
	if(regexp.test(mName)==false)
	{
		jAlert("Please Enter valid middle Name.");
		return false;
	}
	if(regexp.test(lName)==false)
	{
		jAlert("Please Enter valid Last Name.");
		return false;
	}
	if(regexp.test(tenthappearName) == false)
	{
		jAlert("Please Enter valid Name.");
		return false;
	}
	if (cnt == 0) {
		if($('#customerHelper').val()=="BLM_MUNJAL"){
