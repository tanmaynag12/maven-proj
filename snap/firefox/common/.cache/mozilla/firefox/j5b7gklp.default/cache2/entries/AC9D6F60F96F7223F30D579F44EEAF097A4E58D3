function showGradeSystem(totSem, currSem, degreeType,type,StudGraduation) {
	if($('#tableGradingOrPercentageGrade').is(":visible") || $('#tableGradingOrPercentageGrade').is(":hidden")){
		$('#finalLine').hide();
		$('#tableGradingOrPercentage').hide();
		$('#tableGradingOrPercentageGrade').show();
	}
	var tempSemBoiler =

		'<table class="table table-bordered table-condensed" style="width: 100%;">'
		+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
		+ '<tr style="margin-left:10%;" id="marksTypeTr">'
		+ '<td colspan="3">'
		+ '<table class="table table-bordered table-condensed">'
		+ '<tr>';

	tempSemBoiler += '<th>Class</th>'+ 
	/*'<th>Total Marks</th>'+
							'<th>Credit Registered</th>'+
							'<th>Earned Credit</th>'+*/
	'<th>SGPA</th>' + 
	'<th>CGPA</th>'
	+ '</tr>';
	var jArray1= new Array();
	if(StudGraduation!=undefined){
		$.ajax({
		url : 'getGraduationsCgpaSgpa.json?studGraduationId='+StudGraduation,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if(jArray.length>0){
				jArray1=jArray;
			}
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
	}
	var temp = 0;
	var j=0;
	var readOnly = 'disabled="disabled"';
	if($('#customerHelper').val()=="GOKHALE_INSTITUTE" && degreeType == "UG"){
		readOnly = '';
	}
	for (temp = 1; temp <= currSem; temp++) {

		tempSemBoiler += '<tr>'	+ '<td style="text-align: right;">'	+ temp + '</td>';
		if (jArray1[j]!=undefined && jArray1[j]["sgpa"] != undefined){
				tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtsgpa'	+ temp+ '" value="'+jArray1[j]["sgpa"]+'" class="span1" '+readOnly+' /></td>';
		}else
			tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtsgpa'+ temp + '" value="" class="span1 tempSgpaCls" /></td>';

		if (jArray1[j]!=undefined && jArray1[j]["cgpa"] != undefined){
				tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtcgpa'+ temp + '"  value="'+jArray1[j]["cgpa"]+'" class="span1" '+readOnly+' /></td>';
		}else
			tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtcgpa'	+ temp+ '" value="" class="span1 tempCgpaCls"  /></td>';

		tempSemBoiler += '</tr>';
		j++;
	}

	for ( var i = temp; i <= totSem; i++) {
		tempSemBoiler += '<tr>'+
		'<td style="text-align: right;">'+ i+ '</td>'+
		'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtsgpa'+ i + '" value="" class="span1" /></td>'+
		'<td><input type="text" style="text-align: right" Readonly="readonly" id="txtcgpa'	+ i	+ '" value="" class="span1"  /></td>'+
		'</tr>';
	}

	/*var currQualiLiveATKT = jArray[0]["currQualiLiveATKT"] == undefined ? "0"
			: jArray[0]["currQualiLiveATKT"];
	var currQualiDeadATKT = jArray[0]["currQualiDeadATKT"] == undefined ? "0"
			: jArray[0]["currQualiDeadATKT"];*/

	tempSemBoiler += '</table>'
		+ '</td>'
		+ '</tr>'
		+ '</table>'
		+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
		+ '<tr id="atktTypeTr">'
		+ '<td>'
		+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
		+ '<tr>';
		if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
			tempSemBoiler+= '<th>No of Live Backlogs</th>';
		}else{
			tempSemBoiler+= '<th>No of Live ATKT</th>';
		}
		tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeLiveATKT" value="0'
		//+ currQualiLiveATKT
		+ '"  class="input-small"></label></td>'
		+ '</tr>'
		+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeDeadATKT" value="0'
		//+ currQualiDeadATKT
		+ '"  class="input-small"></label></td>'
		+ '</tr>'
		+ '<tr><td colspan="2">'
		+ '<input type="button" id="btnSaveMarksDetailsGrade" onclick="saveCurrentQualiGrade('
		+ totSem
	+ ',\''+degreeType+'\',\''+type+'\',\''+StudGraduation+'\')" class="btn btn-inverse btn-mini btnColorGreen " value="Save">'
	+ '<input type="button" onclick="viewPGDetails(\''
	+ degreeType
	+ '\')" class="btn btn-inverse btn-mini btnColorRed " value="Back" style="margin-left:10px;" >'
	+ '</table>' + '</td>' + '</tr>'
	+ '</table>';
	$('#marksTypeTr').html(tempSemBoiler);
	if($('#tableGradingOrPercentageGrade').is(":visible") || $('#tableGradingOrPercentageGrade').is(":hidden")){
		$('#tableGradingOrPercentageGrade').html(tempSemBoiler);
	}
}


function showGradeSystemForSinhgadPointer(totSem, currSem, degreeType) {
	
	
	var tempSemBoiler = '';
	
	// academic pointer
	tempSemBoiler +=

		'<table class="table table-bordered table-condensed" style="width: 100%;">'
		+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
		+ '<tr style="margin-left:10%;" id="marksTypeTr">'
		+ '<td colspan="3">'
		+ '<table class="table table-bordered table-condensed">'
		+ '<tr>';

	
///-------------------------------- For Sinhgad ----------------------------------------------///
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SINHGAD")
		{
		tempSemBoiler += 
			
		'<td colspan="7" style="height:20px"><font color="red"><b>Click on check box to enter marks.</b></font></td></tr>'+	
		'<tr><th style="width:5%"></th>'+
		'<th style="width:10%">Class/Semester</th>'+ 
		'<th style="width:10%">Credit Registered</th>'+
		'<th style="width:10%">Earned Credit</th>'+
		'<th style="width:10%">Total Marks/Points</th>'+
		'<th style="width:10%">SGPA</th>' + 
		'<th style="width:10%">CGPA</th>'
		+ '</tr>';
		
		var temp = 0;
		//currentSem
		for (temp = 1; temp <=totSem ; temp++) {

			tempSemBoiler += '<tr class="tempTrCls_'+temp+'" sem="'+temp+'">'; 
//			if(jArray[0]["studUniExMarId"+ temp + ""] =="" || jArray[0]["studUniExMarId"+ temp + ""] == undefined || jArray[0]["studUniExMarId"+ temp + ""]=="undefined" || jArray[0]["studUniExMarId"+ temp + ""] =="0")
				tempSemBoiler += '<td style="text-align: center;"><input type="checkbox" value="0" class="semesterChkBoxCls" onclick="onclicksemesterChkBox(this)"> </td>';
//			else
//				{
