					$('input[name = "alternetEmailID"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "personalEmailID") && ($('input[name = "personalEmailID"]')!=undefined) && (($('input[name = "personalEmailID"]').val())!=null) && ($.trim($('input[name = "personalEmailID"]').val()) !="")  && ($('input[name = "personalEmailID"]').val().length > 0) ){
					$('input[name = "personalEmailID"]').attr("disabled", true);
				}
				
				if( (gArray[i]["isCheckedText"] == "Gender") && ($('input[name = "Gender"]') != undefined) && (($('input[name = "Gender"]').val())!=null) && ($('input[name = "Gender"]').is(':checked'))) {
					$('input[name = "Gender"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "Hobbies") && ($('input[name = "Hobbies"]') !=undefined) && (($('input[name = "Hobbies"]').val())!=null) && ($.trim($('input[name = "Hobbies"]').val())!="") && ($('input[name = "Hobbies"]').val().length>0)) {
					$('input[name = "Hobbies"]').attr("disabled", true);
				}
		
				if ((gArray[i]["isCheckedText"] == "selstudbloodgrp") && ($('select[name = "selstudbloodgrp"]')!=undefined) && (($('select[name = "selstudbloodgrp"]').val())!=null) && ($('select[name = "selstudbloodgrp"]')!="Select") && ($('select[name = "selstudbloodgrp"]').val().length>0)) {
					$('select[name = "selstudbloodgrp"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "motherTongue") && ($('select[name = "motherTongue"]')!=undefined) && ( ( $('select[name = "motherTongue"]').val())!=null) && ($('select[name = "motherTongue"]').val() !="0")) {
