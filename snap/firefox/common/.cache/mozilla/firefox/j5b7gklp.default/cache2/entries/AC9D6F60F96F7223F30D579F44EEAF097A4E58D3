		url = 'stu_getMultipleGuradianDetailsStudentProfile.json';
	}
	$.ajax({
		url:url, 
		dataType:'json',
		async:false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success:function(jFinalObj){
			var boilerGuard = '';
			var jArray = jFinalObj.jArray;
			var guradianDetailArray = jArray;
