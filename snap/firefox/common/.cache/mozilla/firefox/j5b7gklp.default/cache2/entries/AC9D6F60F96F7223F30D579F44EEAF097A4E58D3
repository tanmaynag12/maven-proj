		+ '			</tr> ';
		boiler+= '<tr> '
		+ '				<td><b>Institution:</b><b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><input type="text" id="txtInstInStudProUgPg" class="input-medium" name="InstInStudProUgPgName_'+degreeType+'">&nbsp;<span id="instError" style="display:none;color:#f00;">Please, enter the institution.</span></td> '
		+ '			</tr> '
		+ '			<tr> '
		+ '				<td><b>Stream:</b><b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><select id="stream" name="streamName_'+degreeType+'" class="input-medium" onchange="getDegreeName(\''
		+ degreeType
		+ '\')" /> &nbsp;&nbsp;'
		+
		/*
		 * '<img src="dhtmlxToolbar/codebase/imgs/addNew.jpg"
		 * style="cursor:pointer;" title="Add New Stream"
		 * onclick="addNewStreamInStudProContact()" >&nbsp;'+
		 */
		'<span id="strError" style="display:none;color:#f00;">Please, select the stream.</span>'
		+ '					<span style="">* (Science / Management / Commerce/ Arts / Engineering /Hotel Management / Pharmacy etc) '
		+ ' 			</b></span>	</td> '
		+ '			</tr> ';
		boiler+= '			<tr> '
		+ '				<td><b>Degree Name:</b><b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><select id="degName" name="degreeName_'+degreeType+'" class="input-medium" onchange="getDegreeType()"></select>&nbsp;&nbsp;<img src="dhtmlxToolbar/codebase/imgs/addNew.jpg" style="cursor:pointer;display:none;" title="Add Degree Name" onclick="addNewDegreeInStudProContact(\''
		+ degreeType
		+ '\')" >&nbsp;<span id="degError" style="display:none;color:#f00;">Please, select the degree.</span>'
		+ '<span style="">* Degree Name with/without Specialization (Bsc(Comp)/Bsc(Statistics)/BE(IT)/BE(E&TC)/BA(Psychology)/B.Com/MBA(Marketing)/MCA) </span>'
		+ '</td> ' + '			</tr> ';
	

	if (degreeType == "DIP") {
		boiler += '			<tr> '
			+ '				<td><b>Degree Type :</b></td> '
			+ '				<td><input type="text" class="input-medium" disabled="disabled" id="txtDegType" /></td> '
			+ '			</tr> ';
	}
	if (universityName != undefined && universityName == "D.Y. Patil Edu. Soc. Deemed University"){
		boiler += '			<tr> '
				+ '				<td><b>Last Qualifying Exam State :</b></td> '
				+ '				<td><input type="text" id="txtLastQualifyState" class="input-medium" name="lastQualifyExamState'+degreeType+'">&nbsp;</td>'
				+ '			</tr> ';
	}
	boiler += '			<tr> '
		+ '				<td><b>Qualifying Status:</b> <b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><input type="radio" name="qualifying" class="input-medium" value="Completed"> Completed</input>&nbsp;&nbsp;<input type="radio" name="qualifying" class="input-medium" value="Pursuing"> Pursuing</input>&nbsp;<span id="qualError" style="display:none;color:#f00;">Please, select the qualifying status.</span></td> '
		+ '			</tr> '
		+ '			<tr> '
		+ '				<td><b>Year of Passing :</b> <b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><select id="yearPassing" name="yearPassingName_'+degreeType+'" class="input-medium"/>&nbsp;<span id="yopError" style="display:none;color:#f00;">Please, select the year of passing.</span></td> '
		+ '			</tr> '
		+ '			<tr> ';
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="XLRI"){
					if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="HYGIA"){
						boiler	+='				<td><b>University Roll Number :</b> <b style="color:red;font-size:16px;" >*</b></td> '
						+ '				<td><input type="text" id="PRNNumber" name="PRNNumberName_'+degreeType+'" class="input-medium"/> (As per University Marksheet)&nbsp;<span id="prnError" style="display:none;color:#f00;">Please, enter the University Roll Number.</span></td> '
						+ '			</tr> ';
					}
					else if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="ABS") {
						boiler	+='				<td><b>MarkSheet Serial NO. :</b> <b style="color:red;font-size:16px;" >*</b></td> '
							+ '				<td><input type="text" id="PRNNumber" name="PRNNumberName_'+degreeType+'" class="input-medium"/> (As per University Marksheet)&nbsp;<span id="prnError" style="display:none;color:#f00;">Please, enter the University Roll Number.</span></td> '
							+ '			</tr> ';
					}
					else if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="REVA_UNIVERSITY") {
						boiler	+='				<td><b>SRN Number :</b> <b style="color:red;font-size:16px;" >*</b></td> '
							+ '				<td><input type="text" id="PRNNumber" name="PRNNumberName_'+degreeType+'" class="input-medium"/> (As per University Marksheet)&nbsp;<span id="prnError" style="display:none;color:#f00;">Please, enter the University Roll Number.</span></td> '
							+ '			</tr> ';
					}
					else{
						boiler	+='				<td><b>PRN Number :</b> <b style="color:red;font-size:16px;" >*</b></td> '
						+ '				<td><input type="text" id="PRNNumber" name="PRNNumberName_'+degreeType+'" class="input-medium"/> (As per University Marksheet)&nbsp;<span id="prnError" style="display:none;color:#f00;">Please, enter the PRN number.</span></td> '
						+ '			</tr> ';
				}
		}
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="MEGHE") {
					boiler	+= '<td><b>Registration Number :</b></td> '+ 
							   '<td><input type="text" id="registrationNumber" name="registrationNumber" class="input-medium"/></td> '+ 
							   '</tr> ';
				}
				
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="XLRI"){
					if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="HYGIA"){
	boiler	+='			<tr> '
		+ '				<td><b>No of Backlogs(Live) :</b> <b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><input type="text" name="noOfLiveATKTName_'+degreeType+'" id="atktLive" style="text-align: right" class="input-medium validnum" value="0"/> (Specify "0" if no ATKT) &nbsp;<span id="lktError" style="display:none;color:#f00;">Please, enter the live Backlogs.</span>'
		+ '				</td> '
		+ '			</tr> '
		+ '			<tr> '
		+ '				<td><b>No of Backlogs(Throughout(dead)) :</b> <b style="color:red;font-size:16px;" >*</b></td> '
		+ '				<td><input type="text" name="atktDeadName_'+degreeType+'" id="atktDead" style="text-align: right" class="input-medium validnum" value="0"/> (Specify "0" if no ATKT)&nbsp;<span id="dktError" style="display:none;color:#f00;">Please, enter the dead Backlogs.</span></td> '
		+ '			</tr> ';
					}
					else if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="ABS") {
