//----------------------------------------------------------------Print Profile

function getStudentProfilePrintCheck() {
	var cnt = 0;
	var isChecked = new Array();
	$('.top5:checked').each(function() {
		isChecked[cnt] = (this.checked ? $(this).attr("id") : "");
		cnt = cnt + 1;
	});
	window.location.replace("studentPrintResume.json?ID=" + isChecked);

}

function downloadUndertakingForm() {

	window.open("http://www.aicte-india.org/downloads/Undertaking.pdf",
	'_blank');

}

function genratePDFForReRegistration() {
	var isPhysicallyHandicapped = $('#saveBtn').attr('isPhysicallyHandicapped');
	var isAadharCard = $('#saveBtn').attr('isAadharCard');
	
	var boiler =''+
	'<table class="table table-bordered table-condensed" style="">'+
	'<tr>'+
	'<td><label>Do you have Aadhaar card?</label></td>'+
	'<td><input type="radio" name="isAadhaar1" value="Yes"/>Yes'+
	'<input type="radio" name="isAadhaar1" value="No" />No</td>'+
	'</tr>'+	
	'<tr>'+
	'<td><label>Are you Physically Challenged(Handicapped)?</label></td>'+
	'<td><input type="radio" name="isHandicapped" value="Yes"/> Yes '+
	'<input type="radio" name="isHandicapped" value="No"/>No </td>'+
	'</tr></table>';

	$('#printStudentProfileModalDiv').html(boiler);
	$("input[name=isHandicapped][value=" + isPhysicallyHandicapped + "]").attr('checked', 'checked');
	$("input[name=isAadhaar1][value=" + isAadharCard + "]").attr('checked', 'checked');

	$('#printStudentProfileModal').modal('show');
}
function saveStudentChoices(){
	var isHandicapped = $('input[name = "isHandicapped"]:checked').val();
	var isAadhaar = $('input[name = "isAadhaar1"]:checked').val();
		$.ajax({
			url : 'setStudentChoices.json',
			data : {
				isHandicapped : isHandicapped,
				isAadhaar:isAadhaar
			},
			dataType : 'text',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(text) {
				$('#printStudentProfileModal').modal('hide');
				if(text=="Yes"){
					window.location.replace("genratePDFForReRegistration.json?flag=studentProfile&courseYear=1");
				}else{
					alert(text);
				}
				
			},
			error : function(␀␀␀) {
			}
		});
}
function getFresherStudentProfilePrintCheck() {
	var cnt = 0;
	var isChecked = new Array();
	$('.top5:checked').each(function() {
		isChecked[cnt] = (this.checked ? $(this).attr("id") : "");
		cnt = cnt + 1;
	});
	var studId = $('#hiddenStudId').val();
	var academicYear = $('#cbAcademicBatch').find('option:selected').text();
	window.location.replace("fresherStudentPrintResume.json?ID=" + isChecked
			+ "&studId=" + studId + "&academicYear=" + academicYear);
}
//----------------------------------------------------------------------------------------------------------Ajax
//For Student Profile To get The Values In tab

//************************ function for retrieving students ssc info
//*************************

setTimeout(function() {
	$('#tenthboard').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {
					$("#tenthboard").autocomplete(
							sscBoardListForSerching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("
											+ term + ")", "gi"),
									"<strong>$1</strong>");
								},
								formatResult : function(row) {
									// uniIdFromPopUp=row.to;
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {

				}

			});
}, 1000);

setTimeout(function() {
	$('#twelthBoard').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {
					$("#twelthBoard").autocomplete(
							sscBoardListForSerching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("
											+ term + ")", "gi"),
									"<strong>$1</strong>");
								},
								formatResult : function(row) {
									// uniIdFromPopUp=row.to;
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {

				}

			});
}, 1000);

function getstudentsscinfo() {
	$.ajax({
		url : 'getStudent10thinfo.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if(jArray[0]["schoolAndCollegeId"]!=undefined){
				$('#sub10th').data("schoolAndCollegeId",jArray[0]["schoolAndCollegeId"]);
				getDegreeCategoryId(gTabDegreeName.TabName_SSC);
			}
			getState();
			if (jArray[0]["data"] == "Yes" && jArray[0]["1stTimeAddStatus"] == "inPlaceLast") {

				$('#sub10th').data("editingStatus",jArray[0]["editingStatus"]);
				$('#sub10th').data("schoolAndCollegeId",jArray[0]["schoolAndCollegeId"]);
				studProChangeReqId = jArray[0]["studProReqId"];

				$("#txt10Mathematicsmarks").val(jArray[0]["tenthMathematicsmarks"]);
				$("#txt10Mathematicsoutof").val(jArray[0]["tenthMathematicsoutof"]);
				$("#txt10Sciencemarks").val(jArray[0]["tenthSciencemarks"]);
				$("#txt10Scienceoutof").val(jArray[0]["tenthScienceoutof"]);
				
				$('#yrOfPassingId').val(jArray[0]["tenthYearOfPassing"]);
				getBoardNameList();
				//$("#tenthboard option:contains(" + jArray[0]["tenthBoard"] + ")").attr('selected', 'selected');
				if((jArray[0]["tenthBoard"]!=undefined) && (jArray[0]["tenthBoard"]!=null) &&  (jArray[0]["tenthBoard"]!=""))
				$('#tenthboard option:contains("'+jArray[0]["tenthBoard"]+'")').prop('selected', true);
				if((jArray[0]["tenthBoard"]!=undefined) && (jArray[0]["tenthBoard"]!=null) &&  (jArray[0]["tenthBoard"]!="")) {
					if($('#tenthboard option:contains("'+jArray[0]["tenthBoard"]+'")')){
						$('#tenthboard option:contains("'+jArray[0]["tenthBoard"]+'")').prop('selected', true);
						var boards =jArray[0]["tenthBoard"].toLowerCase();
						if(boards.trim()!=""){
							var tenthboardsname=	boards.split(' ').map(w => (w[0]!=undefined ? w[0].toUpperCase():"") + (w!=undefined ? w.substr(1).toLowerCase():"")).join(' ');
							$('#tenthboard option:contains("'+tenthboardsname+'")').prop('selected', true);
						}
					}else{
						var boards =jArray[0]["tenthBoard"];
						if(boards.trim()!=""){
							var tenthboardsname=	boards.split(' ').map(w => (w[0]!=undefined ? w[0].toUpperCase():"") + (w!=undefined ? w.substr(1).toLowerCase():"")).join(' ');
							$('#tenthboard option:contains("'+tenthboardsname+'")').prop('selected', true);
						}
					}
				}
				
				$("#tenthschool").val(jArray[0]["tenthSchoolName"]);
				$("#tenthplace").val(jArray[0]["tenth_Place"]);
				$("#tenthremark").val(jArray[0]["tenthRemarks"]);
				$("#txt10marks").val(jArray[0]["tenthMarks"]);
				$("#txt10outof").val(jArray[0]["tenthOutOf"]);
				$('#tenthSeatNo').val(jArray[0]["tenthSeatNo"]);
				$('#rollNoId').val(jArray[0]["rollNoId"]);
				
				if($('#customerHelper').val() == "MEGHE"){
					$('#txtstateOfSchool').val(jArray[0]["getTenthStateofSchoolId"]);
					$('#txtNoOfAttempt10th').val(jArray[0]["tenthNoofAttempt"]);
				}
				
				$("#schoolTypeTenth option:contains(" + jArray[0]["tenthSchoolType"] + ")").attr('selected', 'selected');
				
				if($('#customerHelper').val() =='MIT'){
					$('#txt10TcNumber').val(jArray[0]["tenthTcNumber"]);
					$('#txt10TcDate').val(jArray[0]["tenthTCDate"]);
				}
							
				if (jArray[0]["tenthPercentGradeFlag"] == "percent")
					$("#txt10percent").val(jArray[0]["tenthPercentOrGrade"]);
				$('#optpercent10').attr('checked', true);
				$('#txt10marks').attr('disabled', false);
				$('#txt10outof').attr('disabled', false);
				$('#txt10percent').attr('disabled', false);
				$('#txt10grade').attr('disabled', true);
				$('#txtgradeconversionmethod10').attr('disabled', true);
				if (jArray[0]["tenthPercentGradeFlag"] == "grade") {
					$('#optgrade10').attr('checked', true);
					$("#txt10grade").val(jArray[0]["tenthPercentOrGrade"]);
					$("#txtgradeconversionmethod10").val(jArray[0]["tenthGradeConversionMethod"]);

					$('#txt10marks').attr('disabled', true);
					$('#txt10outof').attr('disabled', true);
					$('#txt10percent').attr('disabled', true);
					$('#txt10grade').attr('disabled', false);
					$('#txtgradeconversionmethod10').attr('disabled',
							false);
				}

				getDegreeCategoryId(gTabDegreeName.TabName_SSC);
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1st") {
				getBoardNameList();
				$('#sub10th').data("editingStatus", "ReqNotSent");
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1stReqSent") {
				getBoardNameList();
				$('#sub10th').data("editingStatus",
						jArray[0]["editingStatus"]);
				studProChangeReqId = jArray[0]["studProReqId"];
			}else{
				getBoardNameList();
			}

			if (jArray[0]["editingStatus"] == 'ReqNotSent') {
				$('#btnFreeze10th').hide();

				$('#trSendReq10th').hide();
				$('#trReqAccepted10th').hide();
				$('#trReqPending10th').hide();
				$('#certificateAttachmentSSC').hide();
				$('#documentUploadInformation').hide();
				// $('#lblUGPGMsg').text('Send Request for Editing');
			} else if (jArray[0]["editingStatus"] == 'ReqAccepted') {
				$('#trReqAccepted10th').show();
				$('#trSendReq10th').hide();
				$('#trReqPending10th').hide();
				$('#certificateAttachmentSSC').show();
				$('#documentUploadInformation').show();
				// $('#lblUGPGMsg').text('Request has been accepted. You
				// can edit now');
			} else if (jArray[0]["editingStatus"] == 'ReqSent') {
				$('#trReqPending10th').show();
				$('#trReqAccepted10th').hide();
				$('#trSendReq10th').hide();
				$('#certificateAttachmentSSC').hide();
				$('#documentUploadInformation').hide();

				// $('#lblUGPGMsg').text('Request is pending');
			}
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			alert("In Student SSc error");
		}

	});
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		console.log("jhih1");
			if(g_freeze!=undefined && g_freeze)
				{
				disabledAll();
				}
			}
}

//--------------------------------------------------------Hsc Retrive details
function getstudenthscinfo(gStudentId) {
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getStudent12thinfo.json?id='+gStudentId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getStudent12thinfo.json';
	}
	$.ajax({
		url : url,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if(jArray[0]["schoolAndCollegeId"]!=undefined){
				$('#sub12th').data("schoolAndCollegeId",jArray[0]["schoolAndCollegeId"]);
				getDegreeCategoryId(gTabDegreeName.TabName_HSC);
			}
			getState();
			if (jArray[0]["data"] == "Yes" && jArray[0]["1stTimeAddStatus"] == "inPlaceLast") {

				$('#sub12th').data("editingStatus",jArray[0]["editingStatus"]);
				$('#sub12th').data("schoolAndCollegeId",jArray[0]["schoolAndCollegeId"]);
				studProChangeReqId = jArray[0]["studProReqId"];

				$('#twelthYrOfPassing').val(jArray[0]["twelthYearOfPassing"]);
				getBoardNameList();
				// $("#twelthBoard option:contains(" + jArray[0]["twelthBoard"] + ")").attr('selected', 'selected');
				if((jArray[0]["twelthBoard"]!=undefined) && (jArray[0]["twelthBoard"]!=null) &&  (jArray[0]["twelthBoard"]!=""))
				$('#twelthBoard option:contains("'+jArray[0]["twelthBoard"]+'")').prop('selected', true); 
				if((jArray[0]["twelthBoard"]!=undefined) && (jArray[0]["twelthBoard"]!=null) && (jArray[0]["twelthBoard"]!="")){
				if($('#twelthBoard option:contains("'+jArray[0]["twelthBoard"]+'")')){
					$('#twelthBoard option:contains("'+jArray[0]["twelthBoard"]+'")').prop('selected', true);
					var boards =jArray[0]["twelthBoard"].toLowerCase();
					var boardsname=	boards !="" ? boards.split(' ').map(w => (w[0]!=undefined ? w[0].toUpperCase():"") + (w!=undefined ? w.substr(1).toLowerCase():"")).join(' '):"";
					$('#twelthBoard option:contains("'+boardsname+'")').prop('selected', true);
				}else{
					var boards =jArray[0]["twelthBoard"];
					var boardsname= boards !="" ? boards.split(' ').map(w => (w[0]!=undefined ? w[0].toUpperCase():"") + (w!== undefined ? w.substr(1).toLowerCase() : "")).join(' ') : "";
					$('#twelthBoard option:contains("'+boardsname+'")').prop('selected', true);
				}
			}
				$("#studtwelthSchool").val(jArray[0]["twelthSchoolName"]);
				$("#studTwelthPlace").val(jArray[0]["twelth_Place"]);
				$("#studtwelthRemarks").val(jArray[0]["twelthRemarks"]);
				$("#txt12marks").val(jArray[0]["twelthMarks"]);
				$("#txt12outof").val(jArray[0]["twelthOutOf"]);
				
                var streamValue = jArray[0]["twelthStream"].toLowerCase();
                  $("#studStreamCombo option").each(function() {
                 if ($(this).val().toLowerCase() === streamValue || $(this).text().toLowerCase() === streamValue) {
                 $(this).prop('selected', true); 
                          }
                  });
                 $('#studStreamCombo').change();	
                 $('#twelthSeatNo').val(jArray[0]["twelthSeatNo"]);
				
				if($('#customerHelper').val() == "MEGHE"){
					$('#txtstateOfSchool12').val(jArray[0]["twelthSchoolState"]);
					$('#txtNoOfAttempt12th').val(jArray[0]["twelthNoofAttempt"]);
				}
				
				if (jArray[0]["twelthStream"] == "Science") {
					$('#trPCM').show();
					
						$('#txt12Physicsmarks').val(jArray[0]["twelthPhysicsmarks"]);
						$('#txt12Physicsoutof').val(jArray[0]["twelthPhysicsOutOf"]);

						$('#txt12Chemistrymarks').val(jArray[0]["twelthChemistrymarks"]);
						$('#txt12Chemistryoutof').val(jArray[0]["twelthChemistryOutOf"]);

						$('#txt12Mathematicsmarks').val(jArray[0]["twelthMathsmarks"]);
						$('#txt12Mathematicsoutof').val(jArray[0]["twelthMathsOutOf"]);

						$('#txt12Biologymarks').val(jArray[0]["twelthBiomarks"]);
						$('#txt12Biologyoutof').val(jArray[0]["twelthBioOutOf"]);

						$('#txt12PCMPercentage').val(jArray[0]["PCMPercentage"]);
						$('#txt12PCBPercentage').val(jArray[0]["PCBPercentage"]);
				}

				if (jArray[0]["twelthPercentGradeFlag"] != undefined) {
					if (jArray[0]["twelthPercentGradeFlag"] == "percent") {
						$("#txt12percent").val(jArray[0]["twelthPercentOrGrade"]);
					}
				}
				$('#optpercent12').attr('checked', true);
				$('#txt12marks').attr('disabled', false);
				$('#txt12outof').attr('disabled', false);
				$('#txt12percent').attr('disabled', false);
				$('#txt12grade').attr('disabled', true);
				$('#txtgradeconversionmethod12').attr('disabled', true);

				if (jArray[0]["twelthPercentGradeFlag"] == "grade") {
					$('#optgrade12').attr('checked', true);
					$("#txt12grade").val(
							jArray[0]["twelthPercentOrGrade"]);
					$("#txtgradeconversionmethod12").val(jArray[0]["twelthGradeConversionMethod"]);

					$('#txt12marks').attr('disabled', true);
					$('#txt12outof').attr('disabled', true);
					$('#txt12percent').attr('disabled', true);
					$('#txt12grade').attr('disabled', false);
					$('#txtgradeconversionmethod12').attr('disabled',false);
				}

				getDegreeCategoryId(gTabDegreeName.TabName_HSC);
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1st") {
				// $('#sub12th').data("editingStatus",jArray[0]["editingStatus"]);
				getBoardNameList();
				$('#sub12th').data("editingStatus", "ReqNotSent");
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1stReqSent") {
				getBoardNameList();
				$('#sub12th').data("editingStatus",
						jArray[0]["editingStatus"]);
				studProChangeReqId = jArray[0]["studProReqId"];
			}else{
				getBoardNameList();
			}

			if (jArray[0]["editingStatus"] == 'ReqNotSent') {
				$('#btnFreeze12th').hide();

				$('#trSendReq12th').hide();
				$('#trReqAccepted12th').hide();
				$('#trReqPending12th').hide();
				$('#certificateAttachmentHSC').hide();	
				$('#documentUploadInformationHSC').hide();	
			} else if (jArray[0]["editingStatus"] == 'ReqAccepted') {
				$('#trReqAccepted12th').show();
				$('#trSendReq12th').hide();
				$('#trReqPending12th').hide();
				$('#certificateAttachmentHSC').show();
				$('#documentUploadInformationHSC').show();
				// can edit now');
			} else if (jArray[0]["editingStatus"] == 'ReqSent') {
				$('#trReqPending12th').show();
				$('#trReqAccepted12th').hide();
				$('#trSendReq12th').hide();
				$('#certificateAttachmentHSC').hide();
				$('#documentUploadInformationHSC').hide();
			}
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="JGI"){
							
							if(jArray[0]["twelthSecondaryLanguage"]=='OTHER')
							{
										$('#twelthSecondaryLanguageNameTR').show();
										$("#applstud12thSecondaryLanguage").val(jArray[0]["twelthSecondaryLanguage"]);
										$("#txtTwelthSecondaryLanguageName").val(jArray[0]["twelthSecondaryLanguageOther"]);
							}
							else
							{
										$('#twelthSecondaryLanguageNameTR').hide();
										$("#applstud12thSecondaryLanguage").val(jArray[0]["twelthSecondaryLanguage"]);
							}
						}
			if($('#customerHelper').val() =='MIT'){
					$('#txt12TcNumber').val(jArray[0]["twelthTcNumber"]);
					$('#txt12TcDate').val(jArray[0]["twelthTCDate"]);
				}
			getstudProfileConfigurationDetailsCheckingNames();
		},
		
		error : function() {
			alert("In Student HSc error");
		}

	});
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		console.log("jhih1");
			if(g_freeze!=undefined && g_freeze)
				{
				disabledAll();
				}
			}
}

//--------------------------------------------------------Hsc Retrive details
var studentProfileImage = false ; //config student profile image
var studentProfileSignature = false; // confing student signature 
var g_freeze=false;
function getstudentPersonalinfo(studentId) {
	getCountry();
	getAllVoterConstituencys();
	if (studentId == undefined) {
		studentId = 0;
	}
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getStudentPersonalinfo.json?id='+studentId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getStudentPersonalinfo.json';
	}
	$.ajax({
		url : url,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var boiler = '';
			
			if(jArray[0]["currentSem"]!=undefined)
				boiler = '<br/><input type="button" value="Marks Entry for '+jArray[0]["currentSem"]+'" class="btn btn-info btn-mini btnColorBlack "/>';
			if(jArray[0]["nextSem"]!=undefined)
				boiler += '&nbsp;&nbsp;<input type="button" value="Subject Choice for '+jArray[0]["nextSem"]+'" class="btn btn-info btn-mini nextSemSubjectChoice  btnColorBlack "/>';
			boiler += '<input type="hidden" id="isFreezedFlag" value="'+jArray[0]["isProfileFreezed"]+'">';
			$('#divReregisterBtn').html(boiler);

			$('.nextSemSubjectChoice').click(function(){
				editStudentSubjectChoice();
			});
			
			var isForiegner = jArray[0]["isForiegner"];
			if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="INDIRA_PUNE" || $('#customerHelper').val()=="REVA_UNIVERSITY"))
			{
				$('a[href="#passportAndVisaDetailsDetails"]').show();
			}
			else{
				if((jArray[0].isForiegner !="true" && jArray[0].isForiegner !=true && $('#customerHelper').val() == "MEGHE") || (jArray[0].isForiegner && $('#customerHelper').val() != "MEGHE")){
					$('a[href="#passportAndVisaDetailsDetails"]').show();
				}else{
					$('a[href="#passportAndVisaDetailsDetails"]').hide();
				}
			}
			
			if(jArray[0].hideStudentProfileImage!=undefined && jArray[0].hideStudentProfileImage =="true" || jArray[0].hideStudentProfileImage ==true){
				$('#studentProfileImageUpload').remove();
			}
			
			$('#linkdinUrlId').val(jArray[0]["linkdinUrl"]);
			$('#skypeIdDataTxt').val(jArray[0]["skypeId"]);
			$('#ugStudMarksheetnameId').val(jArray[0]["nameAsPerUgMArksheet"]);
			$('#firstNameHindi').val(decodeURIComponent(jArray[0]["studentNameHindi"]));//fatherNameHindi
			$('#fatherNameHindi').val(decodeURIComponent(jArray[0]["studentFatherNameHindi"]));
			$('#motherNameHindi').val(decodeURIComponent(jArray[0]["studentMotherNameHindi"]));
			$('#studSubCategory').val(jArray[0]["subCategory"]);
			$('#studAdmissionType').val(jArray[0]["programType"]);
			$('#studentPanNo').val(jArray[0]["studentPanNo"]);
			$('#voterIdNo').val(jArray[0]["voterId"]);
			$('#voterConstituency').val(jArray[0]["voterConstituencyId"]);
			$('#nameAsPerAdhar').val(jArray[0]["nameAsPerAdhar"]);
			$('#txtAntiRaggingReferenceID').val(jArray[0]["antiRaggingReferenceID"]);
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RCOEM"){		
				$("#stuFatherMobno").val(jArray[0]["fathermobileNo"]);
			}
			if(jArray[0]["profileCompletion"]!=0) {
				$('#profileCompletionId').html('<label><b>Profile Completion('+jArray[0]["profileCompletion"]+'%)</b></label>')
			}
			
			if (jArray[0]["physicallyHandicappedYes"] == "Yes"){
				
				$("#physicallyHandicappedDetailsTR1").show();
				$("#phYes").attr("checked", true);
				$("#physicallyHandicappedDetailsTR1").show();
				
				if(jArray[0]["blind"]=="blind"){
					$("#chkBlind").attr("checked",true);
					}
				if(jArray[0]["hearingDisability"]=="hearingDisability"){
					$("#chkHearingDisability").attr("checked",true);
					}
				if(jArray[0]["orthoImpaired"]=="orthoImpaired"){
					$("#chkOrthoImpaired").attr("checked",true);
					}
				if(jArray[0]["physicallyHandicapped"]=="physicallyHandicapped"){
					$("#chkPhysicallyHandicapped").attr("checked",true);
					}
				if(jArray[0]["mentalDisability"]=="mentalDisability"){
					$("#chkMentalDisability").attr("checked",true);
					}
				if(jArray[0]["txtPhysicallyHandicappedOther"]!=""){
					$('#txtPhysicallyHandicappedOther').val(jArray["txtPhysicallyHandicappedOther"]);
					}
			}
		
			
			if(jArray[0]["is_icard_confirmed"]=="false")
			{
				if(jArray[0]["isForiegner"] == 'true' || jArray[0]["isForiegner"]== true){
						//alert("Nri");
						$('.isMandatory').css('display', 'none');
				}
			
				$('#txtfirstName').val(jArray[0]["firstName"]);
				/*if(jArray[0]["firstName"]!=''){
					$('#txtfirstName').val(jArray[0]["firstName"]);
					$('#txtfirstName').attr('disabled',true);
				}else{
					$('#txtfirstName').val(jArray[0]["firstName"]);
				}*/
				$('#txtmiddleName').val(jArray[0]["middleName"]);
				$('#txtLastName').val(jArray[0]["lastName"]);
				//	admissionQuotaStatus
				//admissionTypeId
				//strengthId
				
				/*alert("admissionQuotaStatus"+jArray[0]["admissionQuotaStatus"]);
				alert("admissionTypeId"+jArray[0]["admissionTypeId"]);
				alert("strengthId"+jArray[0]["strengthId"]);*/
				if(( jArray[0]["admissionQuotaStatus"]==true ||  jArray[0]["admissionQuotaStatus"]=="true" ) && jArray[0]["admissionTypeId"]=="0"  ){
				getAdmissionQuotaType(jArray[0]["strengthId"],jArray[0]["admissionTypeId"],jArray[0]["admissionQuotaStatus"]);
				$('#admissionQuotaAndCategoryTypeTR').show();
				$('#msgAdmissionQuotaAndCategoryTypeTR').show();
				$('#admissionQuotaAndCategoryTypeTR').attr('label',jArray[0]["strengthId"]);
				
				}
				else{
					$('#admissionQuotaAndCategoryTypeTR').hide();
					$('#admissionQuotaAndCategoryTypeTR').attr('label',0);
					$('#msgAdmissionQuotaAndCategoryTypeTR').hide();
				}
				//
				
				$('#saveBtn').attr('instituteCourseStrengthId',jArray[0]["strengthId"]);
				$('#saveBtn').attr('isAdmissionQuota',jArray[0]["admissionQuotaStatus"]);
				$('#saveBtn').attr('isSponseredApplicant',jArray[0]["isSponseredApplicant"]);
				$("#applstudmotherName").val(jArray[0]["motherName"]);
				if($('#customerHelper').val()!=undefined && ($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY')){	
					if(jArray[0]["middleName"]!=""){
				    	$("#applstudfatherName").val(jArray[0]["middleName"]);
					}else{
						$("#applstudfatherName").val(jArray[0]["fatherName"]);
					}
				}else{
					$("#applstudfatherName").val(jArray[0]["fatherName"]);
				}
				
				$("#appl10thMarksheetname").val(jArray[0]["tenthName"]);
				/*if(jArray[0]["tenthName"]!=''){
					$("#appl10thMarksheetname").val(jArray[0]["tenthName"]);
					$('#appl10thMarksheetname').attr('disabled',true);
				}else{
					$("#appl10thMarksheetname").val(jArray[0]["tenthName"]);					
				}*/

				if (jArray[0]["dob"] != undefined) {
					var dob = new Date(jArray[0]["dob"]);
					$('#txtdob').datepicker({
						dateFormat : 'M d,yy',
					});
					$("#txtdob").datepicker('setDate', dob);
				}

				$("#txtmobileNo").val(jArray[0]["mobileNo"]);
				$("#txtstdcode").val(jArray[0]["std"]);
				$("#txtAltNumber").val((jArray[0]["alternetContactNumber"]!=undefined && jArray[0]["alternetContactNumber"]!='undefined'?jArray[0]["alternetContactNumber"]:""));
				$("#txtContactHours").val(jArray[0]["contactHours"]);
				$("#txtAlternetEmailID").val(jArray[0]["alternetEmailID"]);
				$("#txtplaceofBirth").val(jArray[0]["placeOfBirth"]);
				$("#txtEnrollmentNo").val(jArray[0]["enrollmentNo"]);
				$("#txteligibilityNo").val(jArray[0]["eligibilityNo"]);
				$("#txtRollNo").html(jArray[0]["rollNo"]);
				$("#txtPCNo").val(jArray[0]["studPCNumber"]);
				$("#txtVehicleNo").val(jArray[0]["vehicleNo"]);
				$('#txtPersonalEmailID').val(jArray[0]["personalEmailId"]);
				
				var placeOfState = jArray[0]["placeOfBirth"].split(",")[1];
				var optionToSelect = $("#txtstudBirthState option:contains('" + placeOfState + "')");
				optionToSelect.prop("selected", true);
			    $("#txtstudBirthState").change();
				
				
				if($('#customerHelper').val()=="RCOEM" || $('#customerHelper').val()=="MGMU" || $('#customerHelper').val()=="BITS_MESRA" || $('#customerHelper').val()=="MEGHE" || $('#customerHelper').val()=="MATSUNIVERSITY" || $('#customerHelper').val()=="DYPATIL" || $('#customerHelper').val()=="GOKHALE_INSTITUTE" || $('#customerHelper').val()=="BLM_MUNJAL"){
					$('#txtAbcAccountId').val(jArray[0]["abcAccountId"]);
				}else if($('#customerHelper').val()=="RAMAIAH_UNIVERSITY"){
					
					$('#txtAbcAccountId').val(jArray[0]["abcAccountId"]);
					if(jArray[0]["abcAccountId"]!=""){
						$('#txtAbcAccountId').attr('disabled',true);
						$('#txtAbcAccountId').attr('readonly',true);
					}
					
				}
				if($('#customerHelper').val() == "MEGHE"){
					$('#txtOrchidId').val(jArray[0]["orchidId"]);
					$('#txtAyushId').val(jArray[0]["ayushId"]);
				}
				if($('#customerHelper').val() =='MGMU' || $('#customerHelper').val() =='NITIE' || $('#customerHelper').val() =='CMR_UNIVERSITY' || $('#customerHelper').val() =='NIET'){
				$('#txtfirstNameMarathi').val(jArray[0]["displyFirstName"]!=""?decodeURIComponent(jArray[0]["displyFirstName"]):"");
				$('#txtmiddleNameMarathi').val(jArray[0]["displyMiddleName"]!=""?decodeURIComponent(jArray[0]["displyMiddleName"]):"");
				$('#txtLastNameMarathi').val(jArray[0]["displyLastName"]!=""?decodeURIComponent(jArray[0]["displyLastName"]):"");
				
				/*$('#txtfirstNameMarathi').attr('disabled',true);
				$('#txtmiddleNameMarathi').attr('disabled',true);
				$('#txtLastNameMarathi').attr('disabled',true); */
                }



				
				
				
				if($('#customerHelper').val() =='ATAL_MEDICAL')
				    $('#txtAlternetEmailID').attr('readonly',false);
				
				if(jArray[0]["AicteStudentId"]!='0'){
					$('#AICTEStudentUniqueID').val(jArray[0]["AicteStudentId"]);
				}
				
				if((jArray[0]["profileStatus"]==true||jArray[0]["profileStatus"]=="true")||($('#finalSubmissionStatus').val() !=undefined && (jArray[0]["isProfileFreezed"]==true||jArray[0]["isProfileFreezed"]=="true") && $('#customerHelper').val()=="MANDSAUR")){
					$('#frozenDivSmsId').html('<b style="color:red">Your profile is freeze. If you want to edit profile please contact to admin</b>');
					$('#savePersonalDetailsBtnDiv').hide();
					$('#finalSubmissionStatus').val(true);
					hideSaveSubmitButton();
				}else{
					$('#frozenDivSmsId').html('');
					$('#savePersonalDetailsBtnDiv').show();
					$('#finalSubmissionStatus').val(false);
				}
				
				$("#txtRegistrationNo").val(jArray[0]["registrationNo"]);
				
				$("#txtAdmissionDate").html(jArray[0]["admissionDAte"]);
				$('#txtstudreligion').val(jArray[0]["religionId"]);
				/*if (jArray[0]["religionId"] != undefined && jArray[0]["religionId"]!="0"){
					$('#txtstudreligion').val(jArray[0]["religionId"]);
					$('#txtstudreligion').attr('disabled',true);
				}else{
					$('#txtstudreligion').val(jArray[0]["religionId"]);
					$('#txtstudreligion').attr('disabled',false);
				}*/
				
				$('#txtstudnationality').val(jArray[0]["nationalityId"]);
				/*if (jArray[0]["nationalityId"] != undefined && jArray[0]["nationalityId"] != "0"){
					$('#txtstudnationality').val(jArray[0]["nationalityId"]);
					$('#txtstudnationality').attr('disabled',true);
				}else{
					$('#txtstudnationality').val(jArray[0]["nationalityId"]);
					$('#txtstudnationality').attr('disabled',false);
				}*/
				
				$('#txtstudcategory').val(jArray[0]["categoryId"]);
				/*if (jArray[0]["categoryId"] != undefined && jArray[0]["categoryId"]!="0"){
					$('#txtstudcategory').val(jArray[0]["categoryId"]);
					$('#txtstudcategory').attr('disabled',true);
				}else{
					$('#txtstudcategory').val(jArray[0]["categoryId"]);
					$('#txtstudcategory').attr('disabled',false);
				}*/

				$('#txtstuddomacile').val(jArray[0]["stateId"]);
			/*	if (jArray[0]["stateId"] != undefined && jArray[0]["stateId"]!="0"){
					$('#txtstuddomacile').val(jArray[0]["stateId"]);
					$('#txtstuddomacile').attr('disabled',true);
				}else{
					$('#txtstuddomacile').val(jArray[0]["stateId"]);
					$('#txtstuddomacile').attr('disabled',false);
				}*/

				$('#studbloodgrp').val(jArray[0]["bloodGroup"]);
				$('#studHostelite').val(jArray[0]["hostelite"]);
				$('#txtReg').val(jArray[0]["uniRegiNo"]);
				
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RAMAIAH_UNIVERSITY") {
				$('#txtReg').attr('readonly',true);
				}
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="ABS") {
					$('#txtPRN').val(jArray[0]["uniAicteNo"]);
				}else{
					$('#txtPRN').val(jArray[0]["uniPRNNo"]);
				}
				$("#txtHobbies").val(jArray[0]["Hobbies"]);
				$("#studMotherTongue option[value='" +jArray[0]["motherTongue"]+"']").attr('selected', true);
				$("#txtOtherLanguagesKnown").val(jArray[0]["otherKnownLanguages"]);  
				$("#txtLicence").val(jArray[0]["licenceNo"]);
				getCaste();
				
				$("#txtCaste").val(jArray[0]["Caste"]);
				/*if(jArray[0]["Caste"]!= undefined && jArray[0]["Caste"]!='0'){
					$("#txtCaste").val(jArray[0]["Caste"]);
					$("#txtCaste").attr('disabled',true);
				}else{
					$("#txtCaste").val(jArray[0]["Caste"]);
					$("#txtCaste").attr('disabled',false);
				}*/
				$("#NADIdVal").val(jArray[0]["NADId"]);
				$("#utrNoId").val(jArray[0]["utrNo"]);
				$('#txtCaste').trigger("liszt:updated");
				$("#txtStudentSubCaste").val(jArray[0]["subCast"]);
				$("#txtAdharCardNo").val(jArray[0]["adharcardNo"]);
				if($("#txtAdharCardNo").val()!=""){
					$('#saveBtn').attr('isAadharCard',"Yes");
				}else{
					$('#saveBtn').attr('isAadharCard',"No");
				}
				if(jArray[0]["isMinority"]=="true"){
					$('#MinorityYes').attr('checked',true);
				}else{
					$('#MinorityNo').attr('checked',true);
				}
				if(jArray[0]["isFirstGeneration"]=="true"){
					$('#GenerationYes').attr('checked',true);
				}else{
					$('#GenerationNo').attr('checked',true);
				}
				$("#txtStudentSubCaste").val(jArray[0]["subCast"]);
				if(jArray[0]["isForiegner"]=="true")
					$('#yesForiegner').attr('checked',true);
				else if(jArray[0]["isForiegner"]=="false")
					$('#noForiegner').attr('checked',true);
				
				$("#txtStudentSubCaste").val(jArray[0]["subCast"]);
				
				var physicallyHandicappedValue = jArray[0]["physicallyHandicapped"];
				$("input[name=physicallyHandicapped][value='"+physicallyHandicappedValue+"']").attr('checked', 'checked');
				$('#saveBtn').attr('isPhysicallyHandicapped',physicallyHandicappedValue);
				
				var ArmyOrExservicemanValue = jArray[0]["armyOrExserviceman"];
				$("input[name=ArmyOrExserviceman][value='"+ArmyOrExservicemanValue+"']").attr('checked', 'checked');
				
				var stateValue = jArray[0]["sports"];
				$("input[name=radioSports][value='"+stateValue+"']").attr('checked', 'checked');
				
				if (jArray[0]["Gender"] == "Male"){
					$("#male").attr("checked", true);
				//	$("#male").attr("disabled", true);
				//	$("#female").attr("disabled", true);
				}else if(jArray[0]["Gender"] =="Female"){
					$("#female").attr("checked", true);
				//	$("#female").attr("disabled", true);
				//	$("#male").attr("disabled", true);
				}else if(jArray[0]["Gender"] == "Other"){
					  $("#other").attr("checked", true);
				}else{  
					//$("#female").attr("disabled", false);
					//$("#male").attr("disabled", false);
				}
				
				if (jArray[0]["musmino"] == true || jArray[0]["musmino"] == 'true')
					$("#musminoYes").attr("checked", true);
				else if (jArray[0]["musmino"] == false || jArray[0]["musmino"] == 'false')
					$("#musminoNo").attr("checked", true);
				else{
					$("#musminoYes").attr("checked", false);
					$("#musminoNo").attr("checked", false);
				}
				
				if (jArray[0]["othermino"] == true || jArray[0]["othermino"] == 'true')
					$("#othminoYes").attr("checked", true);
				else if (jArray[0]["othermino"] == false || jArray[0]["othermino"] == 'false')
					$("#othminoNo").attr("checked", true);
				else{
					$("#othminoYes").attr("checked", false);
					$("#othminoNo").attr("checked", false);
				}
				
				if (jArray[0]["AnRaggReg"] == true || jArray[0]["AnRaggReg"] == 'true')
					$("#AnRaggRegYes").attr("checked", true);
				else if (jArray[0]["AnRaggReg"] == false || jArray[0]["AnRaggReg"] == 'false')
					$("#AnRaggRegNo").attr("checked", true);
				else{
					$("#AnRaggRegYes").attr("checked", false);
					$("#AnRaggRegNo").attr("checked", false);
				}
				
				if (jArray[0]["CoOfConduct"] == true || jArray[0]["CoOfConduct"] == 'true')
					$("#CoOfConductYes").attr("checked", true);
				else if (jArray[0]["CoOfConduct"] == false || jArray[0]["CoOfConduct"] == 'false')
					$("#CoOfConductNo").attr("checked", true);
				else{
					$("#CoOfConductYes").attr("checked", false);
					$("#CoOfConductYes").attr("checked", false);
				}

				if (jArray[0]["marital_Status"] == "Single")
					$("#single").attr("checked", true);
				else
					$("#married").attr("checked", true);
					
					
					var marstatus= jArray[0]["studMaritalStatus"];
					if(marstatus!=""&&marstatus!=undefined){
					$('#studMaritalStatus option:contains("'+marstatus+'")').prop('selected', true);
					}else{
						$('#studMaritalStatus option:contains("Select")').prop('selected', true);
					}
				if(jArray[0]["studentMeritListPk"]!=0 ){
					$('#txtDteApplicationid').show();
					$('#labelDteApplicationid').show();
					$('#trDteApplicationid').show();
					$('#txtDteApplicationid').val(jArray[0]["applicationId"]);
				}else{
					$('#txtDteApplicationid').hide();
					$('#labelDteApplicationid').hide();
					$('#trDteApplicationid').hide();
				}
				$('#tdAdmissionTypeName').html(jArray[0]["admissionTypeName"]);
				
			}else{
				
				if(( jArray[0]["admissionQuotaStatus"]==true ||  jArray[0]["admissionQuotaStatus"]=="true" ) && jArray[0]["admissionTypeId"]=="0"  ){
					getAdmissionQuotaType(jArray[0]["strengthId"],jArray[0]["admissionTypeId"],jArray[0]["admissionQuotaStatus"]);
					$('#admissionQuotaAndCategoryTypeTR').show();
					$('#msgAdmissionQuotaAndCategoryTypeTR').show();
					
					$('#admissionQuotaAndCategoryTypeTR').attr('label',jArray[0]["strengthId"]);
					}
					else{
						$('#admissionQuotaAndCategoryTypeTR').hide();
						$('#admissionQuotaAndCategoryTypeTR').attr('label',0);
						$('#msgAdmissionQuotaAndCategoryTypeTR').hide();
					}
				
				$('#txtfirstName').val(jArray[0]["firstName"]);
				/*if(jArray[0]["firstName"]!=''){
					$('#txtfirstName').val(jArray[0]["firstName"]);
					$('#txtfirstName').attr('disabled',true);
				}else{
					$('#txtfirstName').val(jArray[0]["firstName"]);
					$('#txtfirstName').attr('disabled', false);
				}*/
				$('#txtmiddleName').val(jArray[0]["middleName"]);
				$('#txtmiddleName').attr('disabled',false);
				$('#txtLastName').val(jArray[0]["lastName"]);
				$('#txtLastName').attr('disabled',false);
				
				//
				$('#saveBtn').attr('instituteCourseStrengthId',jArray[0]["strengthId"]);
				$('#saveBtn').attr('isAdmissionQuota',jArray[0]["admissionQuotaStatus"]);
				$('#saveBtn').attr('isSponseredApplicant',jArray[0]["isSponseredApplicant"]);
				$("#applstudmotherName").val(jArray[0]["motherName"]);
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RCOEM")
			   {		
					$("#applstudfatherName").val(jArray[0]["fatherName"]);
			   }	
				
				$("#appl10thMarksheetname").val(jArray[0]["tenthName"]);
				/*if(jArray[0]["tenthName"]!=''){
					$("#appl10thMarksheetname").val(jArray[0]["tenthName"]);
					$('#appl10thMarksheetname').attr('disabled',true);
					}else{
						$("#appl10thMarksheetname").val(jArray[0]["tenthName"]);					
					}*/
				var dob = new Date(jArray[0]["dob"]);
				$('#txtdob').datepicker({
					dateFormat : 'M d,yy',
				});
				$("#txtdob").datepicker('setDate', dob);
				/*if (jArray[0]["dob"] != undefined && jArray[0]["dob"] !="") {
					var dob = new Date(jArray[0]["dob"]);
					$('#txtdob').datepicker({
						dateFormat : 'M d,yy',
					});
					$("#txtdob").datepicker('setDate', dob);
					$('#txtdob').attr('disabled',true);
				}else{
					var dob = new Date(jArray[0]["dob"]);
					$('#txtdob').attr('disabled',false);
				}*/
				$("#txtmobileNo").val(jArray[0]["mobileNo"]);
				$('#txtmobileNo').attr('disabled',false);
				$("#txtstdcode").val(jArray[0]["std"]);
				$('#txtstdcode').attr('disabled',true);
				$("#txtAltNumber").val((jArray[0]["alternetContactNumber"]!=undefined && jArray[0]["alternetContactNumber"]!='undefined'?jArray[0]["alternetContactNumber"]:""));
				$('#txtAltNumber').attr('disabled',true);
				$("#txtContactHours").val(jArray[0]["contactHours"]);
				$("#txtAlternetEmailID").val(jArray[0]["alternetEmailID"]);
				$("#txtEnrollmentNo").val(jArray[0]["enrollmentNo"]);
				//$('#txtAlternetEmailID').attr('disabled',true);
				$("#txtVehicleNo").val(jArray[0]["vehicleNo"]);
				$('#txtPersonalEmailID').val(jArray[0]["personalEmailId"]);
				
				if($('#customerHelper').val()=="RCOEM" || $('#customerHelper').val()=="MGMU" || $('#customerHelper').val()=="RAMAIAH_UNIVERSITY" || $('#customerHelper').val()=="BITS_MESRA")
					$('#txtAbcAccountId').val(jArray[0]["abcAccountId"]);
				
				$('#txtstudreligion').val(jArray[0]["religionId"]);
				/*if (jArray[0]["religionId"] != undefined && jArray[0]["religionId"]!="0"){
					$('#txtstudreligion').val(jArray[0]["religionId"]);
					$('#txtstudreligion').attr('disabled',true);
				}else{
					$('#txtstudreligion').val(jArray[0]["religionId"]);
					$('#txtstudreligion').attr('disabled',false);
				}*/

				$('#txtstudnationality').val(jArray[0]["nationalityId"]);
				/*if (jArray[0]["nationalityId"] != undefined && jArray[0]["nationalityId"] != "0"){
					$('#txtstudnationality').val(jArray[0]["nationalityId"]);
					$('#txtstudnationality').attr('disabled',true);
				}else{
					$('#txtstudnationality').val(jArray[0]["nationalityId"]);
					$('#txtstudnationality').attr('disabled',false);
				}*/
				
				$('#txtstudcategory').val(jArray[0]["categoryId"]);
				/*if (jArray[0]["categoryId"] != undefined && jArray[0]["categoryId"]!="0"){
					$('#txtstudcategory').val(jArray[0]["categoryId"]);
					$('#txtstudcategory').attr('disabled',true);
				}else{
					$('#txtstudcategory').val(jArray[0]["categoryId"]);
					$('#txtstudcategory').attr('disabled',false);
				}*/

				$('#txtstuddomacile').val(jArray[0]["stateId"]);
				/*if (jArray[0]["stateId"] != undefined && jArray[0]["stateId"]!="0"){
					$('#txtstuddomacile').val(jArray[0]["stateId"]);
					$('#txtstuddomacile').attr('disabled',true);
				}else{
					$('#txtstuddomacile').val(jArray[0]["stateId"]);
					$('#txtstuddomacile').attr('disabled',false);
				}*/


				$('#studbloodgrp').val(jArray[0]["bloodGroup"]);
				
				$('#studbloodgrp').attr('disabled',false);
				$('#studHostelite').val(jArray[0]["hostelite"]);
				$('#txtReg').val(jArray[0]["uniRegiNo"]);
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="ABS") { 
					$('#txtPRN').val(jArray[0]["uniAicteNo"]);  // condition as per requirement 
				
				}else{
					$('#txtPRN').val(jArray[0]["uniPRNNo"]);
				}
				$("#txtHobbies").val(jArray[0]["Hobbies"]);
				$("#studMotherTongue option[value='" +jArray[0]["motherTongue"]+ "']").attr('selected', 'selected');
				$("#txtOtherLanguagesKnown").val(jArray[0]["otherKnownLanguages"]);  
				$("#txtLicence").val(jArray[0]["licenceNo"]);
				getCaste();
				if(jArray[0]["Caste"]!= undefined && jArray[0]["Caste"]!='0'){
					$("#txtCaste").val(jArray[0]["Caste"]);
					if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="SINHGAD") { 
						$("#txtCaste").attr('disabled',true);
					}
				}else{
					$("#txtCaste").val(jArray[0]["Caste"]);
					$("#txtCaste").attr('disabled',false);
				}
					
				$('#txtCaste').trigger("liszt:updated");
				$("#txtStudentSubCaste").val(jArray[0]["subCast"]);
				$("#txtAdharCardNo").val(jArray[0]["adharcardNo"]);
				if($("#txtAdharCardNo").val()!=""){
					$('#saveBtn').attr('isAadharCard',"Yes");
				}else{
					$('#saveBtn').attr('isAadharCard',"No");
				}
				
				$("input [name='ArmyOrExserviceman'] ").val(jArray[0]["armyOrExserviceman"]);
				
				var physicallyHandicappedValue = jArray[0]["physicallyHandicapped"];
//				$("input[name=physicallyHandicapped][value=" + physicallyHandicappedValue + "]").attr('checked', 'checked');
				
				$("input[name=physicallyHandicapped][value='" + physicallyHandicappedValue + "']").attr('checked', 'checked');
				$('#saveBtn').attr('isPhysicallyHandicapped',physicallyHandicappedValue);
				
				var ArmyOrExservicemanValue = jArray[0]["armyOrExserviceman"];
//				$("input[name=ArmyOrExserviceman][value=" + ArmyOrExservicemanValue + "]").prop('checked', 'checked');
				$("input[name=ArmyOrExserviceman][value='"+ArmyOrExservicemanValue+"' ]").attr('checked', 'checked');
				
				var stateValue = jArray[0]["sports"];
				$("input[name=radioSports][value='"+stateValue+"']").attr('checked', 'checked');

				if (jArray[0]["Gender"] == "Male"){
					$("#male").attr("checked", true);
					//$("#male").attr("disabled", true);
					//$("#female").attr("disabled", true);
				}else if(jArray[0]["Gender"] =="Female"){
					$("#female").attr("checked", true);
					//$("#female").attr("disabled", true);
					//$("#male").attr("disabled", true);
				}else if(jArray[0]["Gender"] == "Other"){
					  $("#other").attr("checked", true);
				}else{  
					//$("#female").attr("disabled", false);
					//$("#male").attr("disabled", false);
				}
				
				if (jArray[0]["marital_Status"] == "Single")
					$("#single").attr("checked", true);
				else
					$("#married").attr("checked", true);

				$('#tdAdmissionTypeName').html(jArray[0]["admissionTypeName"]);
				 //alert(txtAdharCardNo.length);
			}
			
			$('#txtmobileNo1').val(jArray[0]["StudPlacementMobNo1"]);
			$('#txtmobileNo2').val(jArray[0]["StudPlacementMobNo2"]);
			
			
			
			if(jArray[0]["studentEntranceMeritListId"]!=0 ){
				$('#txtDteApplicationid').show();
				$('#labelDteApplicationid').show();
				$('#trDteApplicationid').show();
				//if (jArray[0]["applicationId"]!="" && jArray[0]["applicationId"]!=undefined ){
					$('#txtDteApplicationid').val(jArray[0]["applicationId"]);
					//$('#txtDteApplicationid').attr('disabled',true);
				//}else{
				//	$('#txtDteApplicationid').val(jArray[0]["applicationId"]);
				//	$('#txtDteApplicationid').attr('disabled',false);
				//}
			}else{
				$('#txtDteApplicationid').hide();
				$('#labelDteApplicationid').hide();
				$('#trDteApplicationid').hide();
			}

			var txtAdharCardNo=jArray[0]["adharcardNo"];
			if(jArray[0]["adharcardNo"]!=undefined){
				if(txtAdharCardNo.length===12)
					$('#uploadAdhar').show();
			}
			
			 studentProfileImage= jArray[0]["studentProfileImage"];
			 studentProfileSignature = jArray[0]["studentProfileSignature"];
			getstudProfileConfigurationDetailsCheckingNames();
			$('#hidenStudentId').val(jArray[0]["studentId"])
			
			
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RCOEM"){
				//console.log(jArray[0]["profileFreeze"]);
				if((jArray[0]["profileFreeze"]=="true" ||jArray[0]["profileFreeze"]==true) && (jArray[0]["profileFreeze"]!=null  && jArray[0]["profileFreeze"]!=undefined)){
					$('input[name = "firstName"]').attr("disabled", true);
					$('input[name = "middleName"]').attr("disabled", true);
					$('input[name = "lastName"]').attr("disabled", true);
					$('input[name = "motherName"]').attr("disabled", true);
					$('input[name = "fatherName"]').attr("disabled", true);
					$('select[name = "studreligion"]').attr("disabled", true);
					$('select[name = "studcategory"]').attr("disabled", true);
					$('input[name = "subCaste"]').attr("disabled", true);
					$('select[name = "studentcastename"]').attr("disabled", true);
					$('input[name = "Gender"]').attr("disabled", true);
					$('input[name = "appl10thMarksheetname"]').attr("disabled", true);
					if ((($('select[name = "studnationality"]')!=undefined) && (($('select[name = "studnationality"]').val())!=null) && ($('select[name = "studnationality"]').val() !="0"))) {
						$('select[name = "studnationality"]').attr("disabled", true);
					}else if((($('select[name = "studnationality"]')==undefined) || (($('select[name = "studnationality"]').val())==null) || ($('select[name = "studnationality"]').val() =="0"))){
						$('select[name = "studnationality"]').attr("disabled", false);
					}
					
					if ((($('input[name = "dob"]')!=undefined) && (($('input[name = "dob"]').val())!=null) && ($('input[name = "dob"]').val().length > 0))) {
						$('input[name = "dob"]').attr("disabled", true);
					}else if((($('input[name = "dob"]')==undefined) || (($('input[name = "dob"]').val())==null) || ($('input[name = "dob"]').val().length <= 0))){
						$('input[name = "dob"]').attr("disabled", false);
					}
					
					if ( ($('input[name = "enrollmentNo"]')!=undefined) && (($('input[name = "enrollmentNo"]').val())!=null) &&  ($.trim($('input[name = "enrollmentNo"]').val())!="") && ($('input[name = "enrollmentNo"]').val().length>0)) {
						$('input[name = "enrollmentNo"]').attr("disabled", true);
					}else if( ($('input[name = "enrollmentNo"]')==undefined) || (($('input[name = "enrollmentNo"]').val())==null) ||  ($.trim($('input[name = "enrollmentNo"]').val())=="") || ($('input[name = "enrollmentNo"]').val().length<=0)){
						$('input[name = "enrollmentNo"]').attr("disabled", false);
					}
					//$('#savePersonalDetail').hide();
					
				}else{
					//$('#savePersonalDetail').show();
				}
								
			}
			
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="MANDSAUR"){
if((jArray[0]["profileFreeze"]=="true" || jArray[0]["profileFreeze"]==true) && (jArray[0]["profileFreeze"]!=null && jArray[0]["profileFreeze"]!=undefined)){
	$('input[name = "firstName"]').attr("disabled", true);
	$('input[name = "middleName"]').attr("disabled", true);
	$('input[name = "lastName"]').attr("disabled", true);
	$('input[name = "mobileNo"]').attr("disabled", true);
	$('input[name = "std"]').attr("disabled", true);
	$('input[name = "alternetContactNumber"]').attr("disabled", true);
	$('input[name = "studentAadhaarCardNumber"]').attr("disabled", true);
}
}
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="GC_UNIVERSITY"){
				if (jArray[0]["religionId"] != undefined && jArray[0]["religionId"]!="0"){
					$('#txtstudreligion').val(jArray[0]["religionId"]);
					$('#txtstudreligion').attr('disabled',true);
				}else{
					$('#txtstudreligion').val(jArray[0]["religionId"]);
					$('#txtstudreligion').attr('disabled',false);
				}
				if(jArray[0]["Caste"]!= undefined && jArray[0]["Caste"]!='0'){
					$("#txtCaste").val(jArray[0]["Caste"]);
					$("#txtCaste").attr('disabled',true);
				}else{
					$("#txtCaste").val(jArray[0]["Caste"]);
					$("#txtCaste").attr('disabled',false);
				}
				if (jArray[0]["categoryId"] != undefined && jArray[0]["categoryId"]!="0"){
					$('#txtstudcategory').val(jArray[0]["categoryId"]);
					$('#txtstudcategory').attr('disabled',true);
				}else{
					$('#txtstudcategory').val(jArray[0]["categoryId"]);
					$('#txtstudcategory').attr('disabled',false);
				}
				if (jArray[0]["subCast"] != undefined && jArray[0]["subCast"]!=""){
					$('#txtStudentSubCaste').val(jArray[0]["subCast"]);
					$('#txtStudentSubCaste').attr('disabled',true);
				}else{
					$('#txtStudentSubCaste').val(jArray[0]["subCast"]);
					$('#txtStudentSubCaste').attr('disabled',false);
				}
			}
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="XLRI"){
				if (jArray[0]["applicationId"] != undefined && jArray[0]["applicationId"]!=""){
					$('#txtDteApplicationid').val(jArray[0]["applicationId"]);
					$('#txtDteApplicationid').attr('disabled',true);
				}
			}
			
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RAMAIAH_UNIVERSITY"){
				if (jArray[0]["nameAsPerAdhar"] != undefined && jArray[0]["nameAsPerAdhar"]!=""){
					$('#nameAsPerAdhar').val(jArray[0]["nameAsPerAdhar"]);
					$('#nameAsPerAdhar').attr('disabled',true);
				}
				if (jArray[0]["adharcardNo"] != undefined && jArray[0]["adharcardNo"]!=""){
					$('#txtAdharCardNo').val(jArray[0]["adharcardNo"]);
					$('#txtAdharCardNo').attr('disabled',true);
				}
			}
			
			if(($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="XLRI") && (jArray[0]["firstName"] != undefined && jArray[0]["firstName"]!="")){
				$('#txtfirstName').val(jArray[0]["firstName"]);
				$('#txtfirstName').attr('disabled',true);
			}
			if(($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="XLRI") && (jArray[0]["middleName"] != undefined && jArray[0]["middleName"]!="")){
				$('#txtmiddleName').val(jArray[0]["middleName"]);
				$('#txtmiddleName').attr('disabled',true);
			}
			if(($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="XLRI") && (jArray[0]["lastName"] != undefined && jArray[0]["lastName"]!="")){
				$('#txtLastName').val(jArray[0]["lastName"]);
				$('#txtLastName').attr('disabled',true);
			}
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="CMR_IT"){
				$('input[name = "middleName"]').attr("disabled", true);
				$('input[name = "lastName"]').attr("disabled", true);
			}
			if($('#customerHelper').val()=="RAMAIAH_UNIVERSITY")//jArray[0]["isRedirectToConvocation"]=='true'
			$('#ConvoRedirectLinkID').show();
			
			showPhysicalTR();
			if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY' || $('#customerHelper').val()=="CMR_IT" || $('#customerHelper').val()=="CMR_UNIVERSITY" || $('#customerHelper').val()=="ATAL_MEDICAL" ||  $('#customerHelper').val()=="MEGHE"){
				if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY' ){
					if(jArray[0]["degreeType"]!=undefined && jArray[0]["degreeType"]!="" && (jArray[0]["degreeType"]=="UG" || jArray[0]["degreeType"]=="ug"))
					{
						$('#pgLI').hide();
					}
				}
			if((jArray[0]["isProfileFreezed"]=="true" ||jArray[0]["isProfileFreezed"]==true) && (jArray[0]["isProfileFreezed"]!=null  && jArray[0]["isProfileFreezed"]!=undefined))
			{
				g_freeze=true;
				disabledAll();
			}
			}
			
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
	
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		getStudentContactForStudentDetailsTab();
	}
}
//------------------------------------------------------------------------Student
//Gaurdian Details
function getstudentGaurdianinfo(gStudentId) {
	$.ajax({
		url : 'getStudentGaurdianinfo.json?id=' + gStudentId,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			$('#gaurdiName').val(jArray["gaurdianName"]);
			$("#studguardianmiddlename").val(jArray["guardianMiddleName"]);
			$("#studguardianlastname").val(jArray["guardianLastName"]);
			$("#gaurdiEmail").val(jArray["gaurdianEmailID"]);
			$(
					'#guardianRelation option[value='
					+ jArray["guardianRelationship"] + ']').attr(
							"selected", "selected");
			$("#guardiAddress").val(jArray["gaurdianAddress"]);
			if (jArray["cityObj"] != undefined)
				$('#cityCombo').val(jArray["cityObj"]["cityId"]);
			else
				$('#cityCombo').val('');
			$("#studguardiantaluka").val(jArray["guardianTaluka"]);
			$("#studguardianVillage").val(jArray["guardianVillage"]);
			$("#studguardianpincode").val(jArray["guardianPinCode"]);
			$("#studeguardianstdcode").val(jArray["guardianStd"]);
			$("#guardianstudphnumber").val(jArray["guardianPhoneNo"]);
			$("#studguardianfaxno").val(jArray["guardianFax"]);
			$("#studguardiancellno").val(jArray["gaurdianMobileNo"]);
			
		},
		error : function() {
			alert("In Student Guardian Detail error");
		}

	});
}
//-----------------------------------------------------------------------------------------Parent
//Details
function getstudentParentinfoNew2() {
	$.ajax({
		url : 'getStudentParentinfo.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			if (jArray.length > 0) {
				$('#txtParentFirstName').val(jArray[0]["fatherFirstName"]);
				$("#txtParentMiddleName").val(jArray[0]["fatherMiddleName"]);
				$("#txtParentLastName").val(jArray[0]["fatherLastName"]);
				$("#txtProfession2").val(jArray[0]["fatherProfession"]);
				$("#studFatherno").val(jArray[0]["fatherMobileNo"]);
				$("#studParentEmail").val(jArray[0]["fatherAlternetEmailID"]);

				$("#txtMothersFirstName").val(jArray[0]["motherFirstName"]);
				$("#txtMothersMiddleName").val(jArray[0]["motherMiddleName"]);
				$("#txtMothersLastName").val(jArray[0]["motherLastName"]);
				$("#txtMothersProfession2").val(jArray[0]["motherProfession"]);
				$("#studMothersno").val(jArray[0]["motherMobileNo"]);
				$("#studMothersEmail").val(jArray[0]["motherAlternetEmailID"]);

				if (jArray[0]["parentAnnualIncome"] == "Less than 100000")
					$("#LessThanLac").attr("checked", true);
				if (jArray[0]["parentAnnualIncome"] == "Between 100000-300000")
					$("#LessThan3Lac").attr("checked", true);
				if (jArray[0]["parentAnnualIncome"] == "Between 300000-500000")
					$("#LessThan5Lac").attr("checked", true);
				if (jArray[0]["parentAnnualIncome"] == "More than 500000")
					$("#MoreThan5Lac").attr("checked", true);
			}
		},
		error : function() {
			alert("In Student Guardian Detail error");
		}

	});
}

function getstudentParentinfo(gStudentId) {
	$.ajax({
		url : 'getStudentParentinfo.json?id=' + gStudentId,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			$('#studParentfirstname').val(jArray["fatherFirstName"]);
			$("#studParentmiddlename").val(jArray["fatherMiddleName"]);
			$("#studParentlastname").val(jArray["fatherLastName"]);
			$("#studmotherfirstname").val(jArray["motherFirstName"]);
			$("#studmothermiddlename").val(jArray["motherMiddleName"]);
			$("#studmotherlastname").val(jArray["motherLastName"]);
			$("#studFatherEmail").val(jArray["fatherAlternetEmailID"]);
			$("#studMotherEmail").val(jArray["motherAlternetEmailID"]);
			$("#studeparentstdcode").val(jArray["parentStd"]);
			$("#parentstudphnumber").val(jArray["parentPhoneNo"]);
			$("#studFatherno").val(jArray["fatherMobileNo"]);
			$("#studmotherMobile").val(jArray["motherMobileNo"]);
			if (jArray["countryObj"] != undefined)
				$('#parentsCountryCombo')
				.val(jArray["countryObj"]["countryId"]);
			else
				$('#parentsCountryCombo').val('');
			if (jArray["stateObj"] != undefined)
				$('#parentStateCombo').val(jArray["stateObj"]["stateId"]);
			else
				$('#parentStateCombo').val('');
			if (jArray["cityObj"] != undefined)
				$('#parentCityCombo').val(jArray["cityObj"]["cityId"]);
			else
				$('#parentCityCombo').val('');
			$("#studMotherProfession").val(jArray["motherProfession"]);
			$("#applstudparentfaxno").val(jArray["parentFax"]);
			$("#parentAddressResi").val(jArray["parentAddress"]);
			$("#studparentdistrict").val(jArray["parentDistrict"]);
			$("#studParenttaluka").val(jArray["parentTaluka"]);
			$("#studParentvillage").val(jArray["parentVillage"]);
			$("#studparentpincode").val(jArray["parentPinCode"]);

			$("#studParentprofession").val(jArray["fatherProfession"]);
			$("#studparentannulaincome").val(jArray["parentAnnualIncome"]);
			$('#parentCreamyLayer option[value=' + jArray["creamyLayer"] + ']')
			.attr("selected", "selected");

		},
		error : function() {
			alert("In Student Guardian Detail error");
		}

	});
}

//----------------------------------------------------------------------------------Retrive
//function for passport Tab

function getstudentPassportinfo(gStudentId) {
	$.ajax({
		url : 'getStudentPassportinfo.json?id=' + gStudentId,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			$('#studpassportcountrycode').val(jArray["countryCode"]);
			$("#studpassportno").val(jArray["passportNo"]);
			$("#studpassportexpiryDate").val(jArray["expiryDate"]);
			$("#studpassportissueplace").val(jArray["placeOfIssue"]);
			$("#issueDate").val(jArray["issueDate"]);
			$("#studvisaid").val(jArray["visaNo"]);
			$("#studvisanature").val(jArray["natureOfVisa"]);
			$("#visaIssueDate").val(jArray["visaIssueAt"]);

			if (jArray["passportType"] == "Permanent")
				$("#perm").attr("checked", true);
			else if (jArray["passportType"] == "Temporary")
				$("#temp").attr("checked", true);
			else if (jArray["passportType"] == "Tourist")
				$("#tour").attr("checked", true);
			else if (jArray["passportType"] == "Educational")
				$("#edu").attr("checked", true);

		},
		error : function() {
			alert("In Student Passport Detail error");
		}

	});
}

function getCity() {
	$.ajax({
		url : 'getStudentCitylist.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			var html = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++)
				html += '<option value="' + data[i]["cityId"] + '">'
				+ data[i]["cityName"] + '</option>';

			$('#cityCombo').html(html);
			$('#parentCityCombo').html(html);
			$("#studentCity").html(html);
			$('#permstudentCity').html(html);

		},

	});
}

function getCountry() {
	$.ajax({
		url : 'getStudentCountrylist.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			var html = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++)
				html += '<option value="' + data[i]["countryId"] + '">'
				+ data[i]["country"] + '</option>';

			// $('#parentsCountryCombo').html(html);
			$("#studentCountryCombo").html(html);
			$("#icardstudentCountryCombo").html(html);
			$("#permstudentCountryCombo1").html(html);
			$('#txtCountry').html(html);
			if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
				$('#permstudentCountryCombo1_1').html(html);
				$('#studentCountryCombo_1').html(html);
			}
			
		},

	});
}

function getState() {
	$.ajax({
		url : 'getStudentStatelist.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			var html = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++){
				html += '<option value="' + data[i]["stateId"] + '">'
				+ data[i]["stateName"] + '</option>';
			}
			$("#txtstuddomacile").html(html);
			$('#parentStateCombo').html(html);
			$('#txtstateOfSchool').html(html);
			$('#txtstateOfSchool12').html(html);
			$('#stateName').html(html);

			if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
				$('#studentState_1').html(html);
				$('#permstudentState_1').html(html);
			}
		},

	});
}

