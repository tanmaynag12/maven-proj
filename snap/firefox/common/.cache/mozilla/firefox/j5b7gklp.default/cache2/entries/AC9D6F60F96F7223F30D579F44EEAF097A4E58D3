function showUploadModal()
{   
	//flushSessionstudentOtherFile();
	$('#UploadStudentOtherFileDiv').html('');
	//$('#eventPaymentModal').modal('show');
  var index=1;
	initFileUploadPan(index);
	//getStudentOtherFile();
	var boiler='<input class="btn btn-inverse btn-mini saveDocuement btnColorGreen" type="button"  value="Save">'+
	'<input type="button" class="btn btn-inverse btn-mini btnColorRed " value="Close" data-dismiss="modal" onclick="flushSessionstudentOtherFile()"/>';
	//$('#btnGroup').html(boiler);	
	
	$('.saveDocuement').click(function()
	{
		saveStudentOtherFile();
	});
}

function saveStudentOtherFile()
{
	$.ajax({
		url:'saveStudentOtherFile.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray)
		{
			var status=jArray['status'];

			if(status == true)
			{
				getCustomAlertMessage("alert_create");
				flushSessionstudentOtherFile();
				getStudentOtherFile();
				$('#UploadStudentOtherFileDiv').html('');
				//$('#otherDocumentModal').modal('hide');
			}
			else
			{
				jAlert("Supporting Documents Required");
			}
		}
		});
}

function initFileUploadPan(index)
{
	$('document').ready(function() {//
		new AjaxUpload('uploadFile',{
			action :'setshowStudentOtherFileInSession.json',
			data : {},
			name : 'fileData',
			size:AllowedFileSize.DOCUMENT_UPLOAD,
			onSubmit : function(file, extension) {
				if(extension.toString().toLowerCase() == 'doc' || extension.toString().toLowerCase() == 'docx' || extension.toString().toLowerCase() == 'xls'
					|| extension.toString().toLowerCase() == 'xlsx' || extension.toString().toLowerCase() == 'pdf' || extension.toString().toLowerCase() == 'jpg'
					|| extension.toString().toLowerCase() == 'text' || extension.toString().toLowerCase() == 'png' || extension.toString().toLowerCase() == 'gif'
					|| extension.toString().toLowerCase() == 'ppt'  || extension.toString().toLowerCase() == 'zip' || extension.toString().toLowerCase() == 'pptx'){
					startLoading();
					this.setData({
						'index':index,
						'fileSize':AllowedFileSize.DOCUMENT_UPLOAD
					});
				} else {
					jAlert('File Format not supported ! \n Please use .doc, .docx, .xls, .xlsx, .pdf, .jpg, .text, .png, .gif, .ppt, .pptx, .zip files only');
					return false;
				}

			},
			onComplete : function(file, response) {
				stopLoading();
				var jArray = eval('(' + response + ')');
				showStudentOtherFiles(jArray,true);
				saveStudentOtherFile();
				//$('#eventPaymentModal').hide();
			}
		});
	});
}


function showStudentOtherFiles(jArray,tableId,isCreator){

	var attachementBoiler = "<table class = 'table table-bordered' id='tblshowStudentFile'>";
	for(var i = 0 ; i < jArray.length; i++){
		var attachmentFileName = jArray[i]["name"].toString(); 
		var documentId = jArray[i]["documentId"];
		var eDocId =     jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];
		var isTemporary =jArray[i]["isTemporary"];///false;// 
		var trId = 'trAttachmentFile-'+eDocId;
		var fileType = jArray[i]["type"];
		iconPath = gFileTypeIconMapping[fileType];
		if(!isTemporary){
			attachementBoiler += 
				'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+(juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName))+'</td>';
			if(isCreator){
				attachementBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAttachmentAjaxPan(\''+trId+'\',\''+eDocId+'\',\'saved\')" /></td>'; 
			}
			attachementBoiler +=  '</tr>';
		} else {
			attachementBoiler += 
				'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;<a href = "#">'+attachmentFileName+'</a></td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAttachmentAjaxPan(\''+trId+'\',\''+eDocId+'\',\'temporary\')" />&nbsp;&nbsp;&nbsp;&nbsp<img src = "img/unsaved.png" title = "Unsaved"/></td></tr>';
		}
	}
	attachementBoiler += "</table>";
	$('#UploadStudentOtherFileDiv').html(attachementBoiler);
}
function removeAttachmentAjaxPan(trId,documentId,docuementType){
	
	if(docuementType == 'saved'){
		jConfirm("Do you want to Remove Document :","Confirm",function(r)
				{
					if(r ==true)
					{
						removeUploadedDocumentById(documentId,function(jObject){
							$('#'+trId).remove();
						});
					}
					});
	} else {
		removeTemporaryAttachmentAjaxPan(documentId,function(){
			$('#'+trId).remove();
			});
	}
	flushSessionstudentOtherFile();
	$('#otherDocumentModal').modal('hide');
}

function removeTemporaryAttachmentAjaxPan(documentId,callback){
	try{
		$.ajax({
			url:'removeTemporarystudentOtherFileInSession.json',
			dataType:'json',
			type:'POST',
			data:{d__documentId:documentId},
			success:function(){
				try{
					callback();
				}catch(e){

				}
			}	
		});

	}catch(e){
		alert(e);
	}
}
function flushSessionstudentOtherFile(){
	$.ajax({
		url:'flushSessionstudentOtherFile.json',
		type:'POST',
		success:function(){
			
		}
	});
}

function getStudentOtherFile()
{
	getStudentOtherFileByCategoryId(function(categoryId){
		var uploadDocument = new UploadDocument();
		uploadDocument.getDocument(categoryId, function(jArray)
				{
			showStudentOtherFile(jArray,'admissionRecepit',true);
				});
	});
}
function getStudentOtherFileByCategoryId(callback)
{
	$.ajax({
		url:'getStudentOtherFileByCategoryId.json',
		dataType:'text',
		//type:'POST',
		name : 'fileData',
		success:function(categoryId){
			callback(categoryId);
		}
	});
}
function showStudentOtherFile(jArray,tableId,isCreator){
	var StudentOtherFileBoiler = "<table class = 'table table-bordered' id='showStudentOtherFile'>";
	
	for(var i = 0 ; i < jArray.length; i++){
		var attachmentFileName = jArray[i]["name"].toString(); 
		var documentId = jArray[i]["documentId"];
		var eDocId =  jArray[i]["eDocId"]!=undefined?jArray[i]["eDocId"]:jArray[i]["documentId"];
		var isTemporary =jArray[i]["isTemporary"];///false;// 
		var trId1 = 'trAttachmentFile-'+documentId;
		var fileType = jArray[i]["type"];
		iconPath = gFileTypeIconMapping[fileType];
		if(!isTemporary){
			StudentOtherFileBoiler += 
				'<tr id = "'+trId1+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+(juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName))+'</td>';
			if(isCreator){
				StudentOtherFileBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAttachmentAjaxPan(\''+trId1+'\',\''+eDocId+'\',\'saved\')" /></td>'; 
			}
			StudentOtherFileBoiler +=  '</tr>';
		}
	}
	StudentOtherFileBoiler += "</table>";
	$('#otherDocumentsList').html(StudentOtherFileBoiler);
}

function validateStdCode()
{
	var inputtxt = $("#txtstdcode").val();
	 var phoneno = /^\+?[0-9-]+$/;  
	  if(inputtxt.match(phoneno))  
	        {  
	      return true;  
	        }  
	      else  
	        {  
	        jAlert("Enter valid STD code !");
	        $("#txtstdcode").val("");
	        return false;  
	        }  
}

function validateVehicleNo()
{
	var inputtxt = $("#txtVehicleNo").val();
	 var validExp = /^[A-Z]{2}[ -][0-9]{1,2}(?: [A-Z])?(?: [A-Z]*)? [0-9]{4}$/;  
	  if(inputtxt.match(validExp))  
	        {  
	      		return true;  
	        }  
	      else  
	        {  
	        jAlert("Enter valid vehicle no !");
	        $("#txtVehicleNo").val("");
	        return false;  
	        }  
	
}
function saveStudentAdditionalInformation()
{
	var IsCatholic = $('input[name = "catholicname"]:checked').val();
	var Denomination =  $('#DenominationId').val();
	var Diocese =  $('#DioceseId').val();
	var Religious = $('input[name = "religious"]:checked').val();
	var Congregation =  $('#CongregationId').val();
	if(IsCatholic=="Yes" && (Denomination=='' ||  Diocese==''))
	{
		jAlert("Please Enter Denomination and Diocese");
	}
	else if(Religious=="Yes" && Congregation=='')
	{
		jAlert("Please Enter Diocese/Congregation");
	}
	else
	{
		$.ajax({
			url : 'setStudentAdditionalInformation.json',
			data : {
				IsCatholic : IsCatholic,
				Denomination : Denomination,
				Diocese : Diocese,
				Religious : Religious,
				Congregation : Congregation,
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(String) {
				getCustomAlertMessage("alert_create");
			},
			error : function() {
			}
		});
	}
}
function checkDenomination()
{
	var IsCatholic = $('input[name = "catholicname"]:checked').val();
	if(IsCatholic=="Yes") {
		$('#checkDenominationID').show();
		$('#checkDenominationID1').show();
		$('#checkDiocese').show();
		$('#checkDiocese1').show();
	}
	if(IsCatholic=="No"){
		$('#checkDenominationID').hide();
		$('#checkDenominationID1').hide();
		$('#checkDiocese').hide();
		$('#checkDiocese1').hide();
		$('#DenominationId').val("");
		$('#DioceseId').val("");
	}
}
	function checkreligious()
	{
		var Religious = $('input[name = "religious"]:checked').val();
		if(Religious=="Yes"){
			$('#checkCongregationId').show();
			$('#chekCongregationId1').show();
		}
		if(Religious=="No"){
			$('#checkCongregationId').hide();
			$('#chekCongregationId1').hide();
			$('#CongregationId').val("");
		}
}
	
	function getstudentAdditionalInfo() {
		$.ajax({
			url : 'getStudentAdditionalInformation.json',
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
					if((jArray[0]["IsCatholic"])=="Yes")
						$("#rbtnCatholicyes").prop("checked", true);	
					else
						$("#rbtnCatholicno").prop("checked", true);
					
					$("#DenominationId").val(jArray[0]["Denomination"]);
					$("#DioceseId").val(jArray[0]["Diocese"]);
					
					if((jArray[0]["Religious"])=="Yes")
						$("#religiousyes").prop("checked", true);	
					else
						$("#religiousno").prop("checked", true);
					
                    $("#CongregationId").val(jArray[0]["Congregation"]);
					
			},
			error : function() {
				// //alert("in Error stream CMB");
			}
		});
	}
	

function entranceExamDetails(){
	
	$.ajax({
		url : 'getStudentEntranceExamDetails.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
				var boiler= '<table class = "table table-bordered" id="showStudentEntranceExamDetails">'+
				'<tr><th colspane="4">Examination Name : </th><th>Score</th><th>Merit no</th><th>Percentile</th><th>Total Obtain Marks</th><th>Course Name</th><th>Score Card</th></tr><tbody>';
				if(jArray.length==0){
					boiler+='<tr><td><b>Data Not Found.</b></td></tr>';
					$('#showStudentEntranceExamDetails').html(boiler);
					return;
				}
					for(var i=0; i<jArray.length; i++){
					boiler+='<tr><td><b>'+jArray[i]["StudExamName"]+'</b></td>'+
					'<td style="text-align: right">'+jArray[i]["StudScore"]+'</td>'+
					'<td style="text-align: right">'+jArray[i]["StudRank"]+'</td>'+
					'<td style="text-align: right">'+jArray[i]["StudPercentile"]+'</td>'+
					'<td style="text-align: right">'+jArray[i]["totalObtainMarks"]+'</td>'+
					'<td style="text-align: left">'+jArray[i]["COURSE_NAME"]+'</td>'+
					'<td style="text-align: center"><a class="btn btn-mini btn-primary" href="javascript:void(0);" onclick="downloadScoreCard(\''+jArray[i]["testSettingId"]+'\',\''+jArray[i]["StudExamName"]+'\');"><i class="fa fa-download fa lg" aria-hidden="true"></i> Download Score Card</a></label></td></tr>';
				}
				boiler+='</tbody></table>';
			$('#showStudentEntranceExamDetails').html(boiler);
			
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
}

