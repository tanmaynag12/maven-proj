function getStudentCity(state, city) {
	$.ajax({
		url : 'cityList.json',
		data : {
			StateId : $('#' + state).val()
		},
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			var html = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++){
				html += '<option value="' + data[i]["cityId"] + '">'
				+ data[i]["cityName"] + '</option>';
			}
			$('#' + city).html(html);
			/*if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
				$('#studentCity_1').html(html);
				$('#permstudentCity_1').html(html);
			}*/
		},
	});
}
var typeOfCity = '';
function addNewCityInStudProContact(type) {
	typeOfCity = '';
	typeOfCity = type;
	$('#txtCityInStudProf').val('');
	var countryId = 0;
	var stateId = 0;

	var cnt6 = 0;

	if (type == 'studentCity') {
		countryId = $("#studentCountryCombo").val();
		stateId = $("#studentState").val();

		if (countryId == 0 || stateId == 0) {
			jAlert("Please Select Country and State");
			cnt6 = 1;
		}
	} else if (type == 'permStudentCity') {
		countryId = $("#permstudentCountryCombo1").val();
		stateId = $("#permstudentState").val();

		if (countryId == 0 || stateId == 0) {
			jAlert("Please Select Country and State");
			cnt6 = 1;
		}
	}

	if (cnt6 == 0) {
		$('#addcityInStudProContactPopUp').modal('show').css({
			width : '600px',
			'margin-left' : function() {
				return -($(this).width() / 2);
			}
		});
	}
}

function addNewUniversityInStudProContact() {
	$('#frmAddOtherUniversityFrm').reset();
	$('#otherUniversityNameForOther').val('');
	$('#otherUniversityAbbreviationForOther').val('');
	$('#otherUniversityURLForOther').val('');

	$('#addOtherUniversityFrm').modal('show').css({
		width : '600px',
		'margin-left' : function() {
			return -($(this).width() / 2);
		}
	});
}

//add Other UniverSity
function saveOtherUniInfoForOther() {
	var universityName = $('#otherUniversityNameForOther').val();
	var universityAbbreviation = $('#otherUniversityAbbreviationForOther').val();
	var universityURL = $('#otherUniversityURLForOther').val();
	if (universityName == '') {
		jAlert('Enter University name');
	} else {
		$.ajax({
			url : 'addOtherUniversity.json',
			data : {
				universityName : universityName,
				universityAbbreviation : universityAbbreviation,
				universityURL : universityURL
			},
			dataType : 'json',
			type : 'POST',
			success : function(jArray) {
				if (jArray[0]["members"]['responseFlag'].value == '1') {
					// alert("inside succsss");
					$('#addOtherUniversityFrm').modal('hide');
					showSuccessMessage("University Added Successfully",2000);
					var uniId = jArray[1]['otherUniversityId'];
					$.ajax({
						url : 'getOtherUniversity.json',
						dataType : 'json',
						async : false,
						beforeSend : function() {
							startCommonAjaxSpinner();
						},
						complete : function() {
							stopCommonAjaxSpinner();
						},
						success : function(jArray) {
							var j = jArray.length;
							var uniComboBoiler = '<option value="0">Select</option>';
							for ( var i = 0; i < j; i++)
								uniComboBoiler += '<option value="'+ jArray[i]["otherUniversityId"]+ '">'+ jArray[i]["uniName"]	+ '</option>';

							$('#cmbUniNameForStudPlacement').html(uniComboBoiler);
							$('#university').html(uniComboBoiler);
						},
						error : function() {
							// //alert("in Error stream CMB");
						}
					});
					$('#university').val(uniId);
					getCustomAlertMessage("alert_create");
				} else {

					jAlert("University already exist!!");
				}
			},
			error : function() {
				// alert("in Error to add a new University");
			}
		});
	}
}

function addNewInstituteInStudProContact() {
	$('#txtInstituteInStudProf').val('');
	var countryId = $("#studentCountryCombo").val();
	var stateId = $("#studentState").val();
	if (countryId == 0 || stateId == 0) {
		jAlert("Please Select Country and State");
	} else {
		$('#addInstituteInStudProContactPopUp').modal('show').css({
			width : '600px',
			'margin-left' : function() {
				return -($(this).width() / 2);
			}
		});
	}
}

function saveInstituteInStudProf() {
	var instituteName = $("#txtInstituteInStudProfForOther").val();
	if (instituteName == "") {
		jAlert("Enter Institute Name");
	} else {
		$.ajax({
			url : 'addNewInstituteInStudProContact.json?instituteName='+ instituteName,
			dataType : 'text',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(cityId) {
				var temp = cityId;
				$('#addInstituteInStudProContactPopUp').modal('hide');
				getCustomAlertMessage("alert_create");
				$.ajax({
					url : 'getInstitutes.json',
					dataType : 'json',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(jArray) {
						var boiler = '<option value="0">Select</option>';
						for ( var i = 0; i < jArray.length; i++) {
							boiler += '<option id="' + jArray[i]["instId"]+ '" value="' + jArray[i]["instId"] + '" >'+ jArray[i]["instName"] + '</option>';
						}
						$('#institution').html(boiler);
					},
					error : function() {
					}
				});
				$('#institution').val(temp);
			},
			error : function() {
			}
		});
	}
}

function addNewStreamInStudProContact() {
	$('#frmAddOtherStreamFrm').reset();
	$('#addOtherStreamFrm').modal('show').css({
		width : '600px',
		'margin-left' : function() {
			return -($(this).width() / 2);
		}
	});
}

//add other stream
function saveOtherStreamInfo() {
	var streamDescription = $('#otherStreamDescriptionForOther').val();
	if (streamDescription != "") {
		$.ajax({
			url : 'addOtherStream.json',
			data : {
				streamDescription : streamDescription
			},
			dataType : 'json',
			type : 'POST',
			success : function(jArray) {
				$('#addOtherStreamFrm').modal('hide');
				if (jArray[0]["members"]['responseFlag'].value == '1') {
					showSuccessMessage("Stream Added Successfully",	5000);
					var streamName = jArray[1]['degreeStream'];
					$.ajax({
						url : 'getOtherStream.json',
						dataType : 'json',
						async : false,
						beforeSend : function() {
							startCommonAjaxSpinner();
						},
						complete : function() {
							stopCommonAjaxSpinner();
						},
						success : function(jArray) {

							var j = jArray.length;
							var uniComboBoiler = '<option value="0">Select</option>';
							for ( var i = 0; i < j; i++)
								uniComboBoiler += '<option value="'+ jArray[i]["degreeStream"]+ '">'+ jArray[i]["degreeStream"]+ '</option>';

							$('#cmbStreamNameForStudPlacement').html(uniComboBoiler);
							$('#stream').html(uniComboBoiler);
						},
						error : function() {
							// //alert("in Error stream CMB");
						}
					});

					$('#stream').val(streamName);
					var temp = "";
					getDegreeName(temp);
				}
				getCustomAlertMessage("alert_create");
			},
			error : function() {
				// //alert("in Error to add a new Stream");
			}
		});
	} else {
		jAlert("Please enter stream name");
	}
}

var globalDegreeType = "";
function addNewDegreeInStudProContact(degreeType) {

	globalDegreeType = degreeType;
	var streamName = $('#stream').val();
	if (degreeType == 'PG')
		degreeType = 'PG';
	else if (degreeType == 'UG')
		degreeType = 'UG';
	else if (degreeType == 'DIP')
		degreeType = 'DIP';

	if (streamName == 0) {
		jAlert("Select Stream");
	} else {
		$('#frmAddOtherDegreeFrm').reset();
		$('#addOtherDegreeFrm').modal('show').css({
			width : '600px',
			'margin-left' : function() {
				return -($(this).width() / 2);
			}
		});

		if (degreeType == 'DIP') {
			$("#tdCmbDegreeType2").show();
			$("#tdCmbDegreeType1").hide();

			$('#otherStreamNameForOther').val(streamName);
			$('#otherDegreeCmbForOther2').val(degreeType);
		} else {
			$("#tdCmbDegreeType2").hide();
			$("#tdCmbDegreeType1").show();

			$('#otherStreamNameForOther').val(streamName);
			$('#otherStreamNameForOther').attr('disabled', 'disabled');

			$('#otherDegreeCmbForOther1').val(degreeType);
			$('#otherDegreeCmbForOther1').attr('disabled', 'disabled');
		}
		// numberFormatValidation('otherDegreeDuration');
	}
}

function saveOtherDegreeInfoForOther() {
	var streamName = $('#otherStreamNameForOther').val();
	var degreeName = $('#otherDegreeNameForOther').val();
	var degreeDuration = ($('#otherDegreeDurationForOther').val()).trim();

	var degreeTypeCmb = '';
	if (globalDegreeType == "DIP")
		degreeTypeCmb = $('#otherDegreeCmbForOther2').val();
	else
		degreeTypeCmb = $('#otherDegreeCmbForOther1').val();

	if (degreeName == '') {
		jAlert("Enter degree name");
	} else if (degreeTypeCmb == '') {
		jAlert("Enter degree type");
	} else if ($('#otherDegreeCmbForOther').val() == '') {
		jAlert("Degree Type");

	} else if (degreeDuration == '') {
		jAlert("Enter degree duration");
	} else if (isNaN(degreeDuration)) {
		jAlert("Enter duration in numbers only");
		$('#otherDegreeDurationForOther').val('');
	} else {
		$.ajax({
			url : 'addOtherDegree.json',
			data : {
				degreeName : degreeName,
				degreeDuration : degreeDuration,
				degreeTypeCmb : degreeTypeCmb,
				streamName : streamName
			},
			dataType : 'json',
			type : 'POST',
			success : function(jArray) {
				$('#addOtherDegreeFrm').modal('hide');
				if (jArray[0]["members"]['responseFlag'].value == '1') {
					showSuccessMessage("Degree Added Successfully",5000);
					var otherPlacementQualificationId = jArray[1]['otherPlacementQualificationId'];

					getDegreeName(degTypeForAddNewDegInpopup);
					$('#degName').val(otherPlacementQualificationId);

					if (jArray[1]["degreeType"] == 'COURSE_TYPE_Diploma') {
						$('#txtDegType').val("Diploma");
					} else if (jArray[1]["degreeType"] == 'COURSE_TYPE_PG_Diploma') {
						$('#txtDegType').val("PG Diploma");
					} else if (jArray[1]["degreeType"] == 'COURSE_TYPE_UG_Diploma') {
						$('#txtDegType').val("UG Diploma");
					}

				}
				getCustomAlertMessage("alert_create");
			},
			error : function() {
				// alert("in Error to add a new Degree");
			}
		});
	}
}

function saveCityInStudProf() {

	var stateId = 0;
	var cityName = $("#txtCityInStudProf").val().trim();

	if (typeOfCity == 'studentCity') {
		stateId = $("#studentState").val();
	} else {
		stateId = $("#permstudentState").val();
	}

	if (cityName != "" && stateId != 0) {
		$.ajax({
			url : 'addNewCityInStudProContact.json?stateId=' + stateId
			+ '&cityName=' + cityName,
			dataType : 'text',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(cityId) {
				var temp = cityId;
				$('#addcityInStudProContactPopUp').modal('hide');
				getCustomAlertMessage("alert_create");
				var cityblr = '<option value="0">Select</option>';
				$.ajax({
					url : 'getStudentCitylist.json',
					dataType : 'json',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(jArray) {
						for ( var i = 0; i < jArray.length; i++) {
							cityblr += '<option value="' + jArray[i]["cityId"]
							+ '">' + jArray[i]["cityName"]
							+ '</option>';

						}
						if (typeOfCity == 'studentCity')
							$("#studentCity").html(cityblr);
						if (typeOfCity == 'permStudentCity')
							$("#permstudentCity").html(cityblr);
					},
					error : function() {
						alert("In Student City error");
					}
				});

				if (typeOfCity == 'studentCity')
					$('#studentCity').val(temp);
				if (typeOfCity == 'permStudentCity')
					$('#permstudentCity').val(temp);

			},
			error : function() {
				// alert("Problem in fetching Product Name.");
			}
		});
	} else {
		jAlert("Please enter city name");
	}
}
function getStudentContact(gStudentId) {

	getCountry();
	try {
		var hdnUserRole = $('#hdnUserRole').val();
		var url = 'getStudentContactDetails.json?id='+gStudentId;
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_getStudentContactDetails.json';
		}
		$.ajax({
			url : url,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
				
			},
			success : function(jArray) {
				for ( var i = 0; i < jArray.length-1; i++) {

					if (jArray[i]["permanentOrCurrent"] == "current") {
						if(jArray.length>2){
						if(!jArray[2]["is_icard_confirmed"])
						{
							var cntryId = jArray[i]["countryId"];
							$('#studentCountryCombo option[value='+ cntryId + ']').attr("selected", "selected");
							getStudentStates('studentCountryCombo','studentState');
							var stateId = jArray[i]["stateId"];
							$('#studentState option[value=' + stateId + ']').attr("selected", "selected");
							getStudentCity('studentState', 'studentCity');
							var cityId = jArray[i]["cityId"];
							$("#studAddLine1").val(jArray[i]["addressLine1"]);
							$("#studAddLine2").val(jArray[i]["addressLine2"]);
							$('#studentCity').val(cityId);
							$("#studPincode").val(jArray[i]["pinCode"]);

							$('#CurrAddressId').val(jArray[i].studentAddressId);
							$('#txtStudentDistrict').val(jArray[i].studentDistrict);
							
							
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DYPATIL"){
								getTaluka('studentCity', 'txtStudentTaluka');
							 	$("#currstudhouse1").val(jArray[i]["houseNo"]);
							 	$("#currstudStreet1").val(jArray[i]["streetNo"]);
							 	$("#currstudlandmark1").val(jArray[i]["landmark"]);
							 	$("#currstudvillege1").val(jArray[i]["villege"]);
							  	var taluka=jArray[i]["studentTaluka"];
							  
							  	//$('#txtStudentTaluka option[value='+taluka+']').attr("selected", "selected");
							  	//$('#txtStudentTaluka option[value="'+taluka+'"]').attr("selected", "selected");
							  	$('#txtStudentTaluka').val(taluka);
								}else{
                                    $('#txtStudentTaluka').val(jArray[i].studentTaluka);
                                    }
							if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')
							$('#CurrAddressId').val(jArray[i].studentAddressId);
						}else{


							var cntryId = jArray[i]["countryId"];
							$('#studentCountryCombo option[value='+ cntryId + ']').attr("selected", "selected");
							
							getStudentStates('studentCountryCombo','studentState');
							var stateId = jArray[i]["stateId"];
							$('#studentState option[value=' + stateId + ']').attr("selected", "selected");
							
							getStudentCity('studentState', 'studentCity');
							var cityId = jArray[i]["cityId"];
							$("#studAddLine1").val(jArray[i]["addressLine1"]);
							
							$("#studAddLine2").val(jArray[i]["addressLine2"]);
							
							$('#studentCity').val(cityId);
							
							
							$("#studPincode").val(jArray[i]["pinCode"]);
							

							if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')
							$('#CurrAddressId').val(jArray[i].studentAddressId);
							
							
							$('#txtStudentDistrict').val(jArray[i].studentDistrict);
							
							$('#txtStudentTaluka').val(jArray[i].studentTaluka);       
							
						
						}
						}else{

							var cntryId = jArray[i]["countryId"];
							$('#studentCountryCombo option[value='+ cntryId + ']').attr("selected", "selected");
							
							getStudentStates('studentCountryCombo','studentState');
							var stateId = jArray[i]["stateId"];
							$('#studentState option[value=' + stateId + ']').attr("selected", "selected");
							
							getStudentCity('studentState', 'studentCity');
							var cityId = jArray[i]["cityId"];
							$("#studAddLine1").val(jArray[i]["addressLine1"]);
							
							$("#studAddLine2").val(jArray[i]["addressLine2"]);
							
							$('#studentCity').val(cityId);
							
							
							$("#studPincode").val(jArray[i]["pinCode"]);
							

							if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')
							$('#CurrAddressId').val(jArray[i].studentAddressId);
							
							
							$('#txtStudentDistrict').val(jArray[i].studentDistrict);
							
							$('#txtStudentTaluka').val(jArray[i].studentTaluka);
							$('#talukteshil').val(jArray[i]["taluka"]);
							$('#tehsilId').val(jArray[i]["townvillage"]);
							
						} 
					}
					 else {
						if(jArray[i].studentAddressId!=undefined && jArray[i].studentAddressId!='undefined')	{
						var cntryId = jArray[i]["countryId"];
						$('#permstudentCountryCombo1 option[value='+ cntryId + ']').attr("selected", "selected");
						getStudentStates('permstudentCountryCombo1','permstudentState');
						var stateId = jArray[i]["stateId"];
						$('#permstudentState option[value='+ stateId + ']').attr("selected", "selected");
						getStudentCity('permstudentState','permstudentCity');
						var cityId = jArray[i]["cityId"];
						$("#permstudAddLine1").val(jArray[i]["addressLine1"]);
						$("#permstudAddLine2").val(jArray[i]["addressLine2"]);
						$('#permstudentCity').val(cityId);
						$("#permstudPincode").val(jArray[i]["pinCode"]);
						$('#txtStudentPermDistrict').val(jArray[i]["studentDistrict"]);
						
						$('#PermaAddressId').val(jArray[i].studentAddressId);
						
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DYPATIL"){
								getTaluka('permstudentCity', 'txtStudentPermTaluka');
							 	$("#permstudHouse2").val(jArray[i]["houseNo"]);
							 	$("#permstudStreet2").val(jArray[i]["streetNo"]);
							 	$("#permstudLandmark2").val(jArray[i]["landmark"]);
							 	$("#permstudVillage2").val(jArray[i]["villege"]);
							  	var taluka2=jArray[i]["studentTaluka"];
							  	//$('#txtStudentTaluka option[value='+taluka+']').attr("selected", "selected");
							  	//$('#txtStudentPermTaluka option[value='+ taluka2 + ']').attr("selected", "selected");
							  	$('#txtStudentPermTaluka').val(taluka2);
								}else{
								$('#txtStudentPermTaluka').val(jArray[i]["studentTaluka"]);
								}
						}
					 	}
					
					if(jArray[i]["studentContactStatus"] == true || jArray[i]["studentContactStatus"]=="true"){
						$('#studentCountryCombo').attr('disabled', false);
						$('#studentState').attr('disabled', false);
						$('#studAddLine1').attr('disabled', false);
						$('#studAddLine2').attr('disabled', false);
						$('#studentCity').attr('disabled', false);
						$('#studPincode').attr('disabled', false);
						$('#CurrAddressId').attr('disabled', false);
						$('#txtStudentDistrict').attr('disabled', false);
						$('#txtStudentTaluka').attr('disabled', false);
						$('#saveContactBtnDiv').show();
					}else{
						
						$('#studentCountryCombo').attr('disabled', true);
						$('#studentState').attr('disabled', true);
						$('#studAddLine1').attr('disabled', true);
						$('#studAddLine2').attr('disabled', true);
						$('#studentCity').attr('disabled', true);
						$('#studPincode').attr('disabled', true);
						$('#CurrAddressId').attr('disabled', true);
						$('#txtStudentDistrict').attr('disabled', true);
						$('#txtStudentTaluka').attr('disabled', true);
						$('#saveContactBtnDiv').hide();
						
						$('#permstudAddLine1').attr('disabled', true);
						$('#permstudAddLine2').attr('disabled', true);
						$('#permstudentCountryCombo1').attr('disabled', true);
						$('#permstudentState').attr('disabled', true);
						$('#permstudentCity').attr('disabled', true);
						$('#txtStudentPermDistrict').attr('disabled', true);
						$('#txtStudentPermTaluka').attr('disabled', true);
						$('#permstudPincode').attr('disabled', true);
					}
					}
				getstudProfileConfigurationDetailsCheckingNames();
			},
			error : function() {
				alert("In Student Contact error");
			}
		});
	} catch (e) {
	}
}

//-------------------------------------------------------------UG PG DIP status
//for editing

function checkEditStatusForUGPG() {

	var editingStatus = statusForAddingUGPG;

	if (editingStatus == 'ReqSent') {
		jAlert("The request you have sent for editing is pending");
	} else if (editingStatus == 'canChange') {
		addStudentGraduation();
	} else if (editingStatus == 'ReqNotSent') {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "ssc";
						sendRequestForEditing(tab);
					}
				});
	} else if (editingStatus == 'ReqAccepted') {
		$('#btnFreeze10th').show();
		addStudentGraduation();
	} else {
		addStudentGraduation();
	}
}

//-------------------------------------------------------------Tab-3(10Th)

function checkEditStatusFor10th() {

	var editingStatus = $('#sub10th').data("editingStatus");

	if (editingStatus == 'ReqSent') {
		jAlert("The request you have sent for editing is pending");
	} else if (editingStatus == 'canChange') {
		addStudentSscDetails();
	} else if (editingStatus == 'ReqNotSent') {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "ssc";
						sendRequestForEditing(tab);
					}
				});
	} else if (editingStatus == 'ReqAccepted') {
		$('#btnFreeze10th').show();
		addStudentSscDetails();
	} else {
		addStudentSscDetails();
	}
}

function addStudentSscDetails() {
	var flg3 = 0;
	var tenthPercentGradeFlag = $('input[name = "tenthPercentGradeFlag"]:checked').val();
	var tenthMarks = eval($('input[name = "tenthMarks"]').val());
	var tenthOutOf = eval($('input[name = "tenthOutOf"]').val());
	var tenthPercentOrGrade = "";
	var tenthGradeConversionMethod = "";

	if (tenthPercentGradeFlag == "percent") {
		if (tenthMarks == undefined || tenthMarks == '') {
			jAlert("Please enter marks");
			flg3 = 1;
		} else if (tenthOutOf == undefined || tenthOutOf == '') {
			jAlert("Please enter Out of marks");
			flg3 = 1;
		} else {
			flg3 = 0;
		}
	} else {
		tenthPercentOrGrade = $('#txt10grade').val();
		tenthGradeConversionMethod = $(
		'input[name = "tenthGradeConversionMethod"]').val();

		if ((tenthPercentOrGrade == undefined || tenthPercentOrGrade == '') && $('#customerHelper').val()!="REVA_UNIVERSITY") {
			jAlert("Please enter grade");
			flg3 = 1;
		} else if ((tenthGradeConversionMethod == undefined || tenthGradeConversionMethod == '') && $('#customerHelper').val()!="REVA_UNIVERSITY") {
			if($('#customerHelper').val()!=undefined && !$('#customerHelper').val()=="RCOEM"){
				jAlert("Please enter conversion method");
			}else{
				jAlert("Please enter converted percentage of grade..!");
			}
			flg3 = 1;
		} else {
			flg3 = 0;
		}
	}

	if (flg3 == 0) {
		
		if ($('input[name = "tenthPercentGradeFlag"]:checked').val() == "percent") {
			tenthPercentOrGrade = eval($('#txt10percent').val());
			tenthGradeConversionMethod = "Nil";
		} else {
			tenthPercentOrGrade = $('#txt10grade').val();
			tenthGradeConversionMethod = $(
			'input[name = "tenthGradeConversionMethod"]').val();
			tenthMarks = 0;
			tenthOutOf = 0;
		}
		var tenthBoard = $('#tenthboard').val();
		var tenthYearOfPassing = $("#yrOfPassingId").val();
		var tenthSchoolType =  $("#schoolTypeTenth").val();
		
		var tenthMathematicsmarks = $("#txt10Mathematicsmarks").val();
		var tenthMathematicsoutof = $('#txt10Mathematicsoutof').val();
		var tenthSciencemarks = $('#txt10Sciencemarks').val();
		var tenthScienceoutof = $('#txt10Scienceoutof').val();
		var tenthSeatNo = $('#tenthSeatNo').val();
		
		var stateOfSchool = $('#txtstateOfSchool option:selected').val();
		var txtNoOfAttempt10th = $('#txtNoOfAttempt10th').val();
		
		if(tenthSciencemarks == undefined || tenthScienceoutof == undefined){
			tenthSciencemarks = 0;
			tenthScienceoutof = 0;
		}
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="XLRI"){
			if(eval(tenthMathematicsmarks) > eval(tenthMathematicsoutof))
			{
				jAlert("Obtained Mathematics Marks is greater than total");
				return false;
			}
		}
		
		if(eval(tenthSciencemarks) > eval(tenthScienceoutof))
		{
			jAlert("Obtained Science Marks is greater than total");
			return false;
		}
		if(tenthYearOfPassing=="0"){
			jAlert("Please Select Year of Passing");
			return false;
		}
		if($('#customerHelper').val()!="MANDSAUR" && (tenthBoard=="0")){
			jAlert("Please Select Board");
			return false;
		}
		var tenthSchoolName = $('input[name = "tenthSchoolName"]').val();
		if(tenthSchoolName==""){
			jAlert("Please Select School Name");
			return false;
		}
		var tenth_Place = $('input[name="tenth_Place"]').val();
		if(tenth_Place==""){
			jAlert("Please Select Place");
			return false;
		}
		var tenthRemarks = $('input[name="tenthRemarks"]').val();
		
		if(tenthMarks>tenthOutOf)
			{
			jAlert("Obtained Marks Cannot be Greater Than OutOf Marks");
			return false;
			}
		
		var rollNoId=$('#rollNoId').val();
		if(rollNoId=="" && $('#customerHelper').val()!="REVA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR" && $('#customerHelper').val()!="XLRI"){
			jAlert("Please Enter 10th Roll No.");
			return false;
		}
		
		if($('#customerHelper').val() == "MEGHE" && stateOfSchool == ''){
			jAlert("Please Select State of School!!!");
			return false;
		}
		if($('#customerHelper').val() == "MEGHE" && txtNoOfAttempt10th == ''){
			jAlert("Please Select State of School!!!");
			return false;
		}
		var tenthTcNumber = '';
		var tenthTcDate = '';
		if($('#customerHelper').val()=="MIT"){
			tenthTcNumber = $('#txt10TcNumber').val();
			tenthTcDate = $('#txt10TcDate').val();
		}
		$.ajax({
			url : 'studentSsc.json',
			data : {
				tenthPercentGradeFlag : tenthPercentGradeFlag,
				tenthMarks : tenthMarks,
				tenthOutOf : tenthOutOf,
				tenthPercentOrGrade : tenthPercentOrGrade,
				tenthGradeConversionMethod : tenthGradeConversionMethod,
				tenthSchoolName : tenthSchoolName,
				tenth_Place : tenth_Place,
				tenthRemarks : tenthRemarks,
				tenthBoard : tenthBoard,
				tenthMathematicsmarks:tenthMathematicsmarks,
				tenthMathematicsoutof:tenthMathematicsoutof,
				tenthSciencemarks:tenthSciencemarks,
				tenthScienceoutof:tenthScienceoutof,
				tenthYearOfPassing : tenthYearOfPassing,
				tenthSchoolType: tenthSchoolType,
				rollNoId: rollNoId,
				tenthSeatNo:tenthSeatNo,
				tenthTcNumber:tenthTcNumber,
				tenthTcDate:tenthTcDate,
				stateOfSchool:stateOfSchool,
				txtNoOfAttempt10th:txtNoOfAttempt10th
				
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				if (jArray[0].responseFlag == '1'){
					getCustomAlertMessage("alert_create");
					getstudentsscinfo();	
				}
			},
			error : function() {
				// jAlert("Something went wrong, Please try after some time","Error");
			}
		});
	}
}

//---------------------------------------------------------Function to Add Tab-4(12Th)
function checkEditStatusFor12th() {
	var editingStatus = $('#sub12th').data("editingStatus");
	if (editingStatus == 'ReqSent') {
		jAlert("The request you have sent for editing is pending");
	} else if (editingStatus == 'canChange') {
		addStudentHscDetails();
	} else if (editingStatus == 'ReqNotSent') {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "hsc";
						sendRequestForEditing(tab);
					}
				});
	} else if (editingStatus == 'ReqAccepted') {
		$('#btnFreeze12th').show();
		addStudentHscDetails();
	} else {
		addStudentHscDetails();
	}
}

//---------------------------------------------------------Function to Technical Tab
function addTechnicalDetails() {

	var category = $('input[name = "category"]').val();
	var skillName = $('input[name = "skillName"]').val();
	var proficiency = $('input[name = "proficiency"]').val();

	$.ajax({
		url : 'studentTechnical.htm',
		data : {
			category : category,
			skillName : skillName,
			proficiency : proficiency
		},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if (jArray[0].responseFlag == '1')

				showSuccessMessage(jArray[0].responseText, 5000);
			getCustomAlertMessage("alert_create");
		},
		error : function() {
			// jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}

function resetList() {
	$('#selectRight').html('');
}

var subGridIdForDiploma = '';
var superStudDiplomaId = '';
var subDiplomaEditingStatus = '';
var studProChangeReqId = '';
var statusForAddingDiploma = '';


function showAddOtherStreamFrm(selectedStreamId) {
	if (selectedStreamId == "addNew") {
		$('#frmAddOtherStreamFrm2').reset();
		$('#addOtherStreamFrm2').modal('show').css({
			width : '600px',
			'margin-left' : function() {
				return -($(this).width() / 2);
			}
		});
	}
}

function saveOtherStreamInfoInStudProfile() {

	var streamDescription = $('#otherStreamDescription').val();
	$.ajax({
		url : 'addOtherStream.json',
		data : {
			streamDescription : streamDescription
		},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			$('#addOtherStreamFrm2').modal('hide');
			if (jArray[0]["members"]['responseFlag'].value == '1') {
				showSuccessMessage("Stream Added Successfully", 5000);
				var streamName = jArray[1]['degreeStream'];
				var boiler = '<option value = "' + streamName + '">'
				+ streamName + '</option>';
				var rowId = getSelectedRowId('studUGTable');
				$('#' + rowId + '_otherStream').append(boiler);
				$('#' + rowId + '_otherStream').val(streamName);
				$('#' + rowId + '_otherStream').trigger('change');

				var rowId2 = getSelectedRowId('studDiplomaTable');
				$('#' + rowId2 + '_otherStream').append(boiler);
				$('#' + rowId2 + '_otherStream').val(streamName);
				$('#' + rowId2 + '_otherStream').trigger('change');
			}
			getCustomAlertMessage("alert_create");
		},
		error : function() {
			// //alert("in Error to add a new Stream");
		}
	});
}

function populateSelectedRowDegree3() {

	var degreeBoiler = '<option value="-1">Select</option>';
	for ( var i = 0; i < degreeBoilerArray.length; i++) {
		degreeBoiler += '<option value="' + degreeBoilerArray[i].split('-')[0]
		+ '">' + degreeBoilerArray[i].split('-')[1] + '</option>';
	}
	degreeBoiler += '<option value="addNew">Add New</option>';

	var rowId = getSelectedRowId('studDiplomaTable');
	$('#' + rowId + '_otherDegreeName').html(degreeBoiler);
}

//**************************************************************************************************************function
//for displaying students ug/pg information in grid..........

function saveOtherDegreeInfo() {
	var streamName = $('#otherStreamName').val();
	var degreeName = $('#otherDegreeName').val();
	var degreeDuration = $('#otherDegreeDuration').val();
	var degreeTypeCmb = $('#otherDegreeCmb').val();

	if ($('#otherDegreeName').val() == '') {
		jAlert("Enter degree name");
	} else if ($('#otherDegreeCmb').val() == '') {
		jAlert("Select Degree Type");

	} else {
		$
		.ajax({
			url : 'addOtherDegree.json',
			data : {
				degreeName : degreeName,
				degreeDuration : degreeDuration,
				degreeTypeCmb : degreeTypeCmb,
				streamName : streamName
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				$('#addOtherDegreeFrm2').modal('hide');
				if (jArray[0]["members"]['responseFlag'].value == '1') {
					showSuccessMessage("Degree Added Successfully",5000);
					var otherPlacementQualificationId = jArray[1]['otherPlacementQualificationId'];
					var degreeName = jArray[1]['degreeName'];
					var boiler = '<option value = "'+ otherPlacementQualificationId + '">'+ degreeName + '</option>';
					var rowId = getSelectedRowId('studUGTable');
					$('#' + rowId + '_otherDegreeName').append(boiler);
					$('#' + rowId + '_otherDegreeName').val(otherPlacementQualificationId);
					$('#' + rowId + '_otherDegreeName').trigger('change');

					var rowId2 = getSelectedRowId('studDiplomaTable');
					$('#' + rowId2 + '_otherDegreeName').append(boiler);
					$('#' + rowId2 + '_otherDegreeName').val(otherPlacementQualificationId);
					$('#' + rowId2 + '_otherDegreeName').trigger('change');
				}
				getCustomAlertMessage("alert_create");
			},
			error : function() {
				// alert("in Error to add a new Degree");
			}
		});
	}
}

function popAddOtherDegree(selectedDegreeId) {

	if (selectedDegreeId == "addNew") {
		$('#frmAddOtherDegreeFrm2').reset();
		$('#addOtherDegreeFrm2').modal('show').css({
			width : '600px',
			'margin-left' : function() {
				return -($(this).width() / 2);
			}
		});

		var rowId = getSelectedRowId('studUGTable');

		if (rowId != null) {
			var streamName = $('#' + rowId + '_otherStream').val();
			$('#otherStreamName').val(streamName);
			$('#otherStreamName').attr('disabled', 'disabled');

		}

		var rowId1 = getSelectedRowId('studDiplomaTable');

		if (rowId1 != null) {

			var streamName = $('#' + rowId1 + '_otherStream').val();
			$('#otherStreamName').val(streamName);
			$('#otherStreamName').attr('disabled', 'disabled');
		}
	}
}

function saveOtherUniInfo2() {
	var universityName = $('#otherUniversityName').val();
	var universityAbbreviation = $('#otherUniversityAbbreviation').val();
	var universityURL = $('#otherUniversityURL').val();
	if (universityName == '') {
		jAlert('Enter University name');
	} else {
		$.ajax({
			url : 'addOtherUniversity.json',
			data : {
				universityName : universityName,
				universityAbbreviation : universityAbbreviation,
				universityURL : universityURL
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {

				if (jArray[0]["members"]['responseFlag'].value == '1') {
					// alert("inside succsss");
					$('#addOtherUniversityFrmPopUp').modal('hide');

					showSuccessMessage("University Added Successfully", 2000);
					var uniId = jArray[1]['otherUniversityId'];
					var uniName = jArray[1]['uniName'];
					var boiler = '<option value = "' + uniId + '">' + uniName
					+ '</option>';
					var rowId = getSelectedRowId('studUGTable');
					var rowId2 = getSelectedRowId('studDiplomaTable');
					// alert("row Id:" +rowId);
					$('#' + rowId + '_otherUniversityName').append(boiler);
					// fillUniGrid();
					$('#' + rowId + '_otherUniversityName').val(uniId);
					$('#' + rowId + '_otherUniversityName').trigger('change');
					$('#' + rowId2 + '_otherUniversityName').append(boiler);
					// fillUniGrid();
					$('#' + rowId2 + '_otherUniversityName').val(uniId);
					$('#' + rowId2 + '_otherUniversityName').trigger('change');
					getCustomAlertMessage("alert_create");
				} else {

					jAlert("University already exist!!");
				}
			},
			error : function() {
				// alert("in Error to add a new University");
			}
		});
	}
}

function saveOtherInstitute2() {

	var otherInstName = $("#otherInstituteName").val();
	otherInstAbbr = $("#otherInstituteAbbreviation").val();
	$.ajax({
		url : 'addOtherInstituteFromStudProfile.json',
		data : {
			otherInstName : otherInstName,
			otherInstAbbr : otherInstAbbr
		},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			if (jArray[0]["members"]['responseFlag'].value == '1') {
				// alert("inside succsss");
				$('#addOtherInstitutePopUp').modal('hide');

				showSuccessMessage("Institute Added Successfully", 2000);
				var instId = jArray[1]['instituteId'];
				var instName = jArray[1]['instituteName'];
				var boiler = '<option value = "' + instId + '">' + instName
				+ '</option>';
				var rowId = getSelectedRowId('studUGTable');
				var rowId2 = getSelectedRowId('studDiplomaTable');

				$('#' + rowId + '_instituteName').append(boiler);
				// fillUGGrid();
				$('#' + rowId + '_instituteName').val(instId);
				$('#' + rowId + '_instituteName').trigger('change');
				$('#' + rowId2 + '_instituteName').append(boiler);
				// filldiplomaGrid();
				$('#' + rowId2 + '_instituteName').val(instId);
				$('#' + rowId2 + '_instituteName').trigger('change');
				getCustomAlertMessage("alert_create");
			} else {

				jAlert("Institute already exist!!");
			}
		},
		error : function() {
			// alert("in Error to add a new University");
		}
	});

}

function validSkillType(value) { // alert(value);
	if (value == -1)
		return [ false, " Please select Skill Type !!", "" ];
	else
		return [ true, "", "" ];
}

function validUni2(value) { // alert(value);
	if (value == -1)
		return [ false, " Please select University !!", "" ];
	else
		return [ true, "", "" ];
}

function validInstitute2(value) {
	if (value == -1)
		return [ false, " Please select Institute !!", "" ];
	else
		return [ true, "", "" ];
}

function validateTotalMarks(totalMarks) {

	var select = $("#studDiplomaTable").jqGrid('getGridParam', 'selrow');
	var obtainedMarks = $("#" + select + "_totalMark").val();

	if (totalMarks < obtainedMarks)
		return [ false, " Please fill Correct Marks !!", "" ];
	else
		return [ true, "", "" ];
}

function addOtherUniversity2(selectedUniversityId) {
	if (selectedUniversityId != "-1") {
		if (selectedUniversityId == "addNew") {
			showAddOtherUniversityFrm2();
		}
	}
}

function addOtherInstitute2(selectedInstituteId) {
	if (selectedInstituteId != "-1") {
		if (selectedInstituteId == "addNew") {

			showAddOtherInstituteFrm2();
		}
	}
}

