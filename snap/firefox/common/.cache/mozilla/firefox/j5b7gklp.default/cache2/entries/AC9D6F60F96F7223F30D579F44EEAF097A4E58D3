		 */
		'			</tr> '
		+ '			<tr id="finalLine" class="hideSaveSubmitButton"> '
		+ '				<td colspan="4" align="center"> '
		+ '					<div class="login" align="center"> '
		+ '						<input type="button" id="btnFreezePG" style="display: none;" class="btn btn-inverse btn-mini btnColorRed  " value="Freeze" onclick="freezeTab()"/> '
		+ '						<a href="#ug"><input type="submit" class="btn btn-inverse btn-mini btnColorGreen" value="Save" onclick="savePGDetails(\''
		+ degreeType + '\',\'NEW\')"> </a> ';
	// ' <input type="reset" class="btn btn-inverse btn-mini " value="Reset"
	// id="PG"> ';
	if (type == 'ADD') {
		studGraduationId = '';
	} else if (type == 'NEW') {
		boiler += ' or <a class="btn btn-inverse  btn-mini btnColorRed" href="#" onclick="viewPGDetails(\'' + degreeType
		+ '\')">Cancel</a>';
		studGraduationId = '';
	} else {
		boiler += ' or <a class="btn btn-inverse btn-mini btnColorRed" href="#" onclick="viewPGDetails(\'' + degreeType
		+ '\')">Cancel</a>';
		studGraduationId = studEduArr[type]["StudGraduation"];
	}
	'					</div> ' + '				</td> ' + '			</tr> ' + '		</table> ';
	$('#ug').html(boiler);
	$('.validnum').numeric();
	
	$("#txtTcDate_"+degreeType).datepicker({
	dateFormat:"M dd,yy",
	showWeek: true,
	changeMonth: true,
	changeYear: true,
	yearRange:"-60:+5",
	 onSelect: function(selected) {
	}
});
	
	$('.clsPGmarks').each(function(){
		$(this).blur(function(){
			calculateAggregate();
		});
	});
	
	$('.clsPGAggoutof').each(function(){
		$(this).blur(function(){
			calculateAggregateOutOf();
			calculateAggregatePercent();
		});
	});
	
	$('#txtPGoutof').each(function(){
		$(this).blur(function(){
			calculateAggregatePercent();
		});
	});
	
	

	getOtherUniversities(function() {
		getOtherStreams(function() {
			var currentdate = new Date();
			var curYear = (currentdate.getFullYear() + 6);
			var yearListOf20 = '<option value="0">Select</option>';

			for ( var year = curYear; year > curYear - 60; year--)
				yearListOf20 += '<option value=' + year + '>' + year
				+ '</option>';
			$('#yearPassing').html(yearListOf20);

			if (type != 'ADD' && type != 'NEW') {
				$('#university').val(studEduArr[type]["UniId"]);
				$('#txtInstInStudProUgPg').val(studEduArr[type]["InstName"]);
				$('#stream').val(studEduArr[type]["Stream"]);
				getDegreeName(degreeType);
				$('#degName').val(studEduArr[type]["DegreeId"]);
				getDegreeType();
				$('#yearPassing').val(studEduArr[type]["YearOfPass"]);
				$('#PRNNumber').val(studEduArr[type]["PrnNo"]);
				$('#atktLive').val(studEduArr[type]["ATKT"]);
				$('#atktDead').val(studEduArr[type]["DeadATKT"]);
				$('#merit').val(studEduArr[type]["remark"]);
				$('#txtLastQualifyState').val(studEduArr[type]["lastQualifyExamState"]);
				
				if($('#customerHelper').val() == "MEGHE"){
					$('#registrationNumber').val(studEduArr[type]["registrationNumber"]);
				}
				
				if (studEduArr[type]["QualiStatus"] == 'Completed')
					$('input:radio[name=qualifying]')[0].checked = true;
				else
					$('input:radio[name=qualifying]')[1].checked = true;

				if (studEduArr[type]["PercenetGradeFlg"] == 'percent') {
					$('input:radio[name=pgPercentGradeFlag]')[0].checked = true;
					$('input:radio[name=pgPercentGradeFlag]')[1].disabled = true;
					$('#txtPGpercent').val(studEduArr[type]["Percentage"]);
					$('#txtPGgrade').val(studEduArr[type]["Grade"]);
					$('#txtPGmarks').val(studEduArr[type]["totMark"]);
					$('#txtPGoutof').val(studEduArr[type]["totOutOfMark"]);
					
					$('#txtPGmarks1').val(studEduArr[type]["PGmarks1"]);
					$('#txtPGmarks2').val(studEduArr[type]["PGmarks2"]);
					$('#txtPGmarks3').val(studEduArr[type]["PGmarks3"]);
					$('#txtPGmarks4').val(studEduArr[type]["PGmarks4"]);
					$('#txtPGmarks5').val(studEduArr[type]["PGmarks5"]);
					
					$('#txtPGmarks6').val(studEduArr[type]["PGmarks6"]);
					$('#txtPGmarks7').val(studEduArr[type]["PGmarks7"]);
					$('#txtPGmarks8').val(studEduArr[type]["PGmarks8"]);
					$('#txtPGmarks9').val(studEduArr[type]["PGmarks9"]);
					$('#txtPGmarks10').val(studEduArr[type]["PGmarks10"]);
					
					$('#txtPGoutof1').val(studEduArr[type]["PGoutof1"]);
					$('#txtPGoutof2').val(studEduArr[type]["PGoutof2"]);
					$('#txtPGoutof3').val(studEduArr[type]["PGoutof3"]);
					$('#txtPGoutof4').val(studEduArr[type]["PGoutof4"]);
					$('#txtPGoutof5').val(studEduArr[type]["PGoutof5"]);
					
					$('#txtPGoutof6').val(studEduArr[type]["PGoutof6"]);
					$('#txtPGoutof7').val(studEduArr[type]["PGoutof7"]);
					$('#txtPGoutof8').val(studEduArr[type]["PGoutof8"]);
					$('#txtPGoutof9').val(studEduArr[type]["PGoutof9"]);
					$('#txtPGoutof10').val(studEduArr[type]["PGoutof10"]);
					
					
					$('#txtPGmarks').attr('disabled', false);
					$('#txtPGoutof').attr('disabled', false);
					$('#txtPGgrade').attr('disabled', false);
				} else {
					$('input:radio[name=pgPercentGradeFlag]')[1].checked = true;
					$('input:radio[name=pgPercentGradeFlag]')[0].disabled = true;
					$('#txtPGpercent').val(studEduArr[type]["Percentage"]);
					$('#txtPGgrade').val(studEduArr[type]["Grade"]);
					$('#txtPGmarks').val(studEduArr[type]["totMark"]);
					$('#txtPGoutof').val(studEduArr[type]["totOutOfMark"]);
					
					$('#txtPGmarks1').val(studEduArr[type]["PGmarks1"]);
					$('#txtPGmarks2').val(studEduArr[type]["PGmarks2"]);
					$('#txtPGmarks3').val(studEduArr[type]["PGmarks3"]);
					$('#txtPGmarks4').val(studEduArr[type]["PGmarks4"]);
					$('#txtPGmarks5').val(studEduArr[type]["PGmarks5"]);
					
					$('#txtPGoutof1').val(studEduArr[type]["PGoutof1"]);
					$('#txtPGoutof2').val(studEduArr[type]["PGoutof2"]);
					$('#txtPGoutof3').val(studEduArr[type]["PGoutof3"]);
					$('#txtPGoutof4').val(studEduArr[type]["PGoutof4"]);
					$('#txtPGoutof5').val(studEduArr[type]["PGoutof5"]);
					
					$('#txtPGmarks').attr('disabled', false);
					$('#txtPGoutof').attr('disabled', false);
					$('#txtPGgrade').attr('disabled', false);
				}
				$("[name=pgPercentGradeFlag]").attr('checked', true).trigger('click');
				if($('#customerHelper').val() == "MIT"){
					$('#txtTcNumber_'+degreeType).val(studEduArr[type]["tcNumber"]);
					$('#txtTcDate_'+degreeType).val(studEduArr[type]["tcDate"]);
				}
			} else {
					$('input:radio[name=pgPercentGradeFlag]')[1].disabled = true;
					$('input:radio[name=pgPercentGradeFlag]')[0].checked = true;
					$('#optgradePG').attr('disabled',false);
			}
		});

	});

	$('#degName').click(function() {
		var streamName = $('#stream :selected').text();
		if (streamName == 'Select' || streamName == '') {
			$('#strError').show('slow');
		} else {
			$('#strError').hide('slow');
		}
	});
		
}

function calculateAggregate(){
	
	var totalAgg = 0.0;
	
	$('.clsPGmarks').each(function(){
		
	if(!isNaN(this.value) && this.value.length !=0){
	totalAgg+= parseFloat(this.value);
	}
	
	});
	$('#txtPGmarks').val(totalAgg.toFixed(2));
	
}
function calculateAggregateOutOf(){
	
	var totalOutOf = 0.0;
	$('.clsPGAggoutof').each(function(){
		if(!isNaN(this.value) && this.value.length !=0){
		totalOutOf+= parseFloat(this.value);
		}
	});
	$('#txtPGoutof').val(totalOutOf.toFixed(2));
	
}

function calculateAggregatePercent(){
	var totalAgg = $('#txtPGmarks').val();
	var totalOutOf =$('#txtPGoutof').val();
	var totPercent = 0.0;
	$('#txtPGoutof').each(function(){
		if(!isNaN(this.value) && this.value.length !=0){
			totPercent = (totalAgg / totalOutOf)*100;
		}
	});
	$('#txtPGpercent').val(totPercent.toFixed(2));
	
}



function savePGDetails(degreeType, inserttype) {

	if (statusForAddingUGPG == "canChange") {
		savePGDetailsConfirm(degreeType, inserttype);
	} else if (statusForAddingUGPG == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingUGPG == "ReqAccepted") {
		$('#btnFreeze10th').show();
		savePGDetailsConfirm(degreeType, inserttype);
	} else if (statusForAddingUGPG == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "";

						if (degreeType == "UG")
							tab = "UG";
						else if (degreeType == "PG")
							tab = "PG";
						else
							tab = "diploma";

						sendRequestForEditing(tab);
					}
				});
	}

}
function postGraduationDetails(type, degreeType,StudGraduation) {
	
	if (statusForAddingUGPG == "canChange") {
		postGraduationDetailsPart2(type, degreeType,StudGraduation);
	} else if (statusForAddingUGPG == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingUGPG == "ReqAccepted") {
		postGraduationDetailsPart2(type, degreeType,StudGraduation);
	} else if (statusForAddingUGPG == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "";

						if (degreeType == "UG")
							tab = "UG";
						else if (degreeType == "PG")
							tab = "PG";
						else
							tab = "diploma";

						sendRequestForEditing(tab);
					}
				});
	}
	getstudProfileConfigurationDetailsCheckingNames();
}

var degTypeForAddNewDegInpopup = '';
var degNameListForSerching = new Array();

function getDegreeName(degreeType) {
	degTypeForAddNewDegInpopup = degreeType;
	if (degreeType == 'PG'){
		degreeType = "'COURSE_TYPE_PG'";
	}else if (degreeType == 'UG'){
		degreeType = "'COURSE_TYPE_UG'";
	}else if (degreeType == 'DIP'){
		degreeType = "'COURSE_TYPE_Diploma','COURSE_TYPE_PG_Diploma','COURSE_TYPE_POST_PG'";
	}

	var streamName = $('#stream :selected').val();
	$.ajax({
		url : 'getOtherDegreeForStudent.json',
		data : {
			streamName:streamName,
			degreeType:degreeType
		},
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var boiler = '<option value="0">Select</option>';
			degNameListForSerching = new Array();
			for ( var i = 0; i < jArray.length; i++) {
				boiler += '<option id="'+ jArray[i]["otherPlacementQualificationId"]+ '" value="'+ jArray[i]["otherPlacementQualificationId"]+ '" >' + jArray[i]["degreeName"]+ '</option>';
				degNameListForSerching.push({name : jArray[i]["degreeName"],to : jArray[i]["otherPlacementQualificationId"]});
				/*if (degreeType != 'DIP') {
							if (jArray[i]["degreeType"] == degreeType) {
								boiler += '<option id="'+ jArray[i]["otherPlacementQualificationId"]+ '" value="'+ jArray[i]["otherPlacementQualificationId"]+ '" >' + jArray[i]["degreeName"]+ '</option>';
								degNameListForSerching.push({name : jArray[i]["degreeName"],to : jArray[i]["otherPlacementQualificationId"]});
							}
						} else {
							if (jArray[i]["degreeType"] == 'COURSE_TYPE_Diploma'|| jArray[i]["degreeType"] == 'COURSE_TYPE_PG_Diploma' || jArray[i]["degreeType"] == 'COURSE_TYPE_POST_PG') {
								boiler += '<option id="'+ jArray[i]["otherPlacementQualificationId"]+ '" value="'+ jArray[i]["otherPlacementQualificationId"]+ '" >' + jArray[i]["degreeName"]+ '</option>';
								degNameListForSerching.push({name : jArray[i]["degreeName"],to : jArray[i]["otherPlacementQualificationId"]});
							}
						}*/
			}
			$('#degName').html(boiler);
		},
		error : function() {
			// alert("in Error stream CMB");
		}
	});
}

var degIdFromPopUp = '';
setTimeout(function() {
	$('#otherDegreeNameForOther').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {
					$("#otherDegreeNameForOther").autocomplete(
							degNameListForSerching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("
											+ term + ")", "gi"),
									"<strong>$1</strong>");

								},
								formatResult : function(row) {
									degIdFromPopUp = row.to;
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {
					degIdFromPopUp = $('#otherDegreeNameForOther').val();
					$('#addOtherDegreeFrm').modal('hide');
					$('#degName').val(degIdFromPopUp);
				}

			});
}, 1000);

function savePGDetailsConfirm(degreeType, inserttype) {
	var university = $('#university :selected').val();
	var degName = $('#degName :selected').val();
	var passingYear = $('#yearPassing :selected').val();
	var PRNNumber = $('#PRNNumber').val();
	var liveATKT = $('#atktLive').val();
	var deadATKT = $('#atktDead').val();
	var txtInstitute = $('#txtInstInStudProUgPg').val();
	var txtLastQualifyState = $('#txtLastQualifyState').val();	
	
	var TcNumber = "";
	var TcDate = "";
	if($('#customerHelper').val() == "MIT"){
		TcNumber = $('#txtTcNumber_'+degreeType).val();
		TcDate = $('#txtTcDate_'+degreeType).val();
	}

	if (parseInt(liveATKT) > 99 || parseInt(deadATKT) > 99) {
		jAlert("ATKT cannot be greater than 99");
		return;
	}
	if (liveATKT == "")
		liveATKT = 0;

	if (deadATKT == "")
		deadATKT = 0;
	var merit = $('#merit').val();
	var qualifying = $('input[name = "qualifying"]:checked').val();

	var pgPercentGradeFlag = $('input[name = "pgPercentGradeFlag"]:checked')
	.val();
	
	var registrationNumber = $('#registrationNumber').val();
	
	
	/*var PGMarks = $('input[name = "PGMarks"]').val();
	var PGOutOf = $('input[name = "PGOutOf"]').val();*/
	var PGMarks = $('#txtPGmarks').val();
	var PGOutOf = $('#txtPGoutof').val();
	var PGPercent = $('#txtPGpercent').val();
	var PGGrade = $('#txtPGgrade').val();
	
	var PGmarks1 = $('#txtPGmarks1').val();
	var PGmarks2 = $('#txtPGmarks2').val();
	var PGmarks3 = $('#txtPGmarks3').val();
	var PGmarks4 = $('#txtPGmarks4').val();
	var PGmarks5 = $('#txtPGmarks5').val();
	
	var PGmarks6 = $('#txtPGmarks6').val();
	var PGmarks7 = $('#txtPGmarks7').val();
	var PGmarks8 = $('#txtPGmarks8').val();
	var PGmarks9 = $('#txtPGmarks9').val();
	var PGmarks10 = $('#txtPGmarks10').val();
	
	var PGoutof1 = $('#txtPGoutof1').val();
	var PGoutof2 = $('#txtPGoutof2').val();
	var PGoutof3 = $('#txtPGoutof3').val();
	var PGoutof4 = $('#txtPGoutof4').val();
	var PGoutof5 = $('#txtPGoutof5').val();
	
	var PGoutof6 = $('#txtPGoutof6').val();
	var PGoutof7 = $('#txtPGoutof7').val();
	var PGoutof8 = $('#txtPGoutof8').val();
	var PGoutof9 = $('#txtPGoutof9').val();
	var PGoutof10 = $('#txtPGoutof10').val();
	
	var query = 'otherUniversityName=' + university + '&qualificationStatus='
	+ qualifying + '&instituteName=' + txtInstitute
	+ '&otherDegreeName=' + degName + '&yearOfPassing=' + passingYear
	+ '&PRNNo=' + PRNNumber + '&ATKT=' + liveATKT + '&deadATKT='
	+ deadATKT + '&Remark=' + merit;
	query += '&inserttype=' + inserttype + '&degreeType=' + degreeType
	+␀␀␀ '&pgPercentGradeFlag=' + pgPercentGradeFlag + '&totalMark='
	+ PGMarks + '&totalOutOFMark=' + PGOutOf + '&grade=' + PGGrade
	+ '&percentage=' + PGPercent+
	'&PGmarks1='+PGmarks1+'&PGmarks2='+PGmarks2+'&PGmarks3='+PGmarks3+'&PGmarks4='+PGmarks4+'&PGmarks5='+PGmarks5+'&PGmarks6='+PGmarks6+'&PGmarks7='+PGmarks7+'&PGmarks8='+PGmarks8+'&PGmarks9='+PGmarks9+'&PGmarks10='+PGmarks10+
	'&PGoutof1='+PGoutof1+'&PGoutof2='+PGoutof2+'&PGoutof3='+PGoutof3+'&PGoutof4='+PGoutof4+'&PGoutof5='+PGoutof5+'&PGoutof6='+PGoutof6+'&PGoutof7='+PGoutof7+'&PGoutof8='+PGoutof8+'&PGoutof9='+PGoutof9+'&PGoutof10='+PGoutof10;// +'&gradePercentage='+PGGradeConversionMethod;
	if (studGraduationId != '') {
		query += '&studGraduationId=' + studGraduationId;
	}
	if (registrationNumber != '' && registrationNumber !=undefined && registrationNumber !=null) {
		query += '&registrationNumber=' + registrationNumber;
	}
	query +='&TcNumber=' + TcNumber + '&TcDate=' + TcDate + '&txtLastQualifyState=' + txtLastQualifyState;
	if (university == 0 || university =="0" || university == undefined || university == "undefined") {
		$('#uniError').show('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if ($('#txtInstInStudProUgPg').val() == "") {
		$('#instError').show('slow');
		$('#uniError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if ($('#stream :selected').val() == 0
			|| $('#stream :selected').val() == undefined) {
		$('#strError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if (degName == 0 || degName == undefined || degName == "undefined") {
		$('#degError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if (qualifying == undefined) {
		$('#qualError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if (passingYear == 0) {
		$('#yopError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if ($('#PRNNumber').val() == "") {
		$('#prnError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if ($('#atktLive').val() == "") {
		$('#lktError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#dktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if ($('#atktDead').val() == "") {
		$('#dktError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#mksError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if (pgPercentGradeFlag == undefined) {
		$('#mksError').show('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
		$('#percError').hide('slow');
	} else if (($('#txtPGmarks').val() == "") && ($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="XLRI")){
			$('#totError').show('slow');
			$('#mksError').hide('slow');
			$('#uniError').hide('slow');
			$('#instError').hide('slow');
			$('#strError').hide('slow');
			$('#degError').hide('slow');
			$('#qualError').hide('slow');
			$('#yopError').hide('slow');
			$('#prnError').hide('slow');
			$('#lktError').hide('slow');
			$('#dktError').hide('slow');
			$('#outError').hide('slow');
			$('#percError').hide('slow');
	} else if (($('#txtPGoutof').val() == "") && ($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="XLRI")) {
			$('#outError').show('slow');
			$('#mksError').hide('slow');
			$('#uniError').hide('slow');
			$('#instError').hide('slow');
			$('#strError').hide('slow');
			$('#degError').hide('slow');
			$('#qualError').hide('slow');
			$('#yopError').hide('slow');
			$('#prnError').hide('slow');
			$('#lktError').hide('slow');
			$('#dktError').hide('slow');
			$('#totError').hide('slow');
			$('#percError').hide('slow');
	} else if (($('#txtPGpercent').val() == "") && ($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="XLRI")) {
		$('#percError').show('slow');
		$('#mksError').hide('slow');
		$('#uniError').hide('slow');
		$('#instError').hide('slow');
		$('#strError').hide('slow');
		$('#degError').hide('slow');
		$('#qualError').hide('slow');
		$('#yopError').hide('slow');
		$('#prnError').hide('slow');
		$('#lktError').hide('slow');
		$('#dktError').hide('slow');
		$('#totError').hide('slow');
		$('#outError').hide('slow');
	} else {
		$.ajax({
		
			url : 'addUGDegreeInStudProfile.json',
			dataType : 'json',
			data : query,
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function() {
				getCustomAlertMessage("alert_create");
				viewPGDetails(degreeType);
				if (inserttype == 'NEW' && degreeType == 'UG')
					checkOtherQualificationStatus();
			},
			error : function() {
				// alert("in Error stream CMB");
			}
		});
	}

}

function checkOtherQualificationStatus() {
	$
	.ajax({
		url : 'checkOtherQualificationStatusInStudentGraduation.json', // PlacementCell
		// Controler
		dataType : 'json',
		type : 'GET',
		async : false,
		success : function(jArray) {
			var graduationArray = new Array();
			var otherQualiArray = new Array();
			graduationArray = jArray[0]["GraduationDetai"];
			otherQualiArray = jArray[0]["OtherQualificationArray"];
			if (jArray.length > 0) {
				if (otherQualiArray.length > 0) {
					if (graduationArray.length > 0) {
						$('#addOtherQualificationForPlacementDiv')
						.modal('show').css({
							width : '700px',
							'margin-left' : function() {
								return -($(this).width() / 2);
							}
						});

						var boiler = '<table class="table table-bordered">'
							+ '<tr>'
							+ '<th>Current Qualification :</th>'
							+ '<td>'
							+ jArray[0]["currentQuali"]
						+ '</td>'
						+ '</tr>'
						+ '<tr>'
						+ '<td colspan="2" id="qualiTD"></td>'
						+ '</tr>' + '</table>';

						$('#addOtherQualificationForPlacementBody')
						.html(boiler);
						var innerBoiler = '<table class="table table-bordered">';
						for ( var i = 0; i < graduationArray.length; i++) {
							innerBoiler += '<tr id="'
								+ graduationArray[i]["studentGraduationId"]
							+ '" class="graduationIdTrcls" >'
							+ '<td>Select your equivalent to ('
							+ graduationArray[i]["degreeName"]
							+ ') graduation/PG/diploma qualification from the following</td>'
							+ '</tr>'
							+ '<tr>'
							+ '<td><div class="listWrapper" id="div_'
							+ graduationArray[i]["studentGraduationId"]
							+ '" ><ul style="list-style: none;">';
							for ( var j = 0; j < otherQualiArray.length; j++) {
								if (otherQualiArray[j]["DegreeName"] != "Any Degree") {
									innerBoiler += '<li><input type="radio" class="rdoOtherQuali_'
										+ graduationArray[i]["studentGraduationId"]
									+ '" name="rdoOtherQuali_'
									+ graduationArray[i]["studentGraduationId"]
									+ '"  value="'
									+ graduationArray[i]["studentGraduationId"]
									+ '-'
									+ otherQualiArray[j]["otherQualificationId"]
									+ '">'
									+ otherQualiArray[j]["DegreeName"]
									+ '</li>';
								}
							}
							innerBoiler += '</ul></div></td>' + '</tr>';
						}
						innerBoiler += '</table>'
							+ '</br>'
							+ '<center><input type="button" value="Save" class="btn btn-inverse btnColorGreen" onclick="saveStudOtherQualificationForPlacement()" /></center>';
						$('#qualiTD').html(innerBoiler);
					}
				}

			}
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

var studEduArr = new Array();
var degType = '';
function viewPGDetails(degreeType,graduationId) {
	degType = degreeType;
	var boiler = '<table class="table table-bordered table-condensed">'
		+ '<tr><th style="color: #0088CC" colspan="8"><label><b><font>Student Graduation Details</font></b></label></th></tr>';
	if ($('#degType').val() == degreeType) {
		$
		.ajax({
			url : 'getStudentCurrentQualification.json',
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				if (jArray.length > 0) {
					for ( var i = 0; i < jArray.length; i++) {
						studEduArr = jArray;
						boiler += '<tr>'
							+ '<td style="background-color: black;">';
						boiler += '<blink><label style="color: #00FF00"><b>Current Qualification</b></label></blink>';

						boiler += '</td>' + '<td colspan="7">';
						if (i == 0) {
							boiler += '<span style="float:right;"><a  class="btn btn-inverse btn-mini btnColorBlack  freezeProfile digBtn hideSaveSubmitButton"  onclick="postGraduationDetails(\'NEW\',\''+ degreeType+ '\')" ><i class="fa fa-plus"></i>Add Degree</a></span>'+
							'<span><input type="button" id="btnFrezeInStudQuali" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span>';
						}
						boiler += '</td>' + '</tr>' + '<tr>'
						+ '<th><b> Degree Name</b></th>'
						+ '<th><b> '+juno.labelByCustomer('l.SchoolInstitute')+' </b></th>'
						+ '<th><b> University</b></th>'
						+ '<th><b> Aggregate Percentage</b></th>'
						+ '<th><b> Grade</b></th>'
						+ '<th colspan="3"><b></b></th>'
						+ '</tr>' + '<tr id="otherDegreeTr">'
						+ '<td><label>'+ jArray[i]["degreeName"]+ '</label></td>' + '<td><label>'
						+ jArray[i]["instName"]
						+ '</label></td>' + '<td><label>'
						+ jArray[i]["universityName"]
						+ '</label></td>';

						boiler += '<td style="text-align:center;"><label> Pursuing</label></td>';
						boiler += '<td style="text-align:center;"><label> Pursuing</label></td>';

						
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DES"){
							boiler += '<td colspan="2" style="display:none"><a onclick="getStudentCurrentQualification(\''+ degreeType+ '\')" class="pointer  freezeProfile hideSaveSubmitButton" href="javascript:void(0)" ><i class="fa fa-pencil-square-o  iconColorBlack "></i> View</td>';
						}else{
							boiler += '<td colspan="2" style="width:8%"><a onclick="assignValue();getStudentCurrentQualification(\''+ degreeType+ '\')" class="pointer  freezeProfile hideSaveSubmitButton" href="javascript:void(0)" ><i class="fa fa-pencil-square-o iconColorBlack "></i> Edit <br><a onclick="getStudentCurrentQualificationNew(\''+ degreeType+ '\')" class="pointer" href="javascript:void(0)" ><i class="fa fa-eye iconColorBlack"></i> View</td>';	
						}
						

						if ($('#degType').val() == "UG") {
							boiler += '<td><a  class="freezeProfile hideSaveSubmitButton" onclick = "getDegreeCategoryId(\''
								+ gTabDegreeName.TabName_UG
								+ '\',\''
								+ jArray[i]["studentPlacementDetailId"]
							+ '\',\'current\',\''
							+ jArray[i]["studentType"]
							+ '\');" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack "></i>Upload</td>';
						} else if ($('#degType').val() == 'PG') {
							boiler += '<td><a class="freezeProfile hideSaveSubmitButton" onclick = "getDegreeCategoryId(\''
								+ gTabDegreeName.TabName_PG
								+ '\',\''
								+ jArray[i]["studentPlacementDetailId"]
							+ '\',\'current\',\''
							+ jArray[i]["studentType"]
							+ '\');" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack "></i>Upload</td>';
						} else if ($('#degType').val() == 'Diploma') {
							boiler += '<td><a class="freezeProfile hideSaveSubmitButton" onclick = "getDegreeCategoryId(\''
								+ gTabDegreeName.TabName_DIPLOMA
								+ '\',\''
								+ jArray[i]["studentPlacementDetailId"]
							+ '\',\'current\',\''
							+ jArray[i]["studentType"]
							+ '\');" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
						}

						boiler += '</tr>';
						
						/*boiler +=	'<tr>'+
						'<th colspan="2">1st Year Marks</th>'+
						'<td style="text-align:center;"><label id="marks1">'+jArray[i]["PGmarks1"]+'</label></td>'+
						'<th colspan="2">Out Of</th>'+
						'<td colspan="3" style="text-align:center;"><label id="outof1">'+jArray[i]["PGoutof1"]+'</label></td>'+
					'</tr>'+
					'<tr>'+
						'<th colspan="2">2nd Year Marks</th>'+
						'<td style="text-align:center;"><label id="marks2">'+jArray[i]["PGmarks2"]+'</label></td>'+
						'<th colspan="2">Out Of</th>'+
						'<td colspan="3" style="text-align:center;"><label id="outof2">'+jArray[i]["PGoutof2"]+'</label></td>'+
					'</tr>'+
					'<tr>'+
						'<th colspan="2">3rd Year Marks</th>'+
						'<td style="text-align:center;"><label id="marks3">'+jArray[i]["PGmarks3"]+'</label></td>'+
						'<th colspan="2">Out Of</th>'+
						'<td colspan="3" style="text-align:center;"><label id="outof3">'+jArray[i]["PGoutof3"]+'</label></td>'+
					'</tr>'+
					'<tr>'+
						'<th colspan="2">4th Year Marks</th>'+
						'<td style="text-align:center;"><label id="marks4">'+jArray[i]["PGmarks4"]+'</label></td>'+
						'<th colspan="2">Out Of</th>'+
						'<td colspan="3" style="text-align:center;"><label id="outof4">'+jArray[i]["PGoutof4"]+'</label></td>'+
					'</tr>'+
					'<tr>'+
						'<th colspan="2">5th Year Marks</th>'+
						'<td style="text-align:center;"><label id="marks5">'+jArray[i]["PGmarks5"]+'</label></td>'+
						'<th colspan="2">Out Of</th>'+
						'<td colspan="3" style="text-align:center;"><label id="outof5">'+jArray[i]["PGoutof5"]+'</label></td>'+
					'</tr>';*/
						
					}
					$('#ug').html(boiler);
				} else {
					postGraduationDetailsPart2('NEW', 'PG');
				}
				getstudProfileConfigurationDetailsCheckingNames();
			},
			error : function() {
				// alert("in Error stream CMB");
			}
		});
	} else {
		$('#ug').html(boiler);
	}

	$
	.ajax({
		url : 'getstudUGinfo.json?gridDegreeType=' + degreeType,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if(jArray.length>0)
			{
			var flag = 0;
			if (jArray[0]["1stTimeAddStatus"] == "inPlaceLast") {

				statusForAddingUGPG = jArray[0]["editingStatus"];
				studProChangeReqId = jArray[0]["studProReqId"];

				if (jArray[0]["StudGraduation"] != undefined) {
					for ( var i = 0; i < jArray.length; i++) {
						flag = 1;
						studEduArr = jArray;
						boiler += '<tr id="graduationTr'+i+'" value="'+jArray[i]["StudGraduation"]+'">'
							+ '<td style="background-color: black;">';
						if (jArray[i]["QualiStatus"] != 'Completed')
							boiler += '<blink><label style="color: #00FF00"><b>Pursuing</b></label></blink>';
						else
							boiler += '<label style="color: #ffffff"><b>'
								+ jArray[i]["YearOfPass"]
						+ '</b></label>';

						boiler += '</td>' + '<td colspan="7">';
						if (i == 0 && $('#degType').val() != degreeType) {
							boiler += '<span  style="float:right;"><button class="btn btn-inverse btn-mini  btnColorBlack digBtn  freezeProfile hideSaveSubmitButton" onclick="postGraduationDetails(\'NEW\',\''
								+ degreeType
								+ '\')" ><i class="fa fa-plus"></i>Add Degree</button></span>'
								+ '<span><input type="button" id="btnFrezeInStudQuali" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed  freezeProfile hideSaveSubmitButton" value="Freeze" onclick="freezeTab()"/></span>';
						}
						boiler += '</td>' + '</tr>' + '<tr>'
						+ '<th><b> Degree Name</b></th>'
						+ '<th><b> '+juno.labelByCustomer('l.SchoolInstitute')+'</b></th>'
						+ '<th><b> University</b></th>'
						+ '<th><b> Aggregate Percentage</b></th>'
						+ '<th><b> Grade</b></th>'
						+ '<th colspan="3"><b></b></th>'
						+ '</tr>' + '<tr>' + '<td><label>'
						+ jArray[i]["DegreeName"]
						+ '</label></td>'
						+ '<td><label>'
						+ jArray[i]["InstName"]
						+ '</label></td>'
						+ '<td><label>'
						+ jArray[i]["UniName"]
						+ '</label></td>'
						+ '<td style="text-align:center;"><label>'
						+ (jArray[i]["Percentage"]!=undefined && jArray[i]["Percentage"]!=0 ?jArray[i]["Percentage"]:"-")
						+ '</label></td>';
						if(jArray[i]["Grade"]!=undefined) {
						boiler += '<td><label style="text-align:center;">'
						+ jArray[i]["Grade"]
						+ '</label></td>';
						console.log(jArray[i]["Percentage"]);
						}else {
							boiler += '<td><label style="text-align:center;">-</label></td>';	
						}
						boiler += '<td><a class="freezeProfile digBtn hideSaveSubmitButton"  onclick="postGraduationDetails(\''
						+ i
						+ '\',\''
						+ degreeType
						+ '\','+jArray[i]["StudGraduation"]+')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</a></td>'
						+ '<td><a  class="freezeProfile digBtn hideSaveSubmitButton" onclick="delPostGraduationDetails(\''
						+ jArray[i]["StudGraduation"]
						+ '\',\''
						+ degreeType
						+ '\')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</a></td>';

						if (degreeType == "UG") {
							boiler += '<td><a class="freezeProfile hideSaveSubmitButton" onclick = "getDegreeCategoryId(\''
								+ gTabDegreeName.TabName_UG
								+ '\',\''
								+ jArray[i]["StudGraduation"]
							+ '\',\''
							+ jArray[i]["studentType"]
							+ '\');" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</a></td>';
						} else if (degreeType == 'PG') {
							boiler += '<td><a class="freezeProfile hideSaveSubmitButton" onclick = "getDegreeCategoryId(\''
								+ gTabDegreeName.TabName_PG
								+ '\',\''
								+ jArray[i]["StudGraduation"]
							+ '\',\''
							+ jArray[i]["studentType"]
							+ '\');" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</a></td>';
						} else if (degreeType == 'DIP') {
							boiler += '<td><a class="freezeProfile hideSaveSubmitButton"  onclick = "getDegreeCategoryId(\''
								+ gTabDegreeName.TabName_DIPLOMA
								+ '\',\''
								+ jArray[i]["StudGraduation"]
							+ '\',\''
							+ jArray[i]["studentType"]
							+ '\');" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</a></td>';
						}
						boiler += '</tr>';
					if(jArray[i]["PercenetGradeFlg"]!='grade'){
						
						var outOfT1 = jArray[i]["PGoutof1"]!='undefined'?jArray[i]["PGoutof1"]:"";
						var outOfT2 = jArray[i]["PGoutof2"]!='undefined'?jArray[i]["PGoutof2"]:"";
						var outOfT3 = jArray[i]["PGoutof3"]!='undefined'?jArray[i]["PGoutof3"]:"";
						var outOfT4 = jArray[i]["PGoutof4"]!='undefined'?jArray[i]["PGoutof4"]:"";
						var outOfT5 = jArray[i]["PGoutof5"]!='undefined'?jArray[i]["PGoutof5"]:"";
						var outOfT6 = jArray[i]["PGoutof6"]!='undefined'?jArray[i]["PGoutof6"]:"";
						var outOfT7 = jArray[i]["PGoutof7"]!='undefined'?jArray[i]["PGoutof7"]:"";
						var outOfT8 = jArray[i]["PGoutof8"]!='undefined'?jArray[i]["PGoutof8"]:"";
						var outOfT9 = jArray[i]["PGoutof9"]!='undefined'?jArray[i]["PGoutof9"]:"";
						var outOfT10 = jArray[i]["PGoutof10"]!='undefined'?jArray[i]["PGoutof10"]:"";
						
						if(outOfT1!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 1</th>'+
								'<td style="text-align:center;"><label id="marks1">'+(jArray[i]["PGmarks1"]!='undefined'?jArray[i]["PGmarks1"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof1">'+outOfT1+'</label></td>'+
							'</tr>';
						}
						if(outOfT2!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 2</th>'+
								'<td style="text-align:center;"><label id="marks2">'+(jArray[i]["PGmarks2"]!='undefined'?jArray[i]["PGmarks2"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof2">'+outOfT2+'</label></td>'+
							'</tr>';
						}
						if(outOfT3!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 3</th>'+
								'<td style="text-align:center;"><label id="marks3">'+(jArray[i]["PGmarks3"]!='undefined'?jArray[i]["PGmarks3"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof3">'+outOfT3+'</label></td>'+
							'</tr>';
						}
						if(outOfT4!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 4</th>'+
								'<td style="text-align:center;"><label id="marks4">'+(jArray[i]["PGmarks4"]!='undefined'?jArray[i]["PGmarks4"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof4">'+outOfT4+'</label></td>'+
							'</tr>';
						}
						if(outOfT5!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 5</th>'+
								'<td style="text-align:center;"><label id="marks5">'+(jArray[i]["PGmarks5"]!='undefined'?jArray[i]["PGmarks5"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof5">'+outOfT5+'</label></td>'+
							'</tr>';
						}
						if(outOfT6!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 6</th>'+
								'<td style="text-align:center;"><label id="marks6">'+(jArray[i]["PGmarks6"]!='undefined'?jArray[i]["PGmarks6"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof6">'+outOfT6+'</label></td>'+
							'</tr>';
						}
						if(outOfT7!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 7</th>'+
								'<td style="text-align:center;"><label id="marks7">'+(jArray[i]["PGmarks7"]!='undefined'?jArray[i]["PGmarks7"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof7">'+outOfT7+'</label></td>'+
							'</tr>';
						}
						if(outOfT8!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 8</th>'+
								'<td style="text-align:center;"><label id="marks8">'+(jArray[i]["PGmarks8"]!='undefined'?jArray[i]["PGmarks8"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof8">'+outOfT8+'</label></td>'+
							'</tr>';
						}
						if(outOfT9!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 9</th>'+
								'<td style="text-align:center;"><label id="marks9">'+(jArray[i]["PGmarks9"]!='undefined'?jArray[i]["PGmarks9"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof9">'+outOfT9+'</label></td>'+
							'</tr>';
						}
						if(outOfT10!=""){	
							boiler +=	'<tr>'+
								'<th colspan="2">Term 10</th>'+
								'<td style="text-align:center;"><label id="marks10">'+(jArray[i]["PGmarks10"]!='undefined'?jArray[i]["PGmarks10"]:"")+'</label></td>'+
								'<th colspan="2">Out Of</th>'+
								'<td colspan="3" style="text-align:center;"><label id="outof10">'+outOfT10+'</label></td>'+
							'</tr>';
						}
					}
						
					}
					boiler += '</table>';
					$('#ug').html(boiler);
				} else {
					postGraduationDetailsPart2('ADD', degreeType);
				}
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1st") {
				statusForAddingUGPG = "ReqNotSent";
				if ($('#degType').val() != degreeType)
					postGraduationDetailsPart2('ADD', degreeType);
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlaceCanChange") {
				statusForAddingUGPG = "canChange";
				if ($('#degType').val() != degreeType)
					postGraduationDetailsPart2('ADD', degreeType);
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1stReqSent") {
				statusForAddingUGPG = jArray[0]["editingStatus"];
				if ($('#degType').val() != degreeType)
					postGraduationDetailsPart2('ADD', degreeType);
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlacecheckByEditingStatus") {
				statusForAddingUGPG = jArray[0]["editingStatus"];
				if ($('#degType').val() != degreeType)
					postGraduationDetailsPart2('ADD', degreeType);
			}

			if (statusForAddingUGPG == 'ReqNotSent') {
				$('#btnFrezeInStudQuali').hide();
				$('#trSendReq').hide();
				$('#trReqAccepted').hide();
				$('#trReqPending').hide();
			} else if (statusForAddingUGPG == 'ReqAccepted') {
				$('#btnFrezeInStudQuali').show();
				$('#trReqAccepted').show();
				$('#trSendReq').hide();
				$('#trReqPending').hide();
			} else if (statusForAddingUGPG == 'ReqSent') {
				$('#trReqPending').show();
				$('#trReqAccepted').hide();
				$('#trSendReq').hide();
			} else if (statusForAddingUGPG == 'canChange') {
				$('#trReqPending').hide();
				$('#trReqAccepted').hide();
				$('#trSendReq').hide();
			}
			}
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			// alert("in Error stream CMB");
		}
	});
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		console.log("jhih");
			if(g_freeze!=undefined && g_freeze)
				{
				disabledAll();
				}
			}
}
function assignValue(){
	statusForAddingUGPG = "canChange";
}
function delPostGraduationDetailsLast(gradId, degreeType) {
	jConfirm("Do you want to delete? ", "Confirm", function(r) {
		if (r == true) {
			$.ajax({
				url : 'delDegreeDetails.json?gradId=' + gradId,
				dataType : 'text',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(text) {
					getCustomAlertMessage("alert_delete");
					viewPGDetails(degreeType);
				},
				error : function() {
					// jAlert("Something went wrong, Please try after some time","Error");
				}
			});
		}
	});
}
function getStudentCurrentQualification(degreeType) {
	if (statusForAddingUGPG == "canChange") {
		getStudentCurrentQualificationConfirm(degreeType);
	} else if (statusForAddingUGPG == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingUGPG == "ReqAccepted") {
		$('#btnFreeze10th').show();
		getStudentCurrentQualificationConfirm(degreeType);
	} else if (statusForAddingUGPG == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "";

						if (degreeType == "UG")
							tab = "UG";
						else if (degreeType == "PG")
							tab = "PG";
						else
							tab = "diploma";

						sendRequestForEditing(tab);
					}
				});
	}
}

function getStudentCurrentQualificationNew(degreeType) {
	if ("canChange" == "canChange") {
		getStudentCurrentQualificationConfirmToView(degreeType);
	} else if (statusForAddingUGPG == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingUGPG == "ReqAccepted") {
		$('#btnFreeze10th').show();
		getStudentCurrentQualificationConfirm(degreeType);
	} else if (statusForAddingUGPG == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "";

						if (degreeType == "UG")
							tab = "UG";
						else if (degreeType == "PG")
							tab = "PG";
						else
							tab = "diploma";

						sendRequestForEditing(tab);
					}
				});
	}
}

function delPostGraduationDetails(gradId, degreeType) {

	if (statusForAddingUGPG == "canChange") {
		delPostGraduationDetailsLast(gradId, degreeType);
	} else if (statusForAddingUGPG == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingUGPG == "ReqAccepted") {
		$('#btnFreeze10th').show();
		delPostGraduationDetailsLast(gradId, degreeType);
	} else if (statusForAddingUGPG == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "";

						if (degreeType == "UG")
							tab = "UG";
						else if (degreeType == "PG")
							tab = "PG";
						else
							tab = "diploma";

						sendRequestForEditing(tab);
					}
				});
	}
}

function addStudentHscDetails() {

	var flg6 = 0;

	var twelthPercentGradeFlag = $(
	'input[name = "twelthPercentGradeFlag"]:checked').val();
	var twelthMarks = $('input[name = "twelthMarks"]').val();
	var twelthOutOf = $('input[name = "twelthOutOf"]').val();

	var twelthPercentOrGrade = "";
	var twelthGradeConversionMethod = "";

	if (twelthPercentGradeFlag == "percent") {
		if (twelthMarks == undefined || twelthMarks == '') {
			jAlert("Please enter marks");
			flg6 = 1;
		} else if (twelthOutOf == undefined || twelthOutOf == '') {
			jAlert("Please enter Out of marks");
			flg6 = 1;
		} else {
			flg6 = 0;
		}
	} else {
		twelthPercentOrGrade = $('#txt12grade').val();
		twelthGradeConversionMethod = $(
		'input[name = "twelthGradeConversionMethod"]').val();

		if ((twelthPercentOrGrade == undefined || twelthPercentOrGrade == '') && $('#customerHelper').val()!="REVA_UNIVERSITY") {
			jAlert("Please enter grade");
			flg6 = 1;
		} else if ((twelthGradeConversionMethod == undefined || twelthGradeConversionMethod == '') && $('#customerHelper').val()!="REVA_UNIVERSITY") {
			if($('#customerHelper').val()!=undefined && !$('#customerHelper').val()=="RCOEM"){
					jAlert("Please enter conversion method");
			}else{
					jAlert("Please enter converted percentage of grade..!");
			}
			flg6 = 1;
		} else {
			flg6 = 0;
		}
	}

	if (flg6 == 0) {

		if ($('input[name = "twelthPercentGradeFlag"]:checked').val() == "percent") {
			twelthPercentOrGrade = eval($('#txt12percent').val());
			twelthGradeConversionMethod = "Nil";

		} else {
			twelthPercentOrGrade = $('#txt12grade').val();
			twelthGradeConversionMethod = $(
			'input[name = "twelthGradeConversionMethod"]').val();
			twelthMarks = 0;
			twelthOutOf = 0;
		}

		var twelthBoard = $('#twelthBoard').val();
		var twelthYearOfPassing = $("#twelthYrOfPassing").val();
		var twelthStream = $('#studStreamCombo').val();
		var twelthPhysicsmarks = 0;
		var twelthPhysicsOutOf = 0;
		var twelthChemistrymarks = 0;
		var twelthChemistryOutOf = 0;
		var twelthMathematicsmarks = 0;
		var twelthMathematicsOutOf = 0;
		var twelthBiologymarks = 0;
		var twelthBiologyOutOf = 0;
		var PCMPercentage = 0;
		var PCBPercentage = 0;
		
		
		if (twelthStream == "Science") {
			
			twelthPhysicsmarks = $('#txt12Physicsmarks').val() == "" ? 0 : $(
			'#txt12Physicsmarks').val();
			twelthPhysicsOutOf = $('#txt12Physicsoutof').val() == "" ? 0 : $(
			'#txt12Physicsoutof').val();

			twelthChemistrymarks = $('#txt12Chemistrymarks').val() == "" ? 0
					: $('#txt12Chemistrymarks').val();
			twelthChemistryOutOf = $('#txt12Chemistryoutof').val() == "" ? 0
					: $('#txt12Chemistryoutof').val();
			
			twelthMathematicsmarks = $('#txt12Mathematicsmarks').val() == "" ? 0
					: $('#txt12Mathematicsmarks').val();
			twelthMathematicsOutOf = $('#txt12Mathematicsoutof').val() == "" ? 0
					: $('#txt12Mathematicsoutof').val();

			twelthBiologymarks = $('#txt12Biologymarks').val() == "" ? 0 : $('#txt12Biologymarks').val();
			twelthBiologyOutOf = $('#txt12Biologyoutof').val() == "" ? 0 : $('#txt12Biologyoutof').val();
			
			if($('#customerHelper').val() !='HPTU' || $('#customerHelper').val() !='HPTELIBRARY'){
			//if(((twelthMathematicsmarks=="0")||(twelthMathematicsmarks==""))&&((twelthBiologymarks=="0")||(twelthBiologymarks==""))){
			if((twelthMathematicsmarks=="") && (twelthBiologymarks=="")){
				jAlert("Please Enter Marks Of Math Or Biology If Not applicable then enter 0.");
				return;
		}
			}

			PCMPercentage = $('#txt12PCMPercentage').val();
			PCBPercentage = $('#txt12PCBPercentage').val();
		}
		if(twelthYearOfPassing=="0"){
			jAlert("Please select year of passing");
			return;
		}
		if(twelthBoard=="0"){
			jAlert("Please Select Board");
			return;
		}
		var twelthSchoolName = $('input[name = "twelthSchoolName"]').val();
		if(twelthSchoolName==""){
			jAlert("Please Select School Name");
			return;
		}
		var twelth_Place = $('input[name="twelth_Place"]').val();
		if(twelth_Place==""){
			jAlert("Please Select Place");
			return;
		}
		var twelthRemarks = $('input[name="twelthRemarks"]').val();
		
		var schoolTypeTwelth = $('#schoolTypeTwelth').val();
		var twelSeatNo = $('#twelthSeatNo').val();

		if(eval(twelthMarks)>eval(twelthOutOf)){
			jAlert("Obtained Marks Cannot be Greater Than OutOf Marks");
			return false;
		}
		if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		if(twelSeatNo=="")
		{
			jAlert("Enter 12th Marksheet Number");
			return false;
			}
		}
		var stateOfSchoolHsc = $('#txtstateOfSchool12 option:selected').val();
		var txtNoOfAttempt12th = $('#txtNoOfAttempt12th').val();
		
		if($('#customerHelper').val() == "MEGHE" && stateOfSchoolHsc == ''){
			jAlert("Please Select State of School!!!");
			return false;
		}
		if($('#customerHelper').val() == "MEGHE" && txtNoOfAttempt12th == ''){
			jAlert("Please Enter No.of Attempt!!!");
			return false;
		}
		var twelthSecondarySubject="";
	
	var twelthOhterSecondarySubjectName = "";
	if($('#customerHelper').val()=="JGI"){
		twelthSecondarySubject = $('#applstud12thSecondaryLanguage').val();
		if (twelthSecondarySubject.toUpperCase().indexOf('OTHER') != -1) {
		twelthOhterSecondarySubjectName = $('#txtTwelthSecondaryLanguageName').val();
	} else {
		twelthOhterSecondarySubjectName = "";
	}
		
	}
	
	var twelthTcNumber = '';
	var twelthTcDate = '';
	if($('#customerHelper').val()=="MIT"){
		twelthTcNumber = $('#txt12TcNumber').val();
		twelthTcDate = $('#txt12TcDate').val();
	}
		
		$.ajax({
			url : 'studentHsc.json',
			data : {
				twelthPercentGradeFlag : twelthPercentGradeFlag,
				twelthMarks : twelthMarks,
				twelthOutOf : twelthOutOf,
				twelthPercentOrGrade : twelthPercentOrGrade,
				twelthGradeConversionMethod : twelthGradeConversionMethod,
				twelthStream : twelthStream,
				twelthSchoolName : twelthSchoolName,
				twelth_Place : twelth_Place,
				twelthRemarks : twelthRemarks,
				twelthBoard : twelthBoard,
				twelthYearOfPassing : twelthYearOfPassing,
				twelthPhysicsmarks : twelthPhysicsmarks,
				twelthPhysicsOutOf : twelthPhysicsOutOf,
				twelthChemistrymarks : twelthChemistrymarks,
				twelthChemistryOutOf : twelthChemistryOutOf,
				twelthMathematicsmarks : twelthMathematicsmarks,
				twelthMathematicsOutOf : twelthMathematicsOutOf,
				twelthBiologymarks : twelthBiologymarks,
				twelthBiologyOutOf : twelthBiologyOutOf,
				PCMPercentage : PCMPercentage,
				PCBPercentage : PCBPercentage,
				schoolTypeTwelth: schoolTypeTwelth,
				twelSeatNo:twelSeatNo,
				twelthSecondarySubject: twelthSecondarySubject,
				twelthOhterSecondarySubjectName:twelthOhterSecondarySubjectName,
				twelthTcNumber:twelthTcNumber,
				twelthTcDate:twelthTcDate,
				stateOfSchoolHsc:stateOfSchoolHsc,
				txtNoOfAttempt12th:txtNoOfAttempt12th
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				if (jArray[0].responseFlag == '1'){
					getCustomAlertMessage("alert_create");
					getstudenthscinfo();
				}
			},
			error : function() {
				// jAlert("Something went wrong, Please try after some time","Error");
			}
		});

	}

}

function  showMandatoryFildsforFather(){
	
	var fatherName = $('#studParentfirstname').val();
	if(fatherName !=""){
	     $('#fatherMobNo').html('<b style="color:red;font-size:16px;">*</b>');
		
     if($('#customerHelper').val()!="MAHINDRA_UNIVERSITY" && $('#customerHelper').val()!="MEGHE")
	     $('#fatheroccupationId').html('<b style="color:red;font-size:16px;">*</b>');

	     $('#fatherIncomeId').html('<b style="color:red;font-size:16px;">*</b>');
	}else{
		 $('#fatherMobNo').html('');
	     $('#fatheroccupationId').html('');
	     $('#fatherIncomeId').html('');
	}
	
}
function showMandatoryFildsforMother(){
	var motherName = $('#studmotherfirstname').val();
	  if((motherName !="") && $('#customerHelper').val()!="MANDSAUR"){  
	    $('#motherMobNo').html('<b style="color:red;font-size:16px;">*</b>');

     if($('#customerHelper').val()!="MAHINDRA_UNIVERSITY" && $('#customerHelper').val()!="MANDSAUR" && $('#customerHelper').val()!="MEGHE")
	    $('#motheroccupationId').html('<b style="color:red;font-size:16px;">*</b>');

	    $('#motherIncomeId').html('<b style="color:red;font-size:16px;">*</b>');
     }else{
    	 $('#motherMobNo').html('');
  	    $('#motheroccupationId').html('');
  	    $('#motherIncomeId').html('');
     }
}


function addStudentParentDetails()
{

	var studId= 0;
	var fatherFirstName = $('input[name = "fatherFirstName"]').val();
	var fatherMiddleName = $('input[name = "fatherMiddleName"]').val();
	var fatherLastName = $('input[name = "fatherLastName"]').val();
	var motherFirstName = $('input[name = "motherFirstName"]').val();
	var motherMiddleName = $('input[name = "motherMiddleName"]').val();
	var motherLastName = $('input[name = "motherLastName"]').val();
	var fatherAlternetEmailID = $('input[name = "fatherAlternetEmailID"]').val();
	var motherAlternetEmailID =$('input[name = "motherAlternetEmailID"]').val();
