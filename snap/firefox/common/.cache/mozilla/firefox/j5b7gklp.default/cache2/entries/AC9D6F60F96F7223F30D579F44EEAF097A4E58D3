function addStudentMedicalDetails()// addEmployeeMedicalCertificate
{
	 var dateOfBirth = $('input[name = "txtDOB"]').val();
	 var bloodGroup = $('input[name = "bloodGroup"]').val();
	var identificationMark = $('input[name = "identificationMark"]').val();
	var dateOfAdmission = $('input[name = "dateOfAdmission"]').val();
	var familyHistory = $('input[name = "familyHistory"]').val();
	var veg = $('input[name = "veg"]:checked').val();
	var nonVeg = $('input[name = "nonVeg"]:checked').val();
	var smoking = $('input[name = "smoking"]:checked').val();
	var tobaccoChewing = $('input[name = "tobaccoChewing"]:checked').val();
	var drugs = $('input[name = "drugs"]:checked').val();
	var alcohol = $('input[name = "alcohol"]:checked').val();
	var otherHabit = $('input[name = "otherHabit"]:checked').val();
	var noHabit = $('input[name = "noHabit"]:checked').val();
	var menstrual = $('input[name = "menstrual"]').val();
	var obstetric = $('input[name = "obstetric"]').val();
	var other = $('input[name = "other"]').val();
	var height = $('input[name = "height"]').val();
	var weight = $('input[name = "weight"]').val();
	var otherRelevant = $('input[name = "otherRelevant"]').val();
	var pulse = $('input[name = "pulse"]').val();
	var bloodPressure = $('input[name = "bloodPressure"]').val();
	var color = $('input[name = "color"]').val();
	var texture = $('input[name = "texture"]').val();
	var moisture = $('input[name = "moisture"]').val();
	var eruption = $('input[name = "eruption"]').val();
	var nodules = $('input[name = "nodules"]').val();
	var hypopigmentedPatches = $('input[name = "hypopigmentedPatches"]').val();
	var otherLesion = $('input[name = "otherLesion"]').val();
	var hairDistribution = $('input[name = "hairDistribution"]').val();
	var anyInjury = $('input[name = "anyInjury"]').val();
	var otherFinding = $('input[name = "otherFinding"]').val();
	var sclera = $('input[name = "sclera"]').val();
	var conjunctiva = $('input[name = "conjunctiva"]').val();
	var eyesOtherFindings = $('input[name = "eyesOtherFindings"]').val();
	var rightVision = $('input[name = "rightVision"]').val();
	var leftVision = $('input[name = "leftVision"]').val();
	var discharge = $('input[name = "discharge"]').val();
	var tympanicMembrane = $('input[name = "tympanicMembrane"]').val();
	var earsOtherFinding = $('input[name = "earsOtherFinding"]').val();
	var septum = $('input[name = "septum"]').val();
	var turbinates = $('input[name = "turbinates"]').val();
	var noseOtherFindings = $('input[name = "noseOtherFindings"]').val();
	var tenderness = $('input[name = "tenderness"]').val();
	var sinusesOtherFindings = $('input[name = "sinusesOtherFindings"]').val();
	var buccalMucosa = $('input[name = "buccalMucosa"]').val();
	var teeth = $('input[name = "teeth"]').val();
	var tongue = $('input[name = "tongue"]').val();
	var throat = $('input[name = "throat"]').val();
	var nodes = $('input[name = "nodes"]').val();
	var thyroid = $('input[name = "thyroid"]').val();
	var neckOtherFinding = $('input[name = "neckOtherFinding"]').val();
	var upperNail = $('input[name = "upperNail"]').val();
	var upperFinger = $('input[name = "upperFinger"]').val();
	var upperOtherFindings = $('input[name = "upperOtherFindings"]').val();
	var lowerNail = $('input[name = "lowerNail"]').val();
	var lowerFinger = $('input[name = "lowerFinger"]').val();
	var lowerOtherFindings = $('input[name = "lowerOtherFindings"]').val();
	var rsInspection = $('input[name = "rsInspection"]').val();
	var cvsInspection = $('input[name = "cvsInspection"]').val();
	var paInspection = $('input[name = "paInspection"]').val();
	var otherInspection = $('input[name = "otherInspection"]').val();
	var rsPalpation = $('input[name = "rsPalpation"]').val();
	var cvsPalpation = $('input[name = "cvsPalpation"]').val();
	var paPalpation = $('input[name = "paPalpation"]').val();
	var otherPalpation = $('input[name = "otherPalpation"]').val();
	var rsPercussion = $('input[name = "rsPercussion"]').val();
	var cvsPercussion = $('input[name = "cvsPercussion"]').val();
	var paPercussion = $('input[name = "paPercussion"]').val();
	var otherPercussion = $('input[name = "otherPercussion"]').val();
	var rsAuscultation = $('input[name = "rsAuscultation"]').val();
	var cvsAuscultation = $('input[name = "cvsAuscultation"]').val();
	var paAuscultation = $('input[name = "paAuscultation"]').val();
	var otherAuscultation = $('input[name = "otherAuscultation"]').val();
	var specificExamination = $('input[name = "specificExamination"]').val();
	var studMedCertiId = $('#studMedCertiId').val();
	var presentStatAilment = $('#txtPresentStatOfAilment').val();
	var physicalDisability = $('#txtPhysicalDisability').val();
	var mrdNumber = $('input[name = "mrdNumber"]').val();
	if(($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RAMAIAH") || ($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="STJOHNS"))
	{	
		if ((mrdNumber)=='') {
			
			jAlert("Enter MRD Number.");
			$('input[name = "mrdNumber"]').focus();
			return false;
		}
	}
	$.ajax({
		url : 'studMedicalDetails.json',
		data : { dateOfBirth:dateOfBirth,
				bloodGroup:bloodGroup, 
			identificationMark : identificationMark,
			dateOfAdmission : dateOfAdmission,
			familyHistory : familyHistory,
			veg : veg,
			nonVeg : nonVeg,
			smoking : smoking,
			tobaccoChewing : tobaccoChewing,
			drugs : drugs,
			alcohol : alcohol,
			otherHabit : otherHabit,
			menstrual : menstrual,
			obstetric : obstetric,
			other : other,
			height : height,
			weight : weight,
			otherRelevant : otherRelevant,
			pulse : pulse,
			bloodPressure : bloodPressure,
			color : color,
			texture : texture,
			moisture : moisture,
			eruption : eruption,
			nodules : nodules,
			hypopigmentedPatches : hypopigmentedPatches,
			otherLesion : otherLesion,
			hairDistribution : hairDistribution,
			anyInjury : anyInjury,
			otherFinding : otherFinding,
			sclera : sclera,
			specificExamination : specificExamination,
			conjunctiva : conjunctiva,
			eyesOtherFindings : eyesOtherFindings,
			rightVision : rightVision,
			leftVision : leftVision,
			discharge : discharge,
			rsAuscultation : rsAuscultation,
			cvsAuscultation : cvsAuscultation,
			paAuscultation : paAuscultation,
			otherAuscultation : otherAuscultation,
			tympanicMembrane : tympanicMembrane,
			earsOtherFinding : earsOtherFinding,
			septum : septum,
			turbinates : turbinates,
			rsPercussion : rsPercussion,
			cvsPercussion : cvsPercussion,
			paPercussion : paPercussion,
			otherPercussion : otherPercussion,
			otherAuscultation : otherAuscultation,
			noseOtherFindings : noseOtherFindings,
			tenderness : tenderness,
			sinusesOtherFindings : sinusesOtherFindings,
			rsPalpation : rsPalpation,
			cvsPalpation : cvsPalpation,
			paPalpation : paPalpation,
			otherPalpation : otherPalpation,
			buccalMucosa : buccalMucosa,
			teeth : teeth,
			tongue : tongue,
			throat : throat,
			nodes : nodes,
			thyroid : thyroid,
			rsInspection : rsInspection,
			cvsInspection : cvsInspection,
			paInspection : paInspection,
			otherInspection : otherInspection,
			neckOtherFinding : neckOtherFinding,
			upperNail : upperNail,
			upperFinger : upperFinger,
			upperOtherFindings : upperOtherFindings,
			lowerNail : lowerNail,
			lowerFinger : lowerFinger,
			lowerOtherFindings : lowerOtherFindings,
			studMedCertiId : studMedCertiId,
			presentStatAilment : presentStatAilment,
			physicalDisability : physicalDisability,
			mrdNumber : mrdNumber,
			noHabit : noHabit
		},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			if (jArray[0].responseFlag == '1') {
				$('#studMedCertiId').val(jArray[0].id);
				showSuccessMessage(jArray[0].responseText, 5000);
				$('#studMedicalExaminationDiv').modal('hide');
				$('#studSystematicExaminationDiv').modal('hide');
				getCustomAlertMessage("alert_create");
			}
		},
		error : function() {
			// jAlert("Something went wrong, Please try after some time","Error");
		}
	});
}

function getstudentMedicalinfo(gStudentId) {
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getStudentMedicalinfo.json?id='+gStudentId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getStudentMedicalinfo.json';
	}
	$
	.ajax({
		url : url,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			$('input[name = "txtDOB"]').val(jArray["dateOfBirth"]);
			$('input[name = "bloodGroup"]').val(jArray["bloodGroup"]);
			$('input[name = "identificationMark"]').val(
					jArray["identificationMark"]);
			$('input[name = "dateOfAdmission"]').val(
					jArray["dateOfAppointment"]);
			$('input[name = "familyHistory"]').val(
					jArray["familyHistory"]);
			
			if(($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RAMAIAH") || ($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="STJOHNS"))
			{
				$('input[name = "mrdNumber"]').val(jArray["mrdNumber"]);
			}
			
			if (jArray["veg"] != "")
				if (jArray["veg"] != null)
					$('#habit1').attr("checked", true);

			if (jArray["otherHabit"] != "")
				if (jArray["otherHabit"] != null)
					$('#habit7').attr("checked", true);

			if (jArray["noHabit"] != "")
				if (jArray["noHabit"] != null)
					$('#habit8').attr("checked", true);

			if (jArray["nonVeg"] != "")
				if (jArray["nonVeg"] != null)
					$('#habit2').attr("checked", true);

			if (jArray["smoking"] != "")
				if (jArray["smoking"] != null)
					$('#habit3').attr("checked", true);

			if (jArray["alcohol"] != "")
				if (jArray["alcohol"] != null)
					$('#habit4').attr("checked", true);

			if (jArray["tobaccoChewing"] != "")
				if (jArray["tobaccoChewing"] != null)
					$('#habit5').attr("checked", true);

			if (jArray["drugs"] != "")
				if (jArray["drugs"] != null)
					$('#habit9').attr("checked", true);

			$('input[name = "menstrual"]').val(jArray["menstrual"]);
			$('input[name = "obstetric"]').val(jArray["obstetric"]);
			$('input[name = "other"]').val(jArray["other"]);
			$('input[name = "height"]').val(jArray["height"]);
			$('input[name = "weight"]').val(jArray["weight"]);
			$('input[name = "otherRelevant"]').val(
					jArray["otherRelevant"]);
			$('input[name = "pulse"]').val(jArray["pulse"]);
			$('input[name = "bloodPressure"]').val(
					jArray["bloodPressure"]);
			$('input[name = "color"]').val(jArray["color"]);
			$('input[name = "texture"]').val(jArray["texture"]);
			$('input[name = "moisture"]').val(jArray["moisture"]);
			$('input[name = "eruption"]').val(jArray["eruption"]);
			$('input[name = "nodules"]').val(jArray["nodules"]);
			$('input[name = "hypopigmentedPatches"]').val(
					jArray["hypopigmentedPatches"]);
			$('input[name = "otherLesion"]').val(jArray["otherLesion"]);
			$('input[name = "hairDistribution"]').val(
					jArray["hairDistribution"]);
			$('input[name = "anyInjury"]').val(jArray["anyInjury"]);
			$('input[name = "otherFinding"]').val(
					jArray["otherFinding"]);
			$('input[name = "sclera"]').val(jArray["sclera"]);
			$('input[name = "conjunctiva"]').val(jArray["conjunctiva"]);
			$('input[name = "eyesOtherFindings"]').val(
					jArray["eyesOtherFindings"]);
			$('input[name = "rightVision"]').val(jArray["rightVision"]);
			$('input[name = "leftVision"]').val(jArray["leftVision"]);
			$('input[name = "discharge"]').val(jArray["discharge"]);
			$('input[name = "tympanicMembrane"]').val(
					jArray["tympanicMembrane"]);
			$('input[name = "earsOtherFinding"]').val(
					jArray["earsOtherFinding"]);
			$('input[name = "septum"]').val(jArray["septum"]);
			$('input[name = "turbinates"]').val(jArray["turbinates"]);
			$('input[name = "noseOtherFindings"]').val(
					jArray["noseOtherFindings"]);
			$('input[name = "tenderness"]').val(jArray["tenderness"]);
			$('input[name = "sinusesOtherFindings"]').val(
					jArray["sinusesOtherFindings"]);
			$('input[name = "buccalMucosa"]').val(
					jArray["buccalMucosa"]);
			$('input[name = "teeth"]').val(jArray["teeth"]);
			$('input[name = "tongue"]').val(jArray["tongue"]);
			$('input[name = "throat"]').val(jArray["throat"]);
			$('input[name = "nodes"]').val(jArray["nodes"]);
			$('input[name = "thyroid"]').val(jArray["thyroid"]);
			$('input[name = "neckOtherFinding"]').val(
					jArray["neckOtherFinding"]);
			$('input[name = "upperNail"]').val(jArray["upperNail"]);
			$('input[name = "upperFinger"]').val(jArray["upperFinger"]);
			$('input[name = "upperOtherFindings"]').val(
					jArray["upperOtherFindings"]);
			$('input[name = "lowerNail"]').val(jArray["lowerNail"]);
			$('input[name = "lowerFinger"]').val(jArray["lowerFinger"]);
			$('input[name = "lowerOtherFindings"]').val(
					jArray["lowerOtherFindings"]);
			$('input[name = "rsInspection"]').val(
					jArray["rsInspection"]);
			$('input[name = "cvsInspection"]').val(
					jArray["cvsInspection"]);
			$('input[name = "paInspection"]').val(
					jArray["paInspection"]);
			$('input[name = "otherInspection"]').val(
					jArray["otherInspection"]);
			$('input[name = "rsPalpation"]').val(jArray["rsPalpation"]);
			$('input[name = "cvsPalpation"]').val(
					jArray["cvsPalpation"]);
			$('input[name = "paPalpation"]').val(jArray["paPalpation"]);
			$('input[name = "otherPalpation"]').val(
					jArray["otherPalpation"]);
			$('input[name = "rsPercussion"]').val(
					jArray["rsPercussion"]);
			$('input[name = "cvsPercussion"]').val(
					jArray["cvsPercussion"]);
			$('input[name = "paPercussion"]').val(
					jArray["paPercussion"]);
			$('input[name = "otherPercussion"]').val(
					jArray["otherPercussion"]);
			$('input[name = "rsAuscultation"]').val(
					jArray["rsAuscultation"]);
			$('input[name = "cvsAuscultation"]').val(
					jArray["cvsAuscultation"]);
			$('input[name = "paAuscultation"]').val(
					jArray["paAuscultation"]);
			$('input[name = "otherAuscultation"]').val(
					jArray["otherAuscultation"]);
			$('input[name = "specificExamination"]').val(
					jArray["specificExamination"]);
			$('#studMedCertiId').val(jArray["studentMedicalDetailId"]);
			$('#txtPresentStatOfAilment').val(jArray["presentStatusOfAilment"]);
			$('#txtPhysicalDisability').val(jArray["physicalDisability"]);
			var studentMedicalDetailId=jArray["studentMedicalDetailId"];
			$('#uploadMedicalDocBtn').bind('click', function(){
				uploadStudMedicalDocument(studentMedicalDetailId);
			});
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			alert("In Student Medical Detail error");
		}
	});
}
//-------------------------------------------------------------Current
//Qualification Tab

function getStudentCurrentQualificationConfirm(degreeType) {
	$.ajax({
		url : 'getStudentCurrentQualification.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var myboiler = '';

			if (jArray.length > 0) {
				if (jArray[0]["studentType"] == "academic"	|| jArray[0]["studentType"] == "PlacementAcademic") {
					// academic

					var tempSem = 0;
					var tempCnt = 0;
					var currentSem = jArray[0]["cnt"];
					myboiler += '<table class="table table-bordered" style="width: 100%;" >'
						+ '<tr><td colspan="4" style="color: #0088CC"><label><b><font>Student current qualification:</font></b></label></td></tr>'
						+ '<tr></tr>';
					for ( var i = 0; i < jArray.length; i++) {
						myboiler += '<tr><th style="width:30%">University name</th><td><label>'+ jArray[i]["universityName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Degree stream</th><td><label>'+ jArray[i]["degreeStreamName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Degree name</th><td><label>'+ jArray[i]["degreeName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Specialization (if any)</th><td><select id="cmbSpecNew"></select>&nbsp;&nbsp;<input type="button" class="btn btn-inverse btn-mini btnColorGreen " value="Save" id="btnSaveStudSpec" onclick="SaveStudSpec()"/></td></tr>';
						myboiler += '<tr><th style="width:30%">Year of Passing</th><td><label>'+ jArray[i]["yearOfPassing"]+ '</label></td></tr>';
						myboiler += '<tr><td colspan="3" id="tdSem">'+ '</td></tr>';

					}
					myboiler += '</table>';

					$('#ug').html(myboiler);

					var tempSemBoiler = '';
					/*
					 * '<table class="table table-bordered
					 * table-condensed" style="width: 1100px;">'+ '<tr>'+ '<td><b>Term
					 * Pattern</b></td>'+ '<td><label><input
					 * type="radio" name="semYear" id="radSem"
					 * value="Semester" onclick="showSemCmb()"
					 * checked="checked"/> Semester</label></td>'+ '<td><label><input
					 * type="radio" name="semYear" id="radYear"
					 * value="Year" onclick="showSemCmb()"/> Year</label></td>'+ '</tr>';
					 */

					if (currentSem == 1) {

						tempSemBoiler += '<table class="table table-bordered table-condensed"" style="width: 100%;">'
							+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
							+ '<tr style="margin-left:10%;" id="marksTypeTr">'
							+ '</tr>'
							+ '</table>'
							+ '<table >'
							+ '<tr id="atktTypeTr">'
							+ '</tr>'
							+ '</table>';
					} else {
						if (jArray[0]["marksSystem"] == "Percent") {
							// academic percent

							/*
							 * tempSemBoiler += '<tr>'+ '<td><b>System
							 * of marks</b></td>'+ '<td><label><input
							 * type="radio" name="type" id="radPercnt"
							 * value="Percent" checked="checked"
							 * onclick=""/> Percent System</label></td>'+ '<td><label><input
							 * type="radio" name="type" id="radGrade"
							 * value="Grade" disabled="disabled"
							 * onclick=""/> Grade System</label></td>'+ '</tr>';
							 */

