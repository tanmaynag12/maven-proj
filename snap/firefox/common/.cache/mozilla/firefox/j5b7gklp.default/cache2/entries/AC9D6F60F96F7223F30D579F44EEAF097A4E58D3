				}
		
				if ((gArray[i]["isCheckedText"] == "licenceNumber") && ($('input[name = "licenceNumber"]')!=undefined) && (($('input[name = "licenceNumber"]').val())!=null) &&  ($.trim($('input[name = "licenceNumber"]').val())!="") && ($('input[name = "licenceNumber"]').val().length>0)) {
					$('input[name = "licenceNumber"]').attr("disabled", true);
				}
		
				if ( (gArray[i]["isCheckedText"] == "enrollmentNo") && ($('input[name = "enrollmentNo"]')!=undefined) && (($('input[name = "enrollmentNo"]').val())!=null) &&  ($.trim($('input[name = "enrollmentNo"]').val())!="") && ($('input[name = "enrollmentNo"]').val().length>0)) {
					$('input[name = "enrollmentNo"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "placeofBirth") && ($('input[name = "placeofBirth"]')!=undefined) && (($('input[name = "placeofBirth"]').val())!=null) &&  ($.trim($('input[name = "placeofBirth"]').val())!="") && ($('input[name = "placeofBirth"]').val().length>0)) {
					$('input[name = "placeofBirth"]').attr("disabled", true);
				}else if((gArray[i]["isCheckedText"] == "placeofBirth") && ($('input[name = "placeofBirth"]')==undefined) || (($('input[name = "placeofBirth"]').val())==null) ||  ($.trim($('input[name = "placeofBirth"]').val())=="") || ($('input[name = "placeofBirth"]').val().length==0)){
					$('input[name = "placeofBirth"]').attr("disabled", false);
