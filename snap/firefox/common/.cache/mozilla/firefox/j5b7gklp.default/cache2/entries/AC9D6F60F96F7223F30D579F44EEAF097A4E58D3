						getStudentContactForStudentDetailsTab();
					}
				},
				error : function() {
				}
			});
		}
	 }
   }
		
 },2000);
	
}

function checkAlternateNo() {
	var alternetContactNumber = $('input[name = "alternetContactNumber"]').val();
	if (alternetContactNumber == "") {
		jAlert("Please enter alternate no.");
	}
}

function checkStdNoInContTab() {
	var std = $('input[name = "stdContactTab"]').val();
	if (std == "") {
		jAlert("Please enter STD/ISBN no.");
	}
}

//------------------------------------------------------------------------------------------------------------Identifications
function addStudentIdentificationDetails() {
	var studUniRegiNo = $('#studidentityUniRegiNo').val();
	var studMobileNo = $('#studidentityMobileNo').val();
	var studEmailId = $('#studidentityEmailId').val();
	var studPanCard = $('#studidentityPanCard').val();
	var studLicenseNo = $('#studidentityLicenseNo').val();
	var studpassportno = $('#studidentitypassportno').val();
	$.ajax({
		url : 'studentIdentificationInfo.json',
		data : {
			studUniRegiNo : studUniRegiNo,
			studMobileNo : studMobileNo,
			studEmailId : studEmailId,
			studPanCard : studPanCard,
			studLicenseNo : studLicenseNo,
			studpassportno : studpassportno
		},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if (jArray[0].responseFlag == '1')

				showSuccessMessage(jArray[0].responseText, 5000);
			getCustomAlertMessage("alert_create");
		},
		error : function() {
		}
	});
}

//---------------------------------------------------------Function to Add  Tab-2
function addStudentAddress(param) {
	if(param=='edit'){
		$('.className').attr('disabled',false);
		return false;
	}
	
	var addressLine1 = $('input[name = "currentaddressLine1"]').val();
	var addressLine2 = $('input[name = "currentaddressLine2"]').val();
	var cityId = $('#studentCity').val();
	var stateId = $('#studentState').val();
	var countryId = $('#studentCountryCombo').val();
	var pinCode = $('input[name = "currentpinCodeNumber"]').val();
	var CurrAddressId = $('#CurrAddressId').val();
	var currDistrict = $('#txtStudentDistrict').val();
	var currTaluka = $('#txtStudentTaluka').val();
	
	var permaaddressLine1 = $('#permstudAddLine1').val();
	var permaaddressLine2 = $('#permstudAddLine2').val();
	var permstudentCity = $('#permstudentCity').val();
	var permstudentState = $('#permstudentState').val();
	var permstudentCountryCombo1 = $('#permstudentCountryCombo1').val();
	var permstudPincode = $('#permstudPincode').val();
	var PermaAddressId = $('#PermaAddressId').val();
	var permDistrict = $('#txtStudentPermDistrict').val();
	var permTaluka = $('#txtStudentPermTaluka').val();

	/*
	 * var txtMobileContactTab=$('#txtMobileContactTab').val(); var
	 * txtstdcodeContactTab=$('#txtstdcodeContactTab').val(); var
	 * txtAltNumberContactTab = $('#txtAltNumberContactTab').val(); var
	 * txtContactHoursContactTab = $('#txtContactHoursContactTab').val();
	 */

	var cnt2 = 0;

	if (addressLine1 == '') {
		cnt2 = 1;
		jAlert("Please enter address line 1 of current address");
	} else if (countryId == 0) {
		cnt2 = 1;
		jAlert("Please select country of current address");
	} else if (stateId == 0) {
		cnt2 = 1;
		jAlert("Please select state of current address");
	} else if (cityId == 0) {
		cnt2 = 1;
		jAlert("Please select city of current address");
	} else if (pinCode == '' || isNaN(pinCode)) {
		cnt2 = 1;
		jAlert("Please enter valid pin code of current address");
	} else if (permaaddressLine1 == '') {
		cnt2 = 1;
		jAlert("Please enter address line 1 of permanent address");
	} else if (permstudentCountryCombo1 == 0) {
		cnt2 = 1;
		jAlert("Please select country of permanent address");
	} else if (permstudentState == 0) {
		cnt2 = 1;
		jAlert("Please select state of permanent address");
	} else if (permstudentCity == 0) {
		cnt2 = 1;
		jAlert("Please select city of permanent address");
	} else if (permstudPincode == '' || isNaN(permstudPincode)) {
		cnt2 = 1;
		jAlert("Please enter valid pin code of permanent address");
	}
	
	var currstudhouse1 = '';
	var currstudStreet1 = '';
	var currstudlandmark1 = '';
	var currstudvillege1 ='';
	
	var permstudHouse2 = '';
	var permstudStreet2 = '';
	var permstudLandmark2 = '';
	var permstudVillage2 = '';
	
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DYPATIL"){
		currstudhouse1= $('#currstudhouse1').val();
		currstudStreet1= $('#currstudStreet1').val();
		currstudlandmark1 = $('#currstudlandmark1').val();
		currstudvillege1 =$('#currstudvillege1').val();
		permstudHouse2 =$('#permstudHouse2').val();
		permstudStreet2 =$('#permstudStreet2').val();
		permstudLandmark2 =$('#permstudLandmark2').val();
		permstudVillage2 =$('#permstudVillage2').val();
	    
	    if (currstudhouse1 == '') {
		cnt2 = 1;
		jAlert("Please enter House /Building/Apartment  of current address");
	} else if (currstudStreet1 == '') {
		cnt2 = 1;
		jAlert("Please enter Street/Road/Lane of current address");
	} else if (currstudlandmark1 == '') {
		cnt2 = 1;
		jAlert("Please enter Landmark/Area of current address");
	} else if (currstudvillege1 == '') {
		cnt2 = 1;
		jAlert("Please enter Village/Town of current address");
	} else if (permstudHouse2 == '') {
		cnt2 = 1;
		jAlert("Please enter House /Building/Apartment of permanent address");
	} else if (permstudStreet2 == '') {
		cnt2 = 1;
		jAlert("Please enter Street/Road/Lane of permanent address");
	} else if (permstudLandmark2 == '') {
		cnt2 = 1;
		jAlert("Please enter Landmark/Area of permanent address");
	} else if (permstudVillage2 == '') {
		cnt2 = 1;
		jAlert("Please enter Village/Town of permanent address");
	} 
	
	    }

	if (cnt2 == 0) {
		$.ajax({
			url : 'studentAddress.json',
			data : {
				PermaAddressId : PermaAddressId,
				CurrAddressId : CurrAddressId,
				addressLine1 : addressLine1,
				addressLine2 : addressLine2,
				countryId : countryId,
				stateId : stateId,
				cityId : cityId,
				pinCode : pinCode,
				permaaddressLine1 : permaaddressLine1,
				permaaddressLine2 : permaaddressLine2,
				permstudentCity : permstudentCity,
				permstudentState : permstudentState,
				permstudentCountryCombo1 : permstudentCountryCombo1,
				currDistrict:currDistrict,
				currTaluka:currTaluka,
				permDistrict:permDistrict,
				permTaluka:permTaluka,
				permstudPincode : permstudPincode,
				
				//DY PAtil
				currstudhouse1 : currstudhouse1,
				currstudStreet1 : currstudStreet1,
				currstudlandmark1 : currstudlandmark1,
				currstudvillege1 : currstudvillege1,
				permstudHouse2 : permstudHouse2,
				permstudStreet2 : permstudStreet2,
				permstudLandmark2 : permstudLandmark2,
				permstudVillage2 : permstudVillage2		
				
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				if (jArray[0].members.responseFlag.value == '1')

					$('#CurrAddressId').val(jArray[1][0].studentAddressId);
				$('#PermaAddressId').val(jArray[1][1].studentAddressId);
				getStudentContact();
				getCustomAlertMessage("alert_create");
			},
			error : function() {
			}
		});
	}
}

function getStudentStates(cntry, state) {
	$.ajax({
		url : 'stateList.json',
		data : {
			countryId : $('#' + cntry).val()
		},
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			var stateblr = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++)
				stateblr += '<option value="' + data[i]["stateId"] + '">'
				+ data[i]["stateName"] + '</option>';
			$('#' + state).html(stateblr);
		},
	});
	if( state=='studentState' && $('#studentState').val()=='0'){
		$('#studentState').trigger('change');
	//	$('#studentCity').val('0');
		
	}
	else if(state=='permstudentState' && $('#permstudentState').val()=='0'){
		$('#permstudentState').trigger('change');
	//	$('#permstudentCity').val('0');
		
	}

}
