function SaveStudSpec() {
	var studSpec = $('#cmbSpecNew').val();
	if (studSpec == 0) {
		jAlert("Please select specialization");
	} else {

		$.ajax({
			url : 'saveStudCPCSpecializationNew.json?studSpec=' + studSpec,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {

				if (jArray == "noData") {
					jAlert("Your batch has not been activated yet");
				} else {
					$('#cmbSpecNew').val(jArray[0]["specId"]);
					getCustomAlertMessage("alert_create");
				}
			},
		});
	}
}

function getStudCPCSpecialization() {

	$.ajax({
		url : 'getStudCPCSpecializationNew.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var j = jArray.length;
			var uniComboBoiler = '<option value="0">Select</option>';
			if (j > 0) {
				for ( var i = 0; i < j; i++) {
					uniComboBoiler += '<option value="' + jArray[i]["specId"]
					+ '">' + jArray[i]["specName"] + '</option>';
				}
			}
			$('#cmbSpecNew').html(uniComboBoiler);

			if (j > 0) {
				$('#cmbSpecNew').val(jArray[0]["savedSpecId"]);
			}
		},
	});
}

function showSemCmb() {
	var semOrYear = $('input[name = "semYear"]:checked').val();
	if (semOrYear == "Semester") {
		$('#currSemYearTr').html("Pursuing Term");
	} else {
		$('#currSemYearTr').html("Pursuing Year");
	}
}

function showPercenSystem(totRow) {

	var semOrYear = $('input[name = "semYear"]:checked').val();

	var tempSemBoiler = '<td colspan="3" style="align:center;">'
		+ '<table style="align:center;border-collapse:collapse;border:0;" style="width: 400px;">'
		+ '<tr>';

	if (semOrYear == "Semester")
		tempSemBoiler += '<th>Class</th>';
	else
		tempSemBoiler += '<th>Year</th>';

	tempSemBoiler += '<th>Obtained marks</th>' + '<th>Marks out of</th>'
	+ '<th>Percentage</th>' + '</tr>';

	for ( var i = 1; i <= totRow; i++) {

		tempSemBoiler += '<tr>' + '<td style="text-align:center;">'
		+ i
		+ '</td>'
		+ '<td><input type="text" class="validnum" style="text-align: right" name="totMarks" id="txtPercentSysemTotMarks'
		+ i
		+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
		+ i
		+ '\',\'txtPercentSysemMarksOutOf'
		+ i
		+ '\',\'msgmarksPG'
		+ i
		+ '\',\'txtPercentSysemPercnt'
		+ i
		+ '\');validateObtAndTotMarks(\'txtPercentSysemTotMarks'
		+ i
		+ '\')" /></td>'
		+ '<td><input type="text" class="validnum" style="text-align: right" name="outOfMarks" id="txtPercentSysemMarksOutOf'
		+ i
		+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
		+ i
		+ '\',\'txtPercentSysemMarksOutOf'
		+ i
		+ '\',\'msgmarksPG'
		+ i
		+ '\',\'txtPercentSysemPercnt'
		+ i
		+ '\');validateObtAndTotMarks(\'txtPercentSysemMarksOutOf'
		+ i
		+ '\')" /></td>'
		+ '<td><input type="text" class="validnum" style="text-align: right" name="perc" id="txtPercentSysemPercnt'
		+ i
		+ '"/></td>'
		+ '</tr>'
		+ '<tr id="msgmarksPG'
		+ i
		+ '" style="display: none"> '
		+ '<td align="left" colspan="5" style="color: RED">Obtained Marks is greater than total MARKS.</td> '
		+ '</tr>';
	}

	tempSemBoiler += '</table>' + '</td>';

	$('#marksTypeTr').html(tempSemBoiler);
	$('.validnum').numeric();

	var boiler = '<table>'
		+ '<tr>';
		if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
			boiler+= '<th>No of Live Backlogs</th>';
		}else{
			boiler+= '<th>No of Live ATKT</th>';
		}
		boiler+= '<td><label><input type="text" class="validnum" style="text-align: right" id="txtPercntLiveATKT" value="0" onblur="validateObtAndTotMarks(\'txtPercntLiveATKT\')"></label></td>'
		+ '</tr>'
		+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										boiler+= '<th>No of Dead Backlogs</th>';
									}else{
										boiler+= '<th>No of Dead ATKT</th>';
									}
									boiler+= '<td><label><input type="text" class="validnum" style="text-align: right" id="txtPercentDeadATKT" value="0" onblur="validateObtAndTotMarks(\'txtPercentDeadATKT\')"></label></td>'
		+ '</tr>'
		+

		'<tr><td><inpu␀␀␀t type="button" id="btnSaveMarksDetailsPercnt" onclick="saveCurrentQualiPercent('
		+ totRow
		+ ')" class="btn btn-inverse btn-mini btnColorGreen  " value="Save"></td></tr></table>';

	$('#atktTypeTr').html(boiler);
	$('.validnum').numeric();

}

