							'</td></tr>'+
							
							'<tr class="tr" style="display:none;"><td th="th"></td><td th="th">'+
							'<select name="WithinInstituteStudentName" style="display:none;" id="Wrkshop_WithinInstituteStudentName2" multiple></select>'+
							'</td></tr>'+
							
							
							'<tr>'+
								'<td></td>'+
								'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnAddNewStudCerti" onclick="saveNewStudPublicationNew(\''+studReasearchId+'\')"/>'+
								'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="	getResearchPublication();" /></td>'+
							'</tr>'+
							'</tbody>'+
							'</table>';
	
	
		$('#ResearchPublication').html(addSkillBoiler);
		try {
			getAllStudentList('Wrkshop_WithinInstituteStudentName');
			getAllEmployeeListWihtinInstitute('Wrkshop_WithinInstituteAuthorName');
		}catch(e) {console.log(e);}
		convertComboToSearchable('#Wrkshop_WithinInstituteAuthorName');
		
		$('#txtPublishDate').datepicker({
			dateFormat : 'M d,yy',
		});

	if(indexId!="NEW"){
		$('#txtIssnNo').val(studReasearchPubArr[indexId]["issnNo"]);
		$('#txtPaperTitle').val(studReasearchPubArr[indexId]["paperName"]);
		$('#txtName').val(studReasearchPubArr[indexId]["authorName1"]);
		$('#txtPublishDate').val(studReasearchPubArr[indexId]["publishDate"]);
		$('#txtJournalName').val(studReasearchPubArr[indexId]["journalName"]);
		$('#cmbPaperLevel').val(studReasearchPubArr[indexId]["journalLevel"]);
		$('#cmbStatusOfPaper').val(studReasearchPubArr[indexId]["statusOfPaper"]);
		$('#txtRemark').val(studReasearchPubArr[indexId]["remark"]);
		$('#publicationVenue').val(studReasearchPubArr[indexId]["venue"]);
		$('#publicationPrize').val(studReasearchPubArr[indexId]["prize"]);
		$('#txtOrganizer').val(studReasearchPubArr[indexId]["organizer"]);
		$('#txtPublisher').val(studReasearchPubArr[indexId]["publisher"]);
		$('#cmbConferJour').val(studReasearchPubArr[indexId]["conferencejournal"]);
		$('#txtDetails').val(studReasearchPubArr[indexId]["details"]);
		$('#txtOrchidId').val(studReasearchPubArr[indexId]["OrchidId"]);
		
		
		if(studReasearchPubArr[indexId]["participant"] == "true" && studReasearchPubArr[indexId]["winner"] == "false")
			$("input[name=publicationStatus][value='Participate']").prop('checked', true);	
		else if(studReasearchPubArr[indexId]["participant"] == "true" && studReasearchPubArr[indexId]["winner"] == "true")
			$("input[name=publicationStatus][value='Winner']").prop('checked', true);	
		
		
		
		
		
		var authorname='';
		var boiler = '';
		var employeeProfileDetailList = studReasearchPubArr[indexId].employeeProfileDetailList;
		for(var i=0;i<employeeProfileDetailList.length;i++){
			var employeeId=employeeProfileDetailList[i]['employeeId'];
			var EmpProfileDetailId=employeeProfileDetailList[i]['employeeProfileDetailId'];
			var priority=employeeProfileDetailList[i]['priority'];
			boiler+='<li cnt="'+(priority)+'" empId="'+employeeId+'">'+(priority)+'. <span>'+$('#Wrkshop_WithinInstituteAuthorName option[value="'+employeeId+'"]').html()+'</span>&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0)" title="Remove" onclick="deleteAuthorDetailsExceptPublication(\''+EmpProfileDetailId+'\',this,\'\',\'confer\')" ><i class="fa fa-trash iconColorBlack"></i></a></li>';
		}
		
		 employeeProfileDetailList = studReasearchPubArr[indexId].employeeProfileDetailListStudent;
		for(var i=0;i<employeeProfileDetailList.length;i++){
			var employeeId=employeeProfileDetailList[i]['employeeId'];
			var EmpProfileDetailId=employeeProfileDetailList[i]['employeeProfileDetailId'];
			var priority=employeeProfileDetailList[i]['priority'];
			boiler+='<li cnt="'+(priority)+'" empId="'+employeeId+'">'+(priority)+'. <span>'+$('#Wrkshop_WithinInstituteStudentName option[value="'+employeeId+'"]').html()+'</span>&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0)" title="Remove" onclick="deleteAuthorDetailsExceptPublication(\''+EmpProfileDetailId+'\',this,\'\',\'confer\')" ><i class="fa fa-trash iconColorBlack"></i></a></li>';
		}
		
		
		
		employeeProfileDetailList = studReasearchPubArr[indexId].employeeProfileDetailListOutside;
		for(var i=0;i<employeeProfileDetailList.length;i++){
			var employeeId=employeeProfileDetailList[i]['employeeId'];
			var employeeName=employeeProfileDetailList[i]['employeeName'];
			var EmpProfileDetailId=employeeProfileDetailList[i]['employeeProfileDetailId'];
			var priority=employeeProfileDetailList[i]['priority'];
			boiler+='<li cnt="'+(priority)+'" empId="'+employeeId+'">'+(priority)+'. <span>'+employeeName+'</span>&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0)" title="Remove" onclick="deleteAuthorDetailsExceptPublication(\''+EmpProfileDetailId+'\',this,\'\',\'confer\')" ><i class="fa fa-trash iconColorBlack"></i></a></li>';
		}
		$('#Wrkshop_AuthorNameListwithinInstitute').html(boiler);
		
		
		
		
		
	}
}
function saveNewStudPublicationNew(studReasearchId){
	var inserttype='';
	if(studReasearchId==undefined || studReasearchId==''){
		studReasearchId='';
		inserttype='new';
	}else{
		inserttype='edit';
	}

	var issnNo=$('#txtIssnNo').val();
	var paperName=$('#txtPaperTitle').val();
	var authorName1=$('#txtName').val();
	var publishDate=$('#txtPublishDate').val();
	var journalName=$('#txtJournalName').val();
	var journalLevel=$('#cmbPaperLevel').val();
	var statusOfPaper=$('#cmbStatusOfPaper').val();
	var remark=$('#txtRemark').val();
	var venue = $('#publicationVenue').val();
	var participation = $("input[name='publicationStatus']:checked").val();
	var prize = $('#publicationPrize').val();
	var organizer = $('#txtOrganizer').val();
	var publisher = $('#txtPublisher').val();
	var conferJour = $('#cmbConferJour').val();
	var details = $('#txtDetails').val();
	var orchidId=$('#txtOrchidId').val();
	
	
	var AuthorNamewithinInstitute = [];
	if($('#Wrkshop_WithinInstituteAuthorName2').val()!=null)
	AuthorNamewithinInstitute = $('#Wrkshop_WithinInstituteAuthorName2').val();
	
	var outsideAuthorName=[];
	if($('#Wrkshop_otherInstituteAuthorName2').val()!=null)
	outsideAuthorName=$('#Wrkshop_otherInstituteAuthorName2').val();

	var insidesideStudentName=[];
	if($('#Wrkshop_WithinInstituteStudentName2').val()!=null)
		insidesideStudentName=$('#Wrkshop_WithinInstituteStudentName2').val();
	
	

	var cnt=0;
	if(issnNo==''){
		jAlert("Please enter Issn No");
		cnt=1;
	}
	else if(paperName==''){
		jAlert("Please enter Paper Name");
		cnt=1;
	}
	else if(authorName1==''){
		jAlert("Please enter Name");
		cnt=1;
	}
	else if(publishDate==''){
		jAlert("Please select Publish Date");
		cnt=1;
	}
	else if(journalName==''){
		jAlert("Please enter journal Name");
		cnt=1;
	}
	else if(journalLevel==0){
		jAlert("Please enter journal Level");
		cnt=1;
	}
	else if(statusOfPaper==0){
		jAlert("Please select status Of Paper");
		cnt=1;
	}
	else if(remark==''){
		jAlert("Please select Remark");
		cnt=1;
	}
	
	else if(participation==''){
		jAlert("Please Select Participant/Winner");
		cnt=1;
	}
	
	else if(organizer==''){
		jAlert("Please enter Organizerr");
		cnt=1;
	}
	
	else if(publisher==''){
		jAlert("Please Enter publisher");
		cnt=1;
	}
	
	
	else if(conferJour==''){
		jAlert("Please Select Conference/Journal");
		cnt=1;
	}
	
	else if(details==''){
		jAlert("Please Enter Publication Details");
		cnt=1;
	}
	
	
	if(cnt==0){
		
		
		
		var employeeIds = [];
		var currentEmployeeId = $('#studId').val();
		$('#Wrkshop_AuthorNameListwithinInstitute li').each(function(){
			if($(this).attr('empId')!='')
				employeeIds.push($(this).attr('empId'))
		});
	
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="SINHGAD"){	
		if($.inArray(currentEmployeeId,employeeIds) == -1){
			jAlert('Please add yourself in author list and mention your authorship position in Sr.No. field !!');
			return;
		}
}
		
		
		
		
		
		$.ajax({
			url:'addResearchPublicationDetailsInStudProfile.json',
			data:{issnNo:issnNo,paperName:paperName,authorName1:authorName1,publishDate:publishDate,journalName:journalName,journalLevel:journalLevel,statusOfPaper:statusOfPaper,remark:remark,studReasearchId:studReasearchId,inserttype:inserttype,venue:venue,participation:participation,prize:prize,
				organizer:organizer,publisher:publisher,conferJour:conferJour,details:details,AuthorNamewithinInstitute:AuthorNamewithinInstitute.toString(),
				outsideAuthorName:outsideAuthorName.toString(),insidesideStudentName:insidesideStudentName.toString(),orchidId:orchidId},
			type:'POST',
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				getResearchPublication();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}
function checkReqForDelPublication(studReasearchId){
	
	if(statusForReasearchPub=="canChange"){
		deleteStudPublicationNew(studReasearchId);
	}
	else if(statusForReasearchPub=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForReasearchPub=="ReqAccepted"){
		 $('#btnFreezeReasearchPub').show();
		 deleteStudPublicationNew(studReasearchId);
	}
	else if(statusForReasearchPub=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="researchPub";
					sendRequestForEditing(tab);
				}
					});
	}
}

function deleteStudPublicationNew(studReasearchId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudResearch.json?studReasearchId='+studReasearchId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						getResearchPublication();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}
function uploadStudPublicationDocument(studentResearchPublicationId){
	if(studentResearchPublicationId != '' && studentResearchPublicationId != 'null' && studentResearchPublicationId != undefined){
		if(statusForReasearchPub=='canChange' || statusForReasearchPub=='ReqAccepted'){
			getDegreeCategoryId(gTabDegreeName.TabName_RESEARCHPUB,studentResearchPublicationId);
		}
		else if(statusForReasearchPub=='ReqSent'){
			jAlert("The request you have sent for editing is pending");
		}else if(statusForReasearchPub=='ReqNotSent'){
			$('#btnFreezeReasearchPub').show();
			jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r) {
				if(r ==true) {
					var tab="certificate";
					sendRequestForEditing(tab);
				}
			});
		}   
	} else
			jAlert('Student Certificate record not found!');
	
}

/////////ResearchBookPublication
var statusForAddingBookPub = '';
var studBookPubArr = new Array();
function getResearchBookPublication(){

	$.ajax({
		url:'getBookPublicationInStudProfile.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
␀␀␀			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newCertiMsg" style="display: none;">'+
			'	<table>'+
			'	<tr id="trReqPendingBookPub" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
			'	</tr>'+
			'	<tr id="trReqAcceptedBookPub" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
			'	</tr>'+
			'	<tr id="trSendReqBookPub" style="display: none;">'+
			'		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
			'	</tr>'+
			'</table>'+
			'</div>'+
			'<table class="table table-bordered table-condensed">';
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val() != "MEGHE"){
				boiler += '<tr><th style="color: #0088CC" colspan="8"><label><b><font>Research Book Publication Information</font></b></label></th></tr>';
			}else{
				boiler += '<tr><th style="color: #0088CC" colspan="8"><label><b><font>Book Publication Information</font></b></label></th></tr>';
			}
			boiler+='<tr><td colspan="8"><span style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack   freezeProfile hideSaveSubmitButton" onclick="checkReqConditionForAddEditBookPublication(\'NEW\',\'\')" ><i class="fa fa-plus"></i> Add Book Publication</a></span><span><input type="button" id="btnFreezeBookPub" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini  btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForAddingBookPub=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentBookPublicationId"]!=undefined){
						
						boiler+='<tr>';
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){
							boiler += '<th style="text-align:center;"><b>ISBN No</b></th>';
						}else{
							boiler += '<th style="text-align:center;"><b>ISSN No</b></th>';							
						}
						boiler += '<th style="text-align:center;"><b>Course</b></th>'+
						'<th style="text-align:center;"><b>Publisher</b></th>'+
						'<th style="text-align:center;"><b>Type</b></th>'+
						'<th style="text-align:center;"><b>Publish Date</b></th>'+
						'<th colspan="4" style="text-align:center;"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studBookPubArr=jArray;
						
							boiler+=	'<tr>'+
							'<td style="text-align: right;"><label>'+jArray[i]["bookISBN"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["bookSubject"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["bookPublisher"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["bookType"]+'</label></td>'+
							'<td style="text-align: center;"><label>'+jArray[i]["bookPublishYear"]+'</label></td>'+

							'<td style="text-align: right;"><a  class = "freezeProfile hideSaveSubmitButton" onclick="checkReqConditionForAddEditBookPublication(\''+i+'\',\''+jArray[i]["studentBookPublicationId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
							'<td style="text-align: right;"><a  class="freezeProfile hideSaveSubmitButton" onclick="checkReqForDelBookPublication(\''+jArray[i]["studentBookPublicationId"]+'\')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';

							boiler += '<td style="text-align: right;"><a onclick ="uploadStudBookPublicationDocument(\''+jArray[i]["studentBookPublicationId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
							boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#ResearchBookPublication').html(boiler);
			
					}else{
						
						boiler+='</table>';
						$('#ResearchBookPublication').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForAddingBookPub="ReqNotSent";
					boiler+='</table>';
					$('#ResearchBookPublication').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForAddingBookPub="canChange";
					boiler+='</table>';
					$('#ResearchBookPublication').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForAddingBookPub=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#ResearchBookPublication').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForAddingBookPub=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#ResearchBookPublication').html(boiler);
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForAddingBookPub="canChange";
					boiler+='</table>';
					$('#ResearchBookPublication').html(boiler);
				}
			
				if(statusForAddingBookPub=='ReqNotSent'){
					$('#newCertiMsg').show();
					$('#btnFreezeBookPub').hide();
					$('#trSendReqBookPub').show();
					$('#trReqAcceptedBookPub').hide();
					$('#trReqPendingBookPub').hide();
				}
				else if(statusForAddingBookPub=='ReqAccepted'){
					$('#newCertiMsg').show();
					$('#btnFreezeBookPub').show();
					$('#trReqAcceptedBookPub').show();
					$('#trSendReqBookPub').hide();
					$('#trReqPendingBookPub').hide();
				}
				else if(statusForAddingBookPub=='ReqSent'){
					$('#newCertiMsg').show();
					$('#trReqPendingBookPub').show();
					$('#trReqAcceptedBookPub').hide();
					$('#trSendReqBookPub').hide();
				}
				else if(statusForAddingBookPub=='canChange'){
					$('#trReqPendingBookPub').hide();
					$('#trReqAcceptedBookPub').hide();
					$('#trSendReqBookPub').hide();
				}
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
	
	/*if($('#customerHelper').val()=="BLM_MUNJAL") { 
		   $('input').attr('disabled',true);
		   $('button').attr('disabled',true);
		   $('div').attr('disabled',true);
		   $('select').attr('disabled',true).trigger("liszt:updated");
		   $('textArea').attr('disabled',true);
		   $('img').attr('disabled',true);
		   $('.freezeProfile').attr('disabled',true);
		  
	 }*/
}

function checkReqConditionForAddEditBookPublication(indexId, studentBookPublicationId){
	if(statusForAddingBookPub=="canChange"){
		addNewBookPublicationNew(indexId, studentBookPublicationId);
	}
	else if(statusForAddingBookPub=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingBookPub=="ReqAccepted"){
		 $('#btnFreezeBookPub').show();
			addNewBookPublicationNew(indexId, studentBookPublicationId);
	}
	else if(statusForAddingBookPub=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="researchBookPub";
					sendRequestForEditing(tab);
				}
					});
	}
	getstudProfileConfigurationDetailsCheckingNames();
}

function addNewBookPublicationNew(indexId, studBookId){

	var addSkillBoiler='';
	
		addSkillBoiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
								'<th colspan="2" style="color: #0088CC"><h4>Research Book Publication Information</h4></th>'+
							'</tr>'+
							'<tr>';
		if($('#customerHelper').val()!=undefined && ($('#customerHelper').val() == "HYGIA" || $('#customerHelper').val() == "MEGHE")){
			addSkillBoiler +='<td><b>ISBN No</b><b style="color:red;font-size:16px;" >*</b></td>';
		}else{
			addSkillBoiler +='<td><b>ISSN No</b><b style="color:red;font-size:16px;" >*</b></td>';
		}
		addSkillBoiler +='<td><input type="text" id="txtIssnNo" name="rBookISSNNOName"/></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Subject</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtSubject" name="rBookSubjectName" /></td>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Publisher</b> <b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtPublisher" name="rBookPublisherName"/></td>'+
							'</tr>'+
							'<tr>'+
							'<td><b>Type</b> <b style="color:red;font-size:16px;" >*</b></td>'+
							'<td><select id="cmbType" name="rBookTypeName">'+
							'<option value="0">Select</option>'+
							'<option value="Text">Text</option>'+
							'<option value="Reference">Reference</option></select></td>'+
						'</tr>'+
						'<tr>'+
						'<td><b>Publish Date</b> <b style="color:red;font-size:16px;" >*</b></td>'+
						'<td><input type="text" id="txtPublishDate" name="rBookPublisherDate" readonly /></td>'+
					'</tr>'+
							'<tr>'+
								'<td></td>'+
								'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnAddNewStudCerti" onclick="saveNewStudBookPublicationNew(\''+studBookId+'\')"/>'+
								'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="	getResearchBookPublication();" /></td>'+
							'</tr>'+
							'</tbody>'+
							'</table>';
	
	
		$('#ResearchBookPublication').html(addSkillBoiler);	
		
		$('#txtPublishDate').datepicker({
			dateFormat : 'M d,yy',
		});

	if(indexId!="NEW"){
		$('#txtIssnNo').val(studBookPubArr[indexId]["bookISBN"]);
		$('#txtSubject').val(studBookPubArr[indexId]["bookSubject"]);
		$('#txtPublisher').val(studBookPubArr[indexId]["bookPublisher"]);
		$('#cmbType').val(studBookPubArr[indexId]["bookType"]);
		$('#txtPublishDate').val(studBookPubArr[indexId]["bookPublishYear"]);
	}
}

function saveNewStudBookPublicationNew(studBookId){
	var inserttype = '';
	if(studBookId==undefined || studBookId==''){
		studBookId='';
		inserttype = 'new';
	}else{
		inserttype = 'edit';
	}

	var issnNo=$('#txtIssnNo').val();
	var subName=$('#txtSubject').val();
	var publisher=$('#txtPublisher').val();
	var type=$('#cmbType').val();
	var publishDateInBook=$('#txtPublishDate').val();
	//PercentGrade
	var cnt=0;
	if(issnNo==''){
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="HYGIA"){
		jAlert("Please enter Isbn No");
		}else{
			jAlert("Please enter Issn No");
		}
		cnt=1;
	}
	else if(subName==''){
		jAlert("Please enter subject");
		cnt=1;
	}
	else if(publisher==''){
		jAlert("Please enter publisher");
		cnt=1;
	}
	else if(type==0){
		jAlert("Please select type");
		cnt=1;
	}
	else if(publishDateInBook==''){
		jAlert("Please select Publish Date");
		cnt=1;
	}
	
	if(cnt==0){
		$.ajax({
			url:'addBookPublicationDetailsInStudProfile.json',
			data:{issnNo:issnNo,subName:subName,publisher:publisher,type:type,publishDateInBook:publishDateInBook,studBookId:studBookId,inserttype:inserttype},
			type:'POST',
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				getResearchBookPublication();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function checkReqForDelBookPublication(studBookId){
	
	if(statusForAddingBookPub=="canChange"){
		deleteStudBookPublicationNew(studBookId);
	}
	else if(statusForAddingBookPub=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForAddingBookPub=="ReqAccepted"){
		 $('#btnFreezeBookPub').show();
		 deleteStudBookPublicationNew(studBookId);
	}
	else if(statusForAddingBookPub=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="researchBookPub";
					sendRequestForEditing(tab);
				}
					});
	}
}
function deleteStudBookPublicationNew(studBookId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudResearchBook.json?studBookId='+studBookId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						getResearchBookPublication();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}
function uploadStudBookPublicationDocument(studentBookPublicationId){
	if(studentBookPublicationId != '' && studentBookPublicationId != 'null' && studentBookPublicationId != undefined){
		if(statusForAddingBookPub=='canChange' || statusForAddingBookPub=='ReqAccepted'){
			getDegreeCategoryId(gTabDegreeName.TabName_RESEARCHBOOK,studentBookPublicationId);
		}
		else if(statusForAddingBookPub=='ReqSent'){
			jAlert("The request you have sent for editing is pending");
		}else if(statusForAddingBookPub=='ReqNotSent'){
			$('#btnFreezeBookPub').show();
			jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r) {
				if(r ==true) {
					var tab="researchBookPub";
					sendRequestForEditing(tab);
				}
			});
		}   
	} else
			jAlert('Student award record not found!');
	
}

function sendRequestForEditing(tab) {
	// alert("innnn"+tab);
	clearForm('sendReqForEditingPopUp');
	$('#sendReqForEditingPopUp').modal('show').css({
		width : '600px',
		'margin-left' : function() {
			return -($(this).width() / 2);
		}
	});
	$("#txtTab").val(tab);
}

function sendReqForEditing2() {
	var tabVal = $('#txtTab').val();
	var reason = $('#txtReasonForEditing').val();
	// alert("tabVal3..."+tabVal);
	if (reason == "") {
		jAlert("Please enter reason");
	} else {
		$.ajax({
			url : 'sendRequestForEditing.json?reason=' + reason + '&tabVal='
			+ tabVal,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				$('#sendReqForEditingPopUp').modal('hide');
				getCustomAlertMessage("alert_create");
				// alert("tv"+tabVal);
				if (tabVal == "diploma") {
					var degreeType = "DIP";
					viewPGDetails(degreeType);
				} else if (tabVal == "UG" || tabVal == "PG") {
					viewPGDetails(tabVal);
				} else if (tabVal == "certificate") {
					viewStudCertificatesTab555();
				} else if (tabVal == "project") {
					showProjTabDetailsNew333();
				} else if (tabVal == "workExp") {
					viewWorkExpTab444();
				} else if (tabVal == "training") {
					viewTrainingList();
				} else if (tabVal == "gap") {
					getGapDetails();
				} else if (tabVal == "technical") {
					showTechnicalDetailsNew222();
				} else if (tabVal == "achievement") {
					viewAchievementsTab666();
				} else if (tabVal == "researchBookPub") {
					getResearchBookPublication();
				} else if (tabVal == "researchPub") {
					getResearchPublication();
				} else if (tabVal == "ssc") {
					getstudentsscinfo();
				} else if (tabVal == "hsc") {
					getstudenthscinfo();
				}
			},
			error : function() {
				// jAlert("Something went wrong, Please try after some time","Error");
			}
		});
	}
}

function freezeTab() {
	jConfirm("Do you want to freeze? ", "Confirm", function(r) {
		if (r == true) {
			$.ajax({
				url : 'freezeTab.json?studProChangeReqId=' + studProChangeReqId,
				dataType : 'text',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(text) {
					$('#sendReqForEditingPopUp').modal('hide');
					getCustomAlertMessage("alert_frozen");

					if (text == "achievement")
						viewAchievementsTab666();
					if (text == "certificate")
						viewStudCertificatesTab555();
					// if(text=="contactDetails")

					// if(text=="currentQuali")

					if (text == "diploma")
						viewPGDetails("DIP");
					if (text == "gap")
						getGapDetails();
					// if(text=="medicalDetail")

					// if(text=="personalDetail")

					if (text == "pg")
						viewPGDetails("PG");
					if (text == "project")
						showProjTabDetailsNew333();
					if (text == "researchBookPub")
						getResearchBookPublication();
					if (text == "researchPub")
						getResearchPublication();
					if (text == "ssc")
						getstudentsscinfo();
					if (text == "hsc")
						getstudenthscinfo();
					if (text == "technical")
						showTechnicalDetailsNew222();
					if (text == "training")
						viewTrainingList();
					if (text == "ug")
						viewPGDetails("UG");
					if (text == "workExp")
						viewWorkExpTab444();
				},
				error : function() {
					// jAlert("Something went wrong, Please try after some time","Error");
				}
			});
		}
	});
}

function saveCareerObjectives() {
	var studentCareerObjectiveId = $('#studentCareerObjectiveId').val();
	var objectives = '';
	for ( var i = 2; i <= 8; i++){
		if ($('#objCmb' + i).is(':checked')){
			objectives += i + '-' + $("#objTextarea" + i).val() + '/';
		}
			
	}
		if($('.careerObj1').is(':checked')){
			objectives += '1' + '-' + $("#objTextarea" + '1').val() + '/';
		}
	var strengths = '';
	if ($("#strengthTextarea").val() != '')
		strengths += $("#strengthTextarea").val();
	// strengths = $("#strengthCmb").val();

	var weakness = '';
	if ($("#weaknessTextarea").val() != '')
		weakness += $("#weaknessTextarea").val();
	// weakness = $("#weaknessCmb").val();

	var preferedCityOne = '';
	if ($('#locationPrefOneCmb').val() != -1)
		preferedCityOne = $('#locationPrefOneCmb').val();
	else {
		jAlert("Please Select Valid City", "Error");
		return;
	}

	var preferedCityTwo = '';
	if ($('#locationPrefTwoCmb').val() != -1)
		preferedCityTwo = $('#locationPrefTwoCmb').val();
	else {
		jAlert("Please Select Valid City", "Error");
		return;
	}

	var panIndia = '';
	if ($('#panIndia').is(':checked'))
		panIndia = $("#panIndia").val();
	else
		panIndia = 'noPanIndia';

	var cnt5 = 0;
	var cnt6 = 0;

	// if($('#strengthTextarea').val()=='' ||
	// $('#strengthTextarea').val()=='null' || $('#weaknessTextarea').val()==''
	// || $('#weaknessTextarea').val()=='null'){
	if (objectives == '')
		jAlert("Please select atleat one objective");
	else
		cnt5 = 1;

	if (strengths == '' || strengths == 'null' || weakness == ''
		|| weakness == 'null')
		jAlert("Please select atleat one strengh and weakness");
	else
		cnt6 = 1;
	/*
	 * } else{ cnt5=1; }
	 */

	if (cnt5 == 1 && cnt6 == 1) {
		$.ajax({
			url : 'saveCareerObjectives.json',
			data : {
				studentCareerObjectiveId : studentCareerObjectiveId,
				objectives : objectives,
				strengths : strengths,
				weakness : weakness,
				preferedCityOne : preferedCityOne,
				preferedCityTwo : preferedCityTwo,
				panIndia : panIndia
			},
			dataType : 'text',
			type : 'GET',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(msg) {
				getCustomAlertMessage("alert_create");
				getCmbForCareerObj();
				getCareerObjectives();
			},
			error : function() {
				getCustomAlertMessage("alert_error");
			}
		});
	}
}

function moveStrengthToTextArea() {
	var txtAreaStrngth = $('#strengthTextarea').val();
	var cmbVal = $('#strengthCmb').val();

	if (txtAreaStrngth != '' && txtAreaStrngth != null) {
		if (cmbVal != null) {
			if (txtAreaStrngth.match(cmbVal)) {

			} else
				txtAreaStrngth += ',';
		}
	}

	if (cmbVal != null) {
		if (txtAreaStrngth.match(cmbVal)) {
			jAlert("You have already selected this strength");
		} else
			txtAreaStrngth += cmbVal;
	}

	$('#strengthTextarea').val(txtAreaStrngth);
}

function moveWeaknessToTextArea() {
	var txtAreaWeakness = $('#weaknessTextarea').val();
	var cmbVal2 = $('#weaknessCmb').val();

	if (txtAreaWeakness != '' && txtAreaWeakness != null) {
		if (cmbVal2 != null) {
			if (txtAreaWeakness.match(cmbVal2)) {

			} else
				txtAreaWeakness += ',';
		}
	}

	if (cmbVal2 != null) {
		if (txtAreaWeakness.match(cmbVal2)) {
			jAlert("You have already selected this weakness");
		} else
			txtAreaWeakness += cmbVal2;
	}

	$('#weaknessTextarea').val(txtAreaWeakness);
}

function getCmbForCareerObj() {

	$.ajax({
		url : 'getStudentCitylist.json',
		dataType : 'json',
		type : 'GET',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var boiler = '';
			boiler += "<option value='-1'>Select</option>";
			for ( var i = 0; i < jArray.length; i++) {
				boiler += "<option value=" + jArray[i]["cityId"] + ">"
				+ jArray[i]["cityName"] + "</option>";
			}
			$("#locationPrefOneCmb").html(boiler);
			convertComboToSearchable('#locationPrefOneCmb');
			$("#locationPrefTwoCmb").html(boiler);
			convertComboToSearchable('#locationPrefTwoCmb');
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

function getCareerObjectives() {
	$('#studentCareerObjectiveId').val('');
	$.ajax({
		url : 'getCareerObjectives.json',
		dataType : 'json',
		type : 'GET',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			for ( var i = 0; i < jArray.length; i++) {
				if (jArray[i]["objective"] != undefined) {
					var objective = jArray[i]["objective"];
					if (objective.match("1-"))
						$('.careerObj1').attr("checked", true);
					if (objective.match("2-"))
						$('#objCmb2').attr("checked", true);
					if (objective.match("3-"))
						$('#objCmb3').attr("checked", true);
					if (objective.match("4-"))
						$('#objCmb4').attr("checked", true);
					if (objective.match("5-"))
						$('#objCmb5').attr("checked", true);
					if (objective.match("6-"))
						$('#objCmb6').attr("checked", true);
					if (objective.match("7-"))
						$('#objCmb7').attr("checked", true);
					if (objective.match("8-")) {
						$('#objCmb8').attr("checked", true);
						$('#objTextarea8').val(objective.split("8-")[1]);
					}
				}

				$('#studentCareerObjectiveId').val(
						jArray[i]["studentCareerObjectiveId"]);
				$('#strengthTextarea').val(jArray[i]["strength"]);
				$('#weaknessTextarea').val(jArray[i]["weakness"]);
				$('#locationPrefOneCmb').val(jArray[i]["locationId1"]);
				$('#locationPrefTwoCmb').val(jArray[i]["locationId2"]);

				/*
				 * for(var i=0;i<jArray.length;i++) {
				 * $('#selParticipatingInstitute option').each(function(index) {
				 * if($(this).val()==jArray[i]["instituteObj"]["instituteId"]) {
				 * $($(this)).attr('selected','selected');
				 * participatingInstituteForeditOrDelete.push(jArray[i]["instituteObj"]["instituteId"]); }
				 * }); }
				 */

			}
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

function getDegreeType() {
	var degId = $('#degName').val();

	if (degId != 0 && degId != '' && degId != null) {
		$
		.ajax({
			url : 'getDegreeTypeByDegreeName.json?degId=' + degId,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {

				if (jArray[0]["degreeType"] == 'COURSE_TYPE_Diploma') {
					$('#txtDegType').val("Diploma");
				} else if (jArray[0]["degreeType"] == 'COURSE_TYPE_PG_Diploma') {
					$('#txtDegType').val("PG Diploma");
				} else if (jArray[0]["degreeType"] == 'COURSE_TYPE_UG_Diploma') {
					$('#txtDegType').val("UG Diploma");
				}
			},
			error : function() {
				// alert("in Error stream CMB");
			}
		});
	} else {
		jAlert("Please select degree name");
	}
}

function postGraduationDetailsPart2(type, degreeType,StudGraduation) {
	var universityName = $('#universityName').val();
	var boiler = '<blink><p class="body" style="color: red"><b><sup>*</sup></b> Please enter the qualification details other than current qualification.</p></blink>'
		+ '		<table class="table table-bordered table-condensed"> '
		+ '			<tr><th style="color: #0088CC" colspan="3"><label><b><font>Student Graduation Details</font></b></label></th></tr>'
