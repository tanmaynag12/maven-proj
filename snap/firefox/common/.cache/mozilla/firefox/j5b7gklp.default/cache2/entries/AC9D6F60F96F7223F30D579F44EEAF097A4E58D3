function saveOtherDetails(){
	
	var TCIssuedByLastCollege = $('#txtTCIssuedByLastCollege').val();
	var PRNNum = $('#txtPRNNum').val();
	var eligibilityConfirmed = $('#txtEligibilityConfirmed').val();
	var monthYearOfMMSCourse = $('#txtMonthYearOfMMSCourse').val();
	var admitedToOtherInstitute = $('#txtAdmitedToOtherInstitute').val();
	var examType = $('#txtExamType').val();
	var montYearOfTest = $('#txtMontYearOfTest').val();
	var percentileObtained = $('#txtPercentileObtained').val();
	var homeDistrictOfStud = $('#txtHomeDistrictOfStud').val();
	var homeStateOfStud = $('#txtHomeStateOfStud').val();
	var typeOfCandidature = $('#txtTypeOfCandidature1').val();
	var jK_EWS_TFWS = $('#txtJK_EWS_TFWS').val();
	var expInMonths = $('#txtExpInMonths').val(); 
	
	 $.ajax({
				url:'saveOtherDetails.json',
				dataType:'text',
				async:false,
				type : 'GET',
				data:{
				 TCIssuedByLastCollege:TCIssuedByLastCollege,
				 PRNNum:PRNNum,
				 eligibilityConfirmed:eligibilityConfirmed,
				 monthYearOfMMSCourse:monthYearOfMMSCourse,
				 admitedToOtherInstitute:admitedToOtherInstitute,
				 examType:examType,
				 montYearOfTest:montYearOfTest,
				 percentileObtained:percentileObtained,
				 homeDistrictOfStud:homeDistrictOfStud,
				 homeStateOfStud:homeStateOfStud,
				 typeOfCandidature:typeOfCandidature,
				 jK_EWS_TFWS:jK_EWS_TFWS,
				 expInMonths:expInMonths
				},
				beforeSend: function() {
					startCommonAjaxSpinner();
														
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){
						getCustomAlertMessage("alert_create");
				},
				error:function(){
					getCustomAlertMessage("alert_error");
				}
			});
}
function showHideTwelthSecondaryLanguageTxtFun()
{
	if($('#applstud12thSecondaryLanguage').val().toUpperCase().indexOf('OTHER') != -1){
		$('#twelthSecondaryLanguageNameTR').show('slow');
	}else {
		$('#twelthSecondaryLanguageNameTR').hide('slow');
		}	
}

function viewExtraCurricularActivities(){

	$.ajax({
		url:'getStudentExtraCurricularActivity.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var colspan = 4;
			if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){
				colspan = 6;
			}
			var boiler= ''+
			'<table class="table table-bordered table-condensed">'+
			'<tr>'+
			'<th colspan="'+colspan+'" style="color: #0088CC"><h4>Extra Curricular Achievements</h4></th>'+
			'</tr>';
			boiler+='<tr><td colspan="'+colspan+'"><span style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack " onclick="checkReqConditionForExtraActivity(\'NEW\',\'\')" ><i class= "fa fa-plus"></i> Add Extra-curricular activities</a></span></td></tr>';
			
			if(jArray.length == 0){
				boiler+='</table>';
				$('#extraCurricularAvtivityDiv').html(boiler);
			}else{
						boiler+='<tr>'+
						'<th><b>Extra-curricular Activity</b></th>';
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){
							boiler+='<th><b>Level</b></th>'+
							'<th><b>Date</b></th>';
						}
						boiler+='<th></th>'+
						'<th></th>'+
						'<th></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							/***Removed gaziabad Client Condition as Required for Freezing***/
							if( $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD"){
								$('#certificateAttachment').show();
							}else{
							gIsProfileFreezed= jArray[i]["isProfileFreezed"];
							if(gIsProfileFreezed==true){
								$('#certificateAttachment').hide();
							}
						}
							
							studExtraCurrActivityArr=jArray;
						
							boiler+='<tr>'+
								'<td style="text-align: left;"><label>'+jArray[i]["extraCurrActivityName"]+'</label></td>';
								
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){		
							boiler+='<td style="text-align: left;"><label>'+jArray[i]["levelType"]+'</label></td>'+
									'<td style="text-align: left;"><label>'+jArray[i]["dateOfActivity"]+'</label></td>';
						}
								
							boiler+='<td style="text-align: right;"><a onclick="checkReqConditionForExtraActivity(\''+i+'\',\''+jArray[i]["extraCurrActivityId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</td>'+
								'<td style="text-align: right;"><a onclick="checkReqForExtraAchievement('+jArray[i]["extraCurrActivityId"]+')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</td>';
								
								boiler += '<td style="text-align: right;"><a onclick = "uploadExtraActivityDocument(\''+jArray[i]["extraCurrActivityId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack"></i>Upload</td>';
								
								boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#extraCurricularAvtivityDiv').html(boiler);
		}
				
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
}
function checkReqConditionForExtraActivity(indexId, extraCurrActivityId){

	/***Removed gaziabad Client Condition as Required for Freezing***/
	if( $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD"){
		addNewExtraCurrActivity(indexId, extraCurrActivityId);
	}else{
	if(gIsProfileFreezed==true){
		jAlert("Your Profile is Freezed. For any changes contact Academic Admin ");
	}else{
		addNewExtraCurrActivity(indexId, extraCurrActivityId);
	}
	}
}

function addNewExtraCurrActivity(indexId, extraCurrActivityId){
	
	var boiler='';
	
	boiler += '<table class="table table-bordered table-condensed">'+
							'<tbody>'+
							'<tr>'+
								'<th colspan="2" style="color: #0088CC"><h4>Extra Curricular Achievements</h4></th>'+
							'</tr>'+
							'<tr>'+
								'<td><b>Extra Curricular Achievements</b><b style="color:red;font-size:16px;" >*</b></td>'+
								'<td><input type="text" id="txtExtraAchievement" /></td>'+
							'</tr>';
							
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){
								boiler +='<tr>'+
									'<td><b>Level</b><b style="color:red;font-size:16px;" >*</b></td>'+
									'<td><select id="txtExtraAchievLevel" onchange="getLevelTypeExtraCur();">'+
									'<option value="0">Select</option><option value="National">National</option>'+
									'<option value="International">International</option>'+
									'<option value="State">State</option>'+
									'<option value="Regional">Regional</option>'+
									'<option value="Other">Other</option>'+
									'</select>&nbsp;&nbsp;&nbsp;<input type="text" id="txtOtherLevel" style="display:none;  margin-top: 9px;"></td>'+
								'</tr>'+
								'<tr>'+
									'<td><b>Date</b><b style="color:red;font-size:16px;" >*</b></td>'+
									'<td><input type="text" id="txtExtraAchievDate" /></td>'+
								'</tr>';
							}
							boiler += '<tr>'+
								'<td></td>'+
								'<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnExtraAchievement" onclick="saveNewExtraAchievement('+extraCurrActivityId+')"/>'+
								'&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="viewExtraCurricularActivities()" /></td>'+
							'</tr>'+
							'</tbody>'+
							'</table>';
	
	
	$('#extraCurricularAvtivityDiv').html(boiler);
	$("#txtExtraAchievDate").datepicker({
        dateFormat: 'M dd,yy',
    });
	
	if(indexId!="NEW"){
		$('#txtExtraAchievement').val(studExtraCurrActivityArr[indexId]["extraCurrActivityName"]);
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){
			if(studExtraCurrActivityArr[indexId]["levelType"]!="National" && studExtraCurrActivityArr[indexId]["levelType"]!="International" && studExtraCurrActivityArr[indexId]["levelType"]!="State" && studExtraCurrActivityArr[indexId]["levelType"]!="Regional"){
				$("#txtOtherLevel").show();
				$('#txtExtraAchievLevel').val("Other");
				$('#txtOtherLevel').val(studExtraCurrActivityArr[indexId]["levelType"]);
			}else{
				$('#txtExtraAchievLevel').val(studExtraCurrActivityArr[indexId]["levelType"]);
			}
			$('#txtExtraAchievDate').val(studExtraCurrActivityArr[indexId]["dateOfActivity"]);
		}
	}
}
function getLevelTypeExtraCur() {
    var selectedValue = $("#txtExtraAchievLevel").val();
    
    if (selectedValue == "Other") {
        $("#txtOtherLevel").show();
    }else{
        $("#txtOtherLevel").hide();
    }
}

function saveNewExtraAchievement(extraCurrActivityId){
	
	if(extraCurrActivityId==undefined){
		extraCurrActivityId='';
	}

	var extraAchievement = $('#txtExtraAchievement').val();
	var txtExtraAchievLevel = $('#txtExtraAchievLevel option:selected').val();
	if(txtExtraAchievLevel == "Other"){
		txtExtraAchievLevel = $('#txtOtherLevel').val();
	}
	var txtExtraAchievDate = $('#txtExtraAchievDate').val();
	
	var cnt=0;
	if(extraAchievement==''){
		jAlert("Please enter name of Achievement");
		return;
		//cnt=1;
	}
	
	if(cnt==0){
		$.ajax({
			url:'addExtraAchievement.json',
			data:{extraAchievement:extraAchievement,
			      extraCurrActivityId:extraCurrActivityId , 
				  txtExtraAchievLevel : txtExtraAchievLevel ,
				  txtExtraAchievDate : txtExtraAchievDate	
				 },
			dataType:'text',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){
				getCustomAlertMessage("alert_create");
				viewExtraCurricularActivities();
			},
			error:function(){
				////alert("in Error stream CMB");
			}
		});
	}
}

function checkReqForExtraAchievement(extraCurrActivityId){
	/***Removed gaziabad Client Condition as Required for Freezing***/
	if( $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD"){
		deleteStudExtraAchievement(extraCurrActivityId);
	}else{
	if(gIsProfileFreezed==true){
		jAlert("Your Profile is Freezed. For any changes contact Academic Admin ");
	}else{
		deleteStudExtraAchievement(extraCurrActivityId);
	}
	}
}

function deleteStudExtraAchievement(extraCurrActivityId){
	
	jConfirm("Do you want to delete? ","Confirm",function(r)
		{
			if(r ==true)
			{
				$.ajax({
					url:'deleteStudentExtraAchievement.json?extraCurrActivityId='+extraCurrActivityId,
					dataType:'text',
					async:false,
					beforeSend: function() {
						startCommonAjaxSpinner();
					},
					complete: function() {
						stopCommonAjaxSpinner();
					},
					success:function(jArray){
						getCustomAlertMessage("alert_delete");
						viewExtraCurricularActivities();
					},
					error:function(){
						////alert("in Error stream CMB");
					}
				});
			}
		});
	}

function uploadExtraActivityDocument(extraCurrActivityId){
	if(extraCurrActivityId != '' && extraCurrActivityId != 'null' && extraCurrActivityId != undefined){
			getDegreeCategoryId(gTabDegreeName.TabName_EXTRAACHIEVEMENT,extraCurrActivityId);
	} else
			jAlert('Student extra achievement record not found!');
	
}
function placementCVPDF(){
	window.location.replace('genrateStudentsCVZIPAccordingProfileData.json?studentId='+$('#studentIdBySession').val()+'&flag='+'studentProfile')
}

function viewCovidVaccinationDetails(){
		var coviVaccBoiler = 
	`<table  class="table table-bordered">
							<tr> 
								<th colspan = "2" style="color: #0088CC"><label><b><font>Student Covid Vaccination Details :</font></b></label></th>
							</tr>
							<tr>
							<td>Are you vaccinated?</td>
								<td>
								<input type="radio"  name="isVaccinated" value="Yes" id="yesVaccinated" onChange="areYouVaccinated()"><label style="display:inline;">Yes</label>&nbsp;&nbsp;&nbsp;&nbsp;
			   					<input type="radio"  name="isVaccinated" checked="checked" value="No" onclick="areYouVaccinated()" id="noVaccinated" ><label style="display:inline;">No</label>&nbsp;&nbsp;&nbsp;&nbsp;
								</td>
							</tr>
							<tr class="hideRow">
							<td>Name of the vaccine taken</td>
								<td><select id="txtnameOfVaccineTaken"><option value="-1">Select</option></select></td>
								
							</tr>
							<tr class="hideRow">
							<td>First dose taken?</td>
							<td>
								<input type="radio"  name="isFirstDose"  value="Yes" id="yesFirstVaccine"><label style="display:inline;">Yes</label>&nbsp;&nbsp;&nbsp;&nbsp;
			   					<input type="radio"  name="isFirstDose" checked="checked" value="No" id="noFirstVaccine" ><label style="display:inline;">No</label>&nbsp;&nbsp;&nbsp;&nbsp;
										<div class="hideRow" id="firstCowid" first_Dose="">
											</div>
							</td>
							</tr>
							<tr class="hideRow">
							<td>Date on which first dose taken</td>
								<td><input type="text" id="txtDateOnWhichFirstDoseTaken" name="dateOnWhichFirstDose" readonly></td>
							</tr>
							<tr class="hideRow">
							<td>Second dose taken?</td>
							<td>
								<input type="radio"  name="isSecondDose" value="Yes" id="yesSecondVaccine" onchange = "hideDuedateTR()"><label style="display:inline;">Yes</label>&nbsp;&nbsp;&nbsp;&nbsp;
			   					<input type="radio"  name="isSecondDose" checked="checked" value="No" id="noSecondVaccine" onchange = "hideDuedateTR()"><label style="display:inline;">No</label>&nbsp;&nbsp;&nbsp;&nbsp;
								<div class="hideRow" id="secondCovid" second_Dose="">
								</div>
							</td>
							</tr>
							<tr class="hideRow" id="hideDateOnWhichSecondDoseTakenTR">
							<td>Date on which second dose taken</td>
								<td><input type="text" id="txtDateOnWhichSecondDoseTaken" name="dateOnWhichSecondDose" readonly></td>
							</tr>
							<tr class="hideRow" id="hideSecondDueDateTR">
							<td>Due date for second dose.</td>
							<td><input type="text" id="txtDueDateForSecondDose" name="DueDateForSecondDose" readonly></td>
							</tr>
						    
		
						   <tr class="hideRow">
							<td>Booster dose taken?</td>
							<td>
								<input type="radio"  name="isThirdDose" value="Yes" id="yesThirdVaccine" onchange = "hideDuedateTR1()"><label style="display:inline;">Yes</label>&nbsp;&nbsp;&nbsp;&nbsp;
			   					<input type="radio"  name="isThirdDose" checked="checked" value="No" id="noThirdVaccine" onchange = "hideDuedateTR1()"><label style="display:inline;">No</label>&nbsp;&nbsp;&nbsp;&nbsp;
								<div class="hideRow" id="thirdCovid" third_Dose="">
								</div>
							</td>
							</tr>
							
							<tr class="hideRow" id="hideDateOnWhichThirdDoseTakenTR">
							<td>Date on which Booster dose taken</td>
								<td><input type="text" id="txtDateOnWhichThirdDoseTaken" name="dateOnWhichThirdDose" readonly></td>
							</tr>
							
							<tr class="hideRow" id="hideThirdDueDateTR">
							<td>Due date for Booster dose.</td>
							<td><input type="text" id="txtDueDateForThirdDose" name="DueDateForThirdDose" readonly></td>
							</tr>
												
							<tr>
							<td colspan = "2" style="text-align:center;">
								<input type="Button" class="btn btn-inverse btn-mini " onclick="saveCovidVaccineDetails()"  id="saveCovidVaccine" value="Save" >
								<input type="Button" class="btn btn-inverse btn-mini " onclick="cancelCovidVaccineDetails()"  id="cancelCovidVaccine" value="Cancel" >
							</td>
							</tr>
						</table>`;
						$('#covidVaccineData').html(coviVaccBoiler);
						$.ajax({
			url : 'getVaccineListAndDoseType.json',
				dataType : 'json',
				async : false,
				type : 'GET',
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					var boiler = '<option value="0">Select</option>';
					for ( var i = 0; i <jArray[0].length; i++)
					   {
						  boiler += '<option value="' + jArray[0][i]["vaccName"] + '">'
						  +jArray[0][i]["vaccName"] + '</option>';
				          
					   }
		             
					$("#txtnameOfVaccineTaken").html(boiler);
					convertComboToSearchable('#txtnameOfVaccineTaken');
					
					
				},
			});
						
			$.ajax({
			url : 'getStudCovidVaccineDetails.json',
			dataType : 'json',
			async : false,
			type : 'GET',
			beforeSend : function() {
			},
			complete : function() {
			},		
			success:function(jArray){
				
				var isTakenvaccine = jArray[0]["hasTakenvaccine"]; 
				$("input[name=isVaccinated][value='"+isTakenvaccine+"']").attr('checked', 'checked');
				var isFirstDose = jArray[0]["firstDose"]; 
				$("input[name=isFirstDose][value='"+isFirstDose+"']").attr('checked', 'checked');
				var isSecondDose = jArray[0]["secondDose"]; 
				$("input[name=isSecondDose][value='"+isSecondDose+"']").attr('checked', 'checked');
				var isThirdDose = jArray[0]["boosterDose"];
				$("input[name=isThirdDose][value='"+isThirdDose+"']").attr('checked', 'checked');
				
				$('#txtnameOfVaccineTaken').val(jArray[0]["nameOfvaccine"]);
				$('#txtDueDateForSecondDose').val(jArray[0]["secondDoseDueDate"]);	
				
				$('#txtDueDateForThirdDose').val(jArray[0]["boosterDoseDueDate"]);
				
				var studentId = jArray[0]["studentId"];	
				var docPathFirstDose= jArray[0]["docPathFirstDose"]; 
				var docExt = jArray[0]["docExtensionFirstDose"];
				var docName = jArray[0]["docNameFirstDose"];
				var docPathSecondDose= jArray[0]["docPathSecondDose"]; 
				var docSecondDoseExt = jArray[0]["docExtensionSecondDose"];
				var docSecondDoseName = jArray[0]["docNameSecondDose"];
				
				var docPathBoosterDose= jArray[0]["docPathBoosterDose"]; 
				var docBoosterDoseExt = jArray[0]["docExtensionBoosterDose"];
				var docBoosterDoseName = jArray[0]["docNameBoosterDose"];
				
				
				var docIdsFirst = "covidFirst_Dose"+studentId;  
				var docIdSecond = "covidSecond_Dose"+studentId;
				var docIdBooster = "covidBooster_Dose"+studentId;
				$('#txtDateOnWhichFirstDoseTaken').val(jArray[0]["dateFirstDoseTaken"]);	
				$('#txtDateOnWhichSecondDoseTaken').val(jArray[0]["dateSecondDoseTaken"]);	
				$('#txtDateOnWhichThirdDoseTaken').val(jArray[0]["dateBoosterDoseTaken"])
				
			
		var firstCovidVaccine= `	<div id="${docIdsFirst}" class="firstDose" studId="${studentId}"></div>
	 				     		 	<div>
	 				     		 			<div class="warp-text-and-expand-on-hover" id="uploadedDocumentImg_First"></div>
	 				    		    </div>	`;
		$('#firstCowid').html(firstCovidVaccine);
		$("#firstCowid").attr("first_Dose",docPathFirstDose);
		
		var secondCovidVaccine= `	<div id="${docIdSecond}" class="secondDose" studId="${studentId}"></div>
	 				     		 	<div>
	 				     		 			<div class="warp-text-and-expand-on-hover" id="uploadedDocumentImg_Second"></div>
	 				    		    </div>	`;
		$('#secondCovid').html(secondCovidVaccine);
		$("#secondCovid").attr("second_Dose",docPathSecondDose);
		
		
		var boosterCovidVaccine= `	<div id="${docIdBooster}" class="thirdDose" studId="${studentId}"></div>
	 				     		 	<div>
	 				     		 			<div class="warp-text-and-expand-on-hover" id="uploadedDocumentImg_Third"></div>
	 				    		    </div>	`;
		$('#thirdCovid').html(boosterCovidVaccine);
		$("#thirdCovid").attr("third_Dose",docPathBoosterDose);
		
		if(docPathFirstDose!="NA" && docPathFirstDose!=""){
				var uploadedBoiler='<b style ="disply:none" id="covid_1" uploaded="true"></b><img src="img/files/pdf.png" style="height:30px;margin-right:10px;"><a href="viewUploadedDocument.json?uploadedDocumentFilePath='+docPathFirstDose+'&originalDocumentName='+docName+'&documentExtension='+docExt+'" target="_blank" style="cursor:pointer"><b>View Uploaded Document</b></a>';
			$('#uploadedDocumentImg_First').html(uploadedBoiler);
			}else{
			$('#uploadedDocumentImg_First').html('<b style ="disply:none" id="covid_1" uploaded="false">No Document found</b>');	
			}
		
		if(docPathSecondDose!="NA" && docPathSecondDose!=""){
				var uploadedBoiler='<b style ="disply:none" id"covid_2" uploaded="true"></b> <img src="img/files/pdf.png" style="height:30px;margin-right:10px;"><a href="viewUploadedDocument.json?uploadedDocumentFilePath='+docPathSecondDose+'&originalDocumentName='+docSecondDoseName+'&documentExtension='+docSecondDoseExt+'" target="_blank" style="cursor:pointer"><b>View Uploaded Document</b></a>';
			$('#uploadedDocumentImg_Second').html(uploadedBoiler);
			}else{
			$('#uploadedDocumentImg_Second').html('<b style ="disply:none" id="covid_2" uploaded="false">No Document found</b>');	
			}
			
		if(docPathBoosterDose!="NA" && docPathBoosterDose!=""){
				var uploadedBoiler='<b style ="disply:none" id"covid_3" uploaded="true"></b> <img src="img/files/pdf.png" style="height:30px;margin-right:10px;"><a href="viewUploadedDocument.json?uploadedDocumentFilePath='+docPathBoosterDose+'&originalDocumentName='+docBoosterDoseName+'&documentExtension='+docBoosterDoseExt+'" target="_blank" style="cursor:pointer"><b>View Uploaded Document</b></a>';
			$('#uploadedDocumentImg_Third').html(uploadedBoiler);
			}else{
			$('#uploadedDocumentImg_Third').html('<b style ="disply:none" id="covid_3" uploaded="false">No Document found</b>');	
			}	
			
		$('#'+docIdsFirst).uploadDocument({
	        limit:1,
	        fileTypes:['jpg','png','jpeg','pdf']
	    });
		
		$('#'+docIdSecond).uploadDocument({
	        limit:1,
	        fileTypes:['jpg','png','jpeg','pdf']
	    });
			
		$('#'+docIdBooster).uploadDocument({
	        limit:1,
	        fileTypes:['jpg','png','jpeg','pdf']
	    });	
			
		   },
			error:function(){
				getCustomAlertMessage("alert_error");
			}
			});			
				$('#txtDueDateForSecondDose').datepicker(
            {
                dateFormat : "M dd,yy",
                inline : true,
                changeMonth : true,
                changeYear : true,
                yearRange : "-20:+20",
                firstDay : 1,
                onSelect : function() {
            }
            });

            $('#txtDueDateForThirdDose').datepicker(
            {
                dateFormat : "M dd,yy",
                inline : true,
                changeMonth : true,
                changeYear : true,
                yearRange : "-20:+20",
                firstDay : 1,
                onSelect : function() {
            }
            });

			$('#txtDateOnWhichFirstDoseTaken').datepicker(
            {
                dateFormat : "M dd,yy",
                inline : true,
                changeMonth : true,
                changeYear : true,
                yearRange : "-20:+20",
                firstDay : 1,
                onSelect : function() {
            }
            });

			$('#txtDateOnWhichSecondDoseTaken').datepicker(
            {
                dateFormat : "M dd,yy",
                inline : true,
                changeMonth : true,
                changeYear : true,
                yearRange : "-20:+20",
                firstDay : 1,
                onSelect : function() {
            }
            });

            $('#txtDateOnWhichThirdDoseTaken').datepicker(
            {
                dateFormat : "M dd,yy",
                inline : true,
                changeMonth : true,
                changeYear : true,
                yearRange : "-20:+20",
                firstDay : 1,
                onSelect : function() {
            }
            });

     	hideDuedateTR();
		hideDuedateTR1();
     }		
		
function saveCovidVaccineDetails(){
	var hasTakenVaccine = $('input[name = "isVaccinated"]:checked').val(); 
	var nameOfVaccine = $('#txtnameOfVaccineTaken').val();
	var isFirstDose =  $('input[name = "isFirstDose"]:checked').val(); 
	var isSecondDose =  $('input[name = "isSecondDose"]:checked').val();  
	var isThirdDose = $('input[name = "isThirdDose"]:checked').val();
	
	var dueDateForSecondDose = $('#txtDueDateForSecondDose').val();
	var dueDateForThirdDose = $('#txtDueDateForThirdDose').val();
	
	var docIdsFirst = "covidFirst_Dose";
	var docIdSecond = "covidSecond_Dose";
	var docIdBooster = "covidBooster_Dose";
	
	var studentId=$(".secondDose").attr("studId");
	var studentId=$(".thirdDose").attr("studId");
	
	var dateOnWhichFirstDose = $('#txtDateOnWhichFirstDoseTaken').val();
	var dateOnWhichSecondDose = $('#txtDateOnWhichSecondDoseTaken').val();
	var dateOnWhichThirdDose = $('#txtDateOnWhichThirdDoseTaken').val();
	
	var divid ="covidSecond_Dose"+""+studentId+"_uploadedFileList";
	var divid1 ="covidFirst_Dose"+""+studentId+"_uploadedFileList";
	var divid2 ="covidBooster_Dose"+""+studentId+"_uploadedFileList";
	             
	if($('input[name = "isFirstDose"]:checked').val()=="Yes" &&  $("#covid_1").attr("uploaded")=="false")
	{
		if($("#"+divid1).html()==""){
		jAlert('Please Upload Document for First Dose!');
		return false;
		}
	}
	if($('input[name = "isSecondDose"]:checked').val()=="Yes" &&  $("#covid_2").attr("uploaded")=="false")
	{
		if($("#"+divid).html()==""){
		jAlert('Please Upload Document for Second Dose!');
		return false;
		}
	}
	
	if($('input[name = "isThirdDose"]:checked').val()=="Yes" &&  $("#covid_3").attr("uploaded")=="false")
	{
		if($("#"+divid2).html()==""){
		jAlert('Please Upload Document for Booster Dose!');
		return false;
		}
	}
	
		
	if($('input[name = "isFirstDose"]:checked').val()=="Yes")
	{
		if(dateOnWhichFirstDose=="" || dateOnWhichFirstDose==undefined){
		jAlert('Please select Date on which first dose taken!');
		return false;
		}
	} 
	if($('input[name = "isSecondDose"]:checked').val()=="Yes")
	{
		if(dateOnWhichSecondDose=="" || dateOnWhichSecondDose==undefined){
		jAlert('Please select Date on which second dose taken!');
		return false;
		}
	}
	if($('input[name = "isSecondDose"]:checked').val()=="No")
	{
		if(dueDateForSecondDose=="" || dueDateForSecondDose==undefined){
		jAlert('Please select Due Date!');
		return false;
		}
	} 
	
	
	if($('input[name = "isThirdDose"]:checked').val()=="Yes")
	{
		if(dateOnWhichThirdDose=="" || dateOnWhichThirdDose==undefined){
		jAlert('Please select Date on which Booster dose taken!');
		return false;
		}
	}
	if($('input[name = "isThirdDose"]:checked').val()=="No")
	{
		if(dueDateForThirdDose=="" || dueDateForThirdDose==undefined){
		jAlert('Please select Due Date!');
		return false;
		}
	} 

 $.ajax({
				url:'saveCovidVaccineDetails.json',
				dataType:'text',
				async:false,
				type : 'GET',
				data:{
				 hasTakenVaccine : hasTakenVaccine,
				 nameOfVaccine : nameOfVaccine,
				 isFirstDose : isFirstDose,
				 isSecondDose : isSecondDose,
			     isThirdDose:isThirdDose,
				 dueDateForSecondDose : dueDateForSecondDose,
			     dueDateForThirdDose:dueDateForThirdDose,
			     docIdsFirst :docIdsFirst,
				 docIdSecond : docIdSecond,
			     docIdBooster:docIdBooster,
				 dateOnWhichFirstDose : dateOnWhichFirstDose,
				 dateOnWhichSecondDose : dateOnWhichSecondDose,
			     dateOnWhichThirdDose : dateOnWhichThirdDose 
				},
				beforeSend: function() {
				},
				complete: function() {
				},
				success:function(jArray){
						getCustomAlertMessage("alert_create");
				},
				error:function(){
					getCustomAlertMessage("alert_error");
				}
			});
}

function hideDuedateTR(){
var selectedValue = $('input[name = "isSecondDose"]:checked').val();
	if(selectedValue=='Yes'){
		$('#hideSecondDueDateTR').hide();
		$('#hideDateOnWhichSecondDoseTakenTR').show();
	}else{
		$('#hideSecondDueDateTR').show();
		$('#hideDateOnWhichSecondDoseTakenTR').hide();
	}
}

function hideDuedateTR1()
{

var selectedValue1 =$('input[name = "isThirdDose"]:checked').val(); 
if(selectedValue1=='Yes'){
		$('#hideThirdDueDateTR').hide();
		$('#hideDateOnWhichThirdDoseTakenTR').show();
	}else{
		$('#hideThirdDueDateTR').show();
		$('#hideDateOnWhichThirdDoseTakenTR').hide();
	}	
	
}

function areYouVaccinated(){
	var selectedValue = $('input[name = "isVaccinated"]:checked').val();
	if(selectedValue=='No'){
		$('.hideRow').hide();
	}else{
		$('.hideRow').show();
		$('#yesFirstVaccine').prop("checked", true);
		$('#noFirstVaccine').prop("checked", false);
		$('input[name = "isFirstDose"]').prop("disabled",true);
	}
	$.ajax({
			url : 'getVaccineListAndDoseType.json',
				dataType : 'json',
				async : false,
				type : 'GET',
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					var boiler = '<option value="0">Select</option>';
					for ( var i = 0; i <jArray[0].length; i++)
					   {
						  boiler += '<option value="' + jArray[0][i]["vaccName"] + '">'
						  +jArray[0][i]["vaccName"] + '</option>';
					   }
		            $("#txtnameOfVaccineTaken").html(boiler);
					convertComboToSearchable('#txtnameOfVaccineTaken');
				},
			});
	
}

function freezeProfile(){
	jConfirm("Once submitted, Profile Information cannot be changed without prior approval of Academic Admin. \nClick 'Ok' to continue.","Confirm",function(r)
			{
		if(r ==true)
		{
			$.ajax({
				url:'freezeStudentProfileIMT.json', 
				dataType:'json',
				async:false,
				success:function(jArray){
					getCustomAlertMessage("Profile submitted successfully !!");
					$('#freezeDetailsDiv').hide();
					$('#msgHeaderDiv').show();
					// $('#msgHeaderTd').html('<label class = "alert alert-success">Data is non-editable. For any changes contact Academic Admin</label>');
				},

			});	
		}
		else{
			$('#freezeProfileChk').attr('checked',false);
		}
			});
}
function freezeStudentDetails(){
	$.ajax({
		url:'getFreezeStudentDetails.json', 
		dataType:'json',
		async:false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(jArray["isProfileFreezed"]==true){
				$('#freezeDetailsDiv').hide();
				$('#msgHeaderDiv').show();
			}
			else{
				$('#freezeDetailsDiv').show();
				$('#msgHeaderDiv').hide();
				$('#freezeProfileChk').attr('checked',false);
				
				if (jArray["error"] == true){
					if(($('#customerHelper').val()=="MANDSAUR")&&((jArray["errorMsg"]=="Profile image not uploaded")||(jArray["errorMsg"]=="Signature not uploaded")||
							(jArray["errorMsg"]=="10th documents are not uploaded")|| (jArray["errorMsg"]=="Adhar Card not uploaded in Personal Tab")||
							(jArray["errorMsg"]=="UG documents are not uploaded")||(jArray["errorMsg"]=="12th/Diploma documents are not uploaded"))){
								$('#freezeDetailsDiv').hide();
								var errorMsg=jArray["errorMsg"];
								$('#showProfileMsg').html('<label class = "alert alert-important"><strong>'+errorMsg+'.'+' Please fill in your complete profile and upload necessary documents for final submission.</strong></label>');
					}else if($('#customerHelper').val()!="MANDSAUR"){
							$('#freezeDetailsDiv').hide();
							var errorMsg=jArray["errorMsg"];
							$('#showProfileMsg').html('<label class = "alert alert-important"><strong>'+errorMsg+'.'+' Please fill in your complete profile and upload necessary documents for final submission.</strong></label>');
					}
				}
				else{
					$('#showProfileMsg').hide();
				}
			}
		},

	});	

}
function validateMaxValue(i){
	$('#txtPGmarks'+i).css('border-color', '');
	$('#txtPGoutof'+i).css('border-color', '');
	var txtPGmarks=$('#txtPGmarks'+i).val();
	var txtPGoutof=$('#txtPGoutof'+i).val();
	if(txtPGoutof!='' && eval(txtPGmarks)>eval(txtPGoutof)){
		//alert($('#txtPGoutof'+i).val()!=''+"   "+($('#txtPGmarks'+i).val()>$('#txtPGoutof'+i).val()))
		$('#txtPGmarks'+i).css('border-color', 'red');
		$('#txtPGmarks'+i).val('');
		jAlert("You have entered marks more than out of");
		return false;
	}	
}

function allnameinmarathi(id)
{
	if(id=="txtfirstNameMarathi")
	{
		if ($("#cmbGoogleLanguage_txtfirstNameMarathi option:selected").text() == '') {
			googleLanguage([id]);
			$("#cmbGoogleLanguage_txtfirstNameMarathi").css(
				{
					position: 'relative',
					top: '-7.5px',
					left: '19.417px',
					width: '80px',
					opacity: '1',
					height: '27px'
				}
			);
			$("#cmbGoogleLanguage_txtmiddleNameMarathi").hide();
			$("#cmbGoogleLanguage_txtLastNameMarathi").hide();
		}
		else {
			$("#cmbGoogleLanguage_txtfirstNameMarathi").show();
			$("#cmbGoogleLanguage_txtmiddleNameMarathi").hide();
			$("#cmbGoogleLanguage_txtLastNameMarathi").hide();
		}
	}
	
	
	
	if(id=="txtmiddleNameMarathi")
	{
		if ($("#cmbGoogleLanguage_txtmiddleNameMarathi option:selected").text() == '') {
			googleLanguage([id]);
			$("#cmbGoogleLanguage_txtmiddleNameMarathi").css(
				{
					position: 'relative',
					top: '-7.5px',
					left: '19.417px',
					width: '80px',
					opacity: '1',
					height: '27px'
				}
			);
			$("#cmbGoogleLanguage_txtfirstNameMarathi").hide();
			$("#cmbGoogleLanguage_txtLastNameMarathi").hide();
		}
		else {
			$("#cmbGoogleLanguage_txtmiddleNameMarathi").show();
			$("#cmbGoogleLanguage_txtfirstNameMarathi").hide();
			$("#cmbGoogleLanguage_txtLastNameMarathi").hide();
		}
  }
	
	if(id=="txtLastNameMarathi")
	{
		if ($("#cmbGoogleLanguage_txtLastNameMarathi option:selected").text() == '') {
		googleLanguage([id]);
		$("#cmbGoogleLanguage_txtLastNameMarathi").css(
			{
				position: 'relative',
				top: '-7.5px',
				left: '19.417px',
				width: '80px',
				opacity: '1',
				height: '27px'
			}
		);
		$("#cmbGoogleLanguage_txtfirstNameMarathi").hide();
		$("#cmbGoogleLanguage_txtmiddleNameMarathi").hide();
		}
		else
		{
		$("#cmbGoogleLanguage_txtLastNameMarathi").show();
		$("#cmbGoogleLanguage_txtfirstNameMarathi").hide();
		$("#cmbGoogleLanguage_txtmiddleNameMarathi").hide();
		}
	}
	if(id=="motherNameHindi")
	{
		if ($("#cmbGoogleLanguage_motherNameHindi option:selected").text() == '') {
			googleLanguage([id]);
			$("#cmbGoogleLanguage_motherNameHindi").css(
				{
					position: 'relative',
					top: '-7.5px',
					left: '19.417px',
					width: '80px',
					opacity: '1',
					height: '27px'
				}
			);
			$("#cmbGoogleLanguage_motherNameHindi").hide();
		}
		else {
			$("#cmbGoogleLanguage_motherNameHindi").show();
		}
	}
	if(id=="fatherNameHindi")
	{
		if ($("#cmbGoogleLanguage_fatherNameHindi option:selected").text() == '') {
			googleLanguage([id]);
			$("#cmbGoogleLanguage_fatherNameHindi").css(
				{
					position: 'relative',
					top: '-7.5px',
					left: '19.417px',
					width: '80px',
					opacity: '1',
					height: '27px'
				}
			);
			$("#cmbGoogleLanguage_fatherNameHindi").hide();
		}
		else {
			$("#cmbGoogleLanguage_fatherNameHindi").show();
		}
	}

}
function getMinorityTextBox(){
	
			var allowMinorityChoice = $('input[name = "rdoMinorityChoice"]:checked').val();
		if(allowMinorityChoice=="true"|| allowMinorityChoice==true){
			$('#MinoritynameId').show();
		}else{
			$('#MinoritynameId').hide();
		
		}	
}

function abcAccountIdValidation(){
	
	$('[data-type="abc-accountId"]').keyup(function() {
		try{
			var value = $(this).val();
			value = value.replace(/\D/g, "").split(/(?:([\d]{4}))/g).filter(s => s.length > 0).join("-");
			
			var maxLength = $(this).attr("maxLength");
			if (value.length != maxLength) {
				$(this).css("border", "1px solid red");
			} else {
				$(this).css("border", "1px solid black");
			}
		
			$(this).val(value);
		}catch(e){}
	});
}

function downloadScoreCard(testSettingId,paperName) {
	window.location.replace('downloadScoreCardOfEntrance.json?__testSettingId='+testSettingId+'&paperName='+paperName);
}

function changeAccordingToCountry(){
	try{
		var txtstudnationality = 0;
		if($('#customerHelper').val()=="GOKHALE_INSTITUTE"){
			txtstudnationality = 1;
		} 
		if(txtstudnationality>0){
			$('.hideCountryId').hide();
			setTimeout(function() {
				alert('.ShowcountryId_'+txtstudnationality);
				$('.ShowcountryId_'+txtstudnationality).show();
			}, 500);
		}else{
			$('.hideCountryId').show();
		}
	}catch(e){
		$('.hideCountryId').show();
	}
}

function hideSaveSubmitButton(){
	if($('#finalSubmissionStatus').val() !=undefined && ($('#finalSubmissionStatus').val()==1 || $('#finalSubmissionStatus').val()=='1')){
		$('.hideSaveSubmitButton').remove(); 
	}
}

function getTaluka(city, taluka) {
	
	var cityName = $('#'+city+' option:selected').text();
	
	$.ajax({
		url : 'getTalukaDetails.json',
		data : {
			cityName : cityName,
			
		},
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			console.log("data",data)
			var html = '<option value="0">Select</option>';
			var len = data.length;
			for ( var i = 0; i < len; i++){
				html += '<option value="' + data[i]["talukaName"] + '">'
				+ data[i]["talukaName"] + '</option>';
			}
			$('#' + taluka).html(html);
			
		},
	});
}

