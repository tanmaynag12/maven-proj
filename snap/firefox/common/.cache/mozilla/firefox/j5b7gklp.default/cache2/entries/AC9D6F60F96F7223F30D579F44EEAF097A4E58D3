function getStudentPAymentInfo(){
	
	$.ajax({
		url : 'studentAffilateCollegeStudent.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			$('div.preview').addClass('loading');
			$('div.preview').show();
		},
		complete : function() {
			$('div.preview').removeClass('loading');
			$('div.preview').hide();
		},
		success : function(jArray) {
			
			
			/* var boiler='<br><br><table class="table table-bordered " id="studentExamFormSubmittedTLB" style="margin-top:10px">'+
			'<thead>Payement Details For Regsiter Student<tr>'+
			'<th>Student Name :</th>'+jArray[0]["studName"]+'<''>'+
			'<th>Batch </th>'; */
			
			var bankId =jArray[0]["bankId"];
			var studentName  = jArray[0]["studName"];
			var totalRegistatrionAmount =jArray[0]["totalTnxAmount"];
			var requestSendUniversity = jArray[0]["requestSendUniversity"];
			var transactionUniqueRefrence = jArray[0]["transactionUnqieRef"];
			
			$('#tdAmounttoPay').text(totalRegistatrionAmount);
			$('#studentNameDetails').html(studentName);
			$('#tdpayemntStatus').html(requestSendUniversity);
			$('#tdTransactionUniqeRefre').html(transactionUniqueRefrence);
			
			if(transactionUniqueRefrence==""){
				$('#paymentPluginsDiv').show();
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="HPTELIBRARY"){
					$('#onlinePaymentPlugins').ccAvenue8Pymt({
						sPC : 'UNIREG',
						iFBIDs : bankId,
					});
				}				
				
			}
			
		},
		error:function(){
			//jAlert("Error for yeardown fee","Error");
			getCustomAlertMessage("alert_error");
		}
	});


	
}

function getBoardNameList(){
	$.ajax({
		url:'getBoardNameListFor10thAnd12th.json', 
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var boiler = '<option value="0" selected="selected">Select</option>';
			for(var i=0;i<jArray.length;i++){
				boiler += '<option value="'+ jArray[i]["BoardName"] + '">'+ jArray[i]["BoardName"] + '</option>';
			}
			$("#tenthboard").html(boiler);
			$("#twelthBoard").html(boiler);
		}
	});
}

function fetchAdmissionDocumentDetails(){
	var isAllowDocumentDetails = true;
	var sponseredChk = $('#saveBtn').attr('isSponseredApplicant');
	var isAdmissionQuota = $('#saveBtn').attr('isAdmissionQuota');

	if(isAllowDocumentDetails){
		$('#DocDetailTD').html('');
		var instituteCourseStrengthId = $('#saveBtn').attr('institutecoursestrengthid');
		$.ajax({
			url : 'fetchAdmissionDocumentListForStudent.json?instituteCourseStrengthId='+ instituteCourseStrengthId+'&sponseredChk='+sponseredChk+'&isAdmissionQuota='+isAdmissionQuota, // admissionTypeId='+admissionTypeId+'&reservationCatId='+reservationCatId
			dataType : 'json',
			async : false,
			success : function(jArray) {
				$('#ApplicantProfileWizard').find('.content').css({
					'overflow' : 'auto'
				});
				$(".wizard .content").css({
					'min-height' : '95em'
				});

				var boiler = '<center><table class="table table-bordered table-condensed" style="">';

				boiler +='<tr><td colspan="5" style="text-align:center;"></td></tr>'+
				'<tr><th width="5%">Sr no.</th>'+
				'<th width="20%">Document name</th>'+
				'<th width="30%">Description</th>';

				/*
				 * if($('#customerHelper').val()!=undefined &&
				 * $('#customerHelper').val()=="STJOHNS") boiler +='<input
				 * type="checkbox" onclick="selectAllCalculateMarks()"
				 * id="chkStudList23" class="" checked="checked">';
				 */

				boiler +='</th>';
				boiler +='<th width="20%">Attachment</th>' + '</tr>';

				var count = 0;
				for ( var i = 0; i < jArray.length; i++) {

					if(!jArray[i]["isConditionOnBoard"] && !jArray[i]["isConditionOnUniversity"]){
						count++;
						boiler += '<tr id = "'+jArray[i]["admissionDocumentId"] + '">';

						if (jArray[i]["isCompulsory"])
							boiler += '<td style="text-align:center">'+ count + '</td>' + '<td>'+jArray[i]["DocName"]+'<b style="color:red;font-size:16px;" >*</b></td>';
						else
							boiler += '<td style="text-align:center">'+ count + '</td>' + '<td>'+jArray[i]["DocName"]+'</td>';

						boiler +='<td>'+ jArray[i]["DocDesc"] + '</td>';
						boiler += '<td id="td_attachement_'+ jArray[i]["admissionDocumentId"]+'" class="attachement" style="text-align:right"></td>' + '</tr>';

					}else if(jArray[i]["isConditionOnBoard"] && jArray[i]["isAllowed"]){
						count++;
						boiler += '<tr id = "'+jArray[i]["admissionDocumentId"] + '">';

						if (jArray[i]["isCompulsory"])
							boiler += '<td style="text-align:center">'+ count + '</td>' + '<td>'+jArray[i]["DocName"]+'<b style="color:red;font-size:16px;" >*</b></td>';
						else
							boiler += '<td style="text-align:center">'+ count + '</td>' + '<td>'+jArray[i]["DocName"]+'</td>';

						boiler +='<td>'+ jArray[i]["DocDesc"] + '</td>';
						boiler += '<td id="td_attachement_'+ jArray[i]["admissionDocumentId"]+'" class="attachement"></td>' + '</tr>';

					}else if(jArray[i]["isConditionOnUniversity"] && jArray[i]["allowedForGraduation"]){
						count++;
						boiler += '<tr id = "'+jArray[i]["admissionDocumentId"] + '">';

						if (jArray[i]["isCompulsory"])
							boiler += '<td style="text-align:center">'+ count + '</td>' + '<td>'+jArray[i]["DocName"]+'<b style="color:red;font-size:16px;" >*</b></td>';
						else
							boiler += '<td style="text-align:center">'+ count + '</td>' + '<td>'+jArray[i]["DocName"]+'</td>';

						boiler +='<td>'+ jArray[i]["DocDesc"] + '</td>';
						if (jArray[i]["isCompulsory"])
							boiler += '<td class="isDocumentCompulsory" id="td_xerox_'+ jArray[i]["admissionDocumentId"]+ '"></td>';
						else
							boiler += '<td id="td_xerox_'+ jArray[i]["admissionDocumentId"]+'"></td>';

						boiler += '<td id="td_attachement_'+ jArray[i]["admissionDocumentId"]+'" class="attachement"></td>' + '</tr>';
					}
				}

				boiler += '<tr><td colspan="5" style="text-align:center;"></td></tr>'+'</table></center>';
				/*
				 * boiler+='<table class="table table-bordered table-condensed"
				 * id="approvalTable" >'+ '<tr>'+ '<div style="display: block;
				 * position: absolute; overflow: hidden; margin: 0px; padding: 0px;
				 * opacity: 0; direction: ltr; z-index: 2147483583; left: 881.4px;
				 * top: 846.083px; width: 212px; height: 30px;">'+ '<td>Approve
				 * Letter<input type="button" class="btn btn-mini btn-primary"
				 * value="Upload" id="" onclick="uploadApprovalLetter()"/></td>'+ '</div>'+ '</tr>'+ '</table>';
				 */

				$('#DocDetailTD').html(boiler);

				for ( var i = 0; i < jArray.length; i++) {
					if(!jArray[i]["isConditionOnBoard"] && !jArray[i]["isConditionOnUniversity"]){
						var admissionDocumentId = jArray[i]["admissionDocumentId"];
						getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthIdTimedOut(i, instituteCourseStrengthId,admissionDocumentId);
					}else if(jArray[i]["isConditionOnBoard"] && jArray[i]["isAllowed"]){
						var admissionDocumentId = jArray[i]["admissionDocumentId"];
						getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthIdTimedOut(i, instituteCourseStrengthId,admissionDocumentId);
					}else if(jArray[i]["isConditionOnUniversity"] && jArray[i]["allowedForGraduation"]){
						var admissionDocumentId = jArray[i]["admissionDocumentId"];
						getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthIdTimedOut(i, instituteCourseStrengthId,admissionDocumentId);
					}
				}
			},
			error : function() { 
				getCustomAlertMessage("alert_error");
			}
		});
	}
}

function getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthIdTimedOut(i, instituteCourseStrengthId, admissionDocumentId) {
	setTimeout(function() {
		getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthId(instituteCourseStrengthId,admissionDocumentId,
				function(jObj) {
			$('#td_xerox_' + admissionDocumentId).html('<div align="center"><input type="hidden" class="docCount" id="docCount_'+ admissionDocumentId
					+ '" value="0"><input type="checkbox" id="xerox_'+ admissionDocumentId+ '" class="xeroxClass" name="'+ jObj["originalAdmissionDocumentId"]+ '"></div>');
			$('#td_attachement_' + admissionDocumentId).html('<div class="uploadDivSection" id="documentUpload-'+ jObj["DocumentCategoryId"]+ '"></div><span style="color:blue; font-size:16px;">* </span>Allowed File Size =<b> 2MB.');

			if (jObj["xeroxStatus"] == true || $('#customerHelper').val()=="STJOHNS")
				$('#xerox_' + admissionDocumentId).attr('checked', true);

			if ($('#customerHelper').val()=="STJOHNS")
				$('#xerox_' + admissionDocumentId).attr('disabled', 'disabled');

			var documentCategoryId = jObj.DocumentCategoryId;
			var docArray = jObj.docJArray;
			if (docArray.length == 0) {
				var uploadDocument = new UploadStudentDocument();
				uploadDocument.init({
					elementId : 'documentUpload-'+ documentCategoryId,
					categoryId : documentCategoryId,
					fileSize : AllowedFileSize.DOCUMENT_UPLOAD_2MB,
					fileType : [ 'pdf', 'jpeg','jpg', 'bmp', 'png' ],
				},
				function(docId) {
					getUploadedDocumentById(docId,function(jObject) {
						var array = new Array();
						array.push(jObject);
						populateAdmissionSupportedDocumentForStudent(array, jObject.documentCategoryId);
						var trObj = $('#documentUpload-'+ jObject.documentCategoryId).closest('tr');
						$(trObj).find('.xeroxClass').attr('checked','checked');
						$(trObj).find('.docCount').val('1');
						$(trObj).css('color', 'black');
					});
				});
			} else {
				populateAdmissionSupportedDocumentForStudent(docArray, documentCategoryId);
				var trObj = $('#documentUpload-' + documentCategoryId).closest('tr');
				$(trObj).find('.xeroxClass').attr('checked','checked');
				$(trObj).find('.docCount').val('1');
				$(trObj).css('color', 'black');
			}
		});
	}, 500 * i);
}

function getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthId(instituteCourseStrengthId, admissionDocumentId, callback) {
	$.ajax({
		url : 'getUploadedDocumentDetailsByAmissionDocumentIdAndInstituteCourseStrengthId.json',
		dataType : 'json',
		type : 'GET',
		data : {
			instituteCourseStrengthId : instituteCourseStrengthId,
			admissionDocumentId : admissionDocumentId,
		},
		beforeSend : function() {
			$('#td_attachement_' + admissionDocumentId).html('loading...');
		},
		success : function(jArray) {
			callback(jArray);
		},
		error : function() { 
			getCustomAlertMessage("alert_error");
		}
	});
}

function populateAdmissionSupportedDocumentForStudent(jArray,documentCategoryId) {
	var attachementBoiler = "";
	// var categoryId = 0;
	for ( var i = 0; i < jArray.length; i++) {
		// categoryId = jArray[i].categoryId;
		var attachmentFileName = jArray[i]["name"].toString();
		var documentId = jArray[i]["documentId"];
		var fileType = jArray[i]["extension"];
		var eDocId = jArray[i].eDocId!=undefined?jArray[i].eDocId:documentId;//E-N-C
		var downloadLink = (juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
		
		iconPath = gFileTypeIconMapping[fileType];
		attachementBoiler = '<div align="left"><img src="'+ iconPath+ '" />&nbsp;&nbsp;&nbsp;&nbsp;'+downloadLink+'&nbsp;&nbsp;&nbsp;<a  title="Remove" id = "delete-'+ documentId+ '" href="javascript:void(0)" onclick="removeUploadedDocument(this,\''+ documentCategoryId + '\',\'' + documentId+ '\')">'+
		'<i class="icon-remove"></i></a>' + '</div>';
	}
	$('#documentUpload-' + documentCategoryId).html(attachementBoiler);
}

function removeUploadedDocument(currentObj, documentCategoryId, docId) {
	removeUploadedDocumentById(docId, function(jObject) {
		if (jObject.responseFlag == '1') {
			var elementId = $(currentObj).closest('.uploadDivSection').attr(
			'id');
			var trObj = $(currentObj).closest('tr');
			$(trObj).find('.xeroxClass').removeAttr('checked');
			$(trObj).find('.docCount').val('0');
			var uploadDocument = new UploadStudentDocument();
			uploadDocument.init({
				elementId : elementId,
				categoryId : documentCategoryId,
				fileSize : AllowedFileSize.DOCUMENT_UPLOAD,
				fileType : [ 'pdf', 'jpeg', 'jpg', 'bmp', 'png' ],
			}, function(docId) {
				getUploadedDocumentById(docId, function(jObject) {
					var array = new Array();
					array.push(jObject);
					populateAdmissionSupportedDocumentForStudent(array,
							jObject.documentCategoryId);
					var trObj = $('#documentUpload-' + documentCategoryId)
					.closest('tr');
					$(trObj).find('.xeroxClass').attr('checked', 'checked');
					$(trObj).find('.docCount').val('1');
					$(trObj).css('color', 'black');
				});
			});
		} else {
			jAlert('Deletion failed');
		}

	});
}



var gArray= new Array();
function getstudProfileConfigurationDetailsCheckingNames(){

	$.ajax({
	url : 'getAllStudentProfileConfigurationDetails.json',
	dataType : 'json',
	type : 'GET',
	async : false,
	success : function(jArray) {
		gArray= jArray;
		getstudProfileConfigurationDetailsCompareNamesAndDisable();
		
	},
	error : function() {
		 jAlert("Something went wrong, Please try after some time","Error");
	}
});
}

function getstudProfileConfigurationDetailsCompareNamesAndDisable(){
	
	for (var i = 0; i < gArray.length; i++) {
		
//Personal Details.....................................................................................................................................................................................................................................
		
		
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="RAMAIAH_UNIVERSITY" ){
			if ((gArray[i]["isCheckedText"] == "firstName") && ($('input[name = "firstName"]')!=undefined) && (($('input[name = "firstName"]').val())!=null) && ($.trim($('input[name = "firstName"]').val())=="") && ($('input[name = "firstName"]').val().length > 0)) {
			$('input[name = "firstName"]').attr("disabled", true);
			}
			if ((gArray[i]["isCheckedText"] == "middleName") && ($('input[name = "middleName"]')!=undefined) && (($('input[name = "middleName"]').val())!=null) && ($.trim($('input[name = "middleName"]').val()) =="")  && ($('input[name = "middleName"]').val().length > 0)) {
			$('input[name = "middleName"]').attr("disabled", true);
			}
			if ((gArray[i]["isCheckedText"] == "lastName") && ($('input[name="lastName"]')!=undefined) && ($.trim($('input[name="lastName"]').val())!=null) && ($.trim($('input[name="lastName"]').val())=="") && ($('input[name="lastName"]').val().length > 0)) {
			$('input[name = "lastName"]').attr("disabled", true);
			}
		}
		else{
			if ((gArray[i]["isCheckedText"] == "firstName") && ($('input[name = "firstName"]')!=undefined) && (($('input[name = "firstName"]').val())!=null) && ($.trim($('input[name = "firstName"]').val())!="") && ($('input[name = "firstName"]').val().length > 0)) {
			$('input[name = "firstName"]').attr("disabled", true);
			}
			if ((gArray[i]["isCheckedText"] == "middleName") && ($('input[name = "middleName"]')!=undefined) && (($('input[name = "middleName"]').val())!=null) && ($.trim($('input[name = "middleName"]').val()) !="")  && ($('input[name = "middleName"]').val().length > 0)) {
			$('input[name = "middleName"]').attr("disabled", true);
			}
			if ((gArray[i]["isCheckedText"] == "lastName") && ($('input[name="lastName"]')!=undefined) && ($.trim($('input[name="lastName"]').val())!=null) && ($.trim($('input[name="lastName"]').val())!="") && ($('input[name="lastName"]').val().length > 0)) {
			$('input[name = "lastName"]').attr("disabled", true);
			}
		}
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val() == "MEGHE"){
			if(isFreezedFlag == "true" || isFreezedFlag == true){
				if ((gArray[i]["isCheckedText"] == "dob") && ($('input[name = "dob"]')!=undefined) && (($('input[name = "dob"]').val())!=null) && ($('input[name = "dob"]').val().length > 0)) {
					$('input[name = "dob"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "alternetEmailID") && ($('input[name = "alternetEmailID"]')!=undefined) && (($('input[name = "alternetEmailID"]').val())!=null) && ($.trim($('input[name = "alternetEmailID"]').val()) != "") && ($('input[name = "alternetEmailID"]').val().length > 0)) {
					$('input[name = "alternetEmailID"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "personalEmailID") && ($('input[name = "personalEmailID"]')!=undefined) && (($('input[name = "personalEmailID"]').val())!=null) && ($.trim($('input[name = "personalEmailID"]').val()) !="")  && ($('input[name = "personalEmailID"]').val().length > 0) ){
					$('input[name = "personalEmailID"]').attr("disabled", true);
				}
				
				if( (gArray[i]["isCheckedText"] == "Gender") && ($('input[name = "Gender"]') != undefined) && (($('input[name = "Gender"]').val())!=null) && ($('input[name = "Gender"]').is(':checked'))) {
					$('input[name = "Gender"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "Hobbies") && ($('input[name = "Hobbies"]') !=undefined) && (($('input[name = "Hobbies"]').val())!=null) && ($.trim($('input[name = "Hobbies"]').val())!="") && ($('input[name = "Hobbies"]').val().length>0)) {
					$('input[name = "Hobbies"]').attr("disabled", true);
				}
		
				if ((gArray[i]["isCheckedText"] == "selstudbloodgrp") && ($('select[name = "selstudbloodgrp"]')!=undefined) && (($('select[name = "selstudbloodgrp"]').val())!=null) && ($('select[name = "selstudbloodgrp"]')!="Select") && ($('select[name = "selstudbloodgrp"]').val().length>0)) {
					$('select[name = "selstudbloodgrp"]').attr("disabled", true);
				}
				
				if ((gArray[i]["isCheckedText"] == "motherTongue") && ($('select[name = "motherTongue"]')!=undefined) && ( ( $('select[name = "motherTongue"]').val())!=null) && ($('select[name = "motherTongue"]').val() !="0")) {
					$('select[name = "motherTongue"]').attr("disabled", true);
