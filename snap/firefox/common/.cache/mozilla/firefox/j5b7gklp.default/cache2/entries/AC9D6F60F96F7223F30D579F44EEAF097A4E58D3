function addEducationDetails(Id){
	var rowCount =  $('#'+Id+' tr').length;
	
	boiler = '<tr id="row_'+rowCount+'">'+
//	'<td><input type="text" id="Sr_'+rowCount+'" ></td>'+
	'<td><input type="text" id="prn_'+rowCount+'" ></td>'+
	'<td><input type="text" id="clgUni_'+rowCount+'" ></td>'+
	'<td><input type="text" id="board_'+rowCount+'" ></td>'+
	'<td><input type="text" id="examMonthYear_'+rowCount+'" ></td>'+
	'<td><input type="text" id="result_'+rowCount+'" ></td>'+
	'<td><input type="text" id="subject_'+rowCount+'" ></td>'+
	'</tr>';
	$('#'+Id).append(boiler);
}

function saveDetailOfEligibility(){
	var firstName = $('#txtfirstNameElig').val();
	var middleName = $('#txtmiddleNameElig').val();
	var lastName = $('#txtLastNameElig').val();
	var motherName = $('#txtMotherNameElig').val();
	var dob = $('#txtdobElig').val();
	var placeOfDob = $('#txtPlaceOfBirthElig').val();
	var state = $('#txtStateOfDomicileElig').val();
	var country = $('#txtCountry option:selected').val();
	var email = $('#txtAlternetEmailElig').val();
	var mobile = $('#txtmobileNoElig').val();
	var category = $('#txtstudcategoryElig').val();
	var adharNo = $('#txtAdharNoElig').val();
	var address = $('#addressElig').val();
	var pinNo = $('#txtPinElig').val();
	var progName = $('#txtProgrammeNameElig').val();
	var regularOrExternal =  $("input[name=Type]:radio:checked").val();
	var college = $('#clgUniversityNameElig').val();
	var dateOfAdmission = $('#txtdateOfAdmissionElig').val();
	var examSession = $('#txtexamSessionElig').val();
	

		var noOfTr = $('#TblEducationData tr').length;
		var arrEducation = new Array();
		for(var i =0;i<noOfTr; i++){
			console.log($('#prn_'+i).val()+'_'+$('#clgUni_'+i).val()+'_'+$('#board_'+i).val()+'_'+$('#examMonthYear_'+i).val()+'_'+$('#result_'+i).val()+'_'+$('#subject_'+i).val());
			arrEducation.push($('#prn_'+i).val()+'_'+$('#clgUni_'+i).val()+'_'+$('#board_'+i).val()+'_'+$('#examMonthYear_'+i).val()+'_'+$('#result_'+i).val()+'_'+$('#subject_'+i).val());
		}

		var noOfTrOther = $('#OtherExamsData tr').length;
		var arrOtherEducation = new Array();
		for(var i =0;i<noOfTrOther; i++){
			console.log($('#prn_'+i).val()+'_'+$('#clgUni_'+i).val()+'_'+$('#board_'+i).val()+'_'+$('#examMonthYear_'+i).val()+'_'+$('#result_'+i).val()+'_'+$('#subject_'+i).val());
			arrOtherEducation.push($('#prn_'+i).val()+'_'+$('#clgUni_'+i).val()+'_'+$('#board_'+i).val()+'_'+$('#examMonthYear_'+i).val()+'_'+$('#result_'+i).val()+'_'+$('#subject_'+i).val());
		}
	
		 $.ajax({
				url:'saveDetailOfEligibility.json',
				dataType:'json',
				async:false,
				type : 'POST',
				data:{
				 firstName:firstName,
				 middleName:middleName,
				 lastName:lastName,
				 motherName:motherName,
				 dob:dob,
				 placeOfDob:placeOfDob,
				 state:state,
				 country:country,
				 email:email,
				 mobile:mobile,
				 category:category,
				 adharNo:adharNo,
				 address:address,
				 pinNo:pinNo,
				 progName:progName,
				 regularOrExternal:regularOrExternal,
				 college:college,
				 dateOfAdmission:dateOfAdmission,
				 examSession:examSession,
				 arrEducation:arrEducation.toString(),
				 arrOtherEducation:arrOtherEducation.toString()
				},
				beforeSend: function() {
					startCommonAjaxSpinner();
														
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){

				},
				error:function(){
					jAlert("Something went wrong, Please try after some time","Error");
				}
			});
		 
	
}

