function validateStudPersonalDetailTab() {
	var fName = $('input[name = "firstName"]').val();
	var lName = $('input[name = "lastName"]').val();
	var mobileNo = $('input[name = "mobileNo"]').val();
	var dob = $('input[name = "dob"]').val();
	var alternetContactNumber = $('input[name = "alternetContactNumber"]').val();

	if (fName == '' || !(isNaN(fName))) {
		jAlert("Please enter valid first name");
		$("#txtfirstName").focus();
	} else if ($('#customerHelper').val() !='MANDSAUR' &&$('#customerHelper').val() !='CMR_UNIVERSITY' && $('#customerHelper').val() !='CMR_IT' && (lName == '' || !(isNaN(lName)))) {
		jAlert("Please enter valid last name");
		$("#txtLastName").focus();
	} else if (mobileNo == '' || isNaN(mobileNo) || mobileNo.length < 10) {
		jAlert("Please enter valid Mobile number");
		$("#txtmobileNo").focus();
	} else if (dob == '') {
		jAlert("Please enter date of birth");
	} else if (alternetContactNumber != '') {
		if (isNaN(alternetContactNumber)) {
			jAlert("Please enter valid alternate number");
			$("#txtAltNumber").focus();
		}
	}
}

