					vehicleNo:vehicleNo,
					musmino:musmino,
					CoOfConduct:CoOfConduct,
					AnRaggReg:AnRaggReg,
					othermino:othermino,
					personalEmailID:personalEmailID,
					studplacemtmob1 : studplacemtmob1,
					studplacemtmob2 : studplacemtmob2,
					applicationId : applicationId,
					linkdinUrl:linkdinUrl,
					studentPanNo:studentPanNo,
					ugStudMarksheetname:ugStudMarksheetname,
					utrNo:utrNo,
					NADId:NADId,
					skypeId:skypeId,
					studNameHindi:encodeURIComponent(studNameHindi),
					fatherNameHindi:encodeURIComponent(fatherNameHindi),
					motherNameHindi:encodeURIComponent(motherNameHindi),
					subCatId:subCatId,
					categoryValue:categoryValue,
					admissionType:admissionType,
					voterIdNo:voterIdNo,
					abcAccountId:abcAccountId
				},
				dataType : 'json',
				type : 'POST',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
						addStudentAddressForStudDetailsTab();
					}
					if (jArray[0].responseFlag == '1')
						gIsForiegner = jArray[0].isForiegner;
					/*if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="INDIRA_PUNE")
					{
						$('a[href="#passportAndVisaDetailsDetails"]').show();
					}
					else{*/
					if(jArray[0].isForiegner)
					{
						$('a[href="#passportAndVisaDetailsDetails"]').show();
					}else{
						$('a[href="#passportAndVisaDetailsDetails"]').hide();
					}
					//}

					getCustomAlertMessage("alert_create");
					getstudentPersonalinfo();
					if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
						getStudentContactForStudentDetailsTab();
					}
				},
				error : function() {
				}
			});
		
			}
			else if($('#customerHelper').val() =='MGMU' || $('#customerHelper').val() =='NITIE' || $('#customerHelper').val() =='CMR_UNIVERSITY')
			{
			$.ajax({
				url : 'studentProfilePost.json',
				data : {
					txtReg : txtReg,
					mobileNo : mobileNo,
					Gender : Gender,
					dob : dob,
					std : std,
					alternetContactNumber : alternetContactNumber,
