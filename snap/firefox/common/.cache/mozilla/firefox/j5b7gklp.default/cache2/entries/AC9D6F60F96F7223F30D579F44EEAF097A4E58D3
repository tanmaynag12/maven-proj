		}

		if ((gArray[i]["isCheckedText"] == "siblingLastNameConfig") && ($('input[name = "siblingLastNameConfig"]')!=undefined) && (($('input[name = "siblingLastNameConfig"]').val())!=null) &&  ($.trim($('input[name = "siblingLastNameConfig"]').val())!="") && ($('input[name = "siblingLastNameConfig"]').val().length>0)) {
			$('input[name = "siblingLastNameConfig"]').attr("disabled", true);
		}else if((gArray[i]["isCheckedText"] == "siblingLastNameConfig") && ($('input[name = "siblingLastNameConfig"]')==undefined) || (($('input[name = "siblingLastNameConfig"]').val())==null) ||  ($.trim($('input[name = "siblingLastNameConfig"]').val())=="") || ($('input[name = "siblingLastNameConfig"]').val().length==0)){
			$('input[name = "siblingLastNameConfig"]').attr("disabled", false);
		}
		
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SPIT"){

			if ( (gArray[i]["isCheckedText"] == "isSibllingStudying") && ($('input[name = "isSibllingStudying"]')!='') && ($('input[name = "isSibllingStudying"]')!=undefined) && (($('input[name = "isSibllingStudying"]').val())!=null) &&  ($.trim($('input[name = "isSibllingStudying"]').val())!="") && ($('input[name = "isSibllingStudying"]').val().length>0)) {
				$('input[name = "isSibllingStudying"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "isSibllingStudying") && ($('input[name = "isSibllingStudying"]')=='') || ($('input[name = "isSibllingStudying"]')==undefined) || (($('input[name = "isSibllingStudying"]').val())==null) ||  ($.trim($('input[name = "isSibllingStudying"]').val())=="") || ($('input[name = "isSibllingStudying"]').val().length==0)){
				$('input[name = "isSibllingStudying"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "siblingContNo") && ($('input[name = "siblingContNo"]')!='') && ($('input[name = "siblingContNo"]')!=undefined) && (($('input[name = "siblingContNo"]').val())!=null) &&  ($.trim($('input[name = "siblingContNo"]').val())!="") && ($('input[name = "siblingContNo"]').val().length>0)) {
				$('input[name = "siblingContNo"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "siblingContNo") && ($('input[name = "siblingContNo"]')=='') || ($('input[name = "siblingContNo"]')==undefined) || (($('input[name = "siblingContNo"]').val())==null) ||  ($.trim($('input[name = "siblingContNo"]').val())=="") || ($('input[name = "siblingContNo"]').val().length==0)){
				$('input[name = "siblingContNo"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "siblingEmail") && ($('input[name = "siblingEmail"]')!='') && ($('input[name = "siblingEmail"]')!=undefined) && (($('input[name = "siblingEmail"]').val())!=null) &&  ($.trim($('input[name = "siblingEmail"]').val())!="") && ($('input[name = "siblingEmail"]').val().length>0)) {
				$('input[name = "siblingEmail"]').attr("disabled", true);
			}else if((gArray[i]["isCheckedText"] == "siblingEmail") && ($('input[name = "siblingEmail"]')=='') || ($('input[name = "siblingEmail"]')==undefined) || (($('input[name = "siblingEmail"]').val())==null) ||  ($.trim($('input[name = "siblingEmail"]').val())=="") || ($('input[name = "siblingEmail"]').val().length==0)){
				$('input[name = "siblingEmail"]').attr("disabled", false);
			}
			
			if ( (gArray[i]["isCheckedText"] == "isSibllingWorking") && ($('input[name = "isSibllingWorking"]')!='') && ($('input[name = "isSibllingWorking"]')!=undefined) && (($('input[name = "isSibllingWorking"]').val())!=null) &&  ($.trim($('input[name = "isSibllingWorking"]').val())!="") && ($('input[name = "isSibllingWorking"]').val().length>0)) {
