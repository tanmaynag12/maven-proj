								'</td>'+
								'<td>'+
									'<label>Relation with Guardian:<b style="color: red; font-size: 16px;">*</b></label>'+
								'</td>'+
								'<td>'+
									'<select class=" guardInfoCls" name="guardianRelationship" id="guardianRelation">'+
										'<option value="0">Select</option>'+
										'<option value="Mother">Mother</option>'+
										'<option value="Father">Father</option>'+
										'<option value="Husband">Husband</option>'+
										'<option value="Brother">Brother</option>'+
										'<option value="Sister">Sister</option>'+
										'<option value="Uncle">Uncle</option>'+
										'<option value="Aunty">Aunty</option>'+
										
								'</select></td>'+
							'</tr>'+
							'<tr>'+
								'<td>'+
									'<label>Mobile Number:<b style="color: red; font-size: 16px;">*</b></label>'+
								'</td>'+
								'<td>'+
									'<input type="text" class="validnum guardInfoCls" name="gaurdianMobileNo" id="studguardiancellno" maxLength="14" />'+
								'</td>'+
								'<td>'+
									'<label>Phone Number:</label>'+
								'</td>'+
								'<td>'+
									'<input type="text" class="validnum guardInfoCls" name="guardianStd" id="studeguardianstdcode" style="width: 40px;" maxLength="6" />'+ 
									'<input type="text" class="validnum guardInfoCls" name="guardianPhoneNo" id="guardianstudphnumber" maxLength="11" />'+
								'</td>'+
							'</tr>'+
							'<tr>'+
								'<td>'+
									'<label>Residence Address:<b style="color: red; font-size: 16px;">*</b></label>'+
								'</td>'+
								'<td>'+
									'<input type="text" class="guardInfoCls" name="gaurdianAddress" id="guardiAddress" />'+
								'</td>'+
								'<td>';
								if($('#customerHelper').val()=="MEGHE"){
									boilerGuard+=	'<label>Tahsil:</label>';
								}else{
									boilerGuard+=	'<label>Taluka:</label>';
								}
								
								boilerGuard+='</td>'+
								'<td>'+
									'<input type="text" class="guardInfoCls" name="guardianTaluka" id="studguardiantaluka" />'+
								'</td>'+
							'</tr>'+
							'<tr>'+
								'<td>'+
									'<label>Village:</label>'+
								'</td>'+
								'<td>'+
									'<input type="text" class="guardInfoCls" name="guardianVillage" id="studguardianVillage" />'+
								'</td>'+
								'<td>'+
									'<label>Pincode: <b style="color: red; font-size: 16px;">*</b></label>'+
								'</td>'+
								'<td>'+
									'<input type="text" class="validnum guardInfoCls" name="guardianPinCode" id="studguardianpincode" maxLength="7" />'+
								'</td>'+
							'</tr>';
							if($('#customerHelper').val()=="MEGHE"){
							boilerGuard+='<tr>'+
							'<td>'+
									'<label>Photo: </label>'+
								'</td>'+
								'<td>'+
									'<div align="center"><img src="getGuardianProfileImage.json?studentId=0&id='+$('#studentId').val()+'&type=profile&photoOf=Guardian" alt="Photo"  width="120px" height="120px" border="1" align="middle" style="background-color: #FFFFFF" /></div>'+
								'</td>'+
								'<td>'+
									'<label>Signature: </label>'+
								'</td>'+
								'<td>'+
									'<div align="center"><img src="getSignatureImage.json?studentId=0&date='+new Date()+'&photoOf=Guardian " alt="Sign" name="Sign" width="120px" height="50px" border="1" align="center" style="background-color: #FFFFFF" /></div>'+
								'</td>'+
							'</tr>'
							}

//							if(cntr == guradianDetailArray.length-1)
//							{	
								
								if($('#customerHelper').val()=="MEGHE" && (guradianDetailArray[cntr]["isProfileFreeze"] == true || guradianDetailArray[cntr]["isProfileFreeze"] == "true")){
									boilerGuard+='<tr class="newGuradianDetails hideSaveSubmitButton">'+
										'<td colspan="4"  style="text-align: center;"><input type="submit" class="btn btn-inverse btn-mini   btn_medium  btnColorGreen" value="Save" disabled="disabled" onclick="saveGuradianDetailsForStudent(\'add\')"/>';
									boilerGuard+=	'&nbsp;<input type="button" class="btn btn-inverse btn-mini   btn_medium  btnColorGreen" value="Edit" disabled="disabled" onclick="saveGuradianDetailsForStudent(\'edit\')"/>';
									
								}else{
									boilerGuard+='<tr class="newGuradianDetails hideSaveSubmitButton">'+
									'<td colspan="4" style="text-align: center;"><input type="submit" class="btn btn-inverse btn-mini   btn_medium  btnColorGreen" value="Save" onclick="saveGuradianDetailsForStudent(\'add\')"/>';
									if(guradianDetailArray.length>0)
									{	
										boilerGuard+=	'&nbsp;<input type="button" class="btn btn-inverse btn-mini   btn_medium  btnColorGreen" value="Edit" onclick="saveGuradianDetailsForStudent(\'edit\')"/>';
									}
								}
								
								boilerGuard+='</td>'+
							'</tr>';
//							}
							boilerGuard+='</table>';
								
								
							}
						
						boilerGuard+='<div style="width:100%;text-align:center" class="alert alert-info"><b>Previous Guardian Details</b></div>';
						if(guradianDetailArray.length > 1){
						for(cntr = 1 ;cntr < guradianDetailArray.length; cntr++){
							
							
							boilerGuard+='<br><table class="table table-bordered">'+ 	
							'<a style="float:right;"  class="btn btn-danger btn-mini" onclick="deleteGuradianDetailsForStudent('+guradianDetailArray[cntr]["guardianLogsId"]+')">   <i class="fa fa-trash"></i> <b>Delete</b></a>'+
							'<tr>'+
							'<td>'+
								'<label>Name</label>'+
							'</td>'+
							'<td colspan="3">'+
							''+guradianDetailArray[cntr]["guardianFullName"]+''+
//								Full Name 
							'</td>'+
						'</tr>'+
						'<tr>'+
							'<td>'+
								'<label>Email</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianEmail"]+''+
//								Email
							'</td>'+
							'<td>'+
								'<label>Relation with Guardian</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["relationWithGuradian"]+''+
//								Relationship
							'</select></td>'+
						'</tr>'+
						'<tr>'+
							'<td>'+
								'<label>Mobile Number</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianMobileNumber"]+''+
//								Mobile Number
							'</td>'+
							'<td>'+
								'<label>Phone Number</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianSTD"]+' - '+guradianDetailArray[cntr]["guardianPhoneNumber"]+''+
//								STD - PHONE NUMBER 
							'</td>'+
						'</tr>'+
						'<tr>'+
							'<td>'+
								'<label>Residence Address</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianResidanceAddress"]+''+
//							     Residence Address
							'</td>'+
							'<td>'+
								'<label>Taluka</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianTaluka"]+''+
//								Taluka
							'</td>'+
						'</tr>'+
						'<tr>'+
							'<td>'+
								'<label>Village</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianVillage"]+''+
//								Village
							'</td>'+
							'<td>'+
								'<label>Pincode</label>'+
							'</td>'+
							'<td>'+
							''+guradianDetailArray[cntr]["guardianPinCode"]+''+
//								Pincode
							'</td>'+
						'</tr>'+
						//'<tr><td><label>Guardian Image</label></td><td></td>'+
						//'<td><label>Guardian Signature</label></td><td></td></tr>'+
						'</table>';
							
						}
						
						}
						else
							{
								boilerGuard+='<div style="width:100%;text-align:center"><b>No Records Found</b></div>';
							}
					
					$('#tdGuradionDetailsTd').html(boilerGuard);
					
					$('.guardInfoCls').attr('disabled',true);
					
					
					$('#gaurdiName').val(guradianDetailArray[0]["guardianFirstName"]);
					if(guradianDetailArray[0]["guardianFirstName"] == "")
						{
							$('#gaurdiName').attr('disabled',false);
						}
					$("#studguardianmiddlename").val(guradianDetailArray[0]["guardianMiddleName"]);
					if(guradianDetailArray[0]["guardianMiddleName"] == "")
					{
						$('#studguardianmiddlename').attr('disabled',false);
					}
					$("#studguardianlastname").val(guradianDetailArray[0]["guardianLastName"]);
					if(guradianDetailArray[0]["guardianLastName"] == "")
					{
						$('#studguardianlastname').attr('disabled',false);
					}
					$("#gaurdiEmail").val(guradianDetailArray[0]["guardianEmail"]);
					if(guradianDetailArray[0]["guardianEmail"] == "")
					{
						$('#gaurdiEmail').attr('disabled',false);
					}
					$('#guardianRelation option[value='+guradianDetailArray[0]["relationWithGuradian"]+']').attr("selected","selected");
					if(guradianDetailArray[0]["relationWithGuradian"] == "0" || guradianDetailArray[0]["relationWithGuradian"] == "" || 
							guradianDetailArray[0]["relationWithGuradian"] == "NA")
					{
						$('#guardianRelation').attr('disabled',false);
					}
					$("#guardiAddress").val(guradianDetailArray[0]["guardianResidanceAddress"]);
					if(guradianDetailArray[0]["guardianResidanceAddress"] == "")
					{
						$('#guardiAddress').attr('disabled',false);
					}
					
					
					$("#studguardiantaluka").val(guradianDetailArray[0]["guardianTaluka"]);
					if(guradianDetailArray[0]["guardianTaluka"] == "")
					{
						$('#studguardiantaluka').attr('disabled',false);
					}
					$("#studguardianVillage").val(guradianDetailArray[0]["guardianVillage"]);
					if(guradianDetailArray[0]["guardianVillage"] == "")
					{
						$('#studguardianVillage').attr('disabled',false);
					}
					$("#studguardianpincode").val(guradianDetailArray[0]["guardianPinCode"]);
					if(guradianDetailArray[0]["guardianPinCode"] == "")
					{
						$('#studguardianpincode').attr('disabled',false);
					}
					$("#studeguardianstdcode").val(guradianDetailArray[0]["guardianSTD"]);
					if(guradianDetailArray[0]["guardianSTD"] == "")
					{
						$('#studeguardianstdcode').attr('disabled',false);
					}
					$("#guardianstudphnumber").val(guradianDetailArray[0]["guardianPhoneNumber"]);
					if(guradianDetailArray[0]["guardianPhoneNumber"] == "")
					{
						$('#guardianstudphnumber').attr('disabled',false);
					}
					$("#studguardiancellno").val(guradianDetailArray[0]["guardianMobileNumber"]);
					if(guradianDetailArray[0]["guardianMobileNumber"] == "")
					{
						$('#studguardiancellno').attr('disabled',false);
					}
					
					
				}
				
				
				
//				}
			
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error:function(){
			alert("In Student Guardian Detail error");
		}

	});	
	hideSaveSubmitButton();
}


function saveGuradianDetailsForStudentNew()
{
	
	var studId= 0;
	var gaurdinName = $('#gaurdiNameModal').val();
	var guardianMiddleName = $('#studguardianmiddlenameModal').val();
	var guardianLastName = $('input[name = "guardianLastNameModal"]').val();
	var guardianRelation = $('#guardianRelationModal').val();
	var gaurdianEmailID =$('#gaurdiEmailModal').val();
	var gaurdianAddress = $('#guardiAddressModal').val();
	var guardianTaluka = $('input[name = "guardianTalukaModal"]').val();
	var guardianVillage = $('input[name = "guardianVillageModal"]').val();

	var guardianPinCode = $('input[name = "guardianPinCodeModal"]').val();
	var guardianStd = $('input[name = "guardianStdModal"]').val();
	var guardianPhoneNo = $('#guardianstudphnumberModal').val();
	var gaurdianMobileNo = $('#studguardiancellnoModal').val();
	var studGuardianId = $('#studGuardianIdModal').val();
	
	var regExp = /^[A-Za-z. ]+$/;
	if(gaurdinName == "" || !regExp.test(gaurdinName))
	{
		$('#gaurdiNameModal').css("border","1px solid red");
		return false;
	}else{
		$('#gaurdiNameModal').css("border","1px solid #CCCCCC");
	}
	if(guardianMiddleName != "")
	{
		if(!regExp.test(guardianMiddleName))
		{	
			$('#studguardianmiddlenameModal').css("border","1px solid red");
			return false;
		}else{
			$('#studguardianmiddlenameModal').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#studguardianmiddlenameModal').css("border","1px solid #CCCCCC");
	}
	if(guardianLastName == ""  || !regExp.test(guardianLastName))
	{
		$('#studguardianlastnameModal').css("border","1px solid red");
		return false;
	}else{
		$('#studguardianlastnameModal').css("border","1px solid #CCCCCC");
	}
	
	if(guardianRelation == 0)
	{
		$('#guardianRelationModal').css("border","1px solid red");
		return false;
	}else{
		$('#guardianRelationModal').css("border","1px solid #CCCCCC");
	}
	if(gaurdianMobileNo == "" || isNaN(gaurdianMobileNo))
	{
		$('#studguardiancellnoModal').css("border","1px solid red");
		return false;
	}
	else if(gaurdianMobileNo != "" && ((gaurdianMobileNo.length < 10)))
	{
		jAlert("Mobile Number must be of minimum 10 Digits.");
		$('#studguardiancellnoModal').css("border","1px solid red");
		return false;
	}else{
		$('#studguardiancellnoModal').css("border","1px solid #CCCCCC");
	}
	if(guardianPhoneNo != "")
	{
		if(guardianStd=="" || isNaN(guardianStd))
		{
			$('#studeguardianstdcodeModal').css("border","1px solid red");
			return false;
		}else{
			$('#studeguardianstdcodeModal').css("border","1px solid #CCCCCC");
		}	
		if(isNaN(guardianPhoneNo))
		{
			$('#guardianstudphnumberModal').css("border","1px solid red");
			return false;
		}
		else{
			$('#guardianstudphnumberModal').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#guardianstudphnumberModal').css("border","1px solid #CCCCCC");
		$('#studeguardianstdcodeModal').css("border","1px solid #CCCCCC");
	}
	if(gaurdianAddress == "")
	{
		$('#guardiAddressModal').css("border","1px solid red");
		return false;
	}else{
		$('#guardiAddressModal').css("border","1px solid #CCCCCC");
	}
	
	if(guardianTaluka !="" )
	{
		if(!regExp.test(guardianTaluka))
		{
			$('#studguardiantalukaModal').css("border","1px solid red");
			return false;
		}else{
			$('#studguardiantalukaModal').css("border","1px solid #CCCCCC");
		}
		
	}else{
		$('#studguardiantalukaModal').css("border","1px solid #CCCCCC");
	}
	
	if(guardianVillage != "")
	{
		if(!regExp.test(guardianVillage))
		{
			$('#studguardianVillageModal').css("border","1px solid red");
			return false;
		}else{
			$('#studguardianVillageModal').css("border","1px solid #CCCCCC");
		}
	}
	else{
		$('#studguardianVillageModal').css("border","1px solid #CCCCCC");
	}
	
	if(guardianPinCode=="" || isNaN(guardianPinCode))
	{
		$('#studguardianpincodeModal').css("border","1px solid red");
		return false;
	}else{
		$('#studguardianpincodeModal').css("border","1px solid #CCCCCC");
	}
	
	var filter = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	
	if(gaurdianEmailID != "")
	{
		if (!filter.test(gaurdianEmailID))
		{
			$('#gaurdiEmailModal').css("border","1px solid red");
			return false;
		}else{
			$('#gaurdiEmailModal').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#gaurdiEmailModal').css("border","1px solid #CCCCCC");
	}
	$.ajax({
		url:'saveGuradianDetailsForStudentNew.json?studId='+studId,  
		data:{gaurdinName:gaurdinName,
			guardianMiddleName:guardianMiddleName,guardianLastName:guardianLastName,
			gaurdianEmailID:gaurdianEmailID,guardianRelation:guardianRelation,
			gaurdianAddress:gaurdianAddress,guardianTaluka:guardianTaluka,guardianVillage:guardianVillage,
			guardianPinCode:guardianPinCode,guardianStd:guardianStd,guardianPhoneNo:guardianPhoneNo,
			gaurdianMobileNo:gaurdianMobileNo,studGuardianId:studGuardianId},
		dataType:'json',
		type:'POST',
		success:function(jArray){
			getCustomAlertMessage("alert_create");
			$('#addquestionModal').modal('hide');
			getstudentGaurdianinfoNew1();
		},
		error:function(){
			//jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}



function saveGuradianDetailsForStudent(param)
{
	if(param=='edit'){
		$('.guardInfoCls').attr('disabled',false);
		return false;
	}
	var studId= 0;
	var gaurdinName = $('#gaurdiName').val();
	var guardianMiddleName = $('#studguardianmiddlename').val();
	var guardianLastName = $('input[name = "guardianLastName"]').val();
	var guardianRelation = $('#guardianRelation').val();
	var gaurdianEmailID =$('#gaurdiEmail').val();
	var gaurdianAddress = $('#guardiAddress').val();
	
	var guardianTaluka = $('input[name = "guardianTaluka"]').val();
	var guardianVillage = $('input[name = "guardianVillage"]').val();

	var guardianPinCode = $('input[name = "guardianPinCode"]').val();
	var guardianStd = $('input[name = "guardianStd"]').val();
	var guardianPhoneNo = $('#guardianstudphnumber').val();
	
	var gaurdianMobileNo = $('#studguardiancellno').val();
	var studGuardianId = $('#studGuardianId').val();
	
	var regExp = /^[A-Za-z. ]+$/;
	if(gaurdinName == "" || !regExp.test(gaurdinName))
	{
		$('#gaurdiName').css("border","1px solid red");
		return false;
	}else{
		$('#gaurdiName').css("border","1px solid #CCCCCC");
	}
	if(guardianMiddleName != "")
	{
		if(!regExp.test(guardianMiddleName))
		{	
			$('#studguardianmiddlename').css("border","1px solid red");
			return false;
		}else{
			$('#studguardianmiddlename').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#studguardianmiddlename').css("border","1px solid #CCCCCC");
	}
	if(guardianLastName == ""  || !regExp.test(guardianLastName))
	{
		$('#studguardianlastname').css("border","1px solid red");
		return false;
	}else{
		$('#studguardianlastname').css("border","1px solid #CCCCCC");
	}
	
	if(guardianRelation == 0)
	{
		$('#guardianRelation').css("border","1px solid red");
		return false;
	}else{
		$('#guardianRelation').css("border","1px solid #CCCCCC");
	}
	if(gaurdianMobileNo == "" || isNaN(gaurdianMobileNo))
	{
		$('#studguardiancellno').css("border","1px solid red");
		return false;
	}
	else if(gaurdianMobileNo != "" && ((gaurdianMobileNo.length < 10)))
	{
		jAlert("Mobile Number must be of minimum 10 Digits.");
		$('#studguardiancellno').css("border","1px solid red");
		return false;
	}else{
		$('#studguardiancellno').css("border","1px solid #CCCCCC");
	}
	if(guardianPhoneNo != "")
	{
		if(guardianStd=="" || isNaN(guardianStd))
		{
			$('#studeguardianstdcode').css("border","1px solid red");
			return false;
		}else{
			$('#studeguardianstdcode').css("border","1px solid #CCCCCC");
		}	
		if(isNaN(guardianPhoneNo))
		{
			$('#guardianstudphnumber').css("border","1px solid red");
			return false;
		}
		else{
			$('#guardianstudphnumber').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#guardianstudphnumber').css("border","1px solid #CCCCCC");
		$('#studeguardianstdcode').css("border","1px solid #CCCCCC");
	}
	if(gaurdianAddress == "")
	{
		$('#guardiAddress').css("border","1px solid red");
		return false;
	}else{
		$('#guardiAddress').css("border","1px solid #CCCCCC");
	}
	
	if(guardianTaluka !="" )
	{
		if(!regExp.test(guardianTaluka))
		{
			$('#studguardiantaluka').css("border","1px solid red");
			return false;
		}else{
			$('#studguardiantaluka').css("border","1px solid #CCCCCC");
		}
		
	}else{
		$('#studguardiantaluka').css("border","1px solid #CCCCCC");
	}
	
	if(guardianVillage != "")
	{
		if(!regExp.test(guardianVillage))
		{
			$('#studguardianVillage').css("border","1px solid red");
			return false;
		}else{
			$('#studguardianVillage').css("border","1px solid #CCCCCC");
		}
	}
	else{
		$('#studguardianVillage').css("border","1px solid #CCCCCC");
	}
	
	if(guardianPinCode=="" || isNaN(guardianPinCode))
	{
		$('#studguardianpincode').css("border","1px solid red");
		return false;
	}else{
		$('#studguardianpincode').css("border","1px solid #CCCCCC");
	}
	
	var filter = /^(([^<>()[\]\\.,;:\s@"]+(\.[^<>()[\]\\.,;:\s@"]+)*)|(".+"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
	
	if(gaurdianEmailID != "")
	{
		if (!filter.test(gaurdianEmailID))
		{
			$('#gaurdiEmail').css("border","1px solid red");
			return false;
		}else{
			$('#gaurdiEmail').css("border","1px solid #CCCCCC");
		}
	}else{
		$('#gaurdiEmail').css("border","1px solid #CCCCCC");
	}
	
	$.ajax({
		url:'saveGuradianDetailsForStudentUpdate.json?studId='+studId,  
		data:{gaurdinName:gaurdinName,
			guardianMiddleName:guardianMiddleName,guardianLastName:guardianLastName,
			gaurdianEmailID:gaurdianEmailID,guardianRelation:guardianRelation,
			gaurdianAddress:gaurdianAddress,guardianTaluka:guardianTaluka,guardianVillage:guardianVillage,
			guardianPinCode:guardianPinCode,guardianStd:guardianStd,guardianPhoneNo:guardianPhoneNo,
			gaurdianMobileNo:gaurdianMobileNo,studGuardianId:studGuardianId},
		dataType:'json',
		type:'POST',
		success:function(jArray){
			getCustomAlertMessage("alert_create");
			getstudentGaurdianinfoNew1();
			getstudentGaurdianinfo();
		},
		error:function(){
			//jAlert("Something went wrong, Please try after some time","Error");
		}
	});

}

function getstudentGaurdianinfo()
{
	
	var studId= $('#studentId').val();
	var hdnUserRole = $('#hdnUserRole').val();
	var url = 'getParentDetailsProfile.json?studId='+studId;
	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getParentDetailsProfile.json';
	}
	$.ajax({
		url:url, 
		dataType:'json',
		async:false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(jArray.length>0)
			{
				$('#gaurdiName').val(jArray[0]["gaurdianName"]);
				$("#studguardianmiddlename").val(jArray[0]["guardianMiddleName"]);
				$("#studguardianlastname").val(jArray[0]["guardianLastName"]);
				$("#gaurdiEmail").val(jArray[0]["gaurdianEmailID"]);
				$('#guardianRelation option[value='+jArray[0]["guardianRelationship"]+']').attr("selected","selected");
				$("#guardiAddress").val(jArray[0]["gaurdianAddress"]);
				//$('#cityCombo').val(jArray[0]["cityObj"]["cityId"]);
				$("#studguardiantaluka").val(jArray[0]["guardianTaluka"]);
				$("#studguardianVillage").val(jArray[0]["guardianVillage"]);
				$("#studguardianpincode").val(jArray[0]["guardianPinCode"]);
				$("#studeguardianstdcode").val(jArray[0]["guardianStd"]);
				$("#guardianstudphnumber").val(jArray[0]["guardianPhoneNo"]);
				//$("#studguardianfaxno").val(jArray[0]["guardianFax"]);
				$("#studguardiancellno").val(jArray[0]["gaurdianMobileNo"]);
				
				
				//$("#studeparentstdcode").val(jArray[0]["fatherHomeStd"]);
				$("#motherHomestdcode").val(jArray[0]["motherHomeStd"]);
				
				$("#motherstdcode").val(jArray[0]["motherOfficeStd"]); 
			}
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error:function(){
			alert("In Student Guardian Detail error");
		}

	});	
}
			
function editStudentSubjectChoice() {

	$.ajax({
		url:'getApplicantSubjectChoice.json',
		dataType:'json',
		async:false,
		success:function(finalObj){

			var jArray = finalObj.subjectList;
			var subjectIdArray = new Array();
			if(jArray.length > 0)
			{
				var boiler = '<table class="table table-bordered" id="currentSubDiv" ><thead><tr><th colspan="4">Current Term  Subject </th></tr>'+
				'<tr><th></th><th></th><th>Course Code</th><th>Course Name</th></tr></thead>';
				var group="";
				var group1="";
				for(var i=0;i<jArray.length;i++)
				{
					var subjectCode ='';
					var subjectName='';
					var elecriveArray = jArray[i]["electives"]; 
					if(jArray[i]["subjectCode"]!=undefined)
						subjectCode=jArray[i]["subjectCode"];

					if(jArray[i]["subjectName"]!=undefined)
						subjectName=jArray[i]["subjectName"].gemsDecode();

					subjectIdArray.push(jArray[i]["subjectId"]);
					if(jArray[i]["group"]!=undefined && group1 != jArray[i]["group"].replace(/\s+/g,"")) {
						group = "RegGroup"+ i +""; 
						group1 = jArray[i]["group"].replace(/\s+/g,"");
						boiler+='<tr><th colspan="5" id="'+ group +'">'+ jArray[i]["group"] +'</th></tr>';
					}

					boiler+='<tr>';
					if(jArray[i]["subjectChoice"]==0)
						boiler+='<td><input type="checkbox" class="extSubjectCls" value="'+jArray[i]["universitySubjectId"]+'_'+jArray[i]["subjectId"]+'_Current" disabled="disabled" id="'+ jArray[i]["subjectId"] +'" checked="checked" isElectiveSubject = "'+elecriveArray.length+'" ></td>';	
					else {
						if(group == "")
							boiler+='<td><input type="checkbox" class="extSubjectCls" value="'+jArray[i]["universitySubjectId"]+'_'+jArray[i]["subjectId"]+'_Current"  id="'+ jArray[i]["subjectId"] +'" name="'+  jArray[i]["noofExternalChoice"]  +'" isElectiveSubject = "'+elecriveArray.length+'"   ></td>';
						else
							boiler+='<td><input type="checkbox" class="extSubjectCls '+group+'" value="'+jArray[i]["universitySubjectId"]+'_'+jArray[i]["subjectId"]+'_Current"  id="'+ jArray[i]["subjectId"] +'" name="'+  jArray[i]["noofExternalChoice"]  +'" isElectiveSubject = "'+elecriveArray.length+'"   ></td>';
					}

					boiler+='<td></td><td>'+subjectCode+'</td><td colspan="2">'+subjectName;					
					if(elecriveArray.length>0){
						if(jArray[i].noofInternalChoice!=undefined)
							boiler+='  <b>  [  '+ jArray[i].noofInternalChoice +' / '+ elecriveArray.length +' ] </b>';
						else
							boiler+='    [  All Subjects are Compulsory  ]';
					}
					boiler+='</td></tr>';	

					var j=0;
					while(elecriveArray.length>j) {
						if(jArray[i]["subjectChoice"]==0)
							boiler+='<tr class="'+jArray[i]["subjectId"]+'"><td></td>';
						else
							boiler+='<tr class="'+jArray[i]["subjectId"]+'" ><td></td>'; //style="display:none"
						if(elecriveArray[j]["subjectChoice"]==0)
							boiler+='<td><input type="checkbox" value="'+elecriveArray[j]["universitySubjectId"]+'_'+elecriveArray[j]["subjectId"]+'_Current" id="'+ elecriveArray[j]["subjectId"] +'" class="intSubjectCls" name="checked" disabled="disabled" ></td>';	
						else
							boiler+='<td><input type="checkbox" value="'+elecriveArray[j]["universitySubjectId"]+'_'+elecriveArray[j]["subjectId"]+'_Current" id="'+ elecriveArray[j]["subjectId"] +'" class="intSubjectCls" name="'+  jArray[i]["noofInternalChoice"]  +'" disabled="disabled"  ></td>';
						boiler+='<td>'+elecriveArray[j]["subjectCode"] +'</td><td colspan="2">'+elecriveArray[j]["subjectName"].gemsDecode()+'</td></tr>';
						j++;
					}	
				}
				boiler+='</table>';
				$('#bodyStudentSubjectChoice').html(boiler);

				var choiceList = finalObj.subjectChoice.toString().split(",");
				for(var c=0; c<choiceList.length; c++)
					$('#'+choiceList[c]).attr("checked", "checked");

				$('.extSubjectCls').each(function(){
					var status = $(this).is(":checked"); 
					var iselectivesubject = $(this).attr("iselectivesubject");
					$(this).attr("checked",status);	
					$(this).attr("disabled",false);	
					var flag = false;
					if(iselectivesubject == 0){
						if(!status)
							$(this).attr("disabled",true);
					} else {
						var childId = $(this).attr("id");			
						$('.'+ childId +'').find('input[type=checkbox]').each(function(){
							if($(this).is(":checked"))
								flag = true;

							$(this).attr("disabled",true);
						});

						if(flag)
							$(this).attr("checked",true);
						else
							$(this).attr("disabled",true);
					} 
				});


				$('#currentSubDiv input[type=checkbox]').click(function(){
					var id = $(this).attr("id");
					var status = $(this).is(":checked");
					var elemClass1 = $(this).attr("class").split(" ");
					var elemClass = "";
					var elemCount = $(this).attr("name");
					if(elemClass1.length >= 2)
						elemClass = $(this).attr("class").split(" ")[1];
					else
						elemClass = $(this).attr("class");

					$('#currentSubDiv input[class='+ id +']').each(function(){					
						if(status==true)
						{
							if($(this).attr("name")=="checked"){
								$(this).attr("checked",status);
								$(this).attr("disabled",true);
							}
							else
								$(this).attr("disabled",false);	
						} else
							$(this).attr("checked",status);

					});

					if($('#currentSubDiv .'+ elemClass +':checked').length>=elemCount) {
						$('#currentSubDiv .'+ elemClass).each(function(){						
							if(id!=$(this).attr("id")){
								var childId = $(this).attr("id");
								if(!$(this).is(":checked"))
									$(this).attr("disabled",true);				

								$('.'+ childId +'').find('input[type=checkbox]').each(function(){
									$(this).attr("disabled",true);
									$(this).attr("checked",false);
								});
							}
						});
					} else {		

						$('#currentSubDiv .'+ elemClass).each(function(){						
							var childId = $(this).attr("id");
							$('.'+ childId +'').find('input[type=checkbox]').each(function(){
								$(this).attr("disabled",false);
							});
						});
					}
				});

			} else 
				$('#bodyStudentSubjectChoice').html('<br><center><span>Course Not Defined By University</span></center>');

			$('#studentSubjectChoiceModal').modal('show');

			$('.saveSubjectChoice').unbind();
			$('.saveSubjectChoice').html('Save Subject Choice');
			$('.saveSubjectChoice').click(function(){
				saveApplicantSubjectChoice();
			});
		},
		error:function(){
			//jAlert("in Error Admission Type CMB","Error");
		}
	});
}

function saveApplicantSubjectChoice()
{
	var universitySubjectAndSubjectIdArray = new Array();
	var currentSubFlag = false;
	$('#currentSubDiv .extSubjectCls').each(function(){
		var elemClass1 = $(this).attr("class").split(" ");
		var elemClass = "";
		if(elemClass1.length >= 2)
			elemClass = $(this).attr("class").split(" ")[1];
		else
			elemClass = $(this).attr("class");

		currentSubFlag = false;
		$('#currentSubDiv .'+ elemClass+'').each(function(){
			if($(this).is(":checked"))
				currentSubFlag = true;
		});

		if(!currentSubFlag)
			jAlert("Please Enter Subjects");return false;
	});

	$('.extSubjectCls').each(function(){
		if($(this).is(":checked") && $(this).attr('isElectiveSubject') == 0)
			universitySubjectAndSubjectIdArray.push($(this).attr("value"));
		else
			isElectiveSubject = true;
	});

	$('.intSubjectCls').each(function(){
		if($(this).is(":checked")){
			universitySubjectAndSubjectIdArray.push($(this).attr("value"));
			electiveSub = true;
		}
	});

	$.ajax({
		url:'saveApplicantSubjectChoice.json?universitySubjectAndSubjectIdArray='+universitySubjectAndSubjectIdArray,
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray) {
			$('#studentSubjectChoiceModal').modal('hide');
			getCustomAlertMessage("alert_create");
		},
	}); 
}
function getsubmitButton(mode)
{
	
	
	if(mode=="t&c")
	{
		
		
		if($('#objCmb1').is(':checked') )
		{
			//$('.modeCash').removeClass('nonPrintable');
			$('#submit').attr("disabled",false);
			
	
		}
		else
		{
			$('#submit').attr("disabled",true);
			
		}

}
}

function getIcardDetails(){
	
	getCountry();
	
	$.ajax({
		url:'getStudentPersonalDetailsForICard.json?studentId=0',
		dataType:'json',
		type:'post',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray) {
			if(jArray.length > 0){
				if(!jArray[0].is_icard_confirmed)
				{
					$('#icardstudfirstname').val(jArray[0].firstName);
					
					$('#icardstudmiddlename').val(jArray[0].middleName);
					$('#icardstudlastname').val(jArray[0].lastName);
					$('#studclass').val(jArray[0].std);
					if (jArray[0]["Gender"] == "Male")
						$("#Icardmale").attr("checked", true);
					else
						$("#IcardFemale").attr("checked", true);

					if (jArray[0]["dob"] != undefined) {
						var dob = new Date(jArray[0]["dob"]);
						$('#dob').datepicker({
							dateFormat : 'M d,yy',
						});
						$("#dob").datepicker('setDate', dob);

					}
					$('#icardstudbloodgrp').val(jArray[0]["bloodGroup"]);
					$("#icardtxtmobileNo").val(jArray[0]["mobileNo"]);
					for ( var i = 0; i < jArray.length; i++) {
						if (jArray[i]["permanentOrCurrent"] == "current") {

							var cntryId1 = jArray[i]["countryId"];
							$('#icardstudentCountryCombo option[value='+ cntryId1 + ']').attr("selected", "selected");

							getStudentStates('icardstudentCountryCombo','icardstudentState');

							var stateId = jArray[i]["stateId"];
							$('#icardstudentState option[value=' + stateId + ']').attr("selected", "selected");

							getStudentCity('icardstudentState', 'icardstudentCity');
							var cityId = jArray[i]["cityId"];
							$("#icardstudAddLine1").val(
									jArray[i]["addressLine1"]);
							$("#icardstudAddLine2").val(
									jArray[i]["addressLine2"]);
							$('#icardstudentCity').val(cityId);
							$("#icardstudPincode").val(jArray[i]["pinCode"]);
							$('#CurrAddressId').val(jArray[i].studentAddressId);
						} 
					}
					$('#icardEmailid').val(jArray[0]["alternetEmailID"]);
				}
				else
				{
					
					$('#icardstudfirstname').val(jArray[0].firstName);
					$('#icardstudfirstname').attr('disabled',true);

					$('#icardstudmiddlename').val(jArray[0].middleName);
					$('#icardstudmiddlename').attr('disabled',true);

					$('#icardstudlastname').val(jArray[0].lastName);
					$('#icardstudlastname').attr('disabled',true);

					$('#studclass').val(jArray[0].std);
					$('#studclass').attr('disabled',true);

					if (jArray[0]["Gender"] == "Male")
					{
						$("#Icardmale").attr("checked", true);
						$('#Icardmale').attr('disabled',true);
					}
					else if(jArray[0]["Gender"]=="Female")
					{
						$("#IcardFemale").attr("checked", true);
						$('#IcardFemale').attr('disabled',true);
					}
					if (jArray[0]["dob"] != undefined) {
						var dob = new Date(jArray[0]["dob"]);
						$('#dob').attr('disabled',true)
						$('#dob').datepicker({
							dateFormat : 'M d,yy',
						});
						$("#dob").datepicker('setDate', dob);

					}
					$('#icardstudbloodgrp').val(jArray[0]["bloodGroup"]);
					$('#icardstudbloodgrp').attr('disabled',true);
					$("#icardtxtmobileNo").val(jArray[0]["mobileNo"]);
					$('#icardtxtmobileNo').attr('disabled',true);
					
					for ( var i = 0; i < jArray.length; i++) {
						if (jArray[i]["permanentOrCurrent"] == "current") {

							var cntryId1 = jArray[i]["countryId"];
							$('#icardstudentCountryCombo option[value='+ cntryId1 + ']').attr("selected", "selected");
							$('#icardstudentCountryCombo').attr('disabled',true);
							getStudentStates('icardstudentCountryCombo','icardstudentState');

							var stateId = jArray[i]["stateId"];
							$('#icardstudentState option[value=' + stateId + ']').attr("selected", "selected");
							$('#icardstudentCountryCombo').attr('disabled',true);
							getStudentCity('icardstudentState', 'icardstudentCity');
							var cityId = jArray[i]["cityId"];
							$("#icardstudAddLine1").val(
									jArray[i]["addressLine1"]);
							$('#icardstudAddLine1').attr('disabled',true);
							$("#icardstudAddLine2").val(
									jArray[i]["addressLine2"]);
							$('#icardstudAddLine2').attr('disabled',true);
							$('#icardstudentCity').val(cityId);
							$('#icardstudentCity').attr('disabled',true);
							$("#icardstudPincode").val(jArray[i]["pinCode"]);
							$('#icardstudPincode').attr('disabled',true);
							$('#CurrAddressId').val(jArray[i].studentAddressId);
							$('#CurrAddressId').attr('disabled',true);
						} 
					}
					$('#icardEmailid').val(jArray[0]["alternetEmailID"]);
					$('#icardEmailid').attr('disabled',true);
					
					$('#objCmb1').attr('disabled',true);
					$('#btnAddCityInStudContact').attr('disabled',true);
					$('#submit').attr('disabled',true);
					
					
				}
			}
		
		},
	}); 
}
var gIsForiegner = 0;
function addStudentPersonalDetailsforIcard() {
	
	
	var fName = $('input[name = "FirstName"]').val();
	var mName = $('input[name = "MiddleName"]').val();
	var lName = $('input[name = "LastName"]').val();
	var Gender = $('input[name = "Gender"]:checked').val();
	var dob = $('input[name = "dob"]').val();
	var alternetContactNumber = $('input[name = "icardmobileNo"]').val();
	
	var alternetEmailID = $('input[name = "email"]').val();
	var studbloodgrp = $('#icardstudbloodgrp').val();
	
	var addressLine1 = $('input[name = "icardaddressLine1"]').val();
	var addressLine2 = $('input[name = "icardaddressLine2"]').val();
	var cityId = $('#icardstudentCity').val();
	var stateId = $('#icardstudentState').val();
	var countryId = $('#icardstudentCountryCombo').val();
	var pinCode = $('input[name = "icardpinCode"]').val();
	
	var CurrAddressId = $('#icardCurrAddressId').val();
	
	var isConfirmed = "false";
	var isForiegner = $('[input Name="IsForiegner"]:checked').val();;
	 
	var cnt = 0;
	//var signatureUpload = $('#studentSignatureUpload').find('input[name=fileData]').length;
	if (fName == '' || !(isNaN(fName))) {
		cnt = 1;
		jAlert("Please enter valid studentSignatureUploadfirst name");
		$('input[name = "FirstName"]').focus();
	} else if (lName == '' || !(isNaN(lName))) {
		cnt = 1;
		jAlert("Please enter valid last name");
		$('input[name = "Last Name"]').focus();
	}  else if (dob == '') {
		cnt = 1;
		jAlert("Please enter date of birth");
	}
	else if(studbloodgrp==''){
		cnt = 1;
		jAlert("Please select Bloodgroup");
	}
	
	if (alternetContactNumber != '') {
		if (isNaN(alternetContactNumber)) {
			cnt = 1;
			jAlert("Please enter valid alternate number");
			$('input[name = "mobileNo"]').focus();
		}
	}
	
	if (cnt == 0) {
		$.ajax({
			url : 'studentProfilePostforIcard.json',
			data : {
				fName : fName,
				mName : mName,
				lName : lName,
				Gender : Gender,
				dob : dob,
				alternetContactNumber : alternetContactNumber,
				alternetEmailID : alternetEmailID,
				studbloodgrp : studbloodgrp,

				CurrAddressId : CurrAddressId,
				addressLine1 : addressLine1,
				addressLine2 : addressLine2,
				countryId : countryId,
				stateId : stateId,
				cityId : cityId,
				pinCode : pinCode,
				isConfirmed : isConfirmed,
				isForiegner:isForiegner
			},
			dataType : 'json',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				if (jArray[0].responseFlag == '1')
				getCustomAlertMessage("alert_create");
				gIsForiegner = jArray[0].isForiegner;
				if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="INDIRA_PUNE")
				{
					$('a[href="#passportAndVisaDetailsDetails"]').show();
				}
				else{
					if(jArray[0].isForiegner)
					{
						$('a[href="#passportAndVisaDetailsDetails"]').show();
					}else{
						$('a[href="#passportAndVisaDetailsDetails"]').hide();
					}
				}
			},
			error : function() {
			}
		});
	}
}

function getAdmissionQuotaType(strengthId,admissionTypeId,admissionQuotaStatus) {
	var instituteCourseStrengthId = strengthId;//$('#selInstituteCourseStrength').val();
	$.ajax({
		url : 'fillAdmissionTypeDetails.json?instituteCourseStrengthId='
				+ instituteCourseStrengthId,
		dataType : 'json',
		async : false,
		success : function(jArray) {

			var boiler = '<option value="0">Select</option>';
			for ( var i = 0; i < jArray.length; i++) {
				boiler += '<option value="' + jArray[i]["admissionTypeId"]
						+ '" >' + jArray[i]["admissionType"] + '</option>';
			}
			try {
				callback(boiler);
			} catch (e) {
				$('#admissionQuotaType').html(boiler);
			}

			// $('#admissionQuotaTypeCombo').html(boiler);
		},
		error : function() {
			// jAlert("in Error Admission Type CMB","Error");
		}
	});
}
function getAdmissionCategoryList() {
	$('#casteCategoryTR').show();
	var admissionTypeId = $('#admissionQuotaType').val();
	//alert(admissionTypeId);
	$.ajax({
		url : 'getAdmissionCategoryDetails.json?admissionTypeId='
				+ admissionTypeId,
		dataType : 'json',
		async : false,
		success : function(jArray) {
			var boiler = '<option value="0">Select</option>';
			for ( var i = 0; i < jArray.length; i++)
				boiler += '<option value="'
						+ jArray[i]["categoryReservationId"] + '" >'
						+ jArray[i]["categoryName"] + '</option>';

			$('#admissionCategory').html(boiler);
		},
		error : function() {
			// jAlert("in Error Admission Type CMB","Error");
		}
	});
}
function getPassportAndVisaDetails(){
	
	$('#txtCountryAddress').val('');
	$('#txtSponsoringAgency').val('');
	$('#txtPassportNo').val('');
	$('#txtVisaNo').val('');
	$('#txtPassportIssuedDate').val('');
	$('#txtVisaIssuedDate').val('');
	$('#txtPassportValidTillDate').val('');
	$('#txtVisaValidTillDate').val('');
	$('#txtPassportIssuedPlace').val('');
	$('#txtVisaIssuedPlace').val('');
	$('#passportIssuedCountryCmb').val('');
	$('#visaIssuedCountryCmb').val('');
	$('#txtFrroFroRegCertNo').val('');
	$('#txtFrroIssuedDate').val('');
	$('#txtFrroValidTillDate').val('');	
	$('#txtRegistrationIssuedDate').val('');
	$('#txtRegistrationValidTillDate').val('');
	$('#txtRegistrationIssuedPlace').val('');
	$('#registrationIssuedCountryCmb').val('');
	$('#grantNameCmb').val('');
	$('#txtApplicationNo').val('');
	$('#visaTypeCmb').val('');
	$('#visaCategoryCmb').val('');
	$.ajax({
		url : 'getStudentPassportAndVisaDetails.json',
		dataType : 'json',
		async : false,
		success : function(jArray) {
			var nameAsPerPassport = jArray["nameAsPerPassport"];
			var parentName =jArray["parentName"];
			var relationshipWithParent =jArray["relationshipWithParent"];
			var countryAddress=jArray["countryAddress"];
			var sponsoringAgency =jArray["sponsoringAgency"];
			var passportNo =jArray["passportNo"];
			var passIssuedDate =jArray["passIssuedDate"];
			var passValidTillDate=jArray["passValidTillDate"];
			var passPlaceOfIssue =jArray["passPlaceOfIssue"];
			var PassIssuedCountry=jArray["PassIssuedCountry"];
			
			//Visa 
			var visaNo =jArray["visaNo"];
			var visaIssuedDate=jArray["visaIssuedDate"];
			var visaValidTillDate=jArray["visaValidTillDate"];
			var visaPlaceOfIssue=jArray["visaPlaceOfIssue"];
			var visaIssuedCountry=jArray["visaIssuedCountry"];
			
			var FRORegistrationCertificateNo=jArray["FRORegistrationCertificateNo"];
			var FRORegistrationCertificateIssueDate=jArray["FRORegistrationCertificateIssueDate"];
			var FRORegistrationCertificateValidTillDate=jArray["FRORegistrationCertificateValidTillDate"];
			
			//RC/RP 
			var registrationIssuedDate=jArray["registrationIssuedDate"];
			var registrationValidTillDate=jArray["registrationValidTillDate"];
			var registrationIssuedPlace=jArray["registrationIssuedPlace"];
			var registrationIssuedCountry=jArray["registrationIssuedCountry"];
			var grantName =jArray["grantName"];
			var applicationNo =jArray["applicationNo"];
			var visaType =jArray["visaType"];
			var visaCategory = jArray["visaCategory"];
			
			isForiegnerForPassport = jArray["isForiegner"];//NRI
			//hide for NRI STUD
				if(jArray["isForiegner"] == 'true' || jArray["isForiegner"]== true){
						$('.isMandatory').css('display', 'none');
				}
			
			$('#txtNameOnPassport').val(nameAsPerPassport);
			$('#txtNameOfParent').val(parentName);
			
			if(relationshipWithParent=='Father')
			$('#rbtnFather').attr('checked','checked');
			if(relationshipWithParent=='Mother')
			$('#rbtnMother').attr('checked','checked');
			
			$('#txtCountryAddress').val(countryAddress);
			$('#txtSponsoringAgency').val(sponsoringAgency);
			$('#txtPassportNo').val(passportNo);
			$('#txtVisaNo').val(visaNo);
			$('#txtPassportIssuedDate').val(passIssuedDate);
			$('#txtVisaIssuedDate').val(visaIssuedDate);
			$('#txtPassportValidTillDate').val(passValidTillDate);
			$('#txtVisaValidTillDate').val(visaValidTillDate);
			$('#txtPassportIssuedPlace').val(passPlaceOfIssue);
			$('#txtVisaIssuedPlace').val(visaPlaceOfIssue);
			$('#passportIssuedCountryCmb').val(PassIssuedCountry);
			$('#visaIssuedCountryCmb').val(visaIssuedCountry);
			$('#txtFrroFroRegCertNo').val(FRORegistrationCertificateNo);
			$('#txtFrroIssuedDate').val(FRORegistrationCertificateIssueDate);
			$('#txtFrroValidTillDate').val(FRORegistrationCertificateValidTillDate);
			$('#txtRegistrationIssuedDate').val(registrationIssuedDate);
			$('#txtRegistrationValidTillDate').val(registrationValidTillDate);
			$('#txtRegistrationIssuedPlace').val(registrationIssuedPlace);
			$('#registrationIssuedCountryCmb').val(registrationIssuedCountry);
			$('#grantNameCmb').val(grantName);
			$('#txtApplicationNo').val(applicationNo);
			$('#visaTypeCmb').val(visaType);
			$('#visaCategoryCmb').val(visaCategory);
			var studentId=jArray["studentId"];
			$('#uploadPasspostVisaDoc').bind('click', function(){
				uploadStudPassportVisaDocument(studentId);
			});
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			// jAlert("in Error Admission Type CMB","Error");
		}
	});
	
	
}
function addStudentPassportAndVisaDetailsDetails(){
	var isValid = true;
	var nameOfStudent = $('#txtNameOnPassport').val();
	var nameOfParent = $('#txtNameOfParent').val();
	var parentRelationship = $('input[name = "parentRelationship"]:checked').val();
	var countryAddress = $('#txtCountryAddress').val();
	var sponsoringAgency = $('#txtSponsoringAgency').val();
	var passportNo = $('#txtPassportNo').val();
	var visaNo = $('#txtVisaNo').val();
	var passportIssuedDate = $('#txtPassportIssuedDate').val();
	var visaIssuedDate = $('#txtVisaIssuedDate').val();
	var passportValidTillDate = $('#txtPassportValidTillDate').val();
	var visaValidTillDate = $('#txtVisaValidTillDate').val();
	var passportIssuedPlace = $('#txtPassportIssuedPlace').val();
	var visaIssuedPlace = $('#txtVisaIssuedPlace').val();
	var passportIssuedCountry = $('#passportIssuedCountryCmb option:selected').val();
	var visaIssuedCountry = $('#visaIssuedCountryCmb option:selected').val();
	var frroFroRegCertNo = $('#txtFrroFroRegCertNo').val();
	var frroIssuedDate = $('#txtFrroIssuedDate').val();
	var frroValidTillDate = $('#txtFrroValidTillDate').val();
	var regExp = /^[A-Za-z. ]+$/;
	var registrationIssuedDate = $('#txtRegistrationIssuedDate').val();
	var registrationValidTillDate = $('#txtRegistrationValidTillDate').val();
	var registrationIssuedPlace = $('#txtRegistrationIssuedPlace').val();
	var registrationIssuedCountry = $('#registrationIssuedCountryCmb option:selected').val();
	var grantName = $('#grantNameCmb').val();
	var applicationNo = $('#txtApplicationNo').val();
	var visaType = $('#visaTypeCmb').val();
	var visaCategory = $('#visaCategoryCmb').val();

	if (nameOfStudent == "" || !regExp.test(nameOfStudent)) {
		jAlert('Required:Name as per Passport');
		isValid = false;
		return false;
	}
	if (nameOfParent == "" || !regExp.test(nameOfParent)) {
		jAlert('Required:Parent Name (Father/Mother) ');
		isValid = false;
		return false;
	} 
	if (passportNo == "") {
		jAlert('Required:Passport No');
		isValid = false;
		return false;
	}
	if (visaNo == "" && isForiegnerForPassport == 'false') {
		jAlert('Required:Visa No');
		isValid = false;
		return false;
	}
	if(countryAddress==""){
		jAlert('Required:Country Address');
		isValid = false;
		return false;
	}
	if(passportIssuedDate==""){
		jAlert('Required:Passport Issued Date');
		isValid = false;
		return false;
	}
	if(visaIssuedDate=="" && isForiegnerForPassport == 'false'){
		jAlert('Required:Visa Issued Date');
		isValid = false;
		return false;
	}
	if(passportValidTillDate==""){
		jAlert('Required:Passport Valid Till Date');
		isValid = false;
		return false;
	}
	if(visaValidTillDate=="" && isForiegnerForPassport == 'false'){
		jAlert('Required:Visa Valid Till Date');
		isValid = false;
		return false;
	}
	if(passportIssuedCountry=="0"){
		jAlert('Required:Passport Issued Country');
		isValid = false;
		return false;
	}
	if(visaIssuedCountry=="0" && isForiegnerForPassport == 'false'){
		jAlert('Required:Visa Issued Country');
		isValid = false;
		return false;
	}
	
	if($('#RCRPDetails').val()=="true" ||$('#RCRPDetails').val()==true){
		if(registrationIssuedDate==""){
			jAlert('Required:Registration Issued Date');
			isValid = false;
			return false;
		}
		if(registrationValidTillDate==""){
			jAlert('Required:Registration Valid Till Date');
			isValid = false;
			return false;
		}
		if(registrationIssuedCountry=="0"){
			jAlert('Required:Registration Issued Country');
			isValid = false;
			return false;
		}
	}
	if($('#RCRPDetails').val()=="true" ||$('#RCRPDetails').val()==true){
		if(visaType==""){
			jAlert('Required:Visa Issued Type');
			isValid = false;
			return false;
		}
		if(visaCategory==""){
			jAlert('Required:Visa Issued Category');
			isValid = false;
			return false;
		}
	}
	
	if(isValid){
	$.ajax({
		url : 'addForeignerStudentsPassportAndVisaDetails.json',
		data : {
			nameOfStudent : nameOfStudent,
			nameOfParent : nameOfParent,
			parentRelationship ␀␀␀: parentRelationship,
			countryAddress:countryAddress,
			sponsoringAgency:sponsoringAgency,
			passportNo:passportNo,
			visaNo:visaNo,
			passportIssuedDate:passportIssuedDate,
			visaIssuedDate:visaIssuedDate,
			passportValidTillDate:passportValidTillDate,
			visaValidTillDate:visaValidTillDate,
			passportIssuedPlace:passportIssuedPlace,
			visaIssuedPlace:visaIssuedPlace,
			passportIssuedCountry:passportIssuedCountry,
			visaIssuedCountry:visaIssuedCountry,
			frroFroRegCertNo:frroFroRegCertNo,
			frroIssuedDate:frroIssuedDate,
			frroValidTillDate:frroValidTillDate,
			registrationIssuedDate:registrationIssuedDate,
			registrationValidTillDate:registrationValidTillDate,
			registrationIssuedPlace:registrationIssuedPlace,
			registrationIssuedCountry:registrationIssuedCountry,
			grantName:grantName,
			applicationNo:applicationNo,
			visaType:visaType,
			visaCategory:visaCategory
			
			
			
		},
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		async : false,
		dataType : 'text',
		type : 'POST',
		success : function(jArray) {
			getCustomAlertMessage("alert_create");
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
	}
}
function uploadStudMedicalDocument(studentMedicalDetailId){
	if(studentMedicalDetailId != '' && studentMedicalDetailId != 'null' && studentMedicalDetailId != undefined){
			getDegreeCategoryId(gTabDegreeName.TabName_MEDICAL,studentMedicalDetailId);
	} else
			jAlert('Student Certificate record not found!');
}
function uploadStudPassportVisaDocument(studentId){
	if(studentId != '' && studentId != 'null' && studentId != undefined){
			getDegreeCategoryId(gTabDegreeName.TabName_PASSPORTVISA,studentId);
	} else
			jAlert('Student Certificate record not found!');
}

function getCaste()
{
	$.ajax({
		url:'getAllCaste.json', 
		dataType:'json',
		async:false,
		success:function(jArray){
			var html = '<option value="0">Select</option>';
			var len = jArray.length;
			for(var i=0; i<len; i++){
				var html = '<option value="0">Select</option>';
				var len = jArray.length;
				for(var i=0; i<len; i++){
					html += '<option value="'+ jArray[i]["casteId"] + '">'+ jArray[i]["casteName"] + '</option>';
				}
				$("#txtCaste").html(html);
				convertComboToSearchable('#txtCaste');
				$("#txtCasteForEligibility").html(html);
				convertComboToSearchable('#txtCasteForEligibility');
			}
			//$("#txtstudcategory").html(html);
		},

	});	
}
setTimeout(function() {
	$('#txtCaste').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {
					$("#txtCaste").autocomplete(casteListForSearching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("+ term + ")", "gi"),"<strong>$1</strong>");
								},
								formatResult : function(row) {
									// uniIdFromPopUp=row.to;
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {

				}

			});
}, 1000);

