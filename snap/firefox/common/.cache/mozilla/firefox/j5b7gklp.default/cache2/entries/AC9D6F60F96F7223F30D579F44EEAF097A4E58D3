					jArray["cvsPalpation"]);
			$('input[name = "paPalpation"]').val(jArray["paPalpation"]);
			$('input[name = "otherPalpation"]').val(
					jArray["otherPalpation"]);
			$('input[name = "rsPercussion"]').val(
					jArray["rsPercussion"]);
			$('input[name = "cvsPercussion"]').val(
					jArray["cvsPercussion"]);
			$('input[name = "paPercussion"]').val(
					jArray["paPercussion"]);
			$('input[name = "otherPercussion"]').val(
					jArray["otherPercussion"]);
			$('input[name = "rsAuscultation"]').val(
					jArray["rsAuscultation"]);
			$('input[name = "cvsAuscultation"]').val(
					jArray["cvsAuscultation"]);
			$('input[name = "paAuscultation"]').val(
					jArray["paAuscultation"]);
			$('input[name = "otherAuscultation"]').val(
					jArray["otherAuscultation"]);
			$('input[name = "specificExamination"]').val(
					jArray["specificExamination"]);
			$('#studMedCertiId').val(jArray["studentMedicalDetailId"]);
			$('#txtPresentStatOfAilment').val(jArray["presentStatusOfAilment"]);
			$('#txtPhysicalDisability').val(jArray["physicalDisability"]);
			var studentMedicalDetailId=jArray["studentMedicalDetailId"];
			$('#uploadMedicalDocBtn').bind('click', function(){
				uploadStudMedicalDocument(studentMedicalDetailId);
			});
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			alert("In Student Medical Detail error");
		}
	});
}
//-------------------------------------------------------------Current
//Qualification Tab

function getStudentCurrentQualificationConfirm(degreeType) {
	$.ajax({
		url : 'getStudentCurrentQualification.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var myboiler = '';

			if (jArray.length > 0) {
				if (jArray[0]["studentType"] == "academic"	|| jArray[0]["studentType"] == "PlacementAcademic") {
					// academic

					var tempSem = 0;
					var tempCnt = 0;
					var currentSem = jArray[0]["cnt"];
					myboiler += '<table class="table table-bordered" style="width: 100%;" >'
						+ '<tr><td colspan="4" style="color: #0088CC"><label><b><font>Student current qualification:</font></b></label></td></tr>'
						+ '<tr></tr>';
					for ( var i = 0; i < jArray.length; i++) {
						myboiler += '<tr><th style="width:30%">University name</th><td><label>'+ jArray[i]["universityName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Degree stream</th><td><label>'+ jArray[i]["degreeStreamName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Degree name</th><td><label>'+ jArray[i]["degreeName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Specialization (if any)</th><td><select id="cmbSpecNew"></select>&nbsp;&nbsp;<input type="button" class="btn btn-inverse btn-mini btnColorGreen " value="Save" id="btnSaveStudSpec" onclick="SaveStudSpec()"/></td></tr>';
						myboiler += '<tr><th style="width:30%">Year of Passing</th><td><label>'+ jArray[i]["yearOfPassing"]+ '</label></td></tr>';
						myboiler += '<tr><td colspan="3" id="tdSem">'+ '</td></tr>';

					}
					myboiler += '</table>';

					$('#ug').html(myboiler);

					var tempSemBoiler = '';
					/*
					 * '<table class="table table-bordered
					 * table-condensed" style="width: 1100px;">'+ '<tr>'+ '<td><b>Term
					 * Pattern</b></td>'+ '<td><label><input
					 * type="radio" name="semYear" id="radSem"
					 * value="Semester" onclick="showSemCmb()"
					 * checked="checked"/> Semester</label></td>'+ '<td><label><input
					 * type="radio" name="semYear" id="radYear"
					 * value="Year" onclick="showSemCmb()"/> Year</label></td>'+ '</tr>';
					 */

					if (currentSem == 1) {

						tempSemBoiler += '<table class="table table-bordered table-condensed"" style="width: 100%;">'
							+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
							+ '<tr style="margin-left:10%;" id="marksTypeTr">'
							+ '</tr>'
							+ '</table>'
							+ '<table >'
							+ '<tr id="atktTypeTr">'
							+ '</tr>'
							+ '</table>';
					} else {
						if (jArray[0]["marksSystem"] == "Percent") {
							// academic percent

							/*
							 * tempSemBoiler += '<tr>'+ '<td><b>System
							 * of marks</b></td>'+ '<td><label><input
							 * type="radio" name="type" id="radPercnt"
							 * value="Percent" checked="checked"
							 * onclick=""/> Percent System</label></td>'+ '<td><label><input
							 * type="radio" name="type" id="radGrade"
							 * value="Grade" disabled="disabled"
							 * onclick=""/> Grade System</label></td>'+ '</tr>';
							 */

							tempSemBoiler +=
								// '</table>'+
								'<table class="table table-bordered table-condensed" style="width: 100%;">'
								+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
								+ '<tr style="margin-left:10%;" id="marksTypeTr">'
								+ '<td colspan="3">'
								+ '<table class="table table-bordered table-condensed">'
								+ '<tr>';

							tempSemBoiler += '<th>Class</th>'
								+ '<th>Obtained marks</th>'
								+ '<th>Marks out of</th>'
								+ '<th>Percentage</th>' + '</tr>';

							var temp = 0;
							//currentSem
							for (temp = 1; temp <= 10; temp++) {
								// alert(alert);
								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ temp + '</td>';

								if (jArray[0]["totMarksPerc2" + temp+ ""] != undefined)
								{
									if (jArray[0]["isExamStatus" + temp + ""])
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="totMarks" id="txtPercentSysemTotMarks'+ temp+ '" value="'
										+ jArray[0]["totMarksPerc2"
										            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" disabled="disabled" /></td>';
									else
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="totMarks" id="txtPercentSysemTotMarks'+ temp+ '" value="'
										+ jArray[0]["totMarksPerc2"
										            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" /></td>';
								}
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" name="totMarks" id="txtPercentSysemTotMarks'
										+ temp
										+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
										+ temp
										+ '\',\'txtPercentSysemMarksOutOf'
										+ temp
										+ '\',\'msgmarksPG'
										+ temp
										+ '\',\'txtPercentSysemPercnt'
										+ temp + '\')" /></td>';

								if (jArray[0]["outOfMarksPerc2" + temp+ ""] != undefined)
								{
									if (jArray[0]["isExamStatus" + temp + ""])
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="outOfMarks" id="txtPercentSysemMarksOutOf'
											+ temp
											+ '" value="'
											+ jArray[0]["outOfMarksPerc2"
											            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" disabled="disabled" /></td>';
									else
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="outOfMarks" id="txtPercentSysemMarksOutOf'
											+ temp
											+ '" value="'
											+ jArray[0]["outOfMarksPerc2"
											            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" /></td>';
								}
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" name="outOfMarks" id="txtPercentSysemMarksOutOf'
										+ temp
										+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
										+ temp
										+ '\',\'txtPercentSysemMarksOutOf'
										+ temp
										+ '\',\'msgmarksPG'
										+ temp
										+ '\',\'txtPercentSysemPercnt'
										+ temp + '\')" /></td>';

								if (jArray[0]["currSem" + temp + ""] != undefined)
								{
									if (jArray[0]["isExamStatus" + temp + ""])
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="perc" id="txtPercentSysemPercnt'
											+ temp+ '" value="'+ jArray[0]["currSem"+ temp + ""]+ '" disabled="disabled"/></td>';
									else
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="perc" id="txtPercentSysemPercnt'
											+ temp
											+ '" value="'
											+ jArray[0]["currSem"
											            + temp + ""]
									+ '"/></td>';
								}
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" name="perc" id="txtPercentSysemPercnt'
										+ temp
										+ '" value=""/></td>';

								tempSemBoiler += '</tr>'
									+ '<tr id="msgmarksPG'
									+ temp
									+ '" style="display: none"> '
									+ '<td align="left" colspan="5" style="color: RED">Obtained Marks is greater than total MARKS.</td> '
									+ '</tr>';
							}
							//
							for ( var i = temp; i <=10 ; i++) {

								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ i
									+ '</td>'
									+ '<td><input type="text" style="text-align: right"  name="totMarks" id="txtPercentSysemTotMarks'
									+ i
									+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ i
									+ '\',\'txtPercentSysemMarksOutOf'
									+ i
									+ '\',\'msgmarksPG'
									+ i
									+ '\',\'txtPercentSysemPercnt'
									+ i
									+ '\')" /></td>'
									+ '<td><input type="text" style="text-align: right" name="outOfMarks" id="txtPercentSysemMarksOutOf'
									+ i
									+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ i
									+ '\',\'txtPercentSysemMarksOutOf'
									+ i
									+ '\',\'msgmarksPG'
									+ i
									+ '\',\'txtPercentSysemPercnt'
									+ i
									+ '\')" /></td>'
									+ '<td><input type="text" style="text-align: right"  id="txtPercentSysemPercnt'
									+ i
									+ '" value=""/></td>'
									+ '</tr>';
							}
							var currQualiLiveATKT = jArray[0]["currQualiLiveATKT"] == undefined ? "0"
									: jArray[0]["currQualiLiveATKT"];
							var currQualiDeadATKT = jArray[0]["currQualiDeadATKT"] == undefined ? "0"
									: jArray[0]["currQualiDeadATKT"];
							tempSemBoiler += '</table>'
								+ '</td>'
								+ '</tr>'
								+ '</table>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr id="atktTypeTr">'
								+ '<td>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr>';
							if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
								tempSemBoiler+= '<th>No of Live Backlogs</th>';
							}else{
								tempSemBoiler+= '<th>No of Live ATKT</th>';
							}
							tempSemBoiler+= '<td><label><input type="text" id="txtPercntLiveATKT" value="'
								+ currQualiLiveATKT
							+ '" class="input-small" style="text-align: right"></label></td>'
							+ '</tr>'
							+ '<tr>';
								if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
									tempSemBoiler+= '<th>No of Dead Backlogs</th>';
								}else{
									tempSemBoiler+= '<th>No of Dead ATKT</th>';
								}
								tempSemBoiler+= '<td><label><input type="text" id="txtPercentDeadATKT" value="'
							+ currQualiDeadATKT
							+ '" class="input-small" style="text-align: right"></label></td>'
							+ '</tr>'
							+ '<tr>'
							+ '<td>'
							+ '<input type="button" id="btnSaveMarksDetailsPercnt" onclick="saveCurrentQualiPercent('
							+ jArray[0]["currTotSem"]
							+ ')" class="btn btn-inverse btn-mini btnColorGreen " value="Save">'
							+ '<input type="button" onclick="viewPGDetails(\''
							+ degreeType
							+ '\')" class="btn btn-inverse btn-mini btnColorRed " value="Back" style="margin-left:10px;" >'
							+ '</table>' + '</td>' + '</tr>'
							+ '</table>';
						} else {
							// academic pointer
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="RAMAIAH_UNIVERSITY"){
							tempSemBoiler +=

								'<table class="table table-bordered table-condensed" style="width: 100%;">'
								+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
								+ '<tr style="margin-left:10%;" id="marksTypeTr">'
								+ '<td colspan="3">'
								+ '<table class="table table-bordered table-condensed">'
								+ '<tr>';

							}
///-------------------------------- For Sinhgad ----------------------------------------------///
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SINHGAD")
								{
								tempSemBoiler += 
									
								'<td colspan="7" style="height:20px"><font color="red"><b>Click on check box to enter marks.</b></font></td></tr>'+	
								'<tr><th style="width:5%"></th>'+
								'<th style="width:10%">Class/Semester</th>'+ 
								'<th style="width:10%">Credit Registered</th>'+
								'<th style="width:10%">Earned Credit</th>'+
								'<th style="width:10%">Total Marks/Points</th>'+
								'<th style="width:10%">SGPA</th>' + 
								'<th style="width:10%">CGPA</th>'
								+ '</tr>';
								
								var temp = 0;
								//currentSem
								for (temp = 1; temp <=jArray[0]["currTotalNoOfSem"] ; temp++) {

									tempSemBoiler += '<tr class="tempTrCls_'+temp+'" sem="'+temp+'">'; 
									if(jArray[0]["studUniExMarId"+ temp + ""] =="" || jArray[0]["studUniExMarId"+ temp + ""] == undefined || jArray[0]["studUniExMarId"+ temp + ""]=="undefined" || jArray[0]["studUniExMarId"+ temp + ""] =="0")
										tempSemBoiler += '<td style="text-align: center;"><input type="checkbox" value="0" class="semesterChkBoxCls" onclick="onclicksemesterChkBox(this)"> </td>';
									else
										{tempSemBoiler += '<td style="text-align: center;"><input type="checkbox" value="'+jArray[0]["studUniExMarId"+ temp + ""]+'" class="semesterChkBoxCls" checked="checked" disabled="disabled" onclick="onclicksemesterChkBox(this)">'+ 
										
										'<br><input type="button" id="btnEditMarkDetails" onclick="editMarkDetails(this);" class="btn btn-inverse btn-mini  btnColorBlack " value="Edit">'+
										
										
										'</td>';
										}
									tempSemBoiler +='<td style="text-align: right;">'	+ temp + '</td>';

											if (jArray[0]["creditRegistered2"+ temp + ""] != undefined)
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsReg(this);" id="txtCreditRegistered'+ temp+'" readonly="readonly" value="'+ jArray[0]["creditRegistered2"+ temp + ""]+'"  class="span1 credRegistCls forNum" /></td>';
											else
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsReg(this);" id="txtCreditRegistered'	+ temp+ '" readonly="readonly" value="0" class="span1 credRegistCls forNum" /></td>';

											if (jArray[0]["creditEarned2" + temp+ ""] != undefined)
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsEar(this);" id="txtCreditEarned'+ temp+ '" readonly="readonly" value="'+ jArray[0]["creditEarned2"+ temp + ""]+ '" class="span1 credEarCls forNum" /></td>';
											else
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsEar(this);" id="txtCreditEarned'	+ temp+ '" readonly="readonly" value="0" class="span1 credEarCls forNum" /></td>';
											
											if (jArray[0]["totMarksPerc2"+ temp + ""] !="" && jArray[0]["totMarksPerc2"+ temp + ""] != undefined && jArray[0]["totMarksPerc2"+ temp + ""]!="undefined" && jArray[0]["totMarksPerc2" + temp + ""] !="0")
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="calcultateCgpaSgpa(this);" id="txttotalMarks'+ temp+ '" readonly="readonly" value="'+ jArray[0]["totMarksPerc2"+ temp + ""]	+ '" class="span1 totPtsCls forNum" /></td>';
											else
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="calcultateCgpaSgpa(this);" id="txttotalMarks'+ temp+ '" readonly="readonly" value="0" class="span1 totPtsCls forNum" /></td>';
											
									if (jArray[0]["sgpa" + temp + ""] != undefined){
										if (jArray[0]["isExamStatus" + temp + ""])
											tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'	+ temp+ '" value="'+ jArray[0]["sgpa" + temp + ""]+ '" class="span1 sgpaPrCls forNum"  disabled="disabled"  /></td>';
										else
											tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'	+ temp+ '" value="'+ jArray[0]["sgpa" + temp + ""]+ '" class="span1 sgpaPrCls forNum"  /></td>';
									}else
										tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'+ temp + '" value="0" class="span1 sgpaPrCls" /></td>';

									if (jArray[0]["cgpa" + temp + ""] != undefined){
										if (jArray[0]["isExamStatus" + temp + ""])
											tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtcgpa'+ temp + '" value="'+ jArray[0]["cgpa" + temp+ ""]+ '" class="span1 cgpaPrCls forNum" disabled="disabled"  /></td>';
										else
