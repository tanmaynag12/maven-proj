}
function validPNRNO(value) {
	if (isNaN(value))
		return [ false, " Enter Correct PNR No !!", "" ];
	else
		return [ true, "", "" ];
}

function validMarks(totalMarks) {
	var select = $("#studUGTable").jqGrid('getGridParam', 'selrow');
	var obtainedMarks = $("#" + select + "_totalMark").val();

	if (totalMarks < obtainedMarks)
		return [ false, " Please fill Correct Marks !!", "" ];
	else
		return [ true, "", "" ];
}

function pickdatePublish(id) {
	jQuery("#" + id + "_publishDate2", "#publicationtable").datepicker({
		dateFormat : "M d,yy",
		// /showWeek: true,
		changeMonth : true,
		changeYear : true,
		yearRange : "-60:+5",
	});
}

function pickdatePublish2(id) {
	jQuery("#" + id + "_publishDateInBook", "#bookPublicationtable")
	.datepicker({
		dateFormat : "M yy",
		// /showWeek: true,
		changeMonth : true,
		changeYear : true,
		yearRange : "-60:+5",
	});
}

var currentdate = new Date();
var curYear = currentdate.getFullYear();
function pickdates3(id) {

	$("#" + id + "_Remark").elastic();
	$("#" + id + "_PRNNo").elastic();
	$("#" + id + "_grade").elastic();
	$("#" + id + "_specialization").elastic();

	options = {
			pattern : 'mmm yyyy', // Default is 'mm/yyyy' and separator char is
			// not mandatory mmm for using month name
			selectedYear : curYear,
			startYear : 1950,
			finalYear : curYear + 10,
			monthNames : [ 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug',
			               'Sept', 'Oct', 'Nov', 'Dec' ]
	};

	jQuery("#" + id + "_yearOfPassing", "#studUGTable").monthpicker(options);
}

function pickdates(id) {

	$("#" + id + "_specialization").elastic();
	// $("#"+id+"_yearOfPassing").elastic();
	$("#" + id + "_Remark").elastic();

	options = {
			pattern : 'mmm yyyy', // Default is 'mm/yyyy' and separator char is
			// not mandatory mmm for using month name
			selectedYear : curYear,
			startYear : 1950,
			finalYear : curYear + 10,
			monthNames : [ 'Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug',
			               'Sept', 'Oct', 'Nov', 'Dec' ]
	};

	jQuery("#" + id + "_yearOfPassing", "#studDiplomaTable").monthpicker(
			options);
}

function columnColorFormatter(cellValue, opts, rowObject) {

	if (rowObject.status == "accepted")
		return "<img src='img/event/green dot.png' style='width:15px'/>";
	else if (rowObject.status == "pending")
		return "&nbsp;<img src='img/event/yellow dot.png'/>";
	else if (rowObject.status == "rejected")
		return "&nbsp;<img src='img/event/red dot.png'/>";
	else
		return "";
}

function showOtherDegree2(streamName) {
	if (streamName != "addNew") {
		// alert("innn");
		$.ajax({
			url : 'getOtherDegree.json?streamName=' + streamName,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				degreeBoilerArray = new Array();
				for ( var i = 0; i < jArray.length; i++) {
					if (gridDegreeType == "UG") {
						if (jArray[i]["degreeType"] == "COURSE_TYPE_UG") {
							degreeBoilerArray
							.push(jArray[i]["otherPlacementQualificationId"]
							+ '-'
							+ jArray[i]["degreeName"]);
						}
					}
					if (gridDegreeType == "PG") {
						if (jArray[i]["degreeType"] == "COURSE_TYPE_PG") {
							degreeBoilerArray
							.push(jArray[i]["otherPlacementQualificationId"]
							+ '-'
							+ jArray[i]["degreeName"]);
						}
					}

				}
				populateSelectedRowDegree2();
			},
			error : function() {
				// alert("in Error stream CMB");
			}
		});
	} else {
		showAddOtherStreamFrm(streamName);
	}
}

function populateSelectedRowDegree2() {

	var degreeBoiler = '<option value="-1">Select</option>';
	for ( var i = 0; i < degreeBoilerArray.length; i++) {
		degreeBoiler += '<option value="' + degreeBoilerArray[i].split('-')[0]
		+ '">' + degreeBoilerArray[i].split('-')[1] + '</option>';
	}
	degreeBoiler += '<option value="addNew">Add New</option>';

	var rowId = getSelectedRowId('studUGTable');
	$('#' + rowId + '_otherDegreeName').html(degreeBoiler);
}

function getSelectedRowId(tableId) {
	var rowObj = $('#' + tableId).jqGrid('getGridParam', 'selrow');
	if (rowObj != null) {
		var row = $('#' + tableId).jqGrid('getRowData', rowObj);
		return row.id;
	} else {
		return null;
	}
}

var subGridIdForUG = '';
var superStudGraduationId = '';
var totalSemForUGPG = '';
var semList = '';

function getSem(totalSemForUGPG) {
	semList = '';
	$.ajax({
		url : 'getAllSemester.json?totalSemForUGPG=' + totalSemForUGPG,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var len = jArray.length;
			semList += -1 + ':Select;';
			for ( var i = 0; i < len; i++) {
				semList += jArray[i]["semesterId"] + ':'
				+ jArray[i]["semesterName"] + '';
				if (i == (len - 1)) {

				} else {
					semList += ';';
				}
			}
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
}

var gridDegreeType = "";
function studUGGrid(UGorPG) {
	gridDegreeType = UGorPG;
	studUGGrid2();
}

var subUGPGEditingStatus = '';
var statusForAddingUGPG = '';

//function for showing pop up for new degree in UG & PG tab
function addDegreePopUp() {
	$('#studUG').modal('hide');
	$('#addDegreePopUp').modal('show').css({
		width : '400px',
		'margin-left' : function() {
			return -($(this).width() / 2);
		}
	});
}

//function for saving new degree in UG & PG tab
function saveNewDegreeOnPopUp() {

	var duration = $("#txtDegreeDurationOnPopUp").val();
	var stream = $("#txtDegreeStreamOnPopUp").val();
	var degreeName = $("#txtAddDegree").val();

	if (duration == '' || stream == '' || degreeName == '') {
		jAlert("Please Fill The Details");
	} else {
		var degreeTypeForAddinNew = '';
		if (degreeTypeOnPopUp == 1) {
			degreeTypeForAddinNew = 'COURSE_TYPE_UG';
		} else {
			degreeTypeForAddinNew = 'COURSE_TYPE_PG';
		}

		$.ajax({
			url : 'addNewOtherQualificationDegree.json?degreeTypeForAddinNew='
				+ degreeTypeForAddinNew + '&degreeName=' + degreeName
				+ '&stream=' + stream + '&duration=' + duration,
				dataType : 'json',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					getCustomAlertMessage("alert_create");
					$('#addDegreePopUp').modal('hide');
					clearForm('addDegreePopUp');
					if (jArray[0]["type"] == "COURSE_TYPE_UG")
						loadstudugQualification(gCourseType.COURSE_TYPE_UG);
					else {
						loadstudugQualification(gCourseType.COURSE_TYPE_PG);
					}
					$('#studUG').modal('show');
				},
				error : function() {
				}
		});
	}
}

function showTechnicalDetailsNew222() {

	$
	.ajax({
		url : 'getstudentsTechnicalSkills.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var flag = 0;
			var boiler = '<div id="newTechMsg" style="display: none;">'
				+ '	<table>'
				+ '	<tr id="trReqPendingTech" style="display: none;">'
				+ '		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'
				+ '	</tr>'
				+ '	<tr id="trReqAcceptedTech" style="display: none;">'
				+ '		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'
				+ '	</tr>'
				+ '	<tr id="trSendReqTech" style="display: none;">'
				+ '		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'
				+ '	</tr>'
				+ '</table>'
				+ '</div>'
				+ '<table class="table table-bordered table-condensed">'
				+ '<tr><th style="color: #0088CC" colspan="5"><label><b><font>Student Technical Skill</font></b></label></th></tr>';
			boiler += '<tr><td colspan="4"><span style="float:right;"><a class="btn btn-inverse btn-mini btnColorBlack  freezeProfile hideSaveSubmitButton" onclick="checkReqConditionForAddEdit(\'\',\'\',\'\')" ><i class="fa fa-plus">Add Skill</a></span><span><input type="button" id="btnFreezeTechnical" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if (jArray[0]["1stTimeAddStatus"] == "inPlaceLast") {

				statusForAddingTechnical = jArray[0]["editingStatus"];
				studProChangeReqId = jArray[0]["studProReqId"];

				if (jArray[0]["studentSkillSetId"] != undefined) {

					boiler += '<tr>' + '<th><b>Skill Name</b></th>'
					+ '<th><b>Ratings (Out of 10)</b></th>' +

					'<th colspan="2"><b></b></th>' + '</tr>';
					for ( var i = 0; i < jArray.length; i++) {

						boiler += '<tr>'
							+ '<td><label>'
							+ jArray[i]["skillUniverseName"]
						+ '</label></td>'
						+ '<td style="text-align: center;"><label>'
						+ jArray[i]["proficiency"]
						+ '</label></td>'
						+

						'<td style="text-align: right;"><a onclick="checkReqConditionForAddEdit(\''
						+ jArray[i]["studentSkillSetId"]
						+ '\',\''
						+ jArray[i]["skillId"]
						+ '\',\''
						+ jArray[i]["proficiency"]
						+ '\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack"></i> Edit</a></td>'
						+ '<td style="text-align: right;"><a onclick="checkReqForDel('
						+ jArray[i]["studentSkillSetId"]
						+ ')" href="#"><i class="fa fa-trash iconColorBlack"></i> Delete</a></td>';

						// boiler += '<td style="text-align: right;"><a
						// onclick =
						// "uploadTechnicalSkillDocument(\''+jArray[i]["studentSkillSetId"]+'\')"
						// href="javascript:void(0)"><i
						// class="icon-upload"></i>Upload</td>';

						boiler += '</tr>';
					}
					boiler += '</table>';
					$('#technical').html(boiler);

				} else {
					// boiler+='<tr><td colspan="5"><span><input
					// type="button" style="float:right;"value="+ Add
					// Skill" class="btn btn-inverse btn-mini "
					// onclick="checkReqConditionForAddEdit(\'\',\'\',\'\')"
					// /></span></td></tr>';
					// boiler+='<tr>';
					boiler += '</table>';
					$('#technical').html(boiler);

				}
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1st") {
				statusForAddingTechnical = "ReqNotSent";
				boiler += '</table>';
				$('#technical').html(boiler);
				/*
				 * if() postGraduationDetailsPart2('ADD',degreeType);
				 */
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlaceCanChange") {
				statusForAddingTechnical = "canChange";
				boiler += '</table>';
				$('#technical').html(boiler);
				/*
				 * if() postGraduationDetailsPart2('ADD',degreeType);
				 */
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlace1stReqSent") {
				statusForAddingTechnical = jArray[0]["editingStatus"];
				boiler += '</table>';
				$('#technical').html(boiler);
				/*
				 * if() postGraduationDetailsPart2('ADD',degreeType);
				 */
			} else if (jArray[0]["1stTimeAddStatus"] == "inPlacecheckByEditingStatus") {
				statusForAddingTechnical = jArray[0]["editingStatus"];
				boiler += '</table>';
				$('#technical').html(boiler);
				/*
				 * if() postGraduationDetailsPart2('ADD',degreeType);
				 */
			} else if (jArray[0]["1stTimeAddStatus"] == "notInPlace") {
				statusForAddingTechnical = "canChange";
				boiler += '</table>';
				$('#technical').html(boiler);
			}

			if (statusForAddingTechnical == 'ReqNotSent') {
				$('#newTechMsg').show();
				$('#btnFreezeTechnical').hide();
				$('#trSendReqTech').show();
				$('#trReqAcceptedTech').hide();
				$('#trReqPendingTech').hide();
			} else if (statusForAddingTechnical == 'ReqAccepted') {
				$('#newTechMsg').show();
				$('#btnFreezeTechnical').show();
				$('#trReqAcceptedTech').show();
				$('#trSendReqTech').hide();
				$('#trReqPendingTech').hide();
			} else if (statusForAddingTechnical == 'ReqSent') {
				$('#newTechMsg').show();
				$('#trReqPendingTech').show();
				$('#trReqAcceptedTech').hide();
				$('#trSendReqTech').hide();
			} else if (statusForAddingTechnical == 'canChange') {
				$('#trReqPendingTech').hide();
				$('#trReqAcceptedTech').hide();
				$('#trSendReqTech').hide();
			}
			getstudProfileConfigurationDetailsCheckingNames();
		},
		error : function() {
			// alert("in Error stream CMB");
		}
	});

}

function checkReqConditionForAddEdit(studSkillId, skillId, ratings) {
	// addNewSkillNew
	if (statusForAddingTechnical == "canChange") {
		addNewSkillNew(studSkillId, skillId, ratings);
	} else if (statusForAddingTechnical == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingTechnical == "ReqAccepted") {
		$('#btnFreezeTechnical').show();
		addNewSkillNew(studSkillId, skillId, ratings);
	} else if (statusForAddingTechnical == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "technical";

						sendRequestForEditing(tab);
					}
				});
	}
	getstudProfileConfigurationDetailsCheckingNames();
}

function checkReqForDel(studSkillId) {
	// addNewSkillNew

	if (statusForAddingTechnical == "canChange") {
		deleteStudSkillNew(studSkillId);
	} else if (statusForAddingTechnical == "ReqSent") {
		jAlert("The request you have sent for editing is pending");
	} else if (statusForAddingTechnical == "ReqAccepted") {
		$('#btnFreezeTechnical').show();
		deleteStudSkillNew(studSkillId);
	} else if (statusForAddingTechnical == "ReqNotSent") {
		jConfirm(
				"You have to send request for changing. Do you want to send? ",
				"Confirm", function(r) {
					if (r == true) {
						var tab = "technical";

						sendRequestForEditing(tab);
					}
				});
	}
}

function deleteStudSkillNew(studSkillId) {

	jConfirm("Do you want to delete? ", "Confirm", function(r) {
		if (r == true) {
			$.ajax({
				url : 'deleteNewStudentSkillsNew.json?studSkillId='
					+ studSkillId,
					dataType : 'text',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(jArray) {
						getCustomAlertMessage("alert_delete");
						showTechnicalDetailsNew222();
					},
					error : function() {
						// //alert("in Error stream CMB");
					}
			});
		}
	});
}

function saveNewSkillNew(studSkillId) {
	var skillUnivId = $('#cmbSkillNew').val();
	var SkillRate = $('#txtRatingNew').val();
	var cnt = 0;
	if (skillUnivId == 0) {
		jAlert("Please select skill");
		cnt = 1;
	}
	if (SkillRate == '' || SkillRate > 10) {
		jAlert("Please enter ratings below 10");
		cnt = 1;
	}

	if (cnt == 0) {
		$
		.ajax({
			url : 'addNewStudentSkills.json?skillUnivId=' + skillUnivId
			+ '&SkillRate=' + SkillRate + '&studSkillId='
			+ studSkillId,
			dataType : 'text',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				getCustomAlertMessage("alert_create");
				showTechnicalDetailsNew222();
			},
			error : function() {
				// //alert("in Error stream CMB");
			}
		});
	}
}

function addNewSkillNew(studSkillId, skillId, ratings) {

	var addSkillBoiler = '';

	addSkillBoiler += '<table class="table table-bordered table-condensed">'
		+ '<tr>'
		+ '<th colspan="3" style ="text-align:center;"><h4>Please Enter your skills</h4></th>'
		+ '</tr>'
		+ '<tr>'
		+ '<th style ="text-align:center;">Skill</th>'
		+ '<td><select id="cmbSkillNew" name="technicalSkillName"></select></td>'
		+ '<td><a class="freezeProfile hideSaveSubmitButton" onclick="addNewSkillPopUp()" href="#"><i class="fa fa-plus iconColorBlack "></i> Add New</td>'
		+ '</tr>'
		+ '<tr>'
		+ '<th style ="text-align:center;">Ratings</th>'
		+ '<td><input type="text" id="txtRatingNew" class="validnum" name="tachnicalRatingName"/><b>&nbsp;&nbsp;(Out of 10) 1-being lowest and 10 is the highest.</b></td>'
		+ '<td></td>'
		+ '</tr>'
		+ '<tr>'
		+ '<td></td>'
		+ '<td><input type="button" value="Save" class="btn btn-inverse btn-mini btnColorGreen " id="btnAddNewSkill" onclick="saveNewSkillNew('
		+ studSkillId
		+ ')"/>'
		+ '&nbsp;&nbsp;&nbsp;<input type="button" value="Cancel" class="btn btn-inverse btn-mini btnColorRed " onclick="showTechnicalDetailsNew222()" /></td>'
		+ '<td></td>' + '</tr>' + '</table>';

	$('#technical').html(addSkillBoiler);
	$('.validnum').numeric();

	$.ajax({
		url : 'getStudentSkillCategorylist.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var html = '<option value="0">Select</option>';
			var len = jArray.length;
			for ( var i = 0; i < len; i++)
				html += '<option value="' + jArray[i]["skillUniverseId"] + '">'
				+ jArray[i]["skillName"] + '</option>';

			$('#cmbSkillNew').html(html);

		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});

	if (skillId != "") {
		$('#txtRatingNew').val(ratings);
		$('#cmbSkillNew').val(skillId);

	}
}

function addNewSkillPopUp() {
	$('#addNewSkills').modal('show').css({
		width : '600px',
		'margin-left' : function() {
			return -($(this).width() / 2);
		}
	});
}

function saveNewSkill() {

	var skillName = $('#txtSkillName').val();
	var skillDesc = $('#txtSkillDesc').val();

	$.ajax({
		url : 'addNewUniverseSkillsInSkillUnivrNew.json?skillName=' + skillName
		+ '&skillDesc=' + skillDesc,
		dataType : 'text',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(skillId) {
			getCustomAlertMessage("alert_create");
			setNewlyAddedSkill(skillId);
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
}

function setNewlyAddedSkill(skillUniId) {

	var skillName2 = $('#txtSkillName').val();
	if (skillName2 == '') {
		jAlert("Please enter skill name");
	} else {
		$.ajax({
			url : 'getStudentSkillCategorylist.json',
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {
				var html = '<option value="0">Select</option>';
				var len = jArray.length;
				for ( var i = 0; i < len; i++)
					html += '<option value="' + jArray[i]["skillUniverseId"]
				+ '">' + jArray[i]["skillName"] + '</option>';

				$('#cmbSkillNew').html(html);

				$('#cmbSkillNew').val(skillUniId);
				$('#addNewSkills').modal('hide');

			},
			error : function() {
				// //alert("in Error stream CMB");
			}
		});
	}
}

var statusForAddingTechnical = '';

/**
 * @author vimal kr.
 * @param skillId
 */
function uploadTechnicalSkillDocument(skillId) {
	if (skillId != '' && skillId != 'null' && skillId != undefined) {
		if (statusForAddingTechnical == 'canChange'
			|| statusForAddingTechnical == 'ReqAccepted') {
			getDegreeCategoryId(gTabDegreeName.TabName_TECHNICALKNOWLEDGE,
					skillId);
		} else if (statusForAddingTechnical == 'ReqSent') {
			jAlert("The request you have sent for editing is pending");
		} else if (statusForAddingTechnical == 'ReqNotSent') {
			$('#btnFreezeTechnical').show();
			jConfirm(
					"You have to send request for changing. Do you want to send? ",
					"Confirm", function(r) {
						if (r == true) {
							var tab = "technical";
							sendRequestForEditing(tab);
						}
					});
		}
	} else
		jAlert('Student technical skill record not found!');

}

//--------------------------------------------------------------------Document
//Attached
function addStudentDocuments() {
	// alert("in docs");
	var MarksheetDoc = $('#MarksheetDoc').attr('checked');
	var lctcDoc = $('#lctcDoc').attr('checked');
	var eligibilityDoc = $('#eligibilityDoc').attr('checked');
	var bonafideDoc = $('#bonafideDoc').attr('checked');
	var passingDoc = $('#passingDoc').attr('checked');
	var scholershipDoc = $('#scholershipDoc').attr('checked');
	var migrationDoc = $('#migrationDoc').attr('checked');
	var nationalityDoc = $('#nationalityDoc').attr('checked');
	var noObjDoc = $('#noObjDoc').attr('checked');
	var gapDoc = $('#gapDoc').attr('checked');
	var affidavitDoc = $('#affidavitDoc').attr('checked');
	var performaDoc = $('#performaDoc').attr('checked');
	var tributevalidDoc = $('#tributevalidDoc').attr('checked');
	var IncomeDoc = $('#IncomeDoc').attr('checked');
	var noncreamylayerDoc = $('#noncreamylayerDoc').attr('checked');
	var birthDoc = $('#birthDoc').attr('checked');
	var allotDoc = $('#allotDoc').attr('checked');
	var cetDoc = $('#cetDoc').attr('checked');
	var aieeeDoc = $('#aieeeDoc').attr('checked');
	var medicalDoc = $('#medicalDoc').attr('checked');
	var sportsDoc = $('#sportsDoc').attr('checked');
	var casteDoc = $('#casteDoc').attr('checked');

	$.ajax({
		url : 'studentDocsAttachedInfo.htm',
		data : {
			MarksheetDoc : MarksheetDoc,
			lctcDoc : lctcDoc,
			eligibilityDoc : eligibilityDoc,
			bonafideDoc : bonafideDoc,
			passingDoc : passingDoc,
			scholershipDoc : scholershipDoc,
			migrationDoc : migrationDoc,
			nationalityDoc : nationalityDoc,
			noObjDoc : noObjDoc,
			gapDoc : gapDoc,
			affidavitDoc : affidavitDoc,
			performaDoc : performaDoc,
			tributevalidDoc : tributevalidDoc,
			IncomeDoc : IncomeDoc,
			noncreamylayerDoc : noncreamylayerDoc,
			birthDoc : birthDoc,
			allotDoc : allotDoc,
			cetDoc : cetDoc,
			aieeeDoc : aieeeDoc,
			medicalDoc : medicalDoc,
			sportsDoc : sportsDoc,
			casteDoc : casteDoc
		},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if (jArray[0].responseFlag == '1')

				showSuccessMessage(jArray[0].responseText, 5000);
			getCustomAlertMessage("alert_create");
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

function loadStudentDocumentsPage(gStudentId) {
	$.ajax({
		url : 'getStudDocumentsInfo.json?id=' + gStudentId,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if ((jArray["marksheet"]) != "checked")
				$('#MarksheetDoc').attr('checked', false);
			if ((jArray["lcOrTC"]) != "checked")
				$('#lctcDoc').attr('checked', false);
			if ((jArray["eligibilityForm"]) != "checked")
				$('#eligibilityForm').attr('checked', false);
			if ((jArray["bonafide"]) != "checked")
				$('#bonafideDoc').attr('checked', false);
			if ((jArray["passingCertificate"]) != "checked")
				$('#passingDoc').attr('checked', false);
			if ((jArray["scholarship"]) != "checked")
				$('#scholershipDoc').attr('checked', false);
			if ((jArray["migrationCertificate"]) != "checked")
				$('#migrationDoc').attr('checked', false);
			if ((jArray["nationality"]) != "checked")
				$('#nationalityDoc').attr('checked', false);
			if ((jArray["noObjectionCertificate"]) != "checked")
				$('#noObjDoc').attr('checked', false);
			if ((jArray["gapCertificate"]) != "checked")
				$('#gapDoc').attr('checked', false);
			if ((jArray["affidavit"]) != "checked")
				$('#affidavitDoc').attr('checked', false);
			if ((jArray["certificateInDiplomaAtoE"]) != "checked")
				$('#performaDoc').attr('checked', false);
			if ((jArray["casteOrTribeValidityCertificate"]) != "checked")
				$('#tributevalidDoc').attr('checked', false);
			if ((jArray["incomeCertificate"]) != "checked")
				$('#IncomeDoc').attr('checked', false);
			if ((jArray["nonCreamyLayerCertificate"]) != "checked")
				$('#noncreamylayerDoc').attr('checked', false);
			if ((jArray["birthCertificate"]) != "checked")
				$('#birthDoc').attr('checked', false);
			if ((jArray["allotmentLetter"]) != "checked")
				$('#allotDoc').attr('checked', false);
			if ((jArray["mhtCetScoreCard"]) != "checked")
				$('#cetDoc').attr('checked', false);
			if ((jArray["aieeeScoreCard"]) != "checked")
				$('#aieeeDoc').attr('checked', false);
			if ((jArray["medicalFitnessCertificate"]) != "checked")
				$('#medicalDoc').attr('checked', false);
			if ((jArray["sportCertificate"]) != "checked")
				$('#sportsDoc').attr('checked', false);
			if ((jArray["casteCertificate"]) != "checked")
				$('#casteDoc').attr('checked', false);
		},
		error : function() {
			alert("In documents error");
		},

	});
}
//-------------------------------------------------------------send Req For
//Editing stud Profile
function sendReqForEditing() {
	// alert("hi");
}
//----------------------------------------------------------------Print Profile

function getStudentProfilePrintCheck() {
	var cnt = 0;
	var isChecked = new Array();
	$('.top5:checked').each(function() {
		isChecked[cnt] = (this.checked ? $(this).attr("id") : "");
		cnt = cnt + 1;
	});
	window.location.replace("studentPrintResume.json?ID=" + isChecked);

}

function downloadUndertakingForm() {

	window.open("http://www.aicte-india.org/downloads/Undertaking.pdf",
	'_blank');

}

function genratePDFForReRegistration() {
	var isPhysicallyHandicapped = $('#saveBtn').attr('isPhysicallyHandicapped');
	var isAadharCard = $('#saveBtn').attr('isAadharCard');
	
	var boiler =''+
	'<table class="table table-bordered table-condensed" style="">'+
	'<tr>'+
	'<td><label>Do you have Aadhaar card?</label></td>'+
	'<td><input type="radio" name="isAadhaar1" value="Yes"/>Yes'+
	'<input type="radio" name="isAadhaar1" value="No" />No</td>'+
	'</tr>'+	
	'<tr>'+
	'<td><label>Are you Physically Challenged(Handicapped)?</label></td>'+
	'<td><input type="radio" name="isHandicapped" value="Yes"/> Yes '+
	'<input type="radio" name="isHandicapped" value="No"/>No </td>'+
	'</tr></table>';

	$('#printStudentProfileModalDiv').html(boiler);
	$("input[name=isHandicapped][value=" + isPhysicallyHandicapped + "]").attr('checked', 'checked');
	$("input[name=isAadhaar1][value=" + isAadharCard + "]").attr('checked', 'checked');

	$('#printStudentProfileModal').modal('show');
}
function saveStudentChoices(){
	var isHandicapped = $('input[name = "isHandicapped"]:checked').val();
	var isAadhaar = $('input[name = "isAadhaar1"]:checked').val();
		$.ajax({
			url : 'setStudentChoices.json',
			data : {
				isHandicapped : isHandicapped,
				isAadhaar:isAadhaar
			},
			dataType : 'text',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(text) {
				$('#printStudentProfileModal').modal('hide');
				if(text=="Yes"){
					window.location.replace("genratePDFForReRegistration.json?flag=studentProfile&courseYear=1");
				}else{
					alert(text);
				}
				
			},
			error : function() {
			}
		});
}
function getFresherStudentProfilePrintCheck() {
	var cnt = 0;
	var isChecked = new Array();
	$('.top5:checked').each(function() {
		isChecked[cnt] = (this.checked ? $(this).attr("id") : "");
		cnt = cnt + 1;
	});
	var studId = $('#hiddenStudId').val();
	var academicYear = $('#cbAcademicBatch').find('option:selected').text();
	window.location.replace("fresherStudentPrintResume.json?ID=" + isChecked
			+ "&studId=" + studId + "&academicYear=" + academicYear);
}
