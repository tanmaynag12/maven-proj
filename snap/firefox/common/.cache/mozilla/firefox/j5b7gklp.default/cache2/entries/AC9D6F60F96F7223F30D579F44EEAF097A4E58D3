									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" class="validnum" style="text-align: right" id="txtPercentDeadATKT" disabled="disabled" readonly="readonly" value="'
								+ currQualiDeadATKT
								+ '"  class="input-small" onblur="validateObtAndTotMarks(\'txtPercentDeadATKT\')"></label></td>'
								+ '</tr>'
							+ '</table>';
						} else {
							// placement pointer

							tempSemBoiler += '<tr>'
								+ '<th>System of marks</td>'
								+ '<td><label><input type="radio" name="type" id="radPercnt" value="Percent" disabled="disabled" onclick=""/> Percent System</label></td>'
								+ '<td><label><input type="radio" name="type" id="radGrade" value="Grade" checked="checked" disabled="disabled" onclick=""/> Pointer System</label></td>'
								+ '</tr>'
								+ '</table>'
								+ '<table class="table table-bordered table-condensed" style="width: 400px;">'
								+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
								+ '<tr style="margin-left:10%;" id="marksTypeTr">'
								+ '<td colspan="3">'
								+ '<table class="table table-bordered table-condensed">'
								+ '<tr>';
							
							///-------------------------------- For Sinhgad ----------------------------------------------///
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SINHGAD")
								{
								tempSemBoiler += 
									
								'<td colspan="7" style="height:20px"><font color="red"><b>Click on check box to enter marks.</b></font></td></tr>'+	
								'<tr><th style="width:5%"></th>'+
								'<th style="width:10%">Class/Semester</th>'+ 
								'<th style="width:10%">Credit Registered</th>'+
								'<th style="width:10%">Earned Credit</th>'+
								'<th style="width:10%">Total Marks/Points</th>'+
								'<th style="width:10%">SGPA</th>' + 
								'<th style="width:10%">CGPA</th>'
								+ '</tr>';
								
								var temp = 0;
								//currentSem
								for (temp = 1; temp <=jArray[0]["currTotalNoOfSem"] ; temp++) {

									tempSemBoiler += '<tr class="tempTrCls_'+temp+'" sem="'+temp+'">'; 
									if(jArray[0]["studUniExMarId"+ temp + ""] =="" || jArray[0]["studUniExMarId"+ temp + ""] == undefined || jArray[0]["studUniExMarId"+ temp + ""]=="undefined" || jArray[0]["studUniExMarId"+ temp + ""] =="0")
										tempSemBoiler += '<td style="text-align: center;"><input type="checkbox" value="0" disabled="disabled" class="semesterChkBoxCls" onclick="onclicksemesterChkBox(this)"> </td>';
									else
										{tempSemBoiler += '<td style="text-align: center;"><input type="checkbox" value="'+jArray[0]["studUniExMarId"+ temp + ""]+'" class="semesterChkBoxCls" checked="checked" disabled="disabled" onclick="onclicksemesterChkBox(this)">'+ 
										
										'</td>';
										}
									tempSemBoiler +='<td style="text-align: right;">'	+ temp + '</td>';

											if (jArray[0]["creditRegistered2"+ temp + ""] != undefined)
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsReg(this);" id="txtCreditRegistered'+ temp+'" disabled="disabled" readonly="readonly" value="'+ jArray[0]["creditRegistered2"+ temp + ""]+'"  class="span1 credRegistCls forNum" /></td>';
											else
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsReg(this);" id="txtCreditRegistered'	+ temp+ '" disabled="disabled" readonly="readonly" value="0" class="span1 credRegistCls forNum" /></td>';

											if (jArray[0]["creditEarned2" + temp+ ""] != undefined)
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsEar(this);" id="txtCreditEarned'+ temp+ '" disabled="disabled" readonly="readonly" value="'+ jArray[0]["creditEarned2"+ temp + ""]+ '" class="span1 credEarCls forNum" /></td>';
											else
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="checkCreditLimitsEar(this);" id="txtCreditEarned'	+ temp+ '" disabled="disabled" readonly="readonly" value="0" class="span1 credEarCls forNum" /></td>';
											
											if (jArray[0]["totMarksPerc2"+ temp + ""] !="" && jArray[0]["totMarksPerc2"+ temp + ""] != undefined && jArray[0]["totMarksPerc2"+ temp + ""]!="undefined" && jArray[0]["totMarksPerc2" + temp + ""] !="0")
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="calcultateCgpaSgpa(this);" id="txttotalMarks'+ temp+ '" disabled="disabled" readonly="readonly" value="'+ jArray[0]["totMarksPerc2"+ temp + ""]	+ '" class="span1 totPtsCls forNum" /></td>';
											else
												tempSemBoiler += '<td><input type="text" style="text-align: right" onkeyup="calcultateCgpaSgpa(this);" id="txttotalMarks'+ temp+ '" disabled="disabled" readonly="readonly" value="0" class="span1 totPtsCls forNum" /></td>';
											
									if (jArray[0]["sgpa" + temp + ""] != undefined){
										if (jArray[0]["isExamStatus" + temp + ""])
											tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtsgpa'	+ temp+ '" value="'+ jArray[0]["sgpa" + temp + ""]+ '" class="span1 sgpaPrCls forNum"  disabled="disabled"  /></td>';
										else
											tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtsgpa'	+ temp+ '" value="'+ jArray[0]["sgpa" + temp + ""]+ '" class="span1 sgpaPrCls forNum"  /></td>';
									}else
										tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtsgpa'+ temp + '" value="0" class="span1 sgpaPrCls" /></td>';

									if (jArray[0]["cgpa" + temp + ""] != undefined){
										if (jArray[0]["isExamStatus" + temp + ""])
											tempSemBoiler += '<td><input type="text" style="text-align: right" readonly="readonly" id="txtcgpa'+ temp + '" value="'+ jArray[0]["cgpa" + temp+ ""]+ '" class="span1 cgpaPrCls forNum" disabled="disabled"  /></td>';
										else
											tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtcgpa'+ temp + '" value="'+ jArray[0]["cgpa" + temp+ ""]+ '" class="span1 cgpaPrCls forNum"  /></td>';
									}else
										tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtcgpa'	+ temp+ '" value="0" class="span1 cgpaPrCls forNum"  /></td>';

									tempSemBoiler += '</tr>';
								}
								for ( var i = temp; i <=jArray[0]["currSem"] ; i++) {

									tempSemBoiler += '<tr>'+
									'<td style="text-align: right;">'+ i+ '</td>'+
									'<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtsgpa'+ i + '" value="" class="span1 forNum" /></td>'+
									'<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtcgpa'	+ i	+ '" value="" class="span1 forNum"  /></td>'+
									'</tr>';
								}

								var currQualiLiveATKT = jArray[0]["currQualiLiveATKT"] == undefined ? "0"
										: jArray[0]["currQualiLiveATKT"];
								var currQualiDeadATKT = jArray[0]["currQualiDeadATKT"] == undefined ? "0"
										: jArray[0]["currQualiDeadATKT"];

								tempSemBoiler += '</table>'
									+ '</td>'
									+ '</tr>'
									+ '</table>'
									+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
									+ '<tr id="atktTypeTr">'
									+ '<td>'
									+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
									+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Live Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Live ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeLiveATKT" disabled="disabled" readonly="readonly" value="'
									+ currQualiLiveATKT
									+ '"  class="input-small forNum"></label></td>'
									+ '</tr>'
									+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeDeadATKT" disabled="disabled" readonly="readonly" value="'
									+ currQualiDeadATKT
									+ '"  class="input-small forNum"></label></td>'
								+ '</table>';
								
								
								
								}
							
							///-------------------------------- For Sinhgad End----------------------------------------------///
							
							
							else if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="SINHGAD"){
							
							tempSemBoiler += '<th>Class</th>'
								+ '<th>SGPA</th>' + '<th>CGPA</th>'
								+ '</tr>';

							var temp = 0;
							for (temp = 1; temp <= currentSem; temp++) {


								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ temp + '</td>';


								if (jArray[0]["sgpa" + temp + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtsgpa'
										+ temp
										+ '" value="'
										+ jArray[0]["sgpa" + temp
										            + ""]
								+ '" class="span1"  /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtsgpa'
										+ temp
										+ '" value="" class="span1" /></td>';

								if (jArray[0]["cgpa" + temp + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtcgpa'
										+ temp
										+ '" value="'
										+ jArray[0]["cgpa" + temp
										            + ""]
								+ '" class="span1"  /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtcgpa'
										+ temp
										+ '" value="" class="span1"  /></td>';

								tempSemBoiler += '</tr>';
							}

							for ( var i = temp; i <= jArray[0]["currTotSem"]; i++) {

								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ i
									+ '</td>'
									+ '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" id="txtsgpa'
									+ i
									+ '" value="" class="span1" /></td>'
									+ '<td><input type="text" style="text-align: right" readonly="readonly" id="txtcgpa'
									+ i
									+ '" value="" class="span1" disabled=disabled /></td>'
									+ '</tr>';
							}
							var currQualiLiveATKT = jArray[0]["currQualiLiveATKT"] == undefined ? "0"
									: jArray[0]["currQualiLiveATKT"];
							var currQualiDeadATKT = jArray[0]["currQualiDeadATKT"] == undefined ? "0"
									: jArray[0]["currQualiDeadATKT"];

							tempSemBoiler += '</table>'
								+ '</td>'
								+ '</tr>'
								+ '</table>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr id="atktTypeTr">'
								+ '<td>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Live Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Live ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeLiveATKT" disabled="disabled" readonly="readonly" value="'
								+ currQualiLiveATKT
								+ '"  class="input-small"></label></td>'
								+ '</tr>'
								+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeDeadATKT" disabled="disabled" readonly="readonly" value="'
								+ currQualiDeadATKT
								+ '"  class="input-small"></label></td>'
								+ '</tr>'
							+ '</table>';
							
							}
						}
					}

					$('#tdSem').html(tempSemBoiler);
					$('.validnum').numeric();
					

					if (jArray[0]["semYear"] == "Year") {
						$('#radYear').attr('checked', 'checked');
						$('#radSem').attr('disabled', 'disabled');
					} else if (jArray[0]["semYear"] == "Semester") {
						$('#radSem').attr('checked', 'checked');
						$('#radYear').attr('disabled', 'disabled');
					}

					showSemCmb();
					//
					var semCombo = '<option value="0">Select</option>';
					var k = 0;
					for (k = 1; k <= jArray[0]["currTotSem"]; k++) {
						semCombo += '<option value="' + k + '">' + k
						+ '</option>';
					}
					$('#cmbCurrentSem').html(semCombo);

					$('#cmbCurrentSem').val(jArray[0]["currSem"]);
					//
					$('.forNum').numeric();
				}

				getStudCPCSpecialization();
				
			}
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
}

function calcultateCgpaSgpa(ele)
{
	var totPts = $(ele).val() != "" ? $(ele).val() : 0;
	var regCredit = $(ele).closest('tr').find('.credRegistCls').val() !="" ? $(ele).closest('tr').find('.credRegistCls').val() : 0;
	var tempTrNo = eval($(ele).closest('tr').attr('sem'));
	if(eval(tempTrNo) == 1)
			{
		var cgpa = 0;
		var sgpa = 0;
				if(totPts != "0" && regCredit != "0")
				{
					cgpa = eval(totPts)/eval(regCredit);
					sgpa = eval(totPts)/eval(regCredit);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
					
				}
				else 
				{
					cgpa = 0;
					sgpa = 0;
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
					
				}
				
			}
	else if(eval(tempTrNo) > 1)
		{
		var sgpa = 0;
			if(totPts != "0" && regCredit != "0")
			{
				sgpa = eval(totPts)/eval(regCredit);
				$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
			}
			else 
			{
				sgpa = 0;
				$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
			}
			
			
			
			var totRegCredits = 0;
			var totPtsTotal = 0;
			for (var temp = 1; temp <=tempTrNo ; temp++) {
				totRegCredits+= eval($('.tempTrCls_'+temp).find('.credRegistCls').val() !="" ? $('.tempTrCls_'+temp).closest('tr').find('.credRegistCls').val() : 0);
				totPtsTotal+= eval($('.tempTrCls_'+temp).find('.totPtsCls').val() !="" ? $('.tempTrCls_'+temp).find('.totPtsCls').val() : 0);
		}
			
			var cgpa = 0;
			if(totPtsTotal != "0" && totRegCredits != "0")
			{
				cgpa = eval(totPtsTotal)/eval(totRegCredits);
				$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
			}
			else 
			{
				cgpa = 0;
				$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
			}

			
			
			/*for (var temp = 1; temp <=tempTrNo ; temp++) {
				$('.tempTrCls_'+temp).find('.cgpaPrCls').val(cgpa);
			}*/
		}
}

function checkCreditLimitsEar(ele)
{
	var earCredit = $(ele).closest('tr').find('.credEarCls').val() !="" ? $(ele).closest('tr').find('.credEarCls').val() : 0;
	var regCredit = $(ele).closest('tr').find('.credRegistCls').val() !="" ? $(ele).closest('tr').find('.credRegistCls').val() : 0;
	
	if(eval(earCredit) > eval(regCredit))
		{
			getCustomAlertMessage("Earned Credits should not be greater than Registered Credits..!");
			$(ele).closest('tr').find('.credEarCls').val(0);
			/*$(ele).closest('tr').find('.credRegistCls').val(0);
			$(ele).closest('tr').find('.cgpaPrCls').val(0);
			$(ele).closest('tr').find('.sgpaPrCls').val(0);
			$(ele).closest('tr').find('.totPtsCls').val(0);*/
		}
}
function editMarkDetails(ele){
	
	$(ele).closest('tr').find('.credEarCls').attr('readonly',false);
	var earCredits = $(ele).closest('tr').find('.credEarCls').val();
	if(earCredits=="undefined" || isNaN(earCredits) || earCredits==undefined)
		$(ele).closest('tr').find('.credEarCls').val(0);
	else if(earCredits=="0")
		$(ele).closest('tr').find('.credEarCls').val(0);
	else
		$(ele).closest('tr').find('.credEarCls').val(earCredits);
	
	$(ele).closest('tr').find('.credRegistCls').attr('readonly',false);
		var credRegist = $(ele).closest('tr').find('.credRegistCls').val();
	if(credRegist=="undefined" || isNaN(credRegist) || credRegist==undefined)
		$(ele).closest('tr').find('.credRegistCls').val(0);
	else if(credRegist=="0")
		$(ele).closest('tr').find('.credRegistCls').val(0);
	else
		$(ele).closest('tr').find('.credRegistCls').val(credRegist);
	
	$(ele).closest('tr').find('.totPtsCls').attr('readonly',false);
	var totPts = $(ele).closest('tr').find('.totPtsCls').val();
	if(totPts=="undefined" || isNaN(totPts) || totPts==undefined)
		$(ele).closest('tr').find('.totPtsCls').val(0);
	else if(totPts=="0")
		$(ele).closest('tr').find('.totPtsCls').val(0);
	else
		$(ele).closest('tr').find('.totPtsCls').val(totPts);
	
	var sgpa = $(ele).closest('tr').find('.sgpaPrCls').val();
	if(sgpa=="undefined" || isNaN(sgpa) || sgpa==undefined)
		$(ele).closest('tr').find('.sgpaPrCls').val(0);
	else if(totPts=="0")
		$(ele).closest('tr').find('.sgpaPrCls').val(0);
	else
		$(ele).closest('tr').find('.sgpaPrCls').val(sgpa);
	
	var cgpa = $(ele).closest('tr').find('.cgpaPrCls').val();
	if(cgpa=="undefined" || isNaN(cgpa) || cgpa==undefined)
		$(ele).closest('tr').find('.cgpaPrCls').val(0);
	else if(cgpa=="0")
		$(ele).closest('tr').find('.cgpaPrCls').val(0);
	else
		$(ele).closest('tr').find('.cgpaPrCls').val(cgpa);
	
}
function onclicksemesterChkBox(ele)
{
	
	if($(ele).is(':checked'))
		{
			$(ele).closest('tr').find('.credEarCls').attr('readonly',false);
			var earCredits = $(ele).closest('tr').find('.credEarCls').val();
			if(earCredits=="undefined" || isNaN(earCredits) || earCredits==undefined)
				$(ele).closest('tr').find('.credEarCls').val('');
			else
				$(ele).closest('tr').find('.credEarCls').val(earCredits);
			
			$(ele).closest('tr').find('.credRegistCls').attr('readonly',false);
			var credRegistCls = $(ele).closest('tr').find('.credRegistCls').val();
			if(earCredits=="undefined" || isNaN(earCredits) || earCredits==undefined)
				$(ele).closest('tr').find('.credRegistCls').val('');
			else
				$(ele).closest('tr').find('.credRegistCls').val(earCredits);
			
			$(ele).closest('tr').find('.totPtsCls').attr('readonly',false);
			var totPts = $(ele).closest('tr').find('.totPtsCls').val();
			if(totPts=="undefined" || isNaN(totPts) || totPts==undefined)
				$(ele).closest('tr').find('.totPtsCls').val('');
			else
				$(ele).closest('tr').find('.totPtsCls').val(totPts);
			
			var sgpa = $(ele).closest('tr').find('.sgpaPrCls').val();
			if(sgpa=="undefined" || isNaN(sgpa) || sgpa==undefined)
				$(ele).closest('tr').find('.sgpaPrCls').val('');
			else
				$(ele).closest('tr').find('.sgpaPrCls').val(sgpa);
			
			var cgpa = $(ele).closest('tr').find('.cgpaPrCls').val();
			if(sgpa=="undefined" || isNaN(sgpa) || sgpa==undefined)
				$(ele).closest('tr').find('.cgpaPrCls').val('');
			else
				$(ele).closest('tr').find('.cgpaPrCls').val(cgpa);
		}
	else
		{
			/*$(ele).closest('tr').find('.credEarCls').attr('readonly',true);
			$(ele).closest('tr').find('.credEarCls').val('');
			$(ele).closest('tr').find('.credRegistCls').attr('readonly',true);
			$(ele).closest('tr').find('.credRegistCls').val('');
			$(ele).closest('tr').find('.totPtsCls').attr('readonly',true);
			$(ele).closest('tr').find('.totPtsCls').val('');
			
			$(ele).closest('tr').find('.sgpaPrCls').val('');
			$(ele).closest('tr').find('.cgpaPrCls').val('');*/
		
		$(ele).closest('tr').find('.credEarCls').attr('readonly',true);
		var earCredits = $(ele).closest('tr').find('.credEarCls').val();
		if(earCredits=="undefined" || isNaN(earCredits) || earCredits==undefined)
			$(ele).closest('tr').find('.credEarCls').val('');
		else
			$(ele).closest('tr').find('.credEarCls').val(earCredits);
		
		
		$(ele).closest('tr').find('.credRegistCls').attr('readonly',true);
		var credRegistCls = $(ele).closest('tr').find('.credRegistCls').val();
		if(earCredits=="undefined" || isNaN(earCredits) || earCredits==undefined)
			$(ele).closest('tr').find('.credRegistCls').val('');
		else
			$(ele).closest('tr').find('.credRegistCls').val(earCredits);
		
		
		$(ele).closest('tr').find('.totPtsCls').attr('readonly',true);
		var totPts = $(ele).closest('tr').find('.totPtsCls').val();
		if(totPts=="undefined" || isNaN(totPts) || totPts==undefined)
			$(ele).closest('tr').find('.totPtsCls').val('');
		else
			$(ele).closest('tr').find('.totPtsCls').val(totPts);
		
		var sgpa = $(ele).closest('tr').find('.sgpaPrCls').val();
		if(sgpa=="undefined" || isNaN(sgpa) || sgpa==undefined)
			$(ele).closest('tr').find('.sgpaPrCls').val('');
		else
			$(ele).closest('tr').find('.sgpaPrCls').val(sgpa);
		
		var cgpa = $(ele).closest('tr').find('.cgpaPrCls').val();
		if(sgpa=="undefined" || isNaN(sgpa) || sgpa==undefined)
			$(ele).closest('tr').find('.cgpaPrCls').val('');
		else
			$(ele).closest('tr').find('.cgpaPrCls').val(cgpa);
		}
	
		
}

function checkCreditLimitsReg(ele)
{
	
	var earCredit = $(ele).closest('tr').find('.credEarCls').val() !="" ? $(ele).closest('tr').find('.credEarCls').val() : 0;
	var regCredit = $(ele).closest('tr').find('.credRegistCls').val() !="" ? $(ele).closest('tr').find('.credRegistCls').val() : 0;
	
	if(eval(earCredit) > eval(regCredit))
	{
		getCustomAlertMessage("Earned Credits should not be greater than Registered Credits..!");
		$(ele).closest('tr').find('.credEarCls').val(0);
		/*$(ele).closest('tr').find('.credRegistCls').val(0);
		$(ele).closest('tr').find('.cgpaPrCls').val(0);
		$(ele).closest('tr').find('.sgpaPrCls').val(0);
		$(ele).closest('tr').find('.totPtsCls').val(0);*/
	}
}
/*
 * function calculateCGPAPointerSystem(temp){ var totalMarks = 0; var
 * creditRegister = 0; for(var i=1;i<=temp;i++) { totalMarks = totalMarks +
 * eval($('#txttotalMarks'+i).val()); creditRegister = creditRegister +
 * eval($('#txtCreditRegistered'+i).val()); } var cgpa = totalMarks /
 * creditRegister; $('#txtcgpa'+temp).val(cgpa.toFixed(2)); }
 */

function SaveStudSpec() {
	var studSpec = $('#cmbSpecNew').val();
	if (studSpec == 0) {
		jAlert("Please select specialization");
	} else {

		$.ajax({
			url : 'saveStudCPCSpecializationNew.json?studSpec=' + studSpec,
			dataType : 'json',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(jArray) {

				if (jArray == "noData") {
					jAlert("Your batch has not been activated yet");
				} else {
					$('#cmbSpecNew').val(jArray[0]["specId"]);
					getCustomAlertMessage("alert_create");
				}
			},
		});
	}
}

function getStudCPCSpecialization() {

	$.ajax({
		url : 'getStudCPCSpecializationNew.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var j = jArray.length;
			var uniComboBoiler = '<option value="0">Select</option>';
			if (j > 0) {
				for ( var i = 0; i < j; i++) {
					uniComboBoiler += '<option value="' + jArray[i]["specId"]
					+ '">' + jArray[i]["specName"] + '</option>';
				}
			}
			$('#cmbSpecNew').html(uniComboBoiler);

			if (j > 0) {
				$('#cmbSpecNew').val(jArray[0]["savedSpecId"]);
			}
		},
	});
}

function showSemCmb() {
	var semOrYear = $('input[name = "semYear"]:checked').val();
	if (semOrYear == "Semester") {
		$('#currSemYearTr').html("Pursuing Term");
	} else {
		$('#currSemYearTr').html("Pursuing Year");
	}
}

function showPercenSystem(totRow) {

	var semOrYear = $('input[name = "semYear"]:checked').val();

	var tempSemBoiler = '<td colspan="3" style="align:center;">'
		+ '<table style="align:center;border-collapse:collapse;border:0;" style="width: 400px;">'
		+ '<tr>';

	if (semOrYear == "Semester")
		tempSemBoiler += '<th>Class</th>';
	else
		tempSemBoiler += '<th>Year</th>';

	tempSemBoiler += '<th>Obtained marks</th>' + '<th>Marks out of</th>'
	+ '<th>Percentage</th>' + '</tr>';

	for ( var i = 1; i <= totRow; i++) {

		tempSemBoiler += '<tr>' + '<td style="text-align:center;">'
		+ i
		+ '</td>'
		+ '<td><input type="text" class="validnum" style="text-align: right" name="totMarks" id="txtPercentSysemTotMarks'
		+ i
		+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
		+ i
		+ '\',\'txtPercentSysemMarksOutOf'
		+ i
		+ '\',\'msgmarksPG'
		+ i
		+ '\',\'txtPercentSysemPercnt'
		+ i
		+ '\');validateObtAndTotMarks(\'txtPercentSysemTotMarks'
		+ i
		+ '\')" /></td>'
		+ '<td><input type="text" class="validnum" style="text-align: right" name="outOfMarks" id="txtPercentSysemMarksOutOf'
		+ i
		+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
		+ i
		+ '\',\'txtPercentSysemMarksOutOf'
		+ i
		+ '\',\'msgmarksPG'
		+ i
		+ '\',\'txtPercentSysemPercnt'
		+ i
		+ '\');validateObtAndTotMarks(\'txtPercentSysemMarksOutOf'
		+ i
		+ '\')" /></td>'
		+ '<td><input type="text" class="validnum" style="text-align: right" name="perc" id="txtPercentSysemPercnt'
		+ i
		+ '"/></td>'
		+ '</tr>'
		+ '<tr id="msgmarksPG'
		+ i
		+ '" style="display: none"> '
		+ '<td align="left" colspan="5" style="color: RED">Obtained Marks is greater than total MARKS.</td> '
		+ '</tr>';
	}

	tempSemBoiler += '</table>' + '</td>';

	$('#marksTypeTr').html(tempSemBoiler);
	$('.validnum').numeric();

	var boiler = '<table>'
		+ '<tr>';
		if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
			boiler+= '<th>No of Live Backlogs</th>';
		}else{
			boiler+= '<th>No of Live ATKT</th>';
		}
		boiler+= '<td><label><input type="text" class="validnum" style="text-align: right" id="txtPercntLiveATKT" value="0" onblur="validateObtAndTotMarks(\'txtPercntLiveATKT\')"></label></td>'
		+ '</tr>'
		+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										boiler+= '<th>No of Dead Backlogs</th>';
									}else{
										boiler+= '<th>No of Dead ATKT</th>';
									}
									boiler+= '<td><label><input type="text" class="validnum" style="text-align: right" id="txtPercentDeadATKT" value="0" onblur="validateObtAndTotMarks(\'txtPercentDeadATKT\')"></label></td>'
		+ '</tr>'
		+

		'<tr><td><input type="button" id="btnSaveMarksDetailsPercnt" onclick="saveCurrentQualiPercent('
		+ totRow
		+ ')" class="btn btn-inverse btn-mini btnColorGreen  " value="Save"></td></tr></table>';

	$('#atktTypeTr').html(boiler);
	$('.validnum').numeric();

}

