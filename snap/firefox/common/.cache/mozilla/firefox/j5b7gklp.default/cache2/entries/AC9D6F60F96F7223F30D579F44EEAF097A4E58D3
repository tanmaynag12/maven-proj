					lName : lName,
					contactHours : contactHours,
					motherName : motherName,
					fatherName:fatherName,
					tenthappearName : tenthappearName,
					licenceNo:licenceNo,
					physicallyHandicappedDetailsArray : physicallyHandicappedDetailsArray.toString(),
					Caste:Caste,
					isScollership:isScollership,
					adharCardNo:adharCardNo,
					subCaste:subCaste,
					physicallyHandicapped:physicallyHandicapped,
					sports:sports,
					armyOrExserviceman:armyOrExserviceman,
					admissionQuotaType:admissionQuotaType,
					admissionCategory:admissionCategory,
					strengthId:strengthId,
					isForiegner:isForiegner,
					placeOfBirth:placeOfBirth,
					enrollmentNo:enrollmentNo,
