	if(hdnUserRole == "student" || hdnUserRole == "Student"){
		url = 'stu_getMultipleGuradianDetailsStudentProfile.json';
	}
	$.ajax({
		url:url, 
		dataType:'json',
		async:false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
