		var currQualiDeadATKT = 0;
//		jArray[0]["currQualiDeadATKT"] == undefined ? "0"
//				: jArray[0]["currQualiDeadATKT"];

		tempSemBoiler += '</table>'
			+ '</td>'
			+ '</tr>'
			+ '</table>'
			+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
			+ '<tr id="atktTypeTr">'
			+ '<td>'
			+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
			+ '<tr>';
			if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
				tempSemBoiler+= '<th>No of Live Backlogs</th>';
			}else{
				tempSemBoiler+= '<th>No of Live ATKT</th>';
			}
			tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeLiveATKT" value="'
			+ currQualiLiveATKT
			+ '"  class="input-small forNum"></label></td>'
			+ '</tr>'
			+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeDeadATKT" value="'
			+ currQualiDeadATKT
			+ '"  class="input-small forNum"></label></td>'
			+ '</tr>'
			+ '<tr><td colspan="2">'
			+ '<input type="button" id="btnSaveMarksDetailsGrade" onclick="saveCurrentQualiGradeForSinhgad('
			+ totSem
		+ ')" class="btn btn-inverse btn-mini btnColorGreen " value="Save">'
		+ '<input type="button" onclick="viewPGDetails(\''
		+ degreeType
		+ '\')" class="btn btn-inverse btn-mini btnColorRed " value="Back" style="margin-left:10px;" >'
		+ '</table>' + '</td>' + '</tr>'
		+ '</table>';
		
		
		
		}
	
	///-------------------------------- For Sinhgad End----------------------------------------------///
	
	
	$('#tdSem').html(tempSemBoiler);
	$('.validnum').numeric();
	
	
}


function checkNextSemAndAddDetails(currntSem) {
	var type = $('input[name = "percGradPointr"]:checked').val();

	if (type == "Percent")
		addSemWisePercent();
	else if (type == "Grade")
		addSemWiseGrade();
	else if (type == "Pointer")
		addSemWisePointer();
}

function saveCurrentQualiPercent(totSem) {
	var chk = 0;
	for ( var j = 1; j < totSem; j++) {
		if ($('#txtPercentSysemTotMarks' + j + '').val() != ''
			&& $('#txtPercentSysemMarksOutOf' + j + '').val() == '') {
			chk = 1;
			break;
		} else if ($('#txtPercentSysemTotMarks' + j + '').val() == ''
			&& $('#txtPercentSysemMarksOutOf' + j + '').val() != '') {
			chk = 1;
			break;
		}
	}
	/*
	 * if(totSem==""){ jAlert("Please add details"); }
	 */
	if (chk == 1) {
		jAlert("Please add details");
	} else {
		var semArray = new Array();
		var percentArray = new Array();
		var totMarksArray = new Array();
		var outOfMarksArray = new Array();

		var percLiveAtktArray = $("#txtPercntLiveATKT").val();
		var percDeadAtktArray = $("#txtPercentDeadATKT").val();
		if (percLiveAtktArray == '') {
			percLiveAtktArray = 0;
		}

		if (parseInt(percLiveAtktArray) > 99
				|| parseInt(percDeadAtktArray) > 99) {
			jAlert("ATKT cannot be greater than 99");
			return;
		}
		if (percDeadAtktArray == '') {
			percDeadAtktArray = 0;
		}

		var sem = 1;

		for ( var j = 1; j <= totSem; j++) {
			if ($('#txtPercentSysemPercnt' + j + '').val() == "") {
				//break;
			} else {
				sem++;
				semArray.push(j);
				percentArray.push($('#txtPercentSysemPercnt' + j + '').val());
				totMarksArray.push($('#txtPercentSysemTotMarks' + j + '').val());
				outOfMarksArray.push($('#txtPercentSysemMarksOutOf' + j + '').val());
			}
		}
		if (semArray != "") {
			$.ajax({
				url : 'saveCurrentQuali.json?semArray=' + semArray
				+ '&percentArray=' + percentArray
				+ '&totMarksArray=' + totMarksArray
				+ '&outOfMarksArray=' + outOfMarksArray
				+ '&currentSem=' + sem + '&percLiveAtktArray='
				+ percLiveAtktArray + '&percDeadAtktArray='
				+ percDeadAtktArray,
				dataType : 'text',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(txt) {
					if (txt == "successful") {
						jAlert("Current Qualification Details have been saved Successfully","Success");
						getStudentCurrentQualification(degType);
					}
				},
				error : function() {
					// alert("in Error stream CMB");
				}
			});
		} else {
			jAlert("Please add details");
		}
	}
}

//for saving Grade system
function saveCurrentQualiGrade(totSem,degreeType,type,StudGraduation) {
	var chk = 0;
	for ( var j = 1; j < sem; j++) {
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="DYPATIL"){
			if ($('#txtsgpa' + j + '').val() == "" && $('#txtsgpa' + j + '').val() == undefined && $('#txtcgpa' + j + '').val() == "" 
				&& $('#txtcgpa' + j + '').val() == undefined) {
				chk = 1;
			}
		}else{
			if ($('#txtsgpa' + j + '').val() == "" || $('#txtsgpa' + j + '').val() == undefined || $('#txtcgpa' + j + '').val() == "" 
				|| $('#txtcgpa' + j + '').val() == undefined) {
				chk = 1;
			}
		}
	}
	if (chk == 1) {
		jAlert("Please add details");
	} else {
		var semArray = new Array();

		//var totalMarks = new Array();
		//var creditRegistered = new Array();
		//var creditEarned = new Array();
		var sgpa = new Array();
		var cgpa = new Array();
		var sgpaCgpa = new Array();

		var percLiveAtktArray = $("#txtGradeLiveATKT").val();
		var percDeadAtktArray = $("#txtGradeDeadATKT").val();
		var sem = 1;
		if($('#customerHelper').val()!=null && ($('#customerHelper').val() =='CMR_UNIVERSITY'|| $('#customerHelper').val() =='CMR_IT')){
			var tempCount = [];
			var tempSgpaCount = 0;
			$('.tempSgpaCls').each(function(){
				tempSgpaCount++;
				if(!tempCount.includes(tempSgpaCount)){
					tempCount.push(tempSgpaCount);
				}
			});
			
			var tempCgpaCount = 0;
			$('.tempCgpaCls').each(function(){
				tempCgpaCount++;
				if(!tempCount.includes(tempCgpaCount)){
					tempCount.push(tempCgpaCount);
				}
			});
			
			for ( var j = 1; j <= tempCount.length; j++) {
				if ($('#txtsgpa' + j + '').val() == "" || $('#txtcgpa' + j + '').val() == "") {
					
				} else {
					semArray.push(j);
					sem++;
					sgpa.push($('#txtsgpa' + j + '').val());
					cgpa.push($('#txtcgpa' + j + '').val());
					sgpaCgpa.push($('#txtsgpa' + j + '').val()+'$'+$('#txtcgpa' + j + '').val());
				}
			}
		}else{
			for ( var j = 1; j <= totSem; j++) {
				if ($('#customerHelper').val() == "DYPATIL") {
					if ($('#txtsgpa' + j + '').val() == "" && $('#txtcgpa' + j + '').val() == "") {
					
					}else{
						semArray.push(j);
						sem++;
						sgpa.push($('#txtsgpa' + j + '').val());
						cgpa.push($('#txtcgpa' + j + '').val());
						sgpaCgpa.push($('#txtsgpa' + j + '').val()+'$'+$('#txtcgpa' + j + '').val());
					}
				}else{
					if ($('#txtsgpa' + j + '').val() == "" || $('#txtcgpa' + j + '').val() == "") {
				
					}else{
						semArray.push(j);
						sem++;
						sgpa.push($('#txtsgpa' + j + '').val());
						cgpa.push($('#txtcgpa' + j + '').val());
						sgpaCgpa.push($('#txtsgpa' + j + '').val()+'$'+$('#txtcgpa' + j + '').val());
					}
				}
			}
		}
		if (semArray != "") {
			if(degreeType==undefined){	
				$.ajax({
					url : 'saveCurrentQualiForGradeSystem.json',
					data : {
						sgpa : sgpa.toString(),
						cgpa:cgpa.toString(), 
						semArray : semArray.toString(),
						percLiveAtktArray : percLiveAtktArray,
						percDeadAtktArray : percDeadAtktArray,
						currentSem : sem
					},
					dataType : 'text',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(txt) {
						if (txt == "successful") {
							getCustomAlertMessage("alert_create");
							getStudentCurrentQualification(degType);
						}
					},
					error : function() {}
				});
			}else{
				$.ajax({
					url : 'saveCGPASGPAGraduationQualiForGradeSystem.json',
					data : {
						sgpa : sgpaCgpa.toString(),
						cgpa:cgpa.toString(), 
						semArray : semArray.toString(),
						percLiveAtktArray : percLiveAtktArray,
						percDeadAtktArray : percDeadAtktArray,
						currentSem : sem,
						type:type,
						universityId:$('#university option:selected').val(),
						institute:$('#txtInstInStudProUgPg').val(),
						stream:$('#stream option:selected').val(),
						degree:$('#degName :selected').val(),
						status:$('input[name = "qualifying"]:checked').val(),
						passingYear : $('#yearPassing :selected').val(),
						PRNNumber : $('#PRNNumber').val(),
						liveATKT : $('#atktLive').val(),
						deadATKT : $('#atktDead').val(),
						merit : $('#merit').val(),
						pgPercentGradeFlag : $('input[name = "pgPercentGradeFlag"]:checked').val(),
						studGraduationId:StudGraduation,
						degreeType:degreeType
					},
					dataType : 'text',
					async : false,
					beforeSend : function() {
						startCommonAjaxSpinner();
					},
					complete : function() {
						stopCommonAjaxSpinner();
					},
					success : function(txt) {
						if (txt == "successful") {
							getCustomAlertMessage("alert_create");
							UgFun();
						}
					},
					error : function() {}
				});
			}
		} else {
			jAlert("Please add details");
		}
	}
}

function saveCurrentQualiGradeForSinhgad(totSem){
	
	
	var studentCreditDeatiArray = new Array();	
	var sem = 1;
	$('.semesterChkBoxCls').each(function(){
		if($(this).is(':checked'))
		{
		var studUniExMarId = $(this).val();//('studUniExMarId');
		if(studUniExMarId==undefined || studUniExMarId=="undefined"){
			studUniExMarId = 0;
		}
		var totPts = $(this).closest('tr').find('.totPtsCls').val() != "" ? $(this).closest('tr').find('.totPtsCls').val() : 0;
		var regCredit = $(this).closest('tr').find('.credRegistCls').val() !="" ? $(this).closest('tr').find('.credRegistCls').val() : 0;
		var earCredit = $(this).closest('tr').find('.credEarCls').val() !="" ? $(this).closest('tr').find('.credEarCls').val() : 0;
		
		var tempTrNo = eval($(this).closest('tr').attr('sem'));
		sem = tempTrNo;
			if(eval(tempTrNo) == 1)
				{
					/*var cgpa = eval(totPts)/eval(regCredit);
					var sgpa = eval(totPts)/eval(regCredit);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));*/
				var cgpa = 0;
				var sgpa = 0;
				if(totPts != "0" && regCredit != "0")
				{
					cgpa = eval(totPts)/eval(regCredit);
					sgpa = eval(totPts)/eval(regCredit);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
					
				}
				else 
				{
					cgpa = 0;
					sgpa = 0;
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
					
				}
					var creditDetails = regCredit+"~~"+earCredit+"~~"+tempTrNo+"~~"+sgpa.toFixed(2)+"~~"+cgpa.toFixed(2)+'~~'+studUniExMarId+'~~'+totPts;	
				}
			else if(eval(tempTrNo) > 1)
				{
				var sgpa = 0;
				
				if(totPts != "0" && regCredit != "0")
				{
				sgpa = eval(totPts)/eval(regCredit);
				if(eval(sgpa)!="" && !isNaN(sgpa))
					$('.tempTrCls_'+tempTrNo).find('.sgpaPrCls').val(sgpa.toFixed(2));
				}
				
				var totRegCredits = 0;
				var totPtsTotal = 0;
				for (var temp = 1; temp <=tempTrNo ; temp++) {
					totRegCredits+= eval($('.tempTrCls_'+temp).find('.credRegistCls').val() !="" ? $('.tempTrCls_'+temp).closest('tr').find('.credRegistCls').val() : 0);
					totPtsTotal+= eval($('.tempTrCls_'+temp).find('.totPtsCls').val() !="" ? $('.tempTrCls_'+temp).find('.totPtsCls').val() : 0);
					sem = temp;
			}
				
				var cgpa = 0;
				if(totPtsTotal != "0" && totRegCredits != "0")
				{
					cgpa = eval(totPtsTotal)/eval(totRegCredits);
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
				}
				else 
				{
					cgpa = 0;
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
				}
//				var cgpa = eval(totPtsTotal)/eval(totRegCredits);
				var creditDetails = regCredit+"~~"+earCredit+"~~"+tempTrNo+"~~"+sgpa.toFixed(2)+"~~"+cgpa.toFixed(2)+'~~'+studUniExMarId+'~~'+totPts;
				
				if(eval(cgpa)!="" && !isNaN(cgpa))
					$('.tempTrCls_'+tempTrNo).find('.cgpaPrCls').val(cgpa.toFixed(2));
			}
			studentCreditDeatiArray.push(creditDetails);
		}
	});
	
	
	var percLiveAtkt = $("#txtGradeLiveATKT").val();
	var percDeadAtkt = $("#txtGradeDeadATKT").val();
	
	if (studentCreditDeatiArray.length > 0) {
		$.ajax({
			url : 'saveCurrentQualiForGradeSystemForSinhgad.json',
			data : {
				//totalMarks : totalMarks.toString(),
				//creditRegistered : creditRegistered.toString(),
				//creditEarned : creditEarned.toString(),
				studentCreditDeatiArray : studentCreditDeatiArray.toString(),
				percLiveAtkt : percLiveAtkt,
				percDeadAtkt : percDeadAtkt,
				currentSem : sem
			},
			dataType : 'text',
			type : 'POST',
			async : false,
			beforeSend : function() {
				startCommonAjaxSpinner();
			},
			complete : function() {
				stopCommonAjaxSpinner();
			},
			success : function(txt) {
				if (txt == "successful") {
					getCustomAlertMessage("alert_create");
					getStudentCurrentQualification(degType);
				}
			},
			error : function() {
				// alert("in Error stream CMB");
			}
		});
	} else {
		getCustomAlertMessage("Please Enter Details..!");
	}
	
	/*var chk = 0;
	for ( var j = 1; j < sem; j++) {
		if ($('#txtsgpa' + j + '').val() == "" || $('#txtsgpa' + j + '').val() == undefined || $('#txtcgpa' + j + '').val() == "" 
			|| $('#txtcgpa' + j + '').val() == undefined) {
			chk = 1;
		}
	}
	if (chk == 1) {
		jAlert("Please add details");
	} else {
		var semArray = new Array();

		//var totalMarks = new Array();
		//var creditRegistered = new Array();
		//var creditEarned = new Array();
		var sgpa = new Array();
		var cgpa = new Array();

		var percLiveAtktArray = $("#txtGradeLiveATKT").val();
		var percDeadAtktArray = $("#txtGradeDeadATKT").val();
		var sem = 1;
		for ( var j = 1; j <= totSem; j++) {

			if ($('#txtsgpa' + j + '').val() == "" || $('#txtcgpa' + j + '').val() == "") {
				// jAlert("Please enter proper data");
			} else {
				semArray.push(j);
				sem++;
				//totalMarks.push($('#txttotalMarks' + j + '').val());
				//creditRegistered.push($('#txtCreditRegistered' + j + '').val());
				//creditEarned.push($('#txtCreditEarned' + j + '').val());
				sgpa.push($('#txtsgpa' + j + '').val());
				cgpa.push($('#txtcgpa' + j + '').val());
			}
		}

		if (semArray != "") {
			$
			.ajax({
				url : 'saveCurrentQualiForGradeSystem.json',
				data : {
					//totalMarks : totalMarks.toString(),
					//creditRegistered : creditRegistered.toString(),
					//creditEarned : creditEarned.toString(),
					sgpa : sgpa.toString(),
					cgpa:cgpa.toString(), 
					semArray : semArray.toString(),
					percLiveAtktArray : percLiveAtktArray,
					percDeadAtktArray : percDeadAtktArray,
					currentSem : sem
				},
				dataType : 'text',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(txt) {
					if (txt == "successful") {
						getCustomAlertMessage("alert_create");
						getStudentCurrentQualification(degType);
					}
				},
				error : function() {
					// alert("in Error stream CMB");
				}
			});
		} else {
			jAlert("Please add details");
		}
	}*/
	
	
	
	
}

function getCurrentSemDetail() {
	$
	.ajax({
		url : 'getCurrentEducationDetail.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var myboiler2 = '<br><div class="accordion" style="width:100%"  id="accordion2">';

			for ( var i = 0; i < jArray.length; i++) {

				myboiler2 += '<div class="accordion-group">'
					+ '<div class="accordion-heading">'
					+ '<a class="accordion-toggle" data-toggle="collapse"  data-parent="#accordion2" onclick="getSemInfo('
					+ jArray[i]["semId"] + ', '
					+ jArray[i]["studId"] + ')" href="#collapseOne'
					+ jArray[i]["semId"] + '">' + '<h4>Class '
					+ jArray[i]["semId"] + '</h4>' + '</a>'
					+ '</div>' + '<div id="collapseOne'
					+ jArray[i]["semId"]
				+ '" class="accordion-body collapse">'
				+ '<div class="accordion-inner" id="innerBody'
				+ jArray[i]["semId"] + '">' +

				'</div>' + '</div>' + '</div>';
			}
			$('#currentQualification').append(myboiler2);
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
}

function getSemInfo(semId, studId) {

	$
	.ajax({
		url : 'getParticularSemDetail.json?semId=' + semId + '&studId='
		+ studId,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var totSemMarks = 0;
			var totFullMarks = 0;
			var myboiler3 = '<table align="center"  cellpadding="0" cellspacing="0" border="0" class="table table-bordered table-striped table-condensed" id="tabDataTable"><thead>'
				+ '<tr><th style="width:10px;">Course Name</th>'
				+ '<th style="width:10px;">Intenal Marks</th>'
				+ '<th style="width:10px;">External Marks</th>'
				+ '<th style="width:10px;">Marks Obtained</th></tr></thead>';

			for ( var i = 0; i < jArray.length; i++) {
				myboiler3 += '<tbody><tr><td style="text-align:center">'
					+ jArray[i]["subName"]
				+ '</td>'
				+ '<td style="text-align:center">'
				+ jArray[i]["intenalMarks"]
				+ '</td>'
				+ '<td style="text-align:center">'
				+ jArray[i]["externalMarks"]
				+ '</td>'
				+ '<td style="text-align:center">'
				+ jArray[i]["obtainedMarks"]
				+ ' / '
				+ jArray[i]["maxMarks"]
				+ '</td>'
				+ '</tr></tbody>';
				totSemMarks = totSemMarks + jArray[i]["obtainedMarks"];
				totFullMarks += parseInt(jArray[i]["maxMarks"]);
			}
			var num = (totSemMarks / totFullMarks) * 100;
			var number = new Number(num + '').toFixed(3);

			myboiler3 += '</table>' + '<table>' + '<tr>'
			+ '<td><h5>Total Marks : &nbsp;' + totSemMarks
			+ ' / ' + totFullMarks + '</h5></td>' + '<tr>'
			+ '<td><h5>Percentage : &nbsp;'
			+ parseFloat(number) + ' %</h5></td>' + '</table>';
			$('#innerBody' + semId).html(myboiler3);

		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});

}

var otherUniversityListForSerching = new Array();

function getOtherUniversities(callback) {
	$.ajax({
		url : 'getOtherUniversity.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			var j = jArray.length;
			var uniComboBoiler = '<option value="0">Select</option>';
			for ( var i = 0; i < j; i++) {
				uniComboBoiler += '<option value="'
					+ jArray[i]["otherUniversityId"] + '">'
					+ jArray[i]["uniName"] + '</option>';
				otherUniversityListForSerching.push({
					name : jArray[i]["uniName"],
					to : jArray[i]["otherUniversityId"]
				});
			}
			$('#cmbUniNameForStudPlacement').html(uniComboBoiler);
			$('#university').html(uniComboBoiler);
			callback();
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
}

var uniIdFromPopUp = '';
setTimeout(function() {
	$('#otherUniversityNameForOther').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {

					$("#otherUniversityNameForOther").autocomplete(
							otherUniversityListForSerching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("
											+ term + ")", "gi"),
									"<strong>$1</strong>");

								},
								formatResult : function(row) {
									uniIdFromPopUp = row.to;
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {
					uniIdFromPopUp = $('#otherUniversityNameForOther').val();
					$('#addOtherUniversityFrm').modal('hide');
					$('#university').val(uniIdFromPopUp);
				}

			});
}, 1000);

var otherStreamListForSerching = new Array();
function getOtherStreams(callback) {
	$.ajax({
		url : 'getOtherStream.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var j = jArray.length;
			var uniComboBoiler = '<option value="0">Select</option>';
			for ( var i = 0; i < j; i++) {
				if (jArray[i]["degreeStream"] != ""
					&& jArray[i]["degreeStream"] != "Any Graduation") {
						if(($('#customerHelper').val() == "MEGHE" && jArray[i]["degreeStream"]!= "Medical") || $('#customerHelper').val() != "MEGHE"){
							uniComboBoiler += '<option value="'	+ jArray[i]["degreeStream"] + '">'+ jArray[i]["degreeStream"] + '</option>';
						}
					
					otherStreamListForSerching.push({
						name : jArray[i]["degreeStream"],
						to : jArray[i]["degreeStream"]
					});
				}
			}
			$('#cmbStreamNameForStudPlacement').html(uniComboBoiler);
			$('#stream').html(uniComboBoiler);
			callback();
		},
		error : function() {
			// //alert("in Error stream CMB");
		}
	});
}

var streamIdFromPopUp = '';
setTimeout(function() {
	$('#otherStreamDescriptionForOther').bind(
			'keyup',
			function(e) {
				if (e.keyCode == "37" || e.keyCode == "38" || e.keyCode == "39"
					|| e.keyCode == "40" || e.keyCode == "8") {
				} else {

					$("#otherStreamDescriptionForOther").autocomplete(
							otherStreamListForSerching,
							{
								minChars : 0,
								matchContains : true,
								max : 70,
								scroll : true,
								formatItem : function(row, i, max, term) {

									return row.name.replace(new RegExp("("
											+ term + ")", "gi"),
									"<strong>$1</strong>");

								},
								formatResult : function(row) {
									streamIdFromPopUp = row.to;
									return row.to;
								}
							});
				}
				if (e.keyCode == 13) {
					streamIdFromPopUp = $('#otherStreamDescriptionForOther')
					.val();
					$('#addOtherStreamFrm').modal('hide');
					$('#stream').val(streamIdFromPopUp);
					getDegreeName(degType);
				}

			});
}, 1000);

function getOtherDegreeForPlacement() {
	var streamId = $("#cmbStreamNameForStudPlacement").val();

	$.ajax({
		url : 'getOtherDegree.json?streamName=' + streamId,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var j = jArray.length;
			var uniComboBoiler = '<option value="0">Select</option>';
			for ( var i = 0; i < j; i++)
				uniComboBoiler += '<option value="'
					+ jArray[i]["otherPlacementQualificationId"] + '">'
					+ jArray[i]["degreeName"] + '</option>';

			$('#cmbDegreeNameForStudPlacement').html(uniComboBoiler);
		},
		error : function() {
			// alert("in Error stream CMB");
		}
	});
}

/** ************ Reasearch publication Tab ********************** */
function validTypeName(value)

{
	if (value == "select")
		return [ false, " Please select Level !!", "" ];
	else
		return [ true, "", "" ];

}

function validCategoryTypeName(value) {
	if (value == "select")
		return [ false, " Please select Event Category Type !!", "" ];
	else
		return [ true, "", "" ];

}

function validEventType(value) {

	if (value == "select")
		return [ false, " Please select Event Type !!", "" ];
	else
		return [ true, "", "" ];
}

statusForReasearchPub = '';
var studReasearchPubArr = new Array();
function getResearchPublication(){

	$.ajax({
		url:'getResearchPublicationInStudProfile.json',
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var flag = 0;
			var boiler= '<div id="newReasearchPubMsg" style="display: none;">'+
			'	<table>'+
			'	<tr id="trReqPendingReasearchPub" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/yellow dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request for editing is pending</label></td>'+
			'	</tr>'+
			'	<tr id="trReqAcceptedReasearchPub" style="display: none;">'+
			'		<td style="width:5px;"><img src="img/event/green dot.png"/></td><td><label style="font-style: italic;" id="lblUGPGMsg">Request is Accepted. You can edit</label></td>'+
			'	</tr>'+
			'	<tr id="trSendReqReasearchPub" style="display: none;">'+
			'		<td><label style="font-style: italic;" id="lblUGPGMsg">Send request for editing</label></td>'+
			'	</tr>'+
			'</table>'+
			'</div>'+
			'<table class="table table-bordered table-condensed">'+
			'<tr><th style="color: #0088CC" colspan="15"><label><b><font>Research Paper Publication Information</font></b></label></th></tr>';
			boiler+='<tr><td colspan="15"><span style="float:right;"><a class="btn btn-inverse btn-mini  btnColorBlack    freezeProfile hideSaveSubmitButton" onclick="checkReqConditionForAddEditPublication(\'NEW\',\'\')"><i class="fa fa-plus"></i>Add Publication</a></span><span><input type="button" id="btnFreezeReasearchPub" style="float:right;margin-right:1%;display:none;" class="btn btn-inverse btn-mini btnColorRed " value="Freeze" onclick="freezeTab()"/></span></td></tr>';
			if(jArray[0]["1stTimeAddStatus"]=="inPlaceLast"){
				
				statusForReasearchPub=jArray[0]["editingStatus"];
				studProChangeReqId=jArray[0]["studProReqId"];
				
					if(jArray[0]["studentResearchPublicationId"]!=undefined){
						
						boiler+='<tr>'+
						'<th><b>ISSN No</b></th>'+
						'<th><b>Paper Title</b></th>'+
						'<th><b>Name</b></th>'+
						'<th><b>Publish Date</b></th>'+
						'<th><b>Journal Name</b></th>'+
						'<th><b>Paper Level</b></th>'+
						'<th><b>Status Of Paper</b></th>'+
						'<th><b>Venue</b></th>'+
						'<th><b>Prize</b></th>'+
						'<th><b>Participant/Winner</b></th>'+
						'<th><b>Remark</b></th>'+
						'<th></th>'+
						'<th colspan="4"><b></b></th>'+
						'</tr>';
						for(var i=0;i<jArray.length;i++){
							studReasearchPubArr=jArray;
						
							boiler+=	'<tr>'+
							'<td style="text-align: right;"><label>'+jArray[i]["issnNo"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["paperName"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["authorName1"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["publishDate"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["journalName"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["journalLevel"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["statusOfPaper"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["venue"]+'</label></td>'+
							'<td style="text-align: left;"><label>'+jArray[i]["prize"]+'</label></td>';
							
							if(jArray[i]["participant"]=="true" && jArray[i]["winner"]!="true")
								boiler+='<td style="text-align: center;"><span class="label label-warning"> Participant </span></td>';
							if(jArray[i]["winner"]=="true" && jArray[i]["participant"]=="true")
								boiler+='<td style="text-align: center;"><span class="label label-success"> Winner </span></td>'; 
							
							boiler+= '<td style="text-align: left;"><label>'+jArray[i]["remark"]+'</label></td>';
							if(jArray[i]["status"]=="Approved"){
								boiler+= '<td style="text-align: center;"><span class="label label-success"> Approved </span></td>';
							}
							else if(jArray[i]["status"]=="Rejected"){    
							boiler+= '<td style="text-align: center;"><span class="label label-important"> Rejected </span></td>';
							}
							else if(jArray[i]["status"]=="pending"){
								boiler+= '<td style="text-align: center;"><label><b>Pending<b></label></td>';
							}
							if(jArray[i]["status"]=="pending" || jArray[i]["status"]=="Rejected"){
							boiler += '<td style="text-align: right;"><a  class="freezeProfile hideSaveSubmitButton" onclick="checkReqConditionForAddEditPublication(\''+i+'\',\''+jArray[i]["studentResearchPublicationId"]+'\')" href="#"><i class="fa fa-pencil-square-o iconColorBlack "></i> Edit</td>';
							}else{
								boiler += '<td></td>';
							}
							boiler += '<td style="text-align: right;"><a onclick="checkReqForDelPublication(\''+jArray[i]["studentResearchPublicationId"]+'\')" href="#"><i class="fa fa-trash iconColorBlack "></i> Delete</td>';

							boiler += '<td style="text-align: right;"><a onclick ="uploadStudPublicationDocument(\''+jArray[i]["studentResearchPublicationId"]+'\')" href="javascript:void(0)"><i class="fa fa-upload iconColorBlack "></i>Upload</td>';
							boiler += '</tr>';		
						}
						boiler+='</table>';
						$('#ResearchPublication').html(boiler);
			
					}else{
						
						boiler+='</table>';
						$('#ResearchPublication').html(boiler);
			
					}
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1st"){
					statusForReasearchPub="ReqNotSent";
					boiler+='</table>';
					$('#ResearchPublication').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlaceCanChange"){
					statusForReasearchPub="canChange";
					boiler+='</table>';
					$('#ResearchPublication').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlace1stReqSent"){
					statusForReasearchPub=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#ResearchPublication').html(boiler);
					
				}
				else if(jArray[0]["1stTimeAddStatus"]=="inPlacecheckByEditingStatus"){
					statusForReasearchPub=jArray[0]["editingStatus"];
					boiler+='</table>';
					$('#ResearchPublication').html(boiler);
				}
				else if(jArray[0]["1stTimeAddStatus"]=="notInPlace")
				{
					statusForReasearchPub="canChange";
					boiler+='</table>';
					$('#ResearchPublication').html(boiler);
				}
			
				if(statusForReasearchPub=='ReqNotSent'){
					$('#newReasearchPubMsg').show();
					$('#btnFreezeReasearchPub').hide();
					$('#trSendReqReasearchPub').show();
					$('#trReqAcceptedReasearchPub').hide();
					$('#trReqPendingReasearchPub').hide();
				}
				else if(statusForReasearchPub=='ReqAccepted'){
					$('#newReasearchPubMsg').show();
					$('#btnFreezeReasearchPub').show();
					$('#trReqAcceptedReasearchPub').show();
					$('#trSendReqReasearchPub').hide();
					$('#trReqPendingReasearchPub').hide();
				}
				else if(statusForReasearchPub=='ReqSent'){
					$('#newReasearchPubMsg').show();
					$('#trReqPendingReasearchPub').show();
					$('#trReqAcceptedReasearchPub').hide();
					$('#trSendReqReasearchPub').hide();
				}
				else if(statusForReasearchPub=='canChange'){
					$('#trReqPendingReasearchPub').hide();
					$('#trReqAcceptedReasearchPub').hide();
					$('#trSendReqReasearchPub').hide();
				}
		},
		error:function(){
			//alert("in Error stream CMB");
		}
	});
	
	/*if($('#customerHelper').val()=="BLM_MUNJAL") { 
		   $('input').attr('disabled',true);
		   $('button').attr('disabled',true);
		   $('div').attr('disabled',true);
		   $('select').attr('disabled',true).trigger("liszt:updated");
		   $('textArea').attr('disabled',true);
		   $('img').attr('disabled',true);
		   $('.freezeProfile').attr('disabled',true);
		   $('.freezeProfile').off('click').attr('onclick','').css('cursor','not-allowed');
		  
	 }*/
}
function checkReqConditionForAddEditPublication(indexId, studentResearchPublicationId){
	if(statusForReasearchPub=="canChange"){
		addNewPublicationNew(indexId, studentResearchPublicationId);
	}
	else if(statusForAddingCertificate=="ReqSent"){
			jAlert("The request you have sent for editing is pending");
	}
	else if(statusForReasearchPub=="ReqAccepted"){
		 $('#btnFreezeReasearchPub').show();
		 addNewPublicationNew(indexId, studentResearchPublicationId);
	}
	else if(statusForReasearchPub=="ReqNotSent"){
		 jConfirm("You have to send request for changing. Do you want to send? ","Confirm",function(r)
					{
				if(r ==true)
				{
					var tab="researchPub";
					sendRequestForEditing(tab);
				}
					});
	}
	getstudProfileConfigurationDetailsCheckingNames();	
}

