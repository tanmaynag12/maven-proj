function GenrateApplicantEligibilityPDF(studentId) {
	var instituteCourseStrengthId = $('#saveBtn').attr('institutecoursestrengthid');
	window.location.replace('printApplicationForEligibilityPdf.json?instituteCourseStrengthId='+ instituteCourseStrengthId+'&studentId='+studentId);
}

function GenrateApplicantUndertakingPDF(studentId) {
	var instituteCourseStrengthId = $('#saveBtn').attr('institutecoursestrengthid');
	
	 
	 var onlyUndertaken  = false ; 
	 if($('#customerHelper').val()=="REVA_UNIVERSITY"){
		 onlyUndertaken  =true;
	 }
	
	$.ajax({
		url :'checkeStatusOfUndertaking.json',
		type:'GET',
		data:{instituteCourseStrengthId:instituteCourseStrengthId},
		dataType:'text',
		success:function(jArray){
			//if(jArray!='true'){
			if($('#customerHelper').val()=="RAMAIAH"){
				window.location.replace('downloadpdfApplicantUndertakingPDF.json?instituteCourseStrengthId='+ instituteCourseStrengthId);	
			}else{
				window.location.replace('downloadpdfUndertakingPDF.json?instituteCourseStrengthId='+ instituteCourseStrengthId+'&onlyUndertaken='+onlyUndertaken);
			}	
		},
		error:function(){
			getCustomAlertMessage("alert_error");
		}
		
	});
}

function showPhysicalTR(){

	var phYes = $('#phYes').is(":checked");
	if(phYes){
		$('#physicallyHandicappedDetailsTR1').show();
		$('#physicallyHandicappedDetailsTR2').show();
	}
	else
	{
		$('#physicallyHandicappedDetailsTR1').hide();
		$('#physicallyHandicappedDetailsTR2').hide();
	}
}

function reset10thValues(){
	setTimeout(function(){
		$('#schoolTypeTenth').val('Select');
		$('#tenthboard').val('0');
	}, 1500);
}
function reset12thvalues(){
	setTimeout(function(){
		$('#twelthBoard').val('0');
		$('#studStreamCombo').val('Select');
	
	}, 1500);
	
}
function getStudyBatch(){
	var isSibllingStudying=$("input[name='isSibllingStudying']:checked" ).val();
	if(isSibllingStudying == "Yes"){
		$('#studyTr').show();
		$('#isStudyBatch').show();
	//	$('#studiedTr').hide();
	}else{
		$('#studyTr').hide();
		$('#isStudyBatch').hide();
	//	$('#studiedTr').show();
	}
}

function getStudyBatch1(){
	var isExSiblling=$("input[name='isExSiblling']:checked" ).val();

	if(isExSiblling == "Yes"){
		$('#exStudyTr').show();
		$('#isExStudyBatch').show();
	//	$('#studyingTr').hide();
	}else{
		$('#exStudyTr').hide();
		$('#isExStudyBatch').hide();
	//	$('#studyingTr').show();
	}
}



function hideShowWithinOtherAuthorPublication(ele,elePrefix){
	//var customerIdentificationObj = $(selected_custHelperDom).val();
	
	if($(ele).val() == 'within')
	{
		$('#'+elePrefix+'_WithinInstituteAuthorName_chzn').show();
		$('#'+elePrefix+'_WithinInstituteAuthorName_chzn').trigger("liszt:updated");
		$('#'+elePrefix+'_OtherInstituteAuthorSalName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorFName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorMName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorLName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorDesignation').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorInstitute').hide();
		$('#'+elePrefix+'_affiliationOfAuthor_chzn').trigger("liszt:updated");
		$('#'+elePrefix+'_WithinInstituteStudentName_chzn').hide();
		$('#'+elePrefix+'_WithinInstituteAuthorName').trigger("liszt:updated");
		$('#'+elePrefix+'_WithinInstituteStudentName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorDepartment').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorAffiliation').hide();
		$("#rule").show();
	}
	else if($(ele).val() == 'student')
	{
		convertComboToSearchable('#'+elePrefix+'_WithinInstituteStudentName');
		$('#'+elePrefix+'_WithinInstituteAuthorName_chzn').hide();
		$('#'+elePrefix+'_WithinInstituteAuthorName_chzn').trigger("liszt:updated");
		$('#'+elePrefix+'_OtherInstituteAuthorSalName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorFName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorMName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorLName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorDesignation').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorInstitute').hide();
		$('#'+elePrefix+'_affiliationOfAuthor_chzn').trigger("liszt:updated");
		$('#'+elePrefix+'_WithinInstituteStudentName_chzn').show();
		$('#'+elePrefix+'_WithinInstituteStudentName').hide();
		$('#'+elePrefix+'_WithinInstituteStudentName_chzn').trigger("liszt:updated");
		$('#'+elePrefix+'_OtherInstituteAuthorDepartment').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorAffiliation').hide();
		$("#rule").show();	
	}
	else
	{

		$('#'+elePrefix+'_WithinInstituteAuthorName_chzn').hide();
		$('#'+elePrefix+'_WithinInstituteAuthorName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorSalName').show();
		$('#'+elePrefix+'_OtherInstituteAuthorFName').show();
		$('#'+elePrefix+'_OtherInstituteAuthorMName').show();
		$('#'+elePrefix+'_OtherInstituteAuthorLName').show();
		$('#'+elePrefix+'_OtherInstituteAuthorDesignation').show();
		$('#'+elePrefix+'_OtherInstituteAuthorInstitute').show();
		$('#'+elePrefix+'_affiliationOfAuthor_chzn').trigger("liszt:updated");
		$('#'+elePrefix+'_WithinInstituteStudentName_chzn').hide();
		$('#'+elePrefix+'_WithinInstituteStudentName').hide();
		$('#'+elePrefix+'_OtherInstituteAuthorDepartment').show();
		$('#'+elePrefix+'_OtherInstituteAuthorAffiliation').show();
		$("#rule").show();
	}
}



var employeeNames = [];
function addAuthorityInListExceptPublication(ele,elePrefix){
var boiler = '';
var tblBoiler='';
var publicationType=$('#publicationType option:selected').val();
var type = $('input[type="radio"][name="authType"]:checked').val();
var srno = $('#'+elePrefix+'_srNoAuth').val();
var employeeId = '';
var employeeName = '';
employeeNames = [];
var salutation='';
var employeeFName='';
var employeeMName='';
var employeeLName='';
var employeeFullName='';

if(srno == ''){
	jAlert('Please Enter Authorship Position:....!!!');
	return;
}
if(type == 'within'){
	employeeId = $('#'+elePrefix+'_WithinInstituteAuthorName').val();
	employeeFullName = $('#'+elePrefix+'_WithinInstituteAuthorName :selected').text();
	salutation = $('#'+elePrefix+'_WithinInstituteAuthorName :selected').attr('sal');
	employeeFName = $('#'+elePrefix+'_WithinInstituteAuthorName :selected').attr('first');
	employeeMName = $('#'+elePrefix+'_WithinInstituteAuthorName :selected').attr('middle');
	employeeLName = $('#'+elePrefix+'_WithinInstituteAuthorName :selected').attr('last');
	if(employeeId=='')
		{
		jAlert('Please select Employee...!!!');
		return;
		}
}else if(type=='student'){
	
	employeeId = $('#'+elePrefix+'_WithinInstituteStudentName').val();
	employeeFullName = $('#'+elePrefix+'_WithinInstituteStudentName :selected').text();
	salutation = $('#'+elePrefix+'_WithinInstituteStudentName :selected').attr('sal');
	employeeFName = $('#'+elePrefix+'_WithinInstituteStudentName :selected').attr('first');
	employeeMName = $('#'+elePrefix+'_WithinInstituteStudentName :selected').attr('middle');
	employeeLName = $('#'+elePrefix+'_WithinInstituteStudentName :selected').attr('last');	
	
	
	
	
	/*salutation = $('#'+elePrefix+'_OtherInstituteAuthorSalName').val()!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorSalName').val().trim():"-";	
	employeeFName = $('#'+elePrefix+'_OtherInstituteAuthorFName').val()!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorFName').val().trim():"-";
	employeeMName = $('#'+elePrefix+'_OtherInstituteAuthorMName').val()!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorMName').val().trim():"-";
	employeeLName = $('#'+elePrefix+'_OtherInstituteAuthorLName')!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorLName').val().trim():"-";
*/	/*if(salutation=="")
	{
	jAlert('Please Enter Salutation...!!!');
	return;
	}
	if(employeeFName=="")
		{
		jAlert('Please Enter Student First Name...!!!');
		return;
		}
	if(employeeLName=="")
	{
	jAlert('Please Enter Student Last Name...!!!');
	return;
	}*/
	//employeeFullName=salutation+" "+employeeFName+" "+employeeMName+" "+employeeLName;
	
}

else {
	salutation = $('#'+elePrefix+'_OtherInstituteAuthorSalName').val()!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorSalName').val().trim():"-";	
	employeeFName = $('#'+elePrefix+'_OtherInstituteAuthorFName').val()!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorFName').val().trim():"-";
	employeeMName = $('#'+elePrefix+'_OtherInstituteAuthorMName').val()!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorMName').val().trim():"-";
	employeeLName = $('#'+elePrefix+'_OtherInstituteAuthorLName')!=undefined?$('#'+elePrefix+'_OtherInstituteAuthorLName').val().trim():"-";
	if(salutation=="")
	{
	jAlert('Please Enter Salutation...!!!');
	return;
	}
	
	if(employeeFName=="")
		{
		jAlert('Please Enter First Name...!!!');
		return;
		}
	if(employeeLName=="")
	{
	jAlert('Please Enter Last Name...!!!');
	return;
	}
	
	employeeFullName=salutation+" "+employeeFName+" "+employeeMName+" "+employeeLName;
	
}

if(employeeFullName == ''){
	if(type == 'within'){
		jAlert('Please select Employee...!!!');
		return;
	}/*else{
		jAlert('Please Enter Employee Name...!!!');
		return;
	}*/
}
var flag = false;
var flagAlreadyAdded = false;
var prioGivenEmp = '';

$('#'+elePrefix+'_AuthorNameListwithinInstitute li').each(function(){
	if($(this).attr('cnt') == srno){
		flag = true;
		prioGivenEmp = $(this).find('span').text(); 
	}
	if($.inArray(employeeFullName,employeeNames) != -1 || $(this).find('span').text() == employeeFullName){
		flagAlreadyAdded = true;
	}else{
		employeeNames.push($(this).find('span').text());
	}
	
});
if(flagAlreadyAdded){
	jAlert(employeeFullName+' is already added...!!!');
	return;
}
if(flag){
	jAlert('Priority '+srno+' is already set to '+prioGivenEmp);
	return;
}

$('#'+elePrefix+'_WithinInstituteAuthorName').val('');
$('#'+elePrefix+'_OtherInstituteAuthorFName').val('');
$('#'+elePrefix+'_OtherInstituteAuthorMName').val('');
$('#'+elePrefix+'_OtherInstituteAuthorLName').val('');
$('#'+elePrefix+'_OtherInstituteAuthorSalName').val('');
$('#'+elePrefix+'_srNoAuth').val('');
convertComboToSearchable(elePrefix+'_WithinInstituteAuthorName');

boiler+='<li cnt="'+(srno)+'" empId="'+employeeId+'">'+(srno)+'. <span>'+employeeFullName+'</span>&nbsp;&nbsp;&nbsp;&nbsp;<a href="javascript:void(0)" title="Remove" onclick="deleteAuthorDetailsExceptPublication(\'0\',this,\''+(employeeId!=''?employeeId:employeeFullName.replace(/\s/g,"_"))+"@@@"+srno+'\',\''+elePrefix+'\')" ><i class="fa fa-trash iconColorBlack"></i></a></li>';
$('#'+elePrefix+'_AuthorNameListwithinInstitute').append(boiler);

if(type == 'within'){
	boiler = '<option value="'+employeeId+"@@@"+srno+'" selected>'+employeeFullName+'</option>';
	$('#'+elePrefix+'_WithinInstituteAuthorName2').append(boiler);
}else if(type == 'student'){
	boiler = '<option value="'+employeeId+"@@@"+srno+'" selected>'+employeeFullName+'</option>';
	$('#'+elePrefix+'_WithinInstituteStudentName2').append(boiler);
	
}else{

	boiler = '<option value="'+employeeFullName.replace(/\s/g,"_")+"@@@"+srno+'" selected>'+employeeFullName+'</option>';
	$('#'+elePrefix+'_otherInstituteAuthorName2').append(boiler);
}


}



function getAllStudentList(elementId)
{
//var customerIdentificationObj = $(selected_custHelperDom).val();
var successFunTmp = function(jArray) {
	var boiler = '<option value="">select</option>';
	for ( var i = 0; i < jArray.length; i++) {
		/*if(customerIdentificationObj == 'RCOEM'){
			boiler +='<option sal="-" value="'+jArray[i]["studId"]+'" last="'+jArray[i]["last"]+'" middle="'+jArray[i]["middle"]+'" first="'+jArray[i]["first"]+'" department="'+jArray[i]["department"]+'" dept="'+jArray[i]["dept"]+'"  institute="'+jArray[i]["institute"]+'">'+jArray[i]["studName"]+'  ('+jArray[i]["department"]+') ('+jArray[i]["statusStudent"]+')</option>';

		}else{*/
			boiler +='<option sal="-" value="'+jArray[i]["studId"]+'" last="'+jArray[i]["last"]+'" middle="'+jArray[i]["middle"]+'" first="'+jArray[i]["first"]+'">'+jArray[i]["studName"]+'  ('+jArray[i]["department"]+')</option>';
		//}
	}
	$('#'+elementId).html(boiler);
	
	
	var specialKeys = new Array();
    specialKeys.push(8);
       $(".validnum").bind("keypress", function (e) {
           var keyCode = e.which ? e.which : e.keyCode
           var ret = ((keyCode >= 48 && keyCode <= 57) || specialKeys.indexOf(keyCode) != -1);
           return ret;
       });
};
//if(g_getAllStudentListData==null){
	$.ajax({
		url :'getAllStudentListtWithinInstitute.json',
		async : false,
		dataType : 'json',
		success : function(jArray){
			g_getAllStudentListData =jArray; 
			successFunTmp(jArray);
		},
		error : function() {
			jAlert("error,while fetching Student List");
		}
		
	});
/*}else{ 
	successFunTmp(g_getAllStudentListData);
}*/
}


