function tenpercentGradeActivation() {
	var percentgrade = $('input[name = "tenthPercentGradeFlag"]:checked').val();
	if (percentgrade == "percent") {
		$('#txt10marks').attr('disabled', false);
		$('#txt10outof').attr('disabled', false);
		$('#txt10percent').attr('disabled', false);
		$('#txt10grade').attr('disabled', true);
		$('#txtgradeconversionmethod10').attr('disabled', true);
	} else {
		$('#txt10marks').attr('disabled', true);
		$('#txt10outof').attr('disabled', true);
		$('#txt10percent').attr('disabled', true);
		$('#txt10grade').attr('disabled', false);
		$('#txtgradeconversionmethod10').attr('disabled', false);
	}
}

function pgPercentGradeActivation(type,degreeType,StudGraduation) {
	var percentgrade = $('input[name = "pgPercentGradeFlag"]:checked').val();
	if (percentgrade == "percent") {
		$('#tableGradingOrPercentageGrade').hide();
		$('#finalLine').show();
		$('#tableGradingOrPercentage').show();
		$('#markingSystemTr').show();
		$('#marksPointLbl').html('<b>SEM 1 :</b>');
		$('#percLbl').html('<b>Out of :</b>');
	} else {
		showGradeSystem(10,10,degreeType,type,StudGraduation)
		//gradingSystems(degreeType);
		/*$('#markingSystemTr').show();
		$('#marksPointLbl').html('<b>Points :</b>');
		$('#percLbl').html('<b>Equivalent<br>Percentage:</b>');
		var boiler = ' <b>%</b>';
		$('#txtPGpercent').html(boiler);*/
	}
}
var categoryArray = ['General','SC','ST','OBC','EWS'];
function getStateRelNationalityCatCityCountry() { // ---------------------------------------------------------Function
	// to Add Tab-1
	$.ajax({
		url : 'getAllStateRelNationalityCatCityCountry.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(data) {
			// state list
			var html = '<option value="0">Select</option>';
			var len = data[0].length;
			for ( var i = 0; i < len; i++){
				var categoryId = data[0][i]["categoryId"];
				html += '<option value="' + data[0][i]["stateId"] + '" class="hideCountryId ShowcountryId_'+categoryId+'">'+data[0][i]["stateName"]+'</option>';
			}

			$("#txtstuddomacile").html(html);
			$("#txtstudBirthState").html(html);
			convertComboToSearchable('#txtstuddomacile');
			
			changeAccordingToCountry();

			// AllReligion();
			var html2 = '<option value="0">Select</option>';
			var len = data[1].length;
			for ( var i = 0; i < len; i++)
				html2 += '<option value="' + data[1][i]["relId"] + '">'
				+ data[1][i]["relName"] + '</option>';

			$('#txtstudreligion').html(html2);

			// AllNationalities();
			var html3 = '<option value="0">Select</option>';
			var len = data[2].length;
			for ( var i = 0; i < len; i++)
				html3 += '<option value="' + data[2][i]["nationalityId"] + '">'
				+ data[2][i]["nationalityName"] + '</option>';

			$('#txtstudnationality').html(html3);
			convertComboToSearchable('#txtstudnationality');
			/*$('#passportIssuedCountryCmb').html(html3);
			$('#visaIssuedCountryCmb').html(html3);*/

			// AllCategories();
			var html = '<option value="0">Select</option>';
			var len = data[3].length;
			for ( var i = 0; i < len; i++){
				if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
							
								html += '<option value="' + data[3][i]["categoryId"] + '">'
								+ data[3][i]["categoryName"] + '</option>';
							
					}
				else{
					html += '<option value="' + data[3][i]["categoryId"] + '">'
					+ data[3][i]["categoryName"] + '</option>';
				}
				
			}

			$('#txtstudcategory').html(html);
			
			var html4 = '<option value="0">Select</option>';
			var len = data[4].length;
			for ( var i = 0; i < len; i++)
				html4 += '<option value="' + data[4][i]["countryId"] + '">'
				+ data[4][i]["countryName"] + '</option>';

			$('#passportIssuedCountryCmb').html(html4);
			$('#visaIssuedCountryCmb').html(html4);
			$('#registrationIssuedCountryCmb').html(html4);
			
			
			
		},
	});
}

function getStudPersonalInfoStartingFun() {
	getStateRelNationalityCatCityCountry();
	if($('#customerHelper').val()!=null && ($('#customerHelper').val() =='CMR_UNIVERSITY'|| $('#customerHelper').val() =='CMR_IT')){
		getstudentPersonalinfo($('#studId').val());
	}else{
		getstudentPersonalinfo();
	}	
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		getStudentContactForStudentDetailsTab();
	}
	//getSubCategory();
}

