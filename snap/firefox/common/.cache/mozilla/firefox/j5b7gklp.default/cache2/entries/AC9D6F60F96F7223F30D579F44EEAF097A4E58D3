							for (temp = 1; temp <= currentSem; temp++) {

								/*
								 * tempSemBoiler += '<tr>'+ '<td>'+temp+'</td>'+ '<td><input
								 * type="text" class="validnum"
								 * style="text-align: right"
								 * id="txtGradePoint'+temp+'"
								 * value="'+jArray[0]["currGrade"+temp+""]+'"/></td>'+ '<td><input
								 * type="text" class="validnum"
								 * style="text-align: right"
								 * id="txtGradeOutOf'+temp+'"
								 * value="'+jArray[0]["outOfGrade"+temp+""]+'"/></td>'+ '<td><input
								 * type="text" style="text-align: right"
								 * id="txtGradePercnt'+temp+'"
								 * value="'+jArray[0]["currSem"+temp+""]+'"/></td>'+ '</tr>';
								 */

								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ temp + '</td>';

								/*if (jArray[0]["totMarksPerc2" + temp
								              + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txttotalMarks'
										+ temp
										+ '" value="'
										+ jArray[0]["totMarksPerc2"
										            + temp + ""]
								+ '" class="span1" /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txttotalMarks'
										+ temp
										+ '" value="" class="span1" /></td>';

								if (jArray[0]["creditRegistered2"
								              + temp + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtCreditRegistered'
										+ temp
										+ '" value="'
										+ jArray[0]["creditRegistered2"
										            + temp + ""]
								+ '"  class="span1" /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtCreditRegistered'
										+ temp
										+ '" value="" class="span1" /></td>';

								if (jArray[0]["creditEarned2" + temp
								              + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtCreditEarned'
										+ temp
										+ '" value="'
										+ jArray[0]["creditEarned2"
										            + temp + ""]
								+ '" class="span1" /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtCreditEarned'
										+ temp
										+ '" value="" class="span1" /></td>';*/

								if (jArray[0]["sgpa" + temp + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtsgpa'
										+ temp
										+ '" value="'
										+ jArray[0]["sgpa" + temp
										            + ""]
								+ '" class="span1"  /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtsgpa'
										+ temp
										+ '" value="" class="span1" /></td>';

								if (jArray[0]["cgpa" + temp + ""] != undefined)
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtcgpa'
										+ temp
										+ '" value="'
										+ jArray[0]["cgpa" + temp
										            + ""]
								+ '" class="span1"  /></td>';
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" id="txtcgpa'
										+ temp
										+ '" value="" class="span1"  /></td>';

								tempSemBoiler += '</tr>';
							}

							for ( var i = temp; i <= jArray[0]["currTotSem"]; i++) {

								/*
								 * tempSemBoiler += '<tr>'+ '<td>'+i+'</td>'+ '<td><input
								 * type="text" class="validnum"
								 * style="text-align: right"
								 * id="txtGradePoint'+i+'" value=""/></td>'+ '<td><input
								 * type="text" class="validnum"
								 * style="text-align: right"
								 * id="txtGradeOutOf'+i+'" value=""/></td>'+ '<td><input
								 * type="text" style="text-align: right"
								 * id="txtGradePercnt'+i+'" value=""/></td>'+ '</tr>';
								 */

								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ i
									+ '</td>'
									/*+ '<td><input type="text" style="text-align: right" Readonly="readonly" id="txttotalMarks'
									+ i
									+ '" value="" class="span1" /></td>'
									+ '<td><input type="text" style="text-align: right" Readonly="readonly" id="txtCreditRegistered'
									+ i
									+ '" value="" class="span1" /></td>'
									+ '<td><input type="text" style="text-align: right" Readonly="readonly" id="txtCreditEarned'
									+ i
									+ '" value="" class="span1" /></td>'*/
									+ '<td><input type="text" style="text-align: right" id="txtsgpa'
									+ i
									+ '" value="" class="span1" /></td>'
									+ '<td><input type="text" style="text-align: right" id="txtcgpa'
									+ i
									+ '" value="" class="span1" disabled=disabled /></td>'
									+ '</tr>';
							}
							/*
							 * tempSemBoiler += '</table>'+ '</td>'+ '</tr>'+ '</table>'+ '<table
							 * class="table table-bordered
							 * table-condensed" style="width: 300px;">'+ '<tr id="atktTypeTr">'+ '<td>'+ '<table
							 * class="table table-bordered
							 * table-condensed" style="width: 300px;">'+ '<tr>'+ '<th>No
							 * of Live ATKT</th>'+ '<td><label><input
							 * type="text" id="txtGradeLiveATKT"
							 * value="'+jArray[0]["currQualiLiveATKT"]+'"
							 * class="input-small validnum"
							 * style="text-align: right"></label></td>'+ '</tr>'+ '<tr>'+ '<th>No
							 * of Dead ATKT</th>'+ '<td><label><input
							 * type="text" id="txtGradeDeadATKT"
							 * value="'+jArray[0]["currQualiDeadATKT"]+'"
							 * class="input-small validnum"
							 * style="text-align: right"></label></td>'+ '</tr>'+ '<tr><td colspan="2"><input
							 * type="button"
							 * id="btnSaveMarksDetailsGrade"
							 * onclick="saveCurrentQualiGrade('+jArray[0]["currTotSem"]+')"
							 * class="btn btn-inverse btn-mini "
							 * value="Save">'+ ' or <a href="#"
							 * onclick="viewPGDetails(\''+degreeType+'\')">Cancel</a></td></tr>'+ '</table>'+ '</td>'+ '</tr>'+ '</table>';
							 */
							var currQualiLiveATKT = jArray[0]["currQualiLiveATKT"] == undefined ? "0"
									: jArray[0]["currQualiLiveATKT"];
							var currQualiDeadATKT = jArray[0]["currQualiDeadATKT"] == undefined ? "0"
									: jArray[0]["currQualiDeadATKT"];

							tempSemBoiler += '</table>'
								+ '</td>'
								+ '</tr>'
								+ '</table>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr id="atktTypeTr">'
								+ '<td>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Live Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Live ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeLiveATKT" value="'
								+ currQualiLiveATKT
								+ '"  class="input-small"></label></td>'
								+ '</tr>'
								+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" style="text-align: right" id="txtGradeDeadATKT" value="'
								+ currQualiDeadATKT
								+ '"  class="input-small"></label></td>'
								+ '</tr>'
								+ '<tr><td colspan="2">'
								+ '<input type="button" id="btnSaveMarksDetailsGrade" onclick="saveCurrentQualiGrade('
								+ jArray[0]["currTotSem"]
							+ ')" class="btn btn-inverse btn-mini btnColorGreen " value="Save">'
							+ '<input type="button" onclick="viewPGDetails(\''
							+ degreeType
							+ '\')" class="btn btn-inverse btn-mini btnColorRed " value="Back" style="margin-left:10px;" >'
							+ '</table>' + '</td>' + '</tr>'
							+ '</table>';
							
							}
						}
					}

					$('#tdSem').html(tempSemBoiler);
					$('.validnum').numeric();
					

					if (jArray[0]["semYear"] == "Year") {
						$('#radYear').attr('checked', 'checked');
						$('#radSem').attr('disabled', 'disabled');
					} else if (jArray[0]["semYear"] == "Semester") {
						$('#radSem').attr('checked', 'checked');
						$('#radYear').attr('disabled', 'disabled');
					}

					showSemCmb();
					//
					var semCombo = '<option value="0">Select</option>';
					var k = 0;
					for (k = 1; k <= jArray[0]["currTotSem"]; k++) {
						semCombo += '<option value="' + k + '">' + k
						+ '</option>';
					}
					$('#cmbCurrentSem').html(semCombo);

					$('#cmbCurrentSem').val(jArray[0]["currSem"]);
					//
					$('.forNum').numeric();
				}

				getStudCPCSpecialization();

				/*if (jArray[0]["blockSaveButton"] == 'blockSaveButton'){
							$('#btnSaveMarksDetailsPercnt').hide();
							$('#btnSaveMarksDetailsGrade').hide();
						}
						else{
							$('#btnSaveMarksDetailsPercnt').show();
							$('#btnSaveMarksDetailsGrade').show();
						}*/
				
				
			}
		},
		error : function() {
			getCustomAlertMessage("alert_error");
		}
	});
	if($('#customerHelper').val() =='HPTU' || $('#customerHelper').val() =='HPTELIBRARY'){
		console.log("jhih1");
			if(g_freeze!=undefined && g_freeze)
				{
				disabledAll();
				}
			}
}

function getStudentCurrentQualificationConfirmToView(degreeType) {
	$.ajax({
		url : 'getStudentCurrentQualification.json',
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {

			var myboiler = '';

			if (jArray.length > 0) {
				if (jArray[0]["studentType"] == "academic"	|| jArray[0]["studentType"] == "PlacementAcademic") {
					// academic

					var tempSem = 0;
					var tempCnt = 0;
					var currentSem = jArray[0]["cnt"];
					myboiler += '<table class="table table-bordered" style="width: 100%;" >'
						+ '<tr><td colspan="4" style="color: #0088CC"><label><b><font>Student current qualification:</font></b></label></td></tr>'
						+ '<tr></tr>';
					for ( var i = 0; i < jArray.length; i++) {
						myboiler += '<tr><th style="width:30%">University name</th><td><label>'+ jArray[i]["universityName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Degree stream</th><td><label>'+ jArray[i]["degreeStreamName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Degree name</th><td><label>'+ jArray[i]["degreeName"]+ '</label></td></tr>';
						myboiler += '<tr><th style="width:30%">Specialization (if any)</th><td><select id="cmbSpecNew" disabled="disabled"></select>'+
						'</td></tr>';
						myboiler += '<tr><th style="width:30%">Year of Passing</th><td><label>'+ jArray[i]["yearOfPassing"]+ '</label></td></tr>';
						myboiler += '<tr><td colspan="3" id="tdSem">'+ '</td></tr>';

					}
					myboiler += '</table>';

					$('#ug').html(myboiler);

					var tempSemBoiler = '';

					if (currentSem == 1) {

						tempSemBoiler += '<table class="table table-bordered table-condensed"" style="width: 100%;">'
							+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
							+ '<tr style="margin-left:10%;" id="marksTypeTr">'
							+ '</tr>'
							+ '</table>'
							+ '<table >'
							+ '<tr id="atktTypeTr">'
							+ '</tr>'
							+ '</table>';
					} else {
						if (jArray[0]["marksSystem"] == "Percent") {
							var aggPer="0.0";
							style="style='display: none'";
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="INDIRA_PUNE" && jArray[0]["marksSystem"]=="Percent"){
								style="style='display: show'";
							}
							
							tempSemBoiler +=
								// '</table>'+
								'<table class="table table-bordered table-condensed" style="width: 100%;">'
								+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
								+ '<tr style="margin-left:10%;" id="marksTypeTr">'
								+ '<td colspan="3">'
								+ '<table class="table table-bordered table-condensed">'
								+ '<tr>';

							tempSemBoiler += '<th>Class</th>'
								+ '<th>Obtained marks</th>'
								+ '<th>Marks out of</th>'
								+ '<th>Percentage</th>' + '</tr>';

							var temp = 0;
							//currentSem
							for (temp = 1; temp <= 10; temp++) {
								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ temp + '</td>';

								if (jArray[0]["totMarksPerc2" + temp+ ""] != undefined)
								{
									if (jArray[0]["isExamStatus" + temp + ""])
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="totMarks" disabled="disabled" readonly="readonly" id="txtPercentSysemTotMarks'+ temp+ '" value="'
										+ jArray[0]["totMarksPerc2"
										            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" disabled="disabled" /></td>';
									else
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="totMarks" disabled="disabled" readonly="readonly" id="txtPercentSysemTotMarks'+ temp+ '" value="'
										+ jArray[0]["totMarksPerc2"
										            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" /></td>';
								}
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" name="totMarks" disabled="disabled" readonly="readonly" id="txtPercentSysemTotMarks'
										+ temp
										+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
										+ temp
										+ '\',\'txtPercentSysemMarksOutOf'
										+ temp
										+ '\',\'msgmarksPG'
										+ temp
										+ '\',\'txtPercentSysemPercnt'
										+ temp + '\')" /></td>';

								if (jArray[0]["outOfMarksPerc2" + temp+ ""] != undefined)
								{
									if (jArray[0]["isExamStatus" + temp + ""])
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="outOfMarks" disabled="disabled" readonly="readonly" id="txtPercentSysemMarksOutOf'
											+ temp
											+ '" value="'
											+ jArray[0]["outOfMarksPerc2"
											            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" disabled="disabled" /></td>';
									else
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="outOfMarks" disabled="disabled" readonly="readonly" id="txtPercentSysemMarksOutOf'
											+ temp
											+ '" value="'
											+ jArray[0]["outOfMarksPerc2"
											            + temp + ""]
									+ '" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ temp
									+ '\',\'txtPercentSysemMarksOutOf'
									+ temp
									+ '\',\'msgmarksPG'
									+ temp
									+ '\',\'txtPercentSysemPercnt'
									+ temp + '\')" /></td>';
								}
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" name="outOfMarks" disabled="disabled" readonly="readonly" id="txtPercentSysemMarksOutOf'
										+ temp
										+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
										+ temp
										+ '\',\'txtPercentSysemMarksOutOf'
										+ temp
										+ '\',\'msgmarksPG'
										+ temp
										+ '\',\'txtPercentSysemPercnt'
										+ temp + '\')" /></td>';

								if (jArray[0]["currSem" + temp + ""] != undefined)
								{
									if (jArray[0]["isExamStatus" + temp + ""])
										tempSemBoiler += '<td><input type="text" style="text-align: right" name="perc"  id="txtPercentSysemPercnt'
											+ temp+ '" value="'+ jArray[0]["currSem"+ temp + ""]+ '" disabled="disabled" readonly="readonly"/></td>';
									else
										tempSemBoiler += '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" name="perc" id="txtPercentSysemPercnt'
											+ temp
											+ '" value="'
											+ jArray[0]["currSem"
											            + temp + ""]
									+ '"/></td>';
								}
								else
									tempSemBoiler += '<td><input type="text" style="text-align: right" name="perc" disabled="disabled" readonly="readonly" id="txtPercentSysemPercnt'
										+ temp
										+ '" value=""/></td>';
								
								
								if (jArray[0]["aggPer" + temp + ""] != undefined && jArray[0]["aggPer" + temp + ""] !="")
									aggPer = jArray[0]["aggPer" + temp + ""];
									
									/*tempSemBoiler += '<td '+style+'><input type="text"  id="txtaggrpercentage'	+ temp+ '" value="'+ jArray[0]["aggPer" + temp + ""]+ '" class="span1" disabled="disabled" readonly="readonly" /></td>';
								else
									tempSemBoiler += '<td '+style+'><input type="text"  id="txtaggrpercentage'	+ temp+ '" value="" class="span1" disabled="disabled" readonly="readonly" /></td>';*/
								

								tempSemBoiler += '</tr>'
									+ '<tr id="msgmarksPG'
									+ temp
									+ '" style="display: none"> '
									+ '<td align="left" colspan="5" style="color: RED">Obtained Marks is greater than total MARKS.</td> '
									+ '</tr>';
							}
							for ( var i = temp; i <=10 ; i++) {

								tempSemBoiler += '<tr>'
									+ '<td style="text-align: right;">'
									+ i
									+ '</td>'
									+ '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly"  name="totMarks" id="txtPercentSysemTotMarks'
									+ i
									+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ i
									+ '\',\'txtPercentSysemMarksOutOf'
									+ i
									+ '\',\'msgmarksPG'
									+ i
									+ '\',\'txtPercentSysemPercnt'
									+ i
									+ '\')" /></td>'
									+ '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly" name="outOfMarks" id="txtPercentSysemMarksOutOf'
									+ i
									+ '" value="" onblur="caculatePercent(\'txtPercentSysemTotMarks'
									+ i
									+ '\',\'txtPercentSysemMarksOutOf'
									+ i
									+ '\',\'msgmarksPG'
									+ i
									+ '\',\'txtPercentSysemPercnt'
									+ i
									+ '\')" /></td>'
									+ '<td><input type="text" style="text-align: right" disabled="disabled" readonly="readonly"  id="txtPercentSysemPercnt'
									+ i
									+ '" value=""/></td>'
									+ '</tr>';
							}
							var currQualiLiveATKT = jArray[0]["currQualiLiveATKT"] == undefined ? "0"
									: jArray[0]["currQualiLiveATKT"];
							var currQualiDeadATKT = jArray[0]["currQualiDeadATKT"] == undefined ? "0"
									: jArray[0]["currQualiDeadATKT"];
							tempSemBoiler += '</table>'
								+ '</td>'
								+ '</tr>'
								+ '</table>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr id="atktTypeTr">'
								+ '<td>'
								+ '<table class="table table-bordered table-condensed" style="width: 300px;">'
								+ '<tr>'
								+ '<th '+style+'>Aggregate Percentage</th>'
								+ '<td '+style+'><label><input type="text" id="aggPer" disabled="disabled" readonly="readonly" value="'
								+ aggPer
								+ '" class="input-small" style="text-align: right"></label></td>'
								+'</tr>'
								+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Live Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Live ATKT</th>';
									}
									tempSemBoiler+= '<td><label><input type="text" id="txtPercntLiveATKT" disabled="disabled" readonly="readonly" value="'
								+ currQualiLiveATKT
							+ '" class="input-small" style="text-align: right"></label></td>'
							+ '</tr>'
							+ '<tr>';
									if($('#customerHelper').val()!=undefined && ($('#customerHelper').val()=="HYGIA"||$('#customerHelper').val()=="REVA_UNIVERSITY")){
										tempSemBoiler+= '<th>No of Dead Backlogs</th>';
									}else{
										tempSemBoiler+= '<th>No of Dead ATKT</th>';
									}
									tempSemBoiler+=  '<td><label><input type="text" id="txtPercentDeadATKT" disabled="disabled" readonly="readonly" value="'
							+ currQualiDeadATKT
							+ '" class="input-small" style="text-align: right"></label></td>'
							+ '</tr>'
							+ '</table>';
						} else {
							// academic pointer
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()!="RAMAIAH_UNIVERSITY"){
							tempSemBoiler +=

								'<table class="table table-bordered table-condensed" style="width: 100%;">'
								+ '<tr><td colspan="3" style="color: #0088CC"><label><b><font>Marks Details:</font></b></label></td></tr>'
								+ '<tr style="margin-left:10%;" id="marksTypeTr">'
								+ '<td colspan="3">'
								+ '<table class="table table-bordered table-condensed">'
								+ '<tr>';

							}
///-------------------------------- For Sinhgad ----------------------------------------------///
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="SINHGAD")
								{
								tempSemBoiler += 
									
								'<td colspan="7" style="height:20px"><font color="red"><b>Click on check box to enter marks.</b></font></td></tr>'+	
								'<tr><th style="width:5%"></th>'+
								'<th style="width:10%">Class/Semester</th>'+ 
								'<th style="width:10%">Credit Registered</th>'+
