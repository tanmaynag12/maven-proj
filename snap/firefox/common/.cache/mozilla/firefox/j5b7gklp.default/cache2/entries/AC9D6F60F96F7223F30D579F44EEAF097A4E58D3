function ShowTD() {
	if ($('#curFlag').is(':checked')) {
		$('#addrss').show();
	}
	if ($('#PerFlag').is(':checked')) {
		$('#addrss').hide();
	}
}
//----------------------------------------------------------------Function To
//Clear New Form
function clearForm(frm) {
	$('#' + frm).find(':input').each(function() {
		switch (this.type) {
		case 'password':
		case 'select-multiple':
		case 'select-one':
		case 'text':
		case 'textarea':
			$(this).val('');
			break;
		case 'checkbox':
		case 'radio':
			this.checked = false;
		}
	});
}

//---------------------------------------validate current degree obt and tot
//marks
function validateObtAndTotMarks(txtId) {
	var temp = document.getElementById(txtId);
	var marks = temp.value;
	if (isNaN(marks)) {
		jAlert("Please enter numbers only");
	}
}

//--------------------------------------------------------function for
//Calculate percentage
function caculatePercent(marksObt, totMarks, trid, percent) {
	var txtobj = document.getElementById(marksObt);
	var txtobj1 = document.getElementById(totMarks);
	var txtobj2 = document.getElementById(percent);

	var marksobtained = txtobj.value;
	var totalmarks = txtobj1.value;

	if (marksobtained.length == 0 && totalmarks == 0) {
		document.getElementById(percent).value = '';
		$('tr#' + trid).hide();
	} else if (totalmarks != "" && marksobtained != "" && !isNaN(totalmarks)
			&& !isNaN(marksobtained)) {
		marksobtained = parseFloat(marksobtained);
		totalmarks = parseFloat(totalmarks);
		if (marksobtained <= totalmarks) {
			var num = (marksobtained / totalmarks) * 100;

			var number = new Number(num + '').toFixed(2);
			document.getElementById(percent).value = parseFloat(number);
			$('tr#' + trid).hide();
			return true;
		} else {
			$('tr#' + trid).show();
			document.getElementById(percent).value = parseInt('0');

			return false;
		}
	} else {
		document.getElementById(percent).value = parseInt('0');
		return false;
	}
}


function caculatePCMPercent() {
	var twelthPhysicsmarks = $('#txt12Physicsmarks').val() == "" ? 0 : $(
	'#txt12Physicsmarks').val();
	var twelthPhysicsOutOf = $('#txt12Physicsoutof').val() == "" ? 0 : $(
	'#txt12Physicsoutof').val();
	var twelthChemistrymarks = $('#txt12Chemistrymarks').val() == "" ? 0 : $(
	'#txt12Chemistrymarks').val();
	var twelthChemistryOutOf = $('#txt12Chemistryoutof').val() == "" ? 0 : $(
	'#txt12Chemistryoutof').val();
	var twelthMathematicsmarks = $('#txt12Mathematicsmarks').val() == "" ? 0
			: $('#txt12Mathematicsmarks').val();
	var twelthMathematicsOutOf = $('#txt12Mathematicsoutof').val() == "" ? 0
			: $('#txt12Mathematicsoutof').val();

	var Obtainedmarks = eval(twelthPhysicsmarks) + eval(twelthChemistrymarks)
	+ eval(twelthMathematicsmarks);
	var OutOf = eval(twelthPhysicsOutOf) + eval(twelthChemistryOutOf)
	+ eval(twelthMathematicsOutOf);
	var marksobtained = parseFloat(Obtainedmarks);
	var totalmarks = parseFloat(OutOf);
	if (twelthMathematicsmarks != "" && twelthMathematicsmarks != 0
			&& marksobtained <= totalmarks) {
		var num = (marksobtained / totalmarks) * 100;
		var number = new Number(num + '').toFixed(2);
		$('#txt12PCMPercentage').val(parseFloat(number));
		return true;
	}
}

function caculatePCBPercent() {
	var twelthPhysicsmarks = $('#txt12Physicsmarks').val() == "" ? 0 : $(
	'#txt12Physicsmarks').val();
	var twelthPhysicsOutOf = $('#txt12Physicsoutof').val() == "" ? 0 : $(
	'#txt12Physicsoutof').val();
	var twelthChemistrymarks = $('#txt12Chemistrymarks').val() == "" ? 0 : $(
	'#txt12Chemistrymarks').val();
	var twelthChemistryOutOf = $('#txt12Chemistryoutof').val() == "" ? 0 : $(
	'#txt12Chemistryoutof').val();
	var twelthBiologymarks = $('#txt12Biologymarks').val() == "" ? 0 : $(
	'#txt12Biologymarks').val();
	var twelthBiologyOutOf = $('#txt12Biologyoutof').val() == "" ? 0 : $(
	'#txt12Biologyoutof').val();

	var Obtainedmarks = eval(twelthPhysicsmarks) + eval(twelthChemistrymarks)
	+ eval(twelthBiologymarks);
	var OutOf = eval(twelthPhysicsOutOf) + eval(twelthChemistryOutOf)
	+ eval(twelthBiologyOutOf);
	var marksobtained = parseFloat(Obtainedmarks);
	var totalmarks = parseFloat(OutOf);
	if (twelthBiologymarks != 0 && twelthBiologymarks != ""
		&& marksobtained <= totalmarks) {
		var num = (marksobtained / totalmarks) * 100;
		var number = new Number(num + '').toFixed(2);
		$('#txt12PCBPercentage').val(parseFloat(number));
		return true;
	}
}

//--------------------------------------------------------------------------------------------Percent
//Grade Activation
function percentGradeActivation2() {
	value = $('input[name = "diplomaPercentOrGradeFlag"]:checked').val();
	if (value == 'grade') {
		$('.MarksDetailPercent').each(function() {
			$(this).attr('disabled', 'disabled');
		});
		$('.MarksDetailGrade').each(function() {
			$(this).removeAttr('disabled');
		});
	} else if (value == 'percent') {
		$('.MarksDetailGrade').each(function() {
			$(this).attr('disabled', 'disabled');
		});
		$('.MarksDetailPercent').each(function() {
			$(this).removeAttr('disabled');
		});
	}
}

function twelthpercentGradeActivation() {
	var percentgrade = $('input[name = "twelthPercentGradeFlag"]:checked').val();
	if (percentgrade == "percent") {
		$('#txt12marks').attr('disabled', false);
		$('#txt12outof').attr('disabled', false);
		$('#txt12percent').attr('disabled', false);
		$('#txt12grade').attr('disabled', true);
		$('#txtgradeconversionmethod12').attr('disabled', true);
	} else {
		$('#txt12marks').attr('disabled', true);
		$('#txt12outof').attr('disabled', true);
		$('#txt12percent').attr('disabled', true);
		$('#txt12grade').attr('disabled', false);
		$('#txtgradeconversionmethod12').attr('disabled', false);
	}
}
/*
function pcmUi(){
	var pcmOrPcb = $('input[name = "science"]:checked').val();
	if(pcmOrPcb == "PCM"){
		$('#txt12Physicsmarks').attr('disabled', false);
		$('#txt12Physicsoutof').attr('disabled', false);
		$('#txt12Chemistrymarks').attr('disabled', false);
		$('#txt12Chemistryoutof').attr('disabled', false);
		$('#txt12Mathematicsmarks').attr('disabled', false);
		$('#txt12Mathematicsoutof').attr('disabled', false);
		$('#txt12Biologymarks').attr('disabled', true);
		$('#txt12Biologyoutof').attr('disabled', true);
	}else if(pcmOrPcb == "PCB"){
		$('#txt12Physicsmarks').attr('disabled', false);
		$('#txt12Physicsoutof').attr('disabled', false);
		$('#txt12Chemistrymarks').attr('disabled', false);
		$('#txt12Chemistryoutof').attr('disabled', false);
		$('#txt12Mathematicsmarks').attr('disabled', true);
		$('#txt12Mathematicsoutof').attr('disabled', true);
		$('#txt12Biologymarks').attr('disabled', false);
		$('#txt12Biologyoutof').attr('disabled', false);
	}else{
		jAlert("Plese slect Any one type.");
	}
}
