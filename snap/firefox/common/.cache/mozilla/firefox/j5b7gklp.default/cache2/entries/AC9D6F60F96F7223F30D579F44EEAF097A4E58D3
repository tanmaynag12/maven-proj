	var motherProfession = $('input[name = "motherProfession"]').val();
	var parentAnnualIncome = $('input[name = "parentAnnualIncome"]').val();
	var parentCreamyLayer = $('#parentCreamyLayer').val();
	
	
	var motherOfficeStd = $('#motherstdcode').val();
	var motherOfficePhnoneNo= $('#motherphnumber').val();
	var fatherHomeStd = $('#fatherHomestd').val();
	var fatherHomephoneNo = $('#fatherStudPhnumber').val();
	var motherHomeStd = $('#motherHomestdcode').val();
	var motherHomePhnoneNo = $('#motherHomePhnumber').val();
	var motherAnnualIncome = $('input[name = "motherAnnualIncome"]').val();
