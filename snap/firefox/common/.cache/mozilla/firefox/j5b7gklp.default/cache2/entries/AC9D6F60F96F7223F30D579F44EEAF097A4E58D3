			if ((gArray[i]["isCheckedText"] == "lastName") && ($('input[name="lastName"]')!=undefined) && ($.trim($('input[name="lastName"]').val())!=null) && ($.trim($('input[name="lastName"]').val())!="") && ($('input[name="lastName"]').val().length > 0)) {
			$('input[name = "lastName"]').attr("disabled", true);
