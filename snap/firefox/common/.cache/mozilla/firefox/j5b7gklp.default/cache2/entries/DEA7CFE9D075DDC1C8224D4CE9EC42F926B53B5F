											speBoiler += '</table>';
											$('#showMultiplePostListDiv').html(speBoiler);
											$('#trMultiplePost').show();
											}
										}
							 			else
										{
							 				flag = 0;
											boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
										}
									boiler += '</tr>';
							}
							
							
							/*if(jArray[0][0]["CRITERIA_SSC_BOARD"] != "")
							{
								otherCriteriaCounter++;
								boiler +=	'<tr>'+	
										'<th>SSC Board</th>'+
										'<td>'+ jArray[0][0]["STUDENT_SSC_BOARD"]  +'</td>'+
										'<td>'+ jArray[0][0]["CRITERIA_SSC_BOARD"]  +'</td>';
										//'<td>'+ ( eval(jArray[0]["STUDENT_GAP_COUNT"] ) <= eval(jArray[0]["CRITERIA_GAP"]) ) +'</td>'+
										if(( jArray[0][0]["STUDENT_SSC_BOARD"] <=  jArray[0][0]["CRITERIA_SSC_BOARD"]))
										{
											boiler += '<td><a href="javascript:void(0)" class="btn btn-mini btn-success "><i class="icon icon-ok"></i></a></td>';
										}
							 			else
										{flag = 0;
											boiler += '<td><a href="javascript:void(0)" class="btn btn-mini btn-danger "><i class="icon icon-remove"></i></a></td>';
										}
									boiler += '</tr>';
							}*/
							
							//'<table>';
							//$('#btnBack').before(boiler);
							$('#eligibilityTlb'+placemenProjTrainWalktId).html(boiler);
							
							$('th#degreeCriteria').attr("rowspan",degreeCriteriaCounter);
							$('th#otherCriteria').attr("rowspan",otherCriteriaCounter);
							if(jArray[1].length > 0)
							{
								if(jArray[1][0]["specializationStatus"] == "no") // titleSpecialization status
								{
									if(jArray[1][0]["status"] == 1)
									{
										$('#btnSpecialRequest'+placemenProjTrainWalktId).hide();
							        	$('#btnRegister'+placemenProjTrainWalktId).hide();
							        	$('.trPreference').hide();
							        	
							        	var boiler='';
							        	if(jArray[1][0]["specialRequest"] == "no"){
							        		boiler+= '<br><div align="center" class="alert alert-success"><strong>You have already registered for this company</strong></div>';
							        	}else{
							        		boiler+= '<br><div align="center" class="alert alert-success"><strong>Your Special request has been successfully send</strong></div>';
							        	}
							        	$('#tddisplayMsg'+placemenProjTrainWalktId).append(boiler);
									}
									else
									{
										if(jArray[1][0]["PlaceStatus"] == "placed")
										{
											if(jArray[1][0]["dreamCompanyStatus"]){
												if(flag == 1){// Eligible For Placement
													$('#btnRegister'+placemenProjTrainWalktId).show();
													$('.trPreference').show();
												}else{// Not Eligible For Placement
													if(jArray[1][0]["specialReqStatusForNotRegistered"])
														$('#btnSpecialRequest'+placemenProjTrainWalktId).show();
												}
											}else{
												$('#btnSpecialRequest'+placemenProjTrainWalktId).hide();
									        	$('#btnRegister'+placemenProjTrainWalktId).hide();
									        	$('.trPreference').hide();
									        	
									        	var boiler='';
									        		boiler+= '<br><div align="center" class="alert alert-success"><strong>You are already Placed</strong></div>';
									        	$('#tddisplayMsg'+placemenProjTrainWalktId).append(boiler);
											}
										}
										else
										{
												if(jArray[1][0]["statusForAddResult"] != undefined && jArray[1][0]["statusForAddResult"] == "NotAdd")
												{
													$('#addSelectionProcessDialog').modal('show');
													
													postesBoiler =
														
													'<table cellpadding="0" cellspacing="0" border="1" class="table table-striped table-bordered table-condensed" id="DataTablePostResult">'+
														'<thead>'+
															'<tr>'+
																'<th>Company Name :</th>';
													
													var noOfCol=jArray[2][0]["cnt"];
													for(var i=1; i<=jArray[2][0]["cnt"]; i++){
														postesBoiler +=
															'<th>'+jArray[2][0]["name"+i+""]+'</th>';
														}
													postesBoiler +=
														'</tr></thead><tbody>';
														postesBoiler +='<tr><td>'+jArray[1][0]["lastCompanyName"]+'</td>';
														for(var m=1;m<=noOfCol;m++){
															postesBoiler +=
																'<td><input type="checkbox" id="'+jArray[2][0]["id"+m+""]+'" value="'+jArray[2][0]["id"+m+""]+'" name="chkSelection" /></td>';
														}
														postesBoiler +='</tr>';
														postesBoiler +=
															'</tbody></table>';
														postesBoiler += '<div>'+
																		'<input class="btn btn-inverse btn-mini  " type="button" value="Save" id="btnSave" onclick="saveStudentSelectionProcess('+jArray[1][0]["lastPlacementRegistrationId"]+','+flag+','+jArray[1][0]["studentPlacementId"]+','+placemenProjTrainWalktId+')">&nbsp;&nbsp;'+
																		'<input class="btn btn-inverse btn-mini  " type="button" value="Close" onclick="closeStudentSelectionProcessPopup()">'+
															
																		'</div>';
													$("#divSelectionProcess").html(postesBoiler);
													
													if(flag == 1)
													{
														// Eligible For Placement
														$('#btnRegister'+placemenProjTrainWalktId).show();
														$('.trPreference').show();
													}
													else
													{
														// Not Eligible For Placement
														if(jArray[1][0]["specialReqStatusForNotRegistered"])
															$('#btnSpecialRequest'+placemenProjTrainWalktId).show();
													}
													
												}
											else
												{
														if(flag == 1)
														{
															// Eligible For Placement
															$('#btnRegister'+placemenProjTrainWalktId).show();
															$('.trPreference').show();
														}
														else
														{
															// Not Eligible For Placement
															if(jArray[1][0]["specialReqStatusForNotRegistered"])
																$('#btnSpecialRequest'+placemenProjTrainWalktId).show();
														}
												}
										}
									}
								  }
								  else if(jArray[1][0]["specializationStatus"] == "yes")
								  { // with Specialization
									
								  }
								}
							
							// campus Pool
							if(jArray[5][0].CampusPoolStatus != undefined)
							{
								if(jArray[5][0].CampusPoolStatus == true)
								{
									$('#trCampusPool').show();
									if(jArray[5][0].noOfRegistrationAllowed <= jArray[5][0].studentRegisteredCount)
									{
										var poolBoiler = '<b>This is a Campus Pool Drive Placement. '+
										'</br> No of Registration Allowed is '+jArray[5][0].noOfRegistrationAllowed+'</br>'+
										'Registration count '+jArray[5][0].studentRegisteredCount+
										'</br></br><span style="color:red;" >Registration Count reached Maximum ! Cannot Register</span></b>';
										$('#showCampusPoolDiv').html(poolBoiler);
										$('#btnRegister'+placemenProjTrainWalktId).hide();
									}
									else
									{
										var poolBoiler = '<b>This is a Campus Pool Drive Placement. </br> No of Registration Allowed is '+jArray[5][0].noOfRegistrationAllowed+'</br>Registration count '+jArray[5][0].studentRegisteredCount+'</b>';
										$('#showCampusPoolDiv').html(poolBoiler);
										$('#btnRegister'+placemenProjTrainWalktId).show();
									}
								}
							}
							
							// dms
							if(jArray[3].jobMasterId != undefined){
								if(jArray[3].documentUploaded){
									var uploadDocument = new  UploadStudentDocument();
									uploadDocument.getDocument(jArray[3].documentCategoryId,function(jArray){
										showJobAttachment(jArray,'divDownloadAttachmentArea'+placemenProjTrainWalktId,false);
										$('#trDownloadAttachmentArea'+placemenProjTrainWalktId).show();
									});
									/**
									 * Student Upload if placement registration is done and if CV Template 
									 */
									if(jArray[1].length > 0)
									{
										if(jArray[1][0]["status"] == 1){
											initUploadCVForPlacement(placemenProjTrainWalktId,jArray[1][0].placementRegistrationId);
											$('#trUploadStudentAttachmentArea'+placemenProjTrainWalktId).show();
										}
									}
								} else {
									$('#trDownloadAttachmentArea'+placemenProjTrainWalktId).hide();
								}
							}
		},
		error : function(){
			alert(" error ");
			}
		});
	
	getPreferenceLocationNameList(placemenProjTrainWalktId,type);
	return;
}

function fetchCriteriaSpecializationWise(specTitleId)
{
	$(".registerSpecWise").hide();
	var specTitleId = $('input:radio[name=chkSpecRadio]:checked').val();
	
	$.ajax({
        url:'fetchCriteriaSpecializationWise.json',
        data:"specTitleId="+specTitleId,
        dataType:'json',
        async:false,
        beforeSend: function() {
			$('#loading').modal('show');
		},
		complete: function() {
			$('#loading').modal('hide');
		},
        success:function(jArray){
        	var flagspec = 1;
        	if(jArray.length > 0)
        	{
        		if(jArray[0] != "")
        		{
        			// Gender
        			if(jArray[0]["gender"] != "Male & Female")
        			{
        				var studGenderCriteria = $('#studentGenderTD').html();
        				$('#CompanyGenderTD').html(jArray[0]["gender"]);
            			if( studGenderCriteria == jArray[0]["gender"])
    					{
            				$('#GenderEligibilityTD').html('<a href="javascript:void(0)" class="btn btn-mini btn-success "><i class="icon icon-ok"></i></a>');
    					}
    		 			else
    					{	flagspec = 0;
    		 				$('#GenderEligibilityTD').html('<a href="javascript:void(0)" class="btn btn-mini btn-danger "><i class="icon icon-remove"></i></a>');
    					}
            			$('#GenderCriteriaTR').show();
        			}
        			else
        			{
        				$('#CompanyGenderTD').html(jArray[0]["gender"]);
        				$('#GenderEligibilityTD').html('<a href="javascript:void(0)" class="btn btn-mini btn-success "><i class="icon icon-ok"></i></a>');
        			}
        		}
        		if(jArray[0] != 0)
        		{
        			// Experiance
        			if(jArray[0]["Experience"] != "")
					{
        				var studentExperiance = $('#studentExperianceTD').html();
        				$('#companyExperienceTD').ht␀␀␀ml(jArray[0]["Experience"]);
            			if( studentExperiance == jArray[0]["Experience"])
    					{
            				$('#experienceEligibilityTD').html('<a href="javascript:void(0)" class="btn btn-mini btn-success "><i class="icon icon-ok"></i></a>');
    					}
    		 			else
    					{	flagspec = 0;
    		 				$('#experienceEligibilityTD').html('<a href="javascript:void(0)" class="btn btn-mini btn-danger "><i class="icon icon-remove"></i></a>');
    					}
						$('#experienceTR').show();
					}
        		}
        	}
        	if(flagspec == 1 && specFlag == 1)
        		$('#btnRegisterSpecWise'+specTitleId).show();
        	else
        		$('#btnRegisterSpecWise'+specTitleId).hide();
        	
        },
        error:function(){
        	alert("error in Save Registration For Placement");
       }
	});
}

function registerforPlacementSpecwise(specTitleId,placemenProjTrainWalktId,type)
{
	
	$.ajax({
        url:'registerforPlacementSpecwise.json',
        data:"placementId="+placemenProjTrainWalktId+'&specTitleId='+specTitleId+'&type='+type,
        dataType:'json',
        async:false,
        beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
        success:function(jObject){
        	$('#btnRegisterSpecWise'+specTitleId).hide();
        	$('#registerSpecWiseBtnTD'+specTitleId).html('<div align="center" class="alert alert-success"><strong>'+jObject.responseText+'</strong></div>');
        	
        	if(jObject.jobMasterId != undefined){
        		var jobMasterId = jObject.jobMasterId;
        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
        			if(responseObject.documentUploaded){
        				var uploadDocument = new  UploadStudentDocument();
						uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
							showJobAttachment(jArray,'divDownloadAttachmentArea'+placementId,false);
							$('#trDownloadAttachmentArea'+placementId).show();
							
							
							initUploadCVForPlacement(placementId,jObject.id);
							$('#trUploadStudentAttachmentArea'+placementId).show();
						});
        			} else {
        				$('#trDownloadAttachmentArea'+placementId).hide();
        				$('#trUploadStudentAttachmentArea'+placementId).hide();
        			}
        		});
        	}
        	
        	/*
        	$('#btnRegisterSpecWise'+specTitleId).hide();
        	$('.trPreference').hide();
        	$('#btnSpecialRequest'+placementId).hide();
        	
        	var boiler = '<br><br><div align="center" class="alert alert-success"><strong>'+jObject.responseText+'</strong></div>';
        	
        	$('#tddisplayMsg'+placementId).append(boiler);
        	jAlert(jObject.responseText,"Message");
        	if(jObject.responseText=='You have successfully registered for this company'){
        		askStudPreferences(2,placementId);
        		getPreferenceLocationNameList(placementId,2);
        	}
        	if(jObject.jobMasterId != undefined){
        		var jobMasterId = jObject.jobMasterId;
        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
        			if(responseObject.documentUploaded){
        				var uploadDocument = new  UploadStudentDocument();
						uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
							showJobAttachment(jArray,'divDownloadAttachmentArea'+placementId,false);
							$('#trDownloadAttachmentArea'+placementId).show();
							
							
							initUploadCVForPlacement(placementId,jObject.id);
							$('#trUploadStudentAttachmentArea'+placementId).show();
						});
        			} else {
        				$('#trDownloadAttachmentArea'+placementId).hide();
        				$('#trUploadStudentAttachmentArea'+placementId).hide();
        			}
        		});
        	}
        */},
        error:function(){
        	alert("error in Save Registration For Placement");
       }
	});
}


function registerforPlacement(internshipId,type)
{
	//alert("Projectplacement.js");
	var locationPreferenceId = new Array();
	
	if(gNoOfAllowedPreference!=0)
	{
		$('.locationNameClass').each(function(){
			if($(this).is(':checked'))
			{
				locationPreferenceId.push($(this).attr("value"));
			}
		
		});
		
		if(locationPreferenceId.length > gNoOfAllowedPreference || locationPreferenceId.length < gNoOfAllowedPreference)
		{
			jAlert("please Select only '"+gNoOfAllowedPreference+"' Preferences","warning");
			return true;
		}
	}
	
	var boiler='';
	if(type==1){
		$.ajax({
	        url:'registerforProjectPlacement.json',
	        data:"internshipId="+internshipId+'&locationPreferenceId='+locationPreferenceId,
	        dataType:'json',
	        async:false,
	        beforeSend: function() {
				$('#loading').modal('show');
			},
			complete: function() {
				$('#loading').modal('hide');
			},
	        success:function(jObject){
	        	$('#btnRegister'+internshipId).hide();
	        	$('#btnSpecialRequest'+internshipId).hide();
	        	$('.trPreference').hide();
	        	
	        	var boiler = '<br><br><div align="center" class="alert alert-success"><strong>'+jObject.responseText+'</strong></div>';
	        	
	        	$('#tddisplayMsg'+internshipId).append(boiler);
	        	jAlert(jObject.responseText,"Message");
	        	if(jObject.responseText=='You have successfully registered for this company'){
	        		askStudPreferences(1,internshipId);
		        	getPreferenceLocationNameList(internshipId,1);
	        	}
	        	
	        	
	        	
	        	if(jObject.jobMasterId != undefined){
	        		var jobMasterId = jObject.jobMasterId;
	        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
	        			if(responseObject.documentUploaded){
	        				var uploadDocument = new  UploadStudentDocument();
							uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
								showJobAttachment(jArray,'divDownloadAttachmentArea'+internshipId,false);
								$('#trDownloadAttachmentArea'+internshipId).show();
								
								
								initUploadCVForPlacement(internshipId,jObject.id);
								$('#trUploadStudentAttachmentArea'+internshipId).show();
							});
	        			} else {
	        				$('#trDownloadAttachmentArea'+internshipId).hide();
	        				$('#trUploadStudentAttachmentArea'+internshipId).hide();
	        			}
	        		});
	        	}
	        	
	        },
	        error:function(){
	        	alert("error in Save Registration For Placement");
	       }
		});
	}
	else if(type==3){
		$.ajax({
	        url:'registerforTraining.json',
	        data:"trainingId="+internshipId+'&locationPreferenceId='+locationPreferenceId,
	        dataType:'json',
	        async:false,
	        beforeSend: function() {
				$('#loading').modal('show');
			},
			complete: function() {
				$('#loading').modal('hide');
			},
	        success:function(jArray){
	        	$('#btnRegister'+internshipId).hide();
	        	$('#btnSpecialRequest'+internshipId).hide();
	        	$('.trPreference').hide();
	        	
	        	boiler+= '<br><br><div align="center" class="alert alert-success"><strong>'+jArray+'</strong></div>';
	        	
	        	$('#tddisplayMsg'+internshipId).append(boiler);
	        
	        	askStudPreferences(3,internshipId);
	        	getPreferenceLocationNameList(internshipId,3);
	        	
	        	if(jObject.jobMasterId != undefined){
	        		var jobMasterId = jObject.jobMasterId;
	        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
	        			if(responseObject.documentUploaded){
	        				var uploadDocument = new  UploadStudentDocument();
							uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
								showJobAttachment(jArray,'divDownloadAttachmentArea'+internshipId,false);
								$('#trDownloadAttachmentArea'+internshipId).show();
								
								
								initUploadCVForPlacement(internshipId,jObject.id);
								$('#trUploadStudentAttachmentArea'+internshipId).show();
							});
	        			} else {
	        				$('#trDownloadAttachmentArea'+internshipId).hide();
	        				$('#trUploadStudentAttachmentArea'+internshipId).hide();
	        			}
	        		});
	        	}
	        	
	        },
	        error:function(){
	        	alert("error in Save Registration For Placement");
	       }
		});
	}
	else if(type==4){
		$.ajax({
	        url:'registerforWalkIn.json',
	        data:"walkInId="+internshipId+'&locationPreferenceId='+locationPreferenceId,
	        dataType:'json',
	        async:false,
	        beforeSend: function() {
				$('#loading').modal('show');
			},
			complete: function() {
				$('#loading').modal('hide');
			},
	        success:function(jArray){
	        	$('#btnRegister'+internshipId).hide();
	        	$('#btnSpecialRequest'+internshipId).hide();
	        	$('.trPreference').hide();
	        	
	        	boiler+= '<br><br><div align="center" class="alert alert-success"><strong>'+jArray+'</strong></div>';
	        	
	        	$('#tddisplayMsg'+internshipId).append(boiler);
	        
	        	askStudPreferences(4,internshipId);
	        	getPreferenceLocationNameList(internshipId,4);
	        	
	        	if(jObject.jobMasterId != undefined){
	        		var jobMasterId = jObject.jobMasterId;
	        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
	        			if(responseObject.documentUploaded){
	        				var uploadDocument = new  UploadStudentDocument();
							uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
								showJobAttachment(jArray,'divDownloadAttachmentArea'+internshipId,false);
								$('#trDownloadAttachmentArea'+internshipId).show();
								
								
								initUploadCVForPlacement(internshipId,jObject.id);
								$('#trUploadStudentAttachmentArea'+internshipId).show();
							});
	        			} else {
	        				$('#trDownloadAttachmentArea'+internshipId).hide();
	        				$('#trUploadStudentAttachmentArea'+internshipId).hide();
	        			}
	        		});
	        	}
	        },
	        error:function(){
	        	alert("error in Save Registration For Placement");
	       }
		});
	}
}

function displaySpecialRequestTR(internshipId,type)
{
	$('#trSpecialRequest'+internshipId).show();
}

function makeSpecialRequest(internshipId,type)
{
	var specialRequestResoan = $('#txtSpecialRequest').val();
	
	if(type==1){
			$.ajax({
		        url:'makeSpecialRequestforProjectPlacement.json?internshipId='+internshipId+'&specialRequestResoan='+specialRequestResoan,
		        //data:"internshipId="+internshipId+"&specialRequestResoan="+specialRequestResoan,
		        dataType:'json',
		        async:false,
		        beforeSend: function() {
					$('#loading').modal('show');
				},
				complete: function() {
					$('#loading').modal('hide');
				},
		        success:function(jObject){
		        	
		        	$('#trSpecialRequest'+internshipId).hide();
		        	$('#btnSpecialRequest'+internshipId).hide();
		        	$('#btnRegister'+internshipId).hide();
		        	$('.trPreference').hide();
		        	
		        	var boiler='';
		        	//if(flag==1)
		        	//{
		        		boiler+= '<br><br><div align="center" class="alert alert-success"><strong>Your request has been successfully send</strong></div>';
		        	//}
		        	//else
		        	//{
		        	//	boiler+= '<br><br><div align="center" class="alert alert-error"><strong>You have already registered for this company</strong></div>';
		        	$('#tddisplayMsg'+internshipId).append(boiler);
		        	
		        	askStudPreferences(1,internshipId);
		        	
		        	
		        	if(jObject.jobMasterId != undefined){
		        		var jobMasterId = jObject.jobMasterId;
		        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
		        			if(responseObject.documentUploaded){
		        				var uploadDocument = new  UploadStudentDocument();
								uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
									showJobAttachment(jArray,'divDownloadAttachmentArea'+internshipId,false);
									$('#trDownloadAttachmentArea'+internshipId).show();
									initUploadCVForPlacement(internshipId,jObject.id);
									$('#trUploadStudentAttachmentArea'+internshipId).show();
								});
		        			} else {
		        				$('#trDownloadAttachmentArea'+internshipId).hide();
		        				$('#trUploadStudentAttachmentArea'+internshipId).hide();
		        			}
		        		});
		        	}
		        	
		        },
		        error:function(){
		        	alert("error in makeSpecialRequest");
		       }
			});
	}
	else if(type==3){
		$.ajax({
	        url:'makeSpecialRequestforTraining.json?trainingId='+internshipId+'&specialRequestResoan='+specialRequestResoan,
	        //data:"trainingId="+internshipId+"&specialRequestResoan="+specialRequestResoan,
	        dataType:'json',
	        async:false,
	        beforeSend: function() {
				$('#loading').modal('show');
			},
			complete: function() {
				$('#loading').modal('hide');
			},
	        success:function(jObject){
	        	
	        	$('#trSpecialRequest'+internshipId).hide();
	        	$('#btnSpecialRequest'+internshipId).hide();
	        	$('#btnRegister'+internshipId).hide();
	        	$('.trPreference').hide();
	        	
	        	var boiler='';
	        	//if(flag==1)
	        	//{
	        		boiler+= '<br><br><div align="center" class="alert alert-success"><strong>Your request has been successfully send</strong></div>';
	        	//}
	        	//else
	        	//{
	        	//	boiler+= '<br><br><div align="center" class="alert alert-error"><strong>You have already registered for this company</strong></div>';
	        	$('#tddisplayMsg'+internshipId).append(boiler);
	        	
	        	askStudPreferences(3,internshipId);
	        	
	        	
	        	if(jObject.jobMasterId != undefined){
	        		var jobMasterId = jObject.jobMasterId;
	        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
	        			if(responseObject.documentUploaded){
	        				var uploadDocument = new  UploadStudentDocument();
							uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
								showJobAttachment(jArray,'divDownloadAttachmentArea'+internshipId,false);
								$('#trDownloadAttachmentArea'+internshipId).show();
								initUploadCVForPlacement(internshipId,jObject.id);
								$('#trUploadStudentAttachmentArea'+internshipId).show();
							});
	        			} else {
	        				$('#trDownloadAttachmentArea'+internshipId).hide();
	        				$('#trUploadStudentAttachmentArea'+internshipId).hide();
	        			}
	        		});
	        	}
	        	
	        },
	        error:function(){
	        	alert("error in makeSpecialRequest");
	       }
		});
	}
	else if(type==4){
		$.ajax({
	        url:'makeSpecialRequestforWalkIn.json?walkInId='+internshipId+'&specialRequestResoan='+specialRequestResoan,
	        //data:"walkInId="+internshipId+"&specialRequestResoan="+specialRequestResoan,
	        dataType:'json',
	        async:false,
	        beforeSend: function() {
				$('#loading').modal('show');
			},
			complete: function() {
				$('#loading').modal('hide');
			},
	        success:function(flag){
	        	
	        	$('#trSpecialRequest'+internshipId).hide();
	        	$('#btnSpecialRequest'+internshipId).hide();
	        	$('#btnRegister'+internshipId).hide();
	        	$('.trPreference').hide();
	        	
	        	var boiler='';
	        	//if(flag==1)
	        	//{
	        		boiler+= '<br><br><div align="center" class="alert alert-success"><strong>Your request has been successfully send</strong></div>';
	        	//}
	        	//else
	        	//{
	        	//	boiler+= '<br><br><div align="center" class="alert alert-error"><strong>You have already registered for this company</strong></div>';
	        	$('#tddisplayMsg'+internshipId).append(boiler);
	        	
	        	askStudPreferences(4,internshipId);
	        	
	        	if(jObject.jobMasterId != undefined){
	        		var jobMasterId = jObject.jobMasterId;
	        		checkForCVTemplateUpload(jobMasterId,function(responseObject){
	        			if(responseObject.documentUploaded){
	        				var uploadDocument = new  UploadStudentDocument();
							uploadDocument.getDocument(responseObject.documentCategoryId,function(jArray){
								showJobAttachment(jArray,'divDownloadAttachmentArea'+internshipId,false);
								$('#trDownloadAttachmentArea'+internshipId).show();
								initUploadCVForPlacement(internshipId,jObject.id);
								$('#trUploadStudentAttachmentArea'+internshipId).show();
							});
	        			} else {
	        				$('#trDownloadAttachmentArea'+internshipId).hide();
	        				$('#trUploadStudentAttachmentArea'+internshipId).hide();
	        			}
	        		});
	        	}
	        },
	        error:function(){
	        	alert("error in makeSpecialRequest");
	       }
		});
	}
	
}

function askStudPreferences(jobTypeId, placTraiProjWalkId){
	
	jobType=jobTypeId;
	
	$.ajax({
        url:'getPreferencesOfJobIfAny.json',
        data:"jobType="+jobType+"&placTraiProjWalkId="+placTraiProjWalkId,
        dataType:'text',
        async:false,
        beforeSend: function() {
			$('#loading').modal('show');
		},
		complete: function() {
			$('#loading').modal('hide');
		},
        success:function(text){
        	if(text==""){
        	}
        	else{
	        	var boilerLocation="";
	        	var locations = text.split(',');
	        	
	        	$('#askPreferncesListDiv2').modal('show');
	        	
	        	for(var i=0; i<locations.length; i++){
	        		boilerLocation += '<option value="'+locations[i]+'">'+locations[i]+'</option>';
	        	}
	        	$('#selectlist').html(boilerLocation);
	        	
	        	var tempBoiler='<input type="button" class="btn btn-inverse btn-mini  " onclick="savePreferences('+jobTypeId+','+placTraiProjWalkId+')" value="Save" />';
	        	$('#divFooterSavePrefBtn').html(tempBoiler);
        	}
        },
        error:function(){
        	alert("error in fetching job preferences");
       }
	});
}

function savePreferences(jobTypeId, placTraiProjWalkId){

	var cmb1values = document.getElementById("selectlist");
	var cmb1length = cmb1values.length;
	
	var cmb2values = document.getElementById("selectedFieldList1");
	var cmb2length = cmb2values.length;
	
	if(cmb1length!=cmb2length){
		jAlert("Please select all locations");
	}
	else{
		var locationString = "";
		for(var i = 0;i<cmb2length;i++)
		{
			locationString += cmb2values.options[i].text;
			if(i != (cmb2length-1))
			{
				locationString += ",";
			}
		}
		
		$.ajax({
	        url:'saveStudLoactionPreference.json',
	        data:"locationString="+locationString+"&jobType="+jobTypeId+"&placTraiProjWalkId="+placTraiProjWalkId,
	        dataType:'text',
	        async:false,
	        beforeSend: function() {
				$('#loading').modal('show');
			},
			complete: function() {
				$('#loading').modal('hide');
			},
	        success:function(text){
	        	jAlert("Your preferences have been saved successfully");
	        	$('#askPreferncesListDiv2').modal('hide');
	        },
	        error:function(){
	        	alert("error in saving preferences");
	       }
		});
	}
}


function getPreferenceLocationNameList(placemenProjTrainWalktId,type)
{
	$.ajax({
		url : 'getPreferenceLocationNameList.json?placemenProjTrainWalktId='+placemenProjTrainWalktId+'&type='+type,
		dataType : 'json',
		type : 'GET',
		async : false,
		success : function(jArray)
		{
			if(jArray[0].length > 0)
			{
				gNoOfAllowedPreference = jArray[0][0]["noOfAllowedPreference"];
				var boiler = '<table class="table table-bordered">'+
							 '<tr><td colspan="2">Location Preferences</td></tr>'+
							 '<tr><td>No of Allowed Preferences</td><td>'+gNoOfAllowedPreference+'</td></tr>';
				for(var i=0;i<jArray[0].length;i++)
					boiler += '<tr><td><input type="checkbox" class="locationNameClass" value="'+jArray[0][i]["placementLocationPreferenceId"]+'" id="location_'+jArray[0][i]["placementLocationPreferenceId"]+'"></td>'+
							  '<td>'+jArray[0][i]["locationPreferenceName"]+'</td></tr>';
					boiler +='</table>';
							
				$("#showPreferenceList").html(boiler);
				
				if(jArray[1].length >0)
				{
					for(var i=0;i<jArray[1].length;i++)
						$('#location_'+jArray[1][i]["placementLocationPreferenceId"]).attr('checked',true);
						$('.locationNameClass').attr('disabled',true);
						$('.trPreference').show();
				}
			}
		},
		error : function() {
			jAlert("Error in Getting CPC List..", "Error");
		}
	});
}␀␀␀␀␀␀␀␀␀␀␀&␀␀␀␀␀␀jobTabs␀␀␀␀+a␀␀␀␀␀␀#display␀␀␀?`␀␀␀␀␀␀tabs␀␀␀␀␀␀␀␀␀gNoOfAllowedPreference␀␀␀␀␀␀␀␀␀␀␀getProjectPlacementsforStudent␀␀␀␀␀d␀␀␀␀␀␀ajax␀␀␀&␀␀␀␀␀␀GetProjectPlacementListforStudent.json␀␀␀␀␀6(␀␀␀␀␀␀data␀␀␀Dow␀␀␀␀␀␀dataType	␀␀␀
