		},
		error:function()
		{
			alert("Error in GetTPOPlacementList");
		}
	});
}


function fetchSelectionProcess(internshipId,type)
{
     var boilerSelection ='';
	
	boilerSelection+='<div class="ui-widget-header ui-corner-all" style="height: 30px; width: 90%; margin-left: 5%; margin-right: 5%;">'+
	'<h5 align="center" style="margin-top: 5px;">Selection process rounds</h5>'+
	'</div>';

	boilerSelection+='<br/><div align="center"><table class="table table-bordered"><tr>'+
	'<th>Sr.No</th><th>Stage</th><th>Type</th><th>Start Date</th><th>Start Time</th><th>End Date</th><th>End Time</th><th>Infrastructure</th>'+
	'</tr>';
	
	//$("div#SelectionProcessDiv"+PlaceId).html(boilerSelection);
	//$("input[name=SelectionProcess"+PlaceId+"]").attr('disabled',true);
	
	$.ajax({
		url : 'GETJobSelectionProcessById.json',
		data : "jobId=" + type,
		dataType : 'json',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(selectionProcessArray) {

			if (selectionProcessArray.length) {
				
				for ( var i = 0; i < selectionProcessArray.length; i++) {	
				    
						boilerSelection+='<tr><td style="text-align:right">'+(i+1)+'</td><td>'+selectionProcessArray[i]["selectionProcessRounds"]+'</td><td>'+selectionProcessArray[i]["type"]+'</td><td>'+selectionProcessArray[i]["eDate"]+'</td><td>'+selectionProcessArray[i]["startTime"]+'</td><td>'+selectionProcessArray[i]["edDate"]+'</td><td>'+selectionProcessArray[i]["endTime"]+'</td><td>'+selectionProcessArray[i]["infraId"]+'</td></tr>';	
				}
			  
				boilerSelection+='</table></div>';
				$("div#SelectionProcessDiv"+internshipId).html(boilerSelection);
			}
		},
		error : function() {
			jAlert("Error","Error");
			//jAlert("Error While Featching Selection Process By Job Id","Error");
		}
	});
			
}


function fetchSelectionCriteria(interWalkTrainId,type)
{
	var boiler = '<table class="table table-bordered">'+
	'<tr>'+
		'<th>HSC & SSC Criteria</th>' +
		'<td>'+
				'<table>'+
					'<tr>'+
						'<th>10th Percentage :</th>'+
						'<td><lable id="ssclbl'+interWalkTrainId+'"></lable></td>'+
					'</tr>'+
					'<tr>'+
						'<th>12th Percentage :</th>'+
						'<td><lable id="hsclbl'+interWalkTrainId+'"></lable></td>'+
					'</tr>'+
				'</table>'+
		'</td>'+
	'</tr>'+
	'<tr>'+
			'<th>Degree Criteria :</th>'+
			'<td>'+
				'<table id="tlbDegreeCriteria'+interWalkTrainId+'" class="table-bordered">'+
				'</table>'+
			'</td>'+
	'</tr>'+
	'<tr>'+
		'<td colspan="2" ><input  type="button" class="btn btn-inverse btn-mini  " value="Check Your Eligibility" onclick="checkStudentEligibility('+interWalkTrainId+','+type+')" style="position:relative;left:320px" /></td>'+
	'</tr>'+
	'<tr><tr>'+
	'<tr>'+
		'<td colspan="2">'+
			'<table id="eligibilityTlb'+interWalkTrainId+'" class="table-bordered" style="margin:auto">'+
			'</table>'+
		'</td>'+
	'</tr>'+
	'<tr style="display:none;" class="trMultiplePost" id="trMultiplePost" ><td colspan="2"><div id="showMultiplePostListDiv"></div></td></tr>'+
	'<tr style="display:none;" class="trPreference"><td colspan="2"><div id="showPreferenceList"></div></td></tr>'+
	'<tr style="display:none;" class="trCampusPool" id="trCampusPool"><td colspan="2"><div id="showCampusPoolDiv"></div></td></tr>'+
	'<tr>'+
			'<td colspan="2" id="tddisplayMsg'+interWalkTrainId+'">'+
				'<input type="button" id="btnSpecialRequest'+interWalkTrainId+'" class="btn btn-inverse btn-mini  " value="Special Request" onclick="displaySpecialRequestTR('+interWalkTrainId+','+type+')" style="display:none;position:relative;left:320px"/>'+
				'<input type="button" id="btnRegister'+interWalkTrainId+'" class="btn btn-inverse btn-mini  " value="Register" onclick="registerforPlacement('+interWalkTrainId+','+type+')" style="display:none;position:relative;left:320px"/>'+
			'</td>'+
	'</tr>'+
	'<tr id="trSpecialRequest'+interWalkTrainId+'" style="display:none;">'+
			'<td colspan="2">'+
				'<table class="table-bordered" style="margin:auto">'+
						'<tr>'+
							'<th>Special Consideration For Placement Drive :</th>'+
						'</tr>'+
						'<tr>'+
							'<td>'+
								'<textarea id="txtSpecialRequest" style="width:400px" rows="5"></textarea>'+
							'</td>'+
						'</tr>'+
						'<tr>'+
							'<td><input type="button" id="btnSaveSpecialReq" class="btn btn-inverse btn-mini  " value="Save" onclick="makeSpecialRequest('+interWalkTrainId+','+type+')" style="position:relative;left:200px" /></td>'+
						'</tr>'+
				'</table>'+
			'</td>'+
	'</tr>'+
	
	'<tr style="display:none;" id="trDownloadAttachmentArea'+interWalkTrainId+'">'+
	'<td colspan="2" >'+
		'<div class="alert alert-info" style="padding:1px;text-align:center;font-weight:bold">Download Templates</div>'+
		'<div class="well" style="padding:1px;margin-top:-18px;" id="divDownloadAttachmentArea'+interWalkTrainId+'"></div>'+
	'</td>'+
	'</tr>'+
	
	'<tr style="display:none;" id="trUploadStudentAttachmentArea'+interWalkTrainId+'">'+
	'<td colspan="2" >'+
		'<div class="alert alert-info" style="padding:1px;text-align:center;font-weight:bold">Upload / View Your CV (only .doc OR .docx files)</div>'+
		'<div class="well" style="padding:1px;margin-top:-18px;" id="divUploadAttachmentArea'+interWalkTrainId+'">'+
		'</div>'+
	'</td>'+
	'</tr>'+
'</table>';
$('div#CriteriaDiv' + interWalkTrainId).html(boiler);


$.ajax({
	url : 'fetchSscHscCriteriaCommon.json?interWalkTrainId='+interWalkTrainId+'&type='+type,
	//data : "interWalkTrainId=" + interWalkTrainId+'&type='+type,
	dataType : 'json',
	async:false,
	beforeSend : function() {
		startCommonAjaxSpinner();
	},
	complete : function() {
		stopCommonAjaxSpinner();
	},
	success : function(jArray) {
		if(jArray.length > 0)
		{
			if(jArray[0]["sscPercent"] != undefined)
				{
					$("#ssclbl"+interWalkTrainId).html(jArray[0]["sscPercent"] + " %");
				}
			else
			{
				$("#ssclbl"+interWalkTrainId).html("-");
			}
			if(jArray[0]["hscPercent"] != undefined)
				{
					$("#hsclbl"+interWalkTrainId).html(jArray[0]["hscPercent"] + " %");
				}
			else
				{
					$("#hsclbl"+interWalkTrainId).html("-");
				}
		}
		else
		{
			$("#ssclbl"+interWalkTrainId).html("-");
			$("#hsclbl"+interWalkTrainId).html("-");
		}
		
	},
	error : function() {
		jAlert("Error in job SSCHSC CRITERIA", "Error");
	}
});

showDegreeCriteria(interWalkTrainId,type);

}

function showDegreeCriteria(internshipId,type)
{
	var url = "";
	if(type==1){
		url = 'viewDegreeCriteriaForStudentProjectPlacement.json?internshipId='+internshipId;
	}else if(type==2){
		url = 'viewDegreeCriteriaForStudentPlacement.json?placementId='+internshipId;
	}else if(type == 3){
		url = 'viewDegreeCriteriaForStudentTraining.json?trainingId='+internshipId;
	}
	
	var criteriaBoiler ='';
	$.ajax({
		url : url,
		dataType : 'json',
		type:'GET',
		async : false,
		beforeSend : function() {
			startCommonAjaxSpinner();
		},
		complete : function() {
			stopCommonAjaxSpinner();
		},
		success : function(jArray) {
			if(jArray.length > 0)
			{
					 criteriaBoiler +=
						'	<tr class="header">'+
								'<th>Sr.No.</th><th>Degree Name</th><th>Percent %</th><th>Grade</th><th>Live ATKT</th><th>Dead ATKT</th>'+
						'	</tr>';
					 var temp=0;
					// var temp2=0;
							 for(var i=0;i<jArray.length;i++)
							 {
								 if(jArray[i]["idCheck"]=='parent'){
									 if(i==0)
										 {
										 criteriaBoiler +=
												'<tr class="parentDegree">'+
														'<th style="text-align:right">'+(++temp)+'</th><th>'+jArray[i]["ParentDegreeName"]+'</th><th style="text-align:right">'+jArray[i]["ParentPercent"]+'</th><th>'+jArray[i]["ParentGrade"]+'</th><th style="text-align:right">'+jArray[i]["ParentCurrentATKT"]+'</th><th style="text-align:right">'+jArray[i]["ParentDeadATKT"]+'</th>'+
														'</th>'+
												'</tr>';
										 }
									 else
										 {
										 criteriaBoiler +=
												'<tr><th colspan="6">OR</th></tr><tr class="parentDegree">'+
														'<th style="text-align:right">'+(++temp)+'</th><th>'+jArray[i]["ParentDegreeName"]+'</th><th style="text-align:right">'+jArray[i]["ParentPercent"]+'</th><th>'+jArray[i]["ParentGrade"]+'</th><th style="text-align:right">'+jArray[i]["ParentCurrentATKT"]+'</th><th style="text-align:right">'+jArray[i]["ParentDeadATKT"]+'</th>'+
													'</th>'+
												'	</tr>';
										 }
								 }
								 else{
										 criteriaBoiler +=
												'	<tr class="childDegree">'+
														'<td></td><td><center>'+jArray[i]["childDegreeName"]+'<center></td>'+
																 '<td><center>'+jArray[i]["childPercent"]+'<center></td>'+
																 '<td><center>'+jArray[i]["childGrade"]+'<center></td>'+
																 '<td><center>'+jArray[i]["childcurrentATKT"]+'<center></td>'+
																 '<td><center>'+jArray[i]["childDeadATKT"]+'<center></td>'+
												'	</tr>';
								 }
						 }
							// alert(criteriaBoiler);
			$("#tlbDegreeCriteria"+internshipId).html(criteriaBoiler);
			}
		},
		error:function(){
			jAlert("Error In Saving Degree Criteria","Error");
		}
	});	
	
	/*if(type==1){
