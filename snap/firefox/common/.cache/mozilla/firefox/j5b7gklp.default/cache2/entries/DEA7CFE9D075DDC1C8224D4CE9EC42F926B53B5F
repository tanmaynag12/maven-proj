var specFlag = 1;
function checkStudentEligibility(placemenProjTrainWalktId,type)
{
	$.ajax({
		url : 'checkEligibilityforRegistration.json?placementId='+placemenProjTrainWalktId+'&type='+type,
		//data :'placementId='+placemenProjTrainWalktId+'&type='+type,
		dataType : 'json',
		async : false,
		success : function(jArray) {
			//alert(jArray[0]["CRITERIA_OTHER_DEGREE_NAME"]); 
			//alert((jArray[0]["CRITERIA_OTHER_DEGREE_NAME"] == "Any Graduation"));
			specFlag = 1;
			var flag = 1;
			var degreeCriteriaCounter = 0;
			var boiler=''+
							'<tr>'+
							'<th></th>'+
							'<th></th>'+
							'<th>Student Marks</th>'+
							'<th>Company Criteria</th>'+
							'<th>Eligibility</th>'+
						'</tr>'+
						'<tr>'+
							'<th rowspan="3">SSC&HSC Criteria</th>'+
							'<th>10th</th>';
								if(jArray[0][0]["CRITERIA_SSC_PERCENT"] == 0)
									{
										boiler += '<td style="text-align:right">'+ jArray[0][0]["STUDENT_SSC_PERCENT"]  +'</td>'+
										'<td style="text-align:right">'+jArray[0][0]["CRITERIA_SSC_PERCENT"]+'-'+jArray[0][0]["CRITERIA_SSC_MAX_PERCENT"]+'</td>'+
										'<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
										//'<td><a href="javascript:void(0)" class="btn btn-mini btn-success "><i class="icon icon-ok"></i></a></td>';
									}
								else
									{
										boiler += '<td style="text-align:right">'+ jArray[0][0]["STUDENT_SSC_PERCENT"]  +'</td>'+
										'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_SSC_PERCENT"]  +'-'+jArray[0][0]["CRITERIA_SSC_MAX_PERCENT"]+'</td>';
										if(( eval(jArray[0][0]["STUDENT_SSC_PERCENT"] ) >= eval(jArray[0][0]["CRITERIA_SSC_PERCENT"]) && eval(jArray[0][0]["STUDENT_SSC_PERCENT"] ) <= eval(jArray[0][0]["CRITERIA_SSC_MAX_PERCENT"]) ))
											{
												boiler += '<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
											}
										else
											{flag = 0;
											specFlag = 0;
												boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
											}
									}
							boiler +='</tr>'+
							 '<tr>'+
							 '<th>12th / Diploma </th>';
							 		if(jArray[0][0]["CRITERIA_HSC_PERCENT"] == 0)
							 		{
							 			boiler +=	'<td style="text-align:right">'+ jArray[0][0]["STUDENT_HSC_PERCENT"]  +'</td>'+
							 						'<td style="text-align:right">'+jArray[0][0]["CRITERIA_HSC_PERCENT"]+'-'+jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]+'</td>'+
							 						'<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
							 		}
							 		else
							 		{
							 			if(jArray[0][0]["STUDENT_HSC_PERCENT"] == 0 && jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] == 0)
							 			{
							 				boiler +=	'<td>Not Specified</td>'+
		 									'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_HSC_PERCENT"]  +'-'+jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]+'</td>';
											flag = 0;
											specFlag = 0;
												boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
											
							 			}
							 			else if(jArray[0][0]["STUDENT_HSC_PERCENT"] == 0 && jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] != 0)
							 			{
							 				boiler +=	'<td style="text-align:right">'+ jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"]  +'</td>'+
		 									'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_HSC_PERCENT"]  +'-'+jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]+'</td>';
		 									//'<td>'+ ( eval(jArray[0]["STUDENT_HSC_PERCENT"] ) > eval(jArray[0]["CRITERIA_HSC_PERCENT"]) ) +'</td>';
								 			if(( eval(jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] ) >= eval(jArray[0][0]["CRITERIA_HSC_PERCENT"]) && eval(jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] ) <= eval(jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"])))
											{
												boiler += '<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
											}
								 			else
											{flag = 0;
											specFlag = 0;
												boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
											}
							 			}
							 			else if(jArray[0][0]["STUDENT_HSC_PERCENT"] != 0 && jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] == 0)
							 			{
							 				boiler +=	'<td style="text-align:right">'+ jArray[0][0]["STUDENT_HSC_PERCENT"]  +'</td>'+
		 									'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_HSC_PERCENT"]  +'-'+ jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]  +'</td>';
		 									//'<td>'+ ( eval(jArray[0]["STUDENT_HSC_PERCENT"] ) > eval(jArray[0]["CRITERIA_HSC_PERCENT"]) ) +'</td>';
								 			if(( eval(jArray[0][0]["STUDENT_HSC_PERCENT"] ) >= eval(jArray[0][0]["CRITERIA_HSC_PERCENT"]) & eval(jArray[0][0]["STUDENT_HSC_PERCENT"] ) <= eval(jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"])))
											{
												boiler += '<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
											}
								 			else
											{flag = 0;
											specFlag = 0;
												boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
											}
							 			}
							 			else if(jArray[0][0]["STUDENT_HSC_PERCENT"] != 0 && jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] != 0)
							 			{
							 				if(jArray[0][0]["STUDENT_HSC_PERCENT"] > jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"])
							 				{
							 					boiler +=	'<td style="text-align:right">'+ jArray[0][0]["STUDENT_HSC_PERCENT"]  +'</td>'+
			 									'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_HSC_PERCENT"]  +'-'+ jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]  +'</td>';
			 									//'<td>'+ ( eval(jArray[0]["STUDENT_HSC_PERCENT"] ) > eval(jArray[0]["CRITERIA_HSC_PERCENT"]) ) +'</td>';
									 			if(( eval(jArray[0][0]["STUDENT_HSC_PERCENT"] ) >= eval(jArray[0][0]["CRITERIA_HSC_PERCENT"]) && eval(jArray[0][0]["STUDENT_HSC_PERCENT"] ) <= eval(jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"])))
												{
													boiler += '<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
												} 
									 			else
												{flag = 0;
												specFlag = 0;
													boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
												}
							 				}
							 				else
							 				{
							 					boiler +=	'<td style="text-align:right">'+ jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"]  +'</td>'+
			 									'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_HSC_PERCENT"]  +'-'+ jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]  +'</td>';
			 									//'<td>'+ ( eval(jArray[0]["STUDENT_HSC_PERCENT"] ) > eval(jArray[0]["CRITERIA_HSC_PERCENT"]) ) +'</td>';
									 			if(( eval(jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] ) >= eval(jArray[0][0]["CRITERIA_HSC_PERCENT"]) && eval(jArray[0][0]["STUDENT_DIPLOMA_PERCENTAGE"] ) <= eval(jArray[0][0]["CRITERIA_HSC__MAX_PERCENT"]) ))
												{
													boiler += '<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
												}
									 			else
												{flag = 0;
												specFlag = 0;
													boiler += '<td style="text-align:center;" ><img src="css/images/not.png"></td>';
												}
							 				}
							 			}
							 		}
							boiler +=	'</tr>'+
										'<tr>';
											'<th>Diploma</th>'+
											'<td></td>'+
											'<td></td>'+
											'<td></td>';
							
											
							degreeCriteriaCounter++;
							boiler +='</tr>'+
									'<tr>'+
									'<th id="degreeCriteria">Degree Criteria</th>'+
							
									'<th>'+jArray[0][0]["STUDENT_CURRENT_DEGTREE"]+'</th>'+
									'<td style="text-align:right">'+ jArray[0][0]["STUDENT_CURRENT_PERCENTAGE"]  +'</td>'+
									'<td style="text-align:right">'+ jArray[0][0]["CRITERIA_CURRENT_MIN_PERCENT"]  +'-'+ jArray[0][0]["CRITERIA_CURRENT_MAX_PERCENT"]  +'</td>';
									//'<td>'+ ( eval(jArray[0]["STUDENT_CURRENT_PERCENTAGE"] ) >= eval(jArray[0]["CRITERIA_CURRENT_MIN_PERCENT"]) ) +'</td>';
									if(jArray[0][0]["CRITERIA_CURRENT_MIN_PERCENT"] != "-"){
							       if(( eval(jArray[0][0]["STUDENT_CURRENT_PERCENTAGE"] ) >= eval(jArray[0][0]["CRITERIA_CURRENT_MIN_PERCENT"]) && eval(jArray[0][0]["STUDENT_CURRENT_PERCENTAGE"] ) <= eval(jArray[0][0]["CRITERIA_CURRENT_MAX_PERCENT"])))
									{
										boiler += '<td style="text-align:center;" ><img src="css/images/yes.png"></td>';
									}
						 			else
