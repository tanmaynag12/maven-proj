function jobTabs(i) {
	$("#display" + i).tabs();
}
var gNoOfAllowedPreference=0;
function getProjectPlacementsforStudent(type)
{
	$.ajax({
		url:'GetProjectPlacementListforStudent.json',
		data:'',
		dataType:'json',
		async:false,
		beforeSend: function() {
			$('#loading').modal('show');
		},
		complete: function() {
			$('#loading').modal('hide');
		},
		success:function(jArray)
		{
			if(jArray.length == 0)
			{
				var viewJobsBoiler = '<div id="noJobs" class="alert alert-error">No Project Placements Available</div>';
				$("#placementListDivCommon").html(viewJobsBoiler);
			}
		else
			{
					var placementBoiler = '';
					placementBoiler += '<br/><div id="accordion";width:80%;margin-left:10%;margin-right:10%;>';
					for(var i=0;i<jArray.length;i++)
					{
						placementBoiler += '<h6><a href="#" onclick="jobTabs(\''+i+'\')"><table><tr><td>'+jArray[i]["post"]+' at </td>'+
									  	  '<td>'+jArray[i]["companyName"]+ '</td><td> on '+jArray[i]["expiryDate"]+'</td></tr></table></a></h6>'+
											  
											  '<div id="display'+i+'">'+
											  '<ul>'+
													'<li><a href="#tabs-1" onclick="">Job Description</a></li>'+
													'<li><a href="#tabs-2" onclick="fetchSelectionProcess(\''+jArray[i]["projectPlacementId"]+'\',\''+jArray[i]["jobMasterId"]+'\')">Selection Process</a></li>'+
													'<li><a href="#tabs-3" onclick="fetchSelectionCriteria(\''+ jArray[i]["projectPlacementId"]+'\',\''+type+'\')">Selection Criteria</a></li>'+
													/*'<li><a href="#tabs-8" onclick="getData(\''+jArray[i]["placementId"]+'\')">Data</a></li>'+*/
												'</ul>'+
												'<div id="tabs-1">' +
												'<table id="tbl" style="text-align:left;" class="table table-bordered">'+
														'<tr><th>Company Name :</th><td>'+jArray[i]["companyName"]+'</td>'+
															'<th>Post Opened :</th><td>'+jArray[i]["post"]+'</td>'+
														'</tr>'+
														'<tr><th>Year of Passing :</th><td>'+jArray[i]["passingYear"]+'</td>'+
															'<th>Posting Place :</th><td>'+jArray[i]["postingPlace"]+'</td>'+
														'</tr>'+
														'<tr><th>Salary :</th><td>'+jArray[i]["offeredSalary"]+'</td>'+
															'<th>Bond (In Months):</th><td>'+jArray[i]["bond"]+'</td>'+
														'</tr>'+
														'<tr><th>Total Vacancies :</th><td>'+jArray[i]["vacancies"]+'</td>'+
															'<th>Documents Needed :</th><td>'+jArray[i]["documentNeeded"]+'</td>'+
														'</tr>'+
														'<tr><th>Experience (In Months):</th><td>'+jArray[i]["experience"]+'</td>'+
															'<th>Experience Details:</th><td>'+jArray[i]["experienceDetails"]+'</td>'+
														'</tr>'+
														'<tr><th>Job Type :</th><td>'+jArray[i]["jobOrIntern"]+'</td>'+
															'<th>Expiry Date :</th><td>'+jArray[i]["expiryDate"]+'</td>'+
														'</tr>'+
														'<tr>'+
															/*'<th>Website :</th><td>'+jArray[i]["jobMasterObj"]["website"]+'</td>'+*/
															'<th>Age Criteria :</th><td>'+(jArray[i]["ageCriteria"]==undefined?"-":jArray[i]["ageCriteria"])+'</td>'+
															'<th>Gender :</th><td>'+jArray[i]["gender"]+'</td>'+
														'</tr>'+
														'<tr><th>Language Criteria :</th><td>'+jArray[i]["languageCriteria"]+'</td>'+
															'<th>Job Profile :</th><td>'+jArray[i]["jobProfile"]+'</td>'+
														'</tr>'+
														'<tr><th>Company Website :</th><td>'+jArray[i]["companyWebsite"]+'</td>'+
														'<th>Remark :</th><td>'+jArray[i]["remark"]+'</td>'+
													'</tr>'+
														'<tr>'+
															'<th>Applicant :</th><td>'+jArray[i]["applicant"]+'</td>'+
															'<th>location :</th><td>'+jArray[i]["location"]+'</td>'+
														'</tr>';
								
													placementBoiler += '<tr><th>securityDeposite :</th><td>'+jArray[i]["securityDeposite"]+'</td>'+
														'<th>method :</th><td>'+jArray[i]["method"]+'</td>'+
														   '</tr>'+
															'<tr><th>Registration Date :</th><td colspan="3">'+jArray[i]["registrationStartDate"]+' - '+jArray[i]["registrationEndDate"]+'</td>'+
														   	'</tr>'+
														   	'<tr><th>Registration Time :</th><td colspan="3">'+jArray[i]["registrationStartTime"]+' - '+jArray[i]["registrationEndTime"]+'</td>'+
														   	'</tr>'+
														   	'<tr><th>joiningDate :</th><td>'+jArray[i]["joiningDate"]+'</td>'+
														   	'</tr>'+
													'</table>'+
													'</div>'+ 
													'<div id="tabs-2">'+
															'<div id="SelectionProcessDiv'+jArray[i]["projectPlacementId"] + '"></div>'+
													'</div>'+ 
													'<div id="tabs-3">'+
															'<div id="CriteriaDiv'+jArray[i]["projectPlacementId"]+'"></div>'+
													'</div>';
											placementBoiler += '</div>';
						}
					placementBoiler += '</div>';
					$("#placementListDivCommon").html(placementBoiler);
					$("#accordion").accordion({
						active : -1,
						autoHeight : true,
						collapsible : true
					});
					$("#display0").tabs();
			}
		},
		error:function()
		{
			alert("Error in GetTPOPlacementList");
		}
	});
}


function getTrainingforStudent(type)
{
	$.ajax({
		url:'GetTrainingListforStudent.json',
		data:'',
		dataType:'json',
		async:false,
		beforeSend: function() {
			$('#loading').modal('show');
		},
		complete: function() {
			$('#loading').modal('hide');
		},
		success:function(jArray)
		{
			if(jArray.length == 0)
			{
				var viewJobsBoiler = '<div id="noJobs" class="alert alert-error" >No Training Available</div>';
				$("#placementListDivCommon").html(viewJobsBoiler);
			}
		else
			{
					var placementBoiler = '';
					placementBoiler += '<br/><div id="accordion";width:80%;margin-left:10%;margin-right:10%;>';
					for(var i=0;i<jArray.length;i++)
					{
						placementBoiler += '<h6><a href="#" onclick="jobTabs(\''+i+'\')"><table><tr><td>'+jArray[i]["post"]+' at </td>'+
									  	  '<td>'+jArray[i]["companyName"]+ '</td><td> on '+jArray[i]["expiryDate"]+'</td></tr></table></a></h6>'+
											  
											  '<div id="display'+i+'">'+
											  '<ul>'+
													'<li><a href="#tabs-1" onclick="">Job Description</a></li>'+
													'<li><a href="#tabs-2" onclick="fetchSelectionProcess(\''+jArray[i]["trainingId"]+'\',\''+type+'\')">Selection Process</a></li>'+
													'<li><a href="#tabs-3" onclick="fetchSelectionCriteria(\''+ jArray[i]["trainingId"]+'\',\''+type+'\')">Selection Criteria</a></li>'+
													/*'<li><a href="#tabs-8" onclick="getData(\''+jArray[i]["placementId"]+'\')">Data</a></li>'+*/
												'</ul>'+
												'<div id="tabs-1">' +
												'<table id="tbl" style="text-align:left;" class="table table-bordered">'+
														'<tr><th>Company Name :</th><td>'+jArray[i]["companyName"]+'</td>'+
															'<th>Post Opened :</th><td>'+jArray[i]["post"]+'</td>'+
														'</tr>'+
														'<tr><th>Year of Passing :</th><td>'+jArray[i]["passingYear"]+'</td>'+
															'<th>Posting Place :</th><td>'+jArray[i]["postingPlace"]+'</td>'+
														'</tr>'+
														'<tr><th>Salary :</th><td>'+jArray[i]["offeredSalary"]+'</td>'+
															'<th>Bond (In Months):</th><td>'+jArray[i]["bond"]+'</td>'+
														'</tr>'+
														'<tr><th>Total Vacancies :</th><td>'+jArray[i]["vacancies"]+'</td>'+
															'<th>Documents Needed :</th><td>'+jArray[i]["documentNeeded"]+'</td>'+
														'</tr>'+
														'<tr><th>Experience (In Months):</th><td>'+jArray[i]["experience"]+'</td>'+
															'<th>Experience Details:</th><td>'+jArray[i]["experienceDetails"]+'</td>'+
														'</tr>'+
														'<tr><th>Job Type :</th><td>'+jArray[i]["jobOrIntern"]+'</td>'+
															'<th>Expiry Date :</th><td>'+jArray[i]["expiryDate"]+'</td>'+
														'</tr>'+
														'<tr>'+
															/*'<th>Website :</th><td>'+jArray[i]["jobMasterObj"]["website"]+'</td>'+*/
															'<th>Age Criteria :</th><td>'+(jArray[i]["ageCriteria"]==undefined?"-":jArray[i]["ageCriteria"])+'</td>'+
															'<th>Gender :</th><td>'+jArray[i]["gender"]+'</td>'+
														'</tr>'+
														'<tr><th>Language Criteria :</th><td>'+jArray[i]["languageCriteria"]+'</td>'+
															'<th>Job Profile :</th><td>'+jArray[i]["jobProfile"]+'</td>'+
														'</tr>'+
														'<tr><th>Company Website :</th><td>'+jArray[i]["companyWebsite"]+'</td>'+
														'<th>Remark :</th><td>'+jArray[i]["remark"]+'</td>'+
													'</tr>'+
														'<tr>'+
															'<th>Applicant :</th><td>'+jArray[i]["applicant"]+'</td>'+
															'<th>location :</th><td>'+jArray[i]["location"]+'</td>'+
														'</tr>';
								
													placementBoiler += '<tr><th>securityDeposite :</th><td>'+jArray[i]["securityDeposite"]+'</td>'+
														'<th>method :</th><td>'+jArray[i]["method"]+'</td>'+
														   '</tr>'+
														   '<tr><th>Registration Date :</th><td colspan="3">'+jArray[i]["registrationStartDate"]+' - '+jArray[i]["registrationEndDate"]+'</td>'+
														   	'</tr>'+
														   	'<tr><th>Registration Time :</th><td colspan="3">'+jArray[i]["registrationStartTime"]+' - '+jArray[i]["registrationEndTime"]+'</td>'+
														   	'</tr>'+
														   	'<tr><th>joiningDate :</th><td>'+jArray[i]["joiningDate"]+'</td>'+
														   	'</tr>'+
													'</table>'+
													'</div>'+ 
													'<div id="tabs-2">'+
															'<div id="SelectionProcessDiv'+jArray[i]["trainingId"] + '"></div>'+
													'</div>'+ 
													'<div id="tabs-3">'+
															'<div id="CriteriaDiv'+jArray[i]["trainingId"]+'"></div>'+
													'</div>';
											placementBoiler += '</div>';
						}
					placementBoiler += '</div>';
					$("#placementListDivCommon").html(placementBoiler);
					$("#accordion").accordion({
						active : -1,
						autoHeight : true,
						collapsible : true
					});
					$("#display0").tabs();
			}
		},
		error:function()
		{
			alert("Error in GetTPOPlacementList");
		}
	});
}


function getWalkInforStudent(type)
{
	$.ajax({
		url:'GetWalkInListforStudent.json',
		data:'',
		dataType:'json',
		async:false,
		beforeSend: function() {
			$('#loading').modal('show');
		},
		complete: function() {
			$('#loading').modal('hide');
		},
		success:function(jArray)
		{
			if(jArray.length == 0)
			{
				var viewJobsBoiler = '<div id="noJobs" class="alert alert-error" >No Training Available</div>';
				$("#placementListDivCommon").html(viewJobsBoiler);
			}
		else
			{
					var placementBoiler = '';
					placementBoiler += '<br/><div id="accordion";width:80%;margin-left:10%;margin-right:10%;>';
					for(var i=0;i<jArray.length;i++)
					{
						placementBoiler += '<h6><a href="#" onclick="jobTabs(\''+i+'\')"><table><tr><td>'+jArray[i]["post"]+' at </td>'+
									  	  '<td>'+jArray[i]["companyName"]+ '</td><td> on '+jArray[i]["expiryDate"]+'</td></tr></table></a></h6>'+
											  
