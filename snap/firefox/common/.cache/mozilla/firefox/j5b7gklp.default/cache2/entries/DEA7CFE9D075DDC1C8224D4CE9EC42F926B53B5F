function getWalkInforStudent(type)
{
	$.ajax({
		url:'GetWalkInListforStudent.json',
		data:'',
		dataType:'json',
		async:false,
		beforeSend: function() {
			$('#loading').modal('show');
		},
		complete: function() {
			$('#loading').modal('hide');
		},
		success:function(jArray)
