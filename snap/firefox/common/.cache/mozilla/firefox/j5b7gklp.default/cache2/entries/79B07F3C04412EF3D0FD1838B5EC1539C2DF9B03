		 <script>  
        function capitalize(textboxid, str) {
            // string with alteast one character
            if (str && str.length >= 1)
            {       
                var firstChar = str.charAt(0);
                var remainingStr = str.slice(1);
                if($('#customerHelper').val()=="HPTU_SERVER" || $('#customerHelper').val()=="ATAL_MEDICAL_SERVER")
                	str = firstChar.toUpperCase() + remainingStr.toUpperCase();
                else
                	str = firstChar.toUpperCase() + remainingStr.toLowerCase();
            }
            document.getElementById(textboxid).value = str;
        }

    </script>  
	<script type="text/javascript">
	
		var paramVal = getURLParameter('error');
		//alert("paramVal: "+paramVal);
		if(paramVal.toLowerCase() == 'true'){
			var username = getUsernameCookie();
			//alert(username);
			if(username != null &&  username != ''){
				alert('In if');
				window.location.replace('checkUserAccountLocked.json?username='+username);
			}
		}
		
