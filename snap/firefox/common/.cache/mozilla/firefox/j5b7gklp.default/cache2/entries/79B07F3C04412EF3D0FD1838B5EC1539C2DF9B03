				dataType:'json',
				type:'POST',
				async:false,
				beforeSend: function() {
					$('.checkemailpreview').addClass('loading');
				},
				complete: function() {
					$('.checkemailpreview').removeClass('loading');
				},
				success:function(jObject){
						if(jObject.status){
							if(!jObject.AdharCardNoStatus){
							
							}
							else{
								swal("You have already registerd with this Aadhar no contact DEAN ADMISSION");
							}
							if(!jObject.DteApplicationNumberStatus){
								}
								else{
									swal("You have already registerd with this DTE no contact DEAN ADMISSION");
								}
						}
						else{
							
						}
						
				},
				error:function(){
					//jAlert("in Error Admission Type CMB","Error");
				}
			});
	 
}



//function for get all courses
var gFetchCampusInstCourses = null;
function fetchInstituteCourseStrengthAll(){
	var urlDomainName = window.location.host;
	var customerId = 0;
		
	if(urlDomainName=="kjedu.in")
		customerId="db32227ae3fc2517e96ad2a3eed93d62";
	else	
		customerId=$('#customerId').val();
	
	var instituteStrengthId = $('#instituteStrengthId').val();
	
	$.ajax({
		url:'fetchComboCampusInstituteCourseForApplicantAdmission.json?customerId='+customerId,
		headers:{'security-helper-prop':true},
		dataType:'json',
		success:function(jArray){
			gFetchCampusInstCourses = jArray; 
			var boiler="";
			if($('#customerHelper').val()=="MGMU_SERVER"){
				 boiler += '<option value="0" roundStructureId="0" >-- Select Programme --</option>';
			}else{
				 boiler += '<option value="0" roundStructureId="0" >-- Select Course --</option>';
			}
			for(var k in jArray){
				for(var a in jArray[k].institutes){
					var jArrCourse = jArray[k].institutes[a].courses;
					for(var v in jArray[k].institutes[a].courses){
						boiler += '<option courseType="'+jArrCourse[v]["courseType"]+'"  courseName="'+jArrCourse[v]["cNm"]+'" instituteCourseId="'+jArrCourse[v]["instCoId"]+'" roundStructureId="'+jArrCourse[v]["rsId"]+'" value="'+jArrCourse[v]["icsId"]+'" >'+jArrCourse[v]["cNm"]+' '+jArrCourse[v]["rsNm"]+'</option>';
					}
				}
			}
			$('#selInstituteCourseStrength').html(boiler);
			
			if(instituteStrengthId!=undefined && instituteStrengthId!=null && instituteStrengthId!=''){
				$('#selInstituteCourseStrength').val(instituteStrengthId);
				
				
					//$('#selInstituteCourseStrength').attr("disabled",true);
					disableComboCommon('selInstituteCourseStrength');
				fetchCampusByCourseId();
			}else{
				fetchCampusByCourseId();
				$('#selInstituteCourseStrength').attr("disabled",true);
			}
			
		},
		error:function(){
			//jAlert("in Error Admission Type CMB","Error");
		}
	});
	
	if($('#customerHelper').val()=="NITIE_SERVER"){
		setTimeout(function(){
			var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
			if(courseName == 'PGDIE' || courseName == 'PGDSM'){
				$('#candSponsSpan').show();
			}else if(courseName == 'Doctoral Level Fellowship'){
				$('#candSponsSpan').show();
				$('#candInterSpan').hide();
			}else{
				$('#candSponsSpan').hide();
			}		
		},300);
	}
	
}

function resetTextBoxes(){
	
	$('#applicantRegistrationDiv input[type="text"]').val('')
}

function fetchCampusByCourseId(){
	if(gFetchCampusInstCourses!=null){
		var campusId = $('#campusId').val();
		
		$('#cmbCampus').html('');
		var boiler = '';
		var jArray = gFetchCampusInstCourses;
		var courseId = $('#selInstituteCourseStrength').val();
		for(var k in jArray){
			for(var a in jArray[k].institutes){
				var jArrCourse = jArray[k].institutes[a].courses;
				for(var v in jArray[k].institutes[a].courses){
					if(jArrCourse[v]["icsId"]==courseId){
						boiler = '<option trustAbbr="'+jArray[k]["trustAbbr"]+'" value="'+jArray[k]["campId"]+'" >'+jArray[k]["campName"]+'</option>';
						$('#cmbCampus').append(boiler);
						$('#cmbCampus option:last').data('institutes', jArray[k]["institutes"][a]);
					}
				}
			}
		}
		
		if(jArray.length>0){
			if(campusId!=undefined && campusId!=null && campusId!=''){
				
				$('#cmbCampus').val(campusId);
				
				
					//$('#cmbCampus').attr("disabled",true);
					disableComboCommon('cmbCampus');
				
				fetchInstituteListForMats();
			}else{
				fetchInstituteListForMats();
				$('#cmbCampus').attr("disabled",true);
			}
		}
	}
}

function fetchInstituteListForMats(){
	var instituteId = $("#instituteId").val();
	
	var instiArray = $('#cmbCampus option:selected').data('institutes');
	$('#cmbInstitute').html('');
	if(instiArray!=undefined){
		var boiler = '<option value="0">-- Select Institute --</option>';
		$('#cmbInstitute').append(boiler);
		//for(var i in instiArray){
			boiler = '<option instAbbr="'+instiArray["iAbbr"]+'" value="'+instiArray["iId"]+'" >'+instiArray["iName"]+'</option>';
			$('#cmbInstitute').append(boiler);
			$('#cmbInstitute option:last').data('courses', instiArray["courses"]);
		//}
		
		if(instituteId!=undefined && instituteId!=null && instituteId!=''){
			$('#cmbInstitute').val(instituteId);
			
				//$('#cmbInstitute').attr("disabled",true);
				disableComboCommon('cmbInstitute');
			
		}else{
			$('#cmbInstitute').attr("disabled",true);
		}
	}
}

function setSelectedProg(){
	
	setTimeout(function(){
		
	var selProg = $('#selInstituteCourseStrength option:selected').text();
	var selCampus = $('#cmbCampus option:selected').text();
	var selinst = $('#cmbInstitute option:selected').text();
	
	$('#departmentNamelbl').text(selProg);
	$('#campuslbl').text(selCampus);
	$('#institutelbl').text(selinst);
 	
	if($('#customerHelper').val()=='NITIE_SERVER'){
		 getAdmissionTypeByStrength()
	 }	
	if($('#bscId').val()!=null && ($('#customerHelper').val()=='XLRI_SERVER'||$('#customerHelper').val()=='LOCAL_SERVER')){
		 getAdmissionTypeByBatchCapacityId()
	 }
	},1000);
	
}

function dteChangeRegistrationNo(){
	if($('#customerHelper').val() == "RCOEM_SERVER"){
		
		setTimeout(function(){
		var courseNameHolder= $('#selInstituteCourseStrength option:selected').attr('courseName');
		
		 if(courseNameHolder == 'First Year B. Tech.'){
			 $('#Dteno').attr("placeholder", 'STATE CET CELL Registration Number (Ex:EN23104382)');
		 }else if(courseNameHolder == 'MBA'){
			 $('#Dteno').attr("placeholder", 'STATE CET CELL Registration Number (Ex:MB23104382)');
		 }else if(courseNameHolder == 'MCA'){
			 $('#Dteno').attr("placeholder", 'STATE CET CELL Registration Number (Ex:MC23104382)');
		 }
		},500);
	}
}
