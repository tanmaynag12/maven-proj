<script type="text/javascript">
function disableComboCommon(cmb){
	
		$('#'+cmb).attr("disabled",true);
	
}
function fetchCampusName(){
	
    
    if($('#customerHelper').val()=="HPTU_SERVER" || $('#customerHelper').val()=="ATAL_MEDICAL_SERVER"){
    	$('#lastName').hide();
    }
	var urlDomainName = window.location.host;
	var customerId = 0;
		
	if(urlDomainName=="kjedu.in")
		customerId="db32227ae3fc2517e96ad2a3eed93d62";
	else	
		customerId=$('#customerId').val();
	
	
	
	if($('#customerHelper').val()=="RAMAIAH_SERVER"){
		customerId="3d4b02921bef355e733552a30241be1c";
	}
	if($('#customerHelper').val()=="RAMAIAH_UNIVERSITY_SERVER"){
			customerId="fe5b31e6f6e7ed9c86833a85efa904f5";
		}
	
	if($('#customerHelper').val()=="XISS_SERVER"){
		if(window.location.toString().indexOf("erp.xiss.ac.in") != -1){
			customerId="3d4b02921bef355e733552a30241be1c";
		}
		else {
			customerId="fe5b31e6f6e7ed9c86833a85efa904f5";
		}
	}
	
	if($('#customerHelper').val()=="JDGROUP_SERVER"){
		if(window.location.toString().indexOf("erp.jdcoem.ac.in") != -1){
			customerId="3d4b02921bef355e733552a30241be1c";
		}
		else {
			customerId="fe5b31e6f6e7ed9c86833a85efa904f5";
		}
	}
	if($('#customerHelper').val()=="MIT_SERVER"){
		customerId="3d4b02921bef355e733552a30241be1c";
	}
	if($('#customerHelper').val()=="VCET_SERVER"){
		customerId="fe5b31e6f6e7ed9c86833a85efa904f5";
	}
	if($('#customerHelper').val()=="MANDSAUR_SERVER"){
		customerId="6759d798d7e0e2f6d2b133ccc98f2573";
	}
	if($('#customerHelper').val()=="ICT_MUMBAI_SERVER"){
		customerId="fe5b31e6f6e7ed9c86833a85efa904f5";
	}
	if($('#customerHelper').val()=="CMR_SERVER"){
		if(window.location.toString().indexOf("cmr.edu.in") != -1){
			customerId="3d4b02921bef355e733552a30241be1c";
		}else if(window.location.toString().indexOf("edufice.net") != -1){
			customerId="3d4b02921bef355e733552a30241be1c";
		}
		else {
			customerId="fe5b31e6f6e7ed9c86833a85efa904f5";
		}
	}
	
	
	var campusId = $('#campusId').val();
	
	if($('#customerHelper').val()=="MGMU_SERVER"){
		LinkUrl = "anon_fetchAdmisiionComboDataForMGM.json";
	}else{
		LinkUrl = "fetchComboCampusInstituteCourseForApplicantAdmission.json";
	}
	
	$.ajax({
		url: LinkUrl+'?customerId='+customerId,
		headers:{'security-helper-prop':true},
		dataType:'json',
		success:function(jArray){
			$('#cmbCampus').html('');
			var boiler = '';
			for(var i in jArray){
				boiler = '<option trustAbbr="'+jArray[i]["trustAbbr"]+'" value="'+jArray[i]["campId"]+'" >'+jArray[i]["campName"]+'</option>';
				$('#cmbCampus').append(boiler);
				$('#cmbCampus option:last').data('institutes', jArray[i]["institutes"]);
			}
			
			/* for(var i=0;i<jArray.length;i++){
				boiler += '<option trustAbbr="'+jArray[i]["trustAbbr"]+'" value="'+jArray[i]["campusId"]+'" >'+jArray[i]["campusName"]+'</option>';
			}
			$('#cmbCampus').html(boiler); */
			
			if(jArray.length>0){
				if(campusId!=undefined && campusId!=null && campusId!=''){
					
					$('#cmbCampus').val(campusId);
					
					
						//$('#cmbCampus').attr("disabled",true);
						disableComboCommon('cmbCampus');
					
					fetchInstituteListWithoutCall();
				}else{
					fetchInstituteListWithoutCall();
					$('#cmbCampus').attr("disabled",true);
				}
			}
			if($('#customerHelper').val()=="BML_SERVER"){	
					popforbml();
			}
		},
		error:function(){
			//jAlert("in Error Admission Type CMB","Error");
		}
	});
	if($('#customerHelper').val()=="NITIE_SERVER"){
		setTimeout(function(){
			var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
			if(courseName == 'PGDIE' || courseName == 'PGDSM'){
				$('#candSponsSpan').show();
			}else if(courseName == 'Doctoral Level Fellowship'){
				$('#candSponsSpan').show();
				$('#candInterSpan').hide();
			}else{
				$('#candSponsSpan').hide();
			}		
		},300);
	}
}
function fetchInstituteListWithoutCall(){
	var instituteId = $("#instituteId").val();
	if(($('#customerHelper').val()!="DYPATIL") && (instituteId=="ea8c174eb75cdb45fb4592f4665e5d41" ||instituteId=="81f01c10e8dd1ee4ef4e9b16729dc36b" ||instituteId=="0e23d151567edee3f8b2223a22f24d7c" ) ){
		$('#customerLogo').attr("src", "img/dypnewlogo1.png");
		$('#customerLogo').attr("width", "441px");
		$('#customerLogo').attr("height", "400px");
	}
	if($('#customerHelper').val()=="XLRI_SERVER"){
		 $('#customerLogo').attr("width", "441px");
	}
	var instiArray = $('#cmbCampus option:selected').data('institutes');
	$('#cmbInstitute').html('');
	if(instiArray!=undefined){
		var boiler = '';
		if($('#customerHelper').val()!="HPTU_SERVER" &&  $('#customerHelper').val()!="ATAL_MEDICAL_SERVER"){
			boiler+='<option value="0">-- Select Institute --</option>';
		}
		$('#cmbInstitute').append(boiler);
		for(var i in instiArray){
			boiler = '<option instAbbr="'+instiArray[i]["iAbbr"]+'" value="'+instiArray[i]["iId"]+'" >'+instiArray[i]["iName"]+'</option>';
			$('#cmbInstitute').append(boiler);
			$('#cmbInstitute option:last').data('courses', instiArray[i]["courses"]);
		}
		
		if(instituteId!=undefined && instituteId!=null && instituteId!=''){
			$('#cmbInstitute').val(instituteId);
			
				//$('#cmbInstitute').attr("disabled",true);
				disableComboCommon('cmbInstitute');
			
			fetchInstituteCourseStrengthWithoutCall();
		}else{
			$('#cmbInstitute').attr("disabled",true);
		}
	}
}
function fetchInstituteCourseStrengthWithoutCall(){
	var instituteStrengthId = $('#instituteStrengthId').val();
	var jArray = $('#cmbInstitute option:selected').data('courses');
	var multipleProgram=false;
	if(jArray!=undefined){
		var boiler = '<option value="0" roundStructureId="0" >-- Select Course --</option>';
		if($('#customerHelper').val()=="REVA_SERVER"){
		for(var i in jArray)
		{
				boiler += '<option roundStructureId="'+jArray[i]["rsId"]+'" value="'+jArray[i]["icsId"]+'" >'+jArray[i]["rsNm"]+'</option>';
		}
		}else if($('#customerHelper').val()=="XLRI_SERVER"){
			for(var i in jArray)
			{
				boiler += '<option courseType="'+jArray[i]["courseType"]+'"  courseName="'+jArray[i]["cNm"]+'" roundStructureIdDecr="'+jArray[i]["roundStructureIdDecr"]+'" instituteCourseId="'+jArray[i]["instCoId"]+'" roundStructureId="'+jArray[i]["rsId"]+'" value="'+jArray[i]["icsId"]+'" >'+jArray[i]["cNm"]+'</option>';
			}
		
		}else{
			for(var i in jArray)
			{	
				if(jArray[i]["cNm"] == "First Year(BE)")
					boiler += '<option courseType="'+jArray[i]["courseType"]+'"  courseName="'+jArray[i]["cNm"]+'" roundStructureIdDecr="'+jArray[i]["roundStructureIdDecr"]+'" instituteCourseId="'+jArray[i]["instCoId"]+'" roundStructureId="'+jArray[i]["rsId"]+'" value="'+jArray[i]["icsId"]+'" style="font-weight:bold;">'+jArray[i]["cNm"]+' '+jArray[i]["rsNm"]+'</option>';					
				else
					boiler += '<option courseType="'+jArray[i]["courseType"]+'"  courseName="'+jArray[i]["cNm"]+'" roundStructureIdDecr="'+jArray[i]["roundStructureIdDecr"]+'" instituteCourseId="'+jArray[i]["instCoId"]+'" roundStructureId="'+jArray[i]["rsId"]+'" value="'+jArray[i]["icsId"]+'" >'+jArray[i]["cNm"]+' '+jArray[i]["rsNm"]+'</option>';
			}
		}
		$('#selInstituteCourseStrength').html(boiler);
		if(instituteStrengthId!=undefined && instituteStrengthId!=null && instituteStrengthId!=''){
			$('#selInstituteCourseStrength').val(instituteStrengthId);
				//$('#selInstituteCourseStrength').attr("disabled",true);
				disableComboCommon('selInstituteCourseStrength');
			for(var i in jArray){
				if(jArray[i]["icsId"]==instituteStrengthId){
					multipleProgram=jArray[i]["OtherProgramName"]!="-"?true:false;
					$('#multipleProgramSelection').val(multipleProgram);
				}
			}	
			var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
			if(courseName=="BBP"){
				$('#applicantRegistrationTLB').hide();
				$('#eligibilityCritlbl').show();
				$('#disclaimerforbmlTR').show();
				$('#customerLogo').attr("src", "img/bmu_axis.png");
				$('#customerLogo').attr("width", "441px");
			}
		}else{
			$('#selInstituteCourseStrength').attr("disabled",true);
		}
	}
	setSelectedProg();
}
function initUserMerger(adhaarNo,emailId)
{
	var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
	var admissionType = $('input[name = "candidateCategory"]:checked').val();
	
	$("#commonModalHeading").html("<h1>Login merger - Identity Verification</h1>");
	var boiler = '<table class="table table-bordered">'+
	'<tr><td colspan="2">Email id&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td><td colspan="3"><input type="text" id="emailIdModal" length="10" disabled></td></tr>'+
	'<tr class="hideForNITIE"><td colspan="2">Date of birth&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td><td colspan="3"><input type="text" id="dobIdModal" length="10" readonly></td></tr>'+
	'<tr class="hideForNITIE"><td colspan="2">Mothers name&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td><td colspan="3"><input type="text" id="motherNameIdModal" length="10"></td></tr>';
	
	if($('#customerHelper').val()=="NITIE_SERVER"){
		if(admissionType =='Direct'){
			boiler +='<tr class="showForNITIE"><td colspan="2">CAT/GATE Registration Number&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td><td colspan="3"><input type="text" id="CATRegNo" length="10"></td></tr>';
		}
		boiler +='<tr class="hideForVLFM"><td colspan="2">Candidate Category&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td><td colspan="3">'+
		'<select id="cmbCategoryReRegister" ></select>'+
		'</td></tr>';
	}
	
	if(adhaarNo!='' && adhaarNo!= undefined && adhaarNo!= 'undefined'){
		boiler +='<tr class="hideForNITIE" id="adhaarMsg"><td colspan="5">Enter any one following detail,<strong> Same </strong> as that of which you have entered earlier.&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td></tr>';
	    boiler +='<tr class="hideForNITIE" id="adhaarTxt"><td colspan="2">Aadhaar-card No.</td><td><input type="text" id="adharIdModal" maxlength="12"></td><td><strong>Or</strong> Passport No.</td><td><input type="text" id="passportIdModal" length="10"></td></tr>';
	    
	    boiler +='<tr class="hideForNITIE" style="display:none;" id="otpMsg"><td colspan="5">Enter following detail,<strong> Same </strong> as sent on your registered mobile number.&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td></tr>';
		boiler +='<tr class="hideForNITIE" style="display:none;" id="otpTxt"><td colspan="2">OTP</td><td><input type="text" id="otpIdModal" length="8"><span></span>'+
			'<i class="fa fa-spinner fa-pulse fa-lg fa-fw" style="position: absolute;right: 35%;top: 9%;display:none;" id="sendOTPmailSpinner"></i>'+
		'</td><td> <a class="btn btn-success" id="btnSendOTP" onclick="sendOTP()"><b>Send OTP</b></a> &nbsp;&nbsp;</td></tr>';
	}else{
		
		boiler +='<tr class="hideForNITIE" style="display:none;" id="adhaarMsg"><td colspan="5">Enter any one following detail,<strong> Same </strong> as that of which you have entered earlier.&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font></td></tr>';
	    boiler +='<tr class="hideForNITIE" style="display:none;" id="adhaarTxt"><td colspan="2">Aadhaar-card No.</td><td><input type="text" id="adharIdModal" maxlength="12"></td><td><strong>Or</strong> Passport No.</td><td><input type="text" id="passportIdModal" length="10"></td></tr>';
	    if(emailId!='' && emailId!= undefined){
		boiler +='<tr class="hideForNITIE" id="otpMsg"><td colspan="5">Enter following detail,<strong> Same </strong> as sent on your registered emailId.&nbsp;&nbsp;<font style="color:red;font-weight:bold;">*</font><strong> '+emailId+' </strong></td></tr>';
	    }
		boiler +='<tr class="hideForNITIE" id="otpTxt"><td colspan="2">OTP</td><td><input type="text" id="otpIdModal" length="8"></td><td><a class="btn btn-success" id="btnSendOTP" onclick="sendOTP()"><b>Send OTP</b></a> &nbsp;&nbsp;</td></tr>';
	}
	
	boiler +='<tr><td colspan="3"> All fields marked with <font style="color:red;font-weight:bold;">*</font> are mandatory. <br> Your login id is your e-mail id. </td><td><div class="preview"></div></td></tr>'+
	'</table>';
	$("#commonModalBody").html(boiler);
	if($('#customerHelper').val()=="NITIE_SERVER"){
		$('.hideForNITIE').hide();	
	}
	var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
	
	if(courseName =='VLFM'){
		$('.hideForVLFM').hide();		
	}
	
	fetchCategoryReservations();
	$("#modalFooter").html();
	$("#commonModal").modal('show');
	$('#emailIdModal').val($('#emailIdName').val());
	
	$('#dobIdModal').datepicker({
		dateFormat : 'M dd,yy',
		changeMonth : true,
		changeYear : true,
		yearRange : "-80:+0",
		firstDay : 1
	});
}


function sendOTP()
{
	var EmailId = $("#emailIdModal").val();
	var InstituteCourseStrength = $('#selInstituteCourseStrength option:selected').val();
	var customerId = $('#customerId').val();

	if(EmailId == "")
		{
			swal("E-mail id can not be blank !");
			return false;
		}
	
	
	$.ajax({
		url:'generateOTP.json?EmailId='+EmailId+'&InstituteCourseStrength='+InstituteCourseStrength+'&customerId='+customerId, 
		dataType:'json',
		type : 'POST',
		async:false,
		beforeSend: function() {
			$('#sendOTPmailSpinner').show();
		},
		complete: function() {
			$('#sendOTPmailSpinner').fadeOut(2000);
		},
		headers:{'security-helper-prop':true},
		success:function(jObject)
		{
			if (jObject != null && jObject != undefined) 
			{
				var status = jObject["status"];
				if(status==true){
				  swal("OTP sent on your registered Email Id");
				}else{
					swal("Failed to Send OTP");
				}
			}else{
				jAlert("Please try again later","Message");
			}
			
			
		},
		error:function(){
			jAlert("In Course Duration error","Error");
		}
	});	
	
	
}


function saveMergerDetails()
{
	var emailId = $("#emailIdModal").val();
	var dob = $("#dobIdModal").val();
	var motherName = $("#motherNameIdModal").val();
	var adhar = $("#adharIdModal").val();
	var passport = $("#passportIdModal").val();
	var otp = $("#otpIdModal").val();
	var categoryId = $("#cmbCategoryReRegister").val();
	var admissionTypeId = 0;
	var catRegNo = '';
	
	if($('#customerHelper').val()=="NITIE_SERVER" && $('#CATRegNo').is(":visible") ){
		catRegNo = $('#CATRegNo').val();
		
		if(catRegNo == ""){
			swal("CAT Registration Number can not be blank !");
			return false;
		}
		
	}if($('#customerHelper').val()=="NITIE_SERVER" && $('#emailIdModal').is(":visible") ){
		
		if(emailId == ""){
			swal("E-mail id can not be blank !");
			return false;
		}
		
	}
	
	if($('#customerHelper').val()!="NITIE_SERVER"){
	
	if(emailId == "")
		{
			swal("E-mail id can not be blank !");
			return false;
		}
	if(dob == "")
		{
		swal("Date of birth can not be blank !");
			return false;
		}
	if(motherName == "")
		{
		swal("Mother name can not be blank !");
			return false;
		}
	
	if($('#adharIdModal').is(":visible") && $('#passportIdModal').is(":visible")){
		if(adhar == "" && passport == "")
			{
			swal("Please enter either adhaar no. or passport no. Or login to old profile and edit details !");
				return false;
			}
		}else if($('#otpIdModal').is(":visible")){
			if(otp == "")
			{
				swal("Please enter OTP Or login to old profile and edit details !");
				return false;
			}
		}
	}
	
	if($('#cmbCategoryReRegister').is(':visible') && $('#customerHelper').val()=="NITIE_SERVER"){
		if(categoryId =='0'){
			swal("Please Select Category!");
			return false;
		}
		admissionTypeId = $('#cmbCategoryReRegister option:selected').attr("admissionTypeId");
	}
	
	var roundStructureId = $('#selInstituteCourseStrength option:selected').attr("roundStructureId");
	var InstituteCourseStrength = $('#selInstituteCourseStrength option:selected').val();
	var customerId = $('#customerId').val();
	var instituteCourseId = $('#selInstituteCourseStrength option:selected').attr("instituteCourseId");
	var instName = $('#cmbInstitute option:selected').text();
	var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
	var courseType = $('#selInstituteCourseStrength option:selected').attr('courseType');
	var instAbb = $('#cmbInstitute option:selected').attr('instAbbr');
	
	if(InstituteCourseStrength == null || InstituteCourseStrength == undefined || InstituteCourseStrength == "" || InstituteCourseStrength == "0" || roundStructureId =="0" )
	{
		//$("#selInstituteCourseStrength")
		swal("Please select course name !");
		return false;
		
	}
	
	$.ajax({
		url : 'restApi/applicantStudentAlreadyRegisteredCloning.json', // saveMergerOfUserLogins.json
		//url : 'restApi/saveMergerOfUserLogins.json', // saveMergerOfUserLogins.json
		data : {
			emailId : emailId,
			dob : dob,
			motherName : motherName,
			adhar : adhar,
			passport : passport,
			otp : otp,
			roundStructureId : roundStructureId,
			InstituteCourseStrength : InstituteCourseStrength,
			customerId : customerId,
			instituteCourseId : instituteCourseId,
			instName : instName,
			courseName : courseName,
			courseType : courseType,
			instAbb : instAbb,
			categoryId : categoryId,
			admissionTypeId : admissionTypeId,
			catRegNo : catRegNo
			
		},
		headers:{'security-helper-prop':true},
		dataType : 'json',
		type : 'POST',
		async : false,
		beforeSend : function() {
			$('div.preview').addClass('loading');
		},
		complete : function() {
			$('div.preview').removeClass('loading');	
		},
		success : function(jObject) {
			
			
			
			if (jObject != null && jObject != undefined) 
			{
				var errorMsg = "";
				var errArr = jObject["errorArr"];
				if(errArr.length > 0)
				{
					for(var a=0; a<errArr.length; a++)
					{
						errorMsg += errArr[a]["errMsg"]+"<br>";
					}
					jAlert(errorMsg);
					return false;
				}else
				{
					//jAlert("Identity verfied successfully ! <br> Please login with your user id and password again. <br> Redirecting to login page in 5 Sec..");
					swal("Identity verified successfully ! Please login with your user id and password again.")
					.then((value) => {
						window.location="login.json";
					});
					
				}
				
			}else{
				jAlert("Technical error occurred !");
			}
		},
		error : function() {
			jAlert("Error in merging user logins !","Error");
		}
	});
	
}
function fetchInstituteList()
{
	var campusId = $('#cmbCampus').val();
	var instituteId = $("#instituteId").val();
	$.ajax({
		url:'instituteList.json?campusId='+campusId,	
		dataType:'json',
		headers:{'security-helper-prop':true},
		//async:false,
		success:function(jArray){
			var boiler = '<option value="0">-- Select Institute --</option>';
			for(var i=0;i<jArray.length;i++){
				boiler += '<option value="'+jArray[i]["instituteId"]+'" >'+jArray[i]["instituteName"]+'</option>';
			}
			$('#cmbInstitute').html(boiler);
			if(instituteId!=undefined && instituteId!=null && instituteId!='' && instituteId!='null'){
				$('#cmbInstitute').val(instituteId);
				
					//$('#cmbInstitute').attr("disabled",true);
					disableComboCommon('cmbInstitute');
				
				fetchInstituteCourseStrength();
			}else{
				$('#cmbInstitute').attr("disabled",true);
			}
		},
		error:function(){
			//jAlert("in Error Admission Type CMB","Error");
		}
	});
}

function fetchInstituteCourseStrength()
{
	var instituteId = $('#cmbInstitute').val();
	var instituteStrengthId = $('#instituteStrengthId').val();
	$.ajax({
		url:'fetchInstituteCoursesForApplicantAdmission.json?instituteId='+instituteId, 
		dataType:'json',
		headers:{'security-helper-prop':true},
		//async:false,
		success:function(jArray){
			var boiler = '<option value="0" roundStructureId="0" >-- Select Course --</option>';
			for(var i=0;i<jArray.length;i++)
			{
				if(jArray[i]["courseName"] == "First Year(BE)")
					boiler += '<option courseName="'+jArray[i]["courseName"]+'" roundStructureIdDecr="'+jArray[i]["roundStructureIdDecr"]+'" instituteCourseId="'+jArray[i]["instituteCourseId"]+'" roundStructureId="'+jArray[i]["roundStructureId"]+'" value="'+jArray[i]["instituteStrengthId"]+'" style="font-weight:bold;">'+jArray[i]["courseName"]+' '+jArray[i]["roundName"]+'</option>';					
				else
					boiler += '<option courseName="'+jArray[i]["courseName"]+'" roundStructureIdDecr="'+jArray[i]["roundStructureIdDecr"]+'" instituteCourseId="'+jArray[i]["instituteCourseId"]+'" roundStructureId="'+jArray[i]["roundStructureId"]+'" value="'+jArray[i]["instituteStrengthId"]+'" >'+jArray[i]["courseName"]+' '+jArray[i]["roundName"]+'</option>';
			}
			$('#selInstituteCourseStrength').html(boiler);
			if(instituteStrengthId!=undefined && instituteStrengthId!=null && instituteStrengthId!='' && instituteStrengthId!='null'){
				$('#selInstituteCourseStrength').val(instituteStrengthId);
				
				
					//$('#selInstituteCourseStrength').attr("disabled",true);
					disableComboCommon('selInstituteCourseStrength');
				
			}else{
				$('#selInstituteCourseStrength').attr("disabled",true);
			}
		},
		error:function(){
			//jAlert("in Error Admission Type CMB","Error");
		}
	});
}
function addAdmissionApplicant()
{
	
	if($('#customerHelper').val()=="NITIE_SERVER"||$('#customerHelper').val()=="LOCAL_SERVER"){
		$('#commonModalNITIE').modal('hide');
	}
	
	//var customerId = $('#customerId').val();
	var urlDomainName = window.location.host;
	var customerId = 0;
	var validationLogStr = new Array();	
	if(urlDomainName=="kjedu.in")
		customerId="db32227ae3fc2517e96ad2a3eed93d62";
	else	
		customerId=$('#customerId').val();

	var firstName = $('#firstName').val().trim();
	var middleName = $('#middleName').val().trim();
	var lastName = $('#lastName').val().trim();
	var emailIdName = $('#emailIdName').val().trim();
	var mobileNo = $('#mobileNo').val().trim();
	var InstituteCourseStrength = '';
	var txtCapecha = $("#txtCapecha").val().trim();
	var FixedGender = $("#fixedGender").val().trim();
	var StudentElective = $("#StudentElective").val().trim();
	var isEntrance = $("#isEntrance").val().trim();
	var Aadharno = $('#AdharcardNo').val();
	var DteNo = $('#Dteno').val();
	var countryCode = ini_CountryCode.getSelectedCountryData().dialCode;
	var instName = $('#cmbInstitute option:selected').text();
	var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
	var courseType = $('#selInstituteCourseStrength option:selected').attr('courseType');
	var instAbb = $('#cmbInstitute option:selected').attr('instAbbr');
	var candidateCategory = '';
	var CATRegistrationNumber = '';
	var categoryReservationId = 0;
	var admissionTypeId = 0;
	var physicallyHandicapped = '';
	var abcId = '';
	var dateofbirth='null';
	var otherprogram='';
	InstituteCourseStrength =  $('#selInstituteCourseStrength').val();
	
	
	var txtCapecha1 = $("#captchaInfo");
	var isValid = true;
	$('#applicantRegistrationTLB input[type=text]').each(function(){
		if(!($(this).val()!=undefined && $(this).val().trim()!='')){
			if($(this).attr('id') != 'middleName' && $(this).attr('id') != 'lastName' && $(this).attr('id') != 'CATRegistrationNumber'){
				$(this).css("border","1px solid red");
				isValid = false;
				if($(this).prop('placeholder')!=undefined){
					validationLogStr.push($(this).prop('placeholder'));
				}
			}
		}else{
			$(this).css("border","1px solid #CCCCCC");
		}
		
	});
	
	 if (emailIdName != ""  && emailIdName.includes(',')){
		 $('#emailIdName').css("border","1px solid red");
		 isValid = false;
	 }else{
		 $('#emailIdName').css("border","");		
	 }
	
	 if (validateEmail() && firstName != "" && emailIdName != "" && mobileNo != "") {
		 
		var regExp = /^[A-Za-z]+$/;
		var firstNameInfo = $("#firstNameInfo");
		var mobileInfo = $("#mobileInfo");
		var mobileNoObk = $("#mobileNo");
		var emailIdObk = $("#emailIdName");
		var txtCapecha1 = $("#captchaInfo");
		
 		if (mobileNo.length < 10 && countryCode=="91") 
 		{
 			//lastNameInfo.hide();
			//mobileInfo.text("Enter valid mobile number");
			
			errorAlertBootstrap("Enter valid mobile number");
			mobileNoObk.css("border","1px solid red");
			//mobileInfo.show();
			mobileInfo.hide();
			isValid = false;
			return ;
		}else{
			mobileNoObk.css("border","");
			mobileInfo.hide();
			isValid = true;
		}
 		
 		/* if(($('#customerHelper').val()=="MGMU_SERVER")){
			var isOtpVer = mobileNoObk.data('isOTPVerified');
			if(isOtpVer!=true){
				errorAlertBootstrap("Please verify OTP of mobile number!");
				mobileNoObk.css("border","1px solid red");
				//mobileInfo.show();
				mobileInfo.hide();
				isValid = false;
			}else{
				mobileNoObk.css("border","");
				mobileInfo.hide();
				isValid = true;
			}
		}	 */
 		if($('#customerHelper').val()=="HPTU_SERVER" || $('#customerHelper').val()=="ATAL_MEDICAL_SERVER"){
			var isOtpVer = emailIdObk.data('isOTPVerified');
			if(isOtpVer!=true){
				errorAlertBootstrap("Please verify OTP of Email Id!");
				emailIdObk.css("border","1px solid red");
				//mobileInfo.show();
				mobileInfo.hide();
				isValid = false;
			}else{
				mobileNoObk.css("border","");
				mobileInfo.hide();
				isValid = true;
			}
		}	
		if($('#customerHelper').val()=="NITIE_SERVER" && courseName !="VLFM"){
			
			if($("#candidateCategoryTRCls").is(":visible") && $('[name="candidateCategory"]:checked').val()==undefined){
				$('#candidateCategoryTRCls').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#candidateCategoryTRCls').css('border', 'none');
			}
			
			if($("#CATRegistrationNumber").is(":visible") && $('#CATRegistrationNumber').val().trim() == ""){
				$('#CATRegistrationNumber').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#CATRegistrationNumber').css('border', 'none');
			}
			
			if($("#cmbCategory").is(":visible") && $('#cmbCategory').val() == "0"){
				$('#cmbCategory').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#cmbCategory').css('border', 'none');
			}
			if($("#physicallyHandicappedTR").is(":visible") && $('[name="physicallyHandicapped"]:checked').val()==undefined){
				$('#physicallyHandicappedTR').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#physicallyHandicappedTR').css('border', 'none');
			}
			if($("#ApplicantBirthDayTR").is(":visible") &&  $('#ApplicantBirthdate').val().trim() == ""){
				$('#ApplicantBirthdate').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#ApplicantBirthdate').css('border', 'none');
			}
			
		}
		if($('#customerHelper').val()=="BML_SERVER"){
			if($("#ApplicantBirthDayTR").is(":visible") &&  $('#ApplicantBirthdate').val().trim() == ""){
				$('#ApplicantBirthdate').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#ApplicantBirthdate').css('border', 'none');
			}
			 var checkedCheckboxes = $(".disclaimerforbml:checked").length;
			if($("#disclaimerforbmlTR").is(":visible") && checkedCheckboxes!=1){
				$('.disclaimerforbml').css("border", "1px solid red");
				isValid = false;
			}
		}
		 //isValid = true;
	 } else {
		 isValid = false;
	 }
	
	if(InstituteCourseStrength==null || InstituteCourseStrength=='null' || InstituteCourseStrength=="0")
	{
		swal("Please Select Course Name");
		isValid = false;
	}
	if($('#customerHelper').val()=="BML_SERVER"){
		dateofbirth=$('#ApplicantBirthdate').val().trim();
	}
	if($('#customerHelper').val()=="NITIE_SERVER"){
		 candidateCategory = $('input[name = "candidateCategory"]:checked').val();
		 CATRegistrationNumber = $('#CATRegistrationNumber').val().trim();
		 categoryReservationId =  $('#cmbCategory option:selected').val();	
		 admissionTypeId = $('#cmbCategory option:selected').attr("admissionTypeId");	
		 physicallyHandicapped = $('input[name = "physicallyHandicapped"]:checked').val();
		 dateofbirth=$('#ApplicantBirthdate').val().trim();
		 $('.ckallProgram:checked').each(function() {
			 var courseName = $(this).attr('data-name');
			  var ICS =  $(this).attr('data-icsid');
			 var RoundStructureId = $(this).attr('roundStructureId');
			
			if(InstituteCourseStrength!=ICS){
				otherprogram=otherprogram!=''?otherprogram+","+ICS+"_"+RoundStructureId+"_"+courseName:ICS+"_"+RoundStructureId+"_"+courseName;
			}
			 
		 });
		 console.log("otherprogram",otherprogram);
	}
	if($('#customerHelper').val()=="XLRI_SERVER"||$('#customerHelper').val()=="LOCAL_SERVER"){
	if($("#candidateCategoryTRCls").is(":visible") && $('[name="candidateCategory"]:checked').val()==undefined){
		$('#candidateCategoryTRCls').css("border", "1px solid red");
		isValid = false;
	}else{
		$('#candidateCategoryTRCls').css('border', 'none');
	}
	if($("#cmbCategory").is(":visible") && $('#cmbCategory').val() == "0"){
		$('#cmbCategory').css("border", "1px solid red");
		isValid = false;
	}else{
		$('#cmbCategory').css('border', 'none');
	}
	categoryReservationId =  $('#cmbCategory option:selected').val();	
	 admissionTypeId = $('#cmbCategory option:selected').attr("admissionTypeId");
	}
	if($('#customerHelper').val()=="ICT_MUMBAI_SERVER"){
		
		abcId = $('#abcId').val().trim();
		if(abcId==""){
			swal("Please Enter ABC Id");
			isValid = false;
		}
		
	}
	
	
	
	if(isValid)
	{
		var appliedForCampus = false;
		var appliedForCampusName = '';
		if($('#customerHelper').val()=='IMT_DUBAI_SERVER'){
			appliedForCampus = true;
			appliedForCampusName = "Dubai";
		}
		
		var roundStructureId = $('#selInstituteCourseStrength option:selected').attr("roundStructureId");
		var roundStructureIdNew = $('#selInstituteCourseStrength option:selected').attr("roundStructureIdDecr");
		var instituteCourseId = $('#selInstituteCourseStrength option:selected').attr("instituteCourseId");
		if(instituteCourseId == undefined || instituteCourseId == 'undefined'){
			//instituteCourseId =  data-instituteCourseId
			instituteCourseId = $('#cmbCampus option:selected').attr("data-instituteCourseId");
		}
		
		var URL1=window.location.href;
		var admFormCampSource = undefined;
		var campaignName=$('#campaignNameId').val()!=null?$('#campaignNameId').val():"undefined";
		var campaignMedium=$('#campaignMediumId').val()!=null?$('#campaignMediumId').val():"undefined";
		var campaignSource=$('#campaignSourceId').val()!=null?$('#campaignSourceId').val():"undefined";
		var bscId=$('#bscId').val()!=null?$('#bscId').val():"undefined";

	 $.ajax({
			url:'restApi/applicantStudentRegistration.json',//addAdmissionApplicant.json  addAdmissionApplicantSP.json   
			//url:'restApi/addAdmissionApplicantSP.json',//addAdmissionApplicant.json  addAdmissionApplicantSP.json
			data:{
				InstituteCourseStrength : InstituteCourseStrength,
				customerId : customerId,
				firstName :firstName,
				middleName : middleName,
				lastName : lastName,
				emailIdName : emailIdName,
				mobileNo:mobileNo,
				txtCapecha : txtCapecha,
				URL1 : URL1 , 
				roundStructureId : roundStructureId,
				FixedGender : FixedGender,
				StudentElective :StudentElective,
				isEntrance:isEntrance,
				Aadharno:Aadharno,
				DteNo:DteNo,
				admFormCampSource:admFormCampSource,
				appliedForCampusName:appliedForCampusName,
				countryCode:countryCode,
				instituteCourseId:instituteCourseId,
				instName:instName,
				courseName:courseName,
				courseType:courseType,
				instAbb:instAbb,
				candidateCategory:candidateCategory,
				CATRegistrationNumber:CATRegistrationNumber,
				categoryReservationId:categoryReservationId,
				admissionTypeId:admissionTypeId,
				physicallyHandicapped:physicallyHandicapped,
				abcId:abcId,
				dateofbirth:dateofbirth,
				otherprogram:otherprogram,
				campaignName:campaignName,
				campaignMedium:campaignMedium,
				campaignSource:campaignSource,
				bscId:bscId
			},
			dataType:'json',
			type:'POST',
			headers:{'security-helper-prop':true},
			async:false,
			beforeSend: function() {
				$('.preview').addClass('loading');
				$('.lblTimeWarn').show();
				$('#btnRegister').hide();
			},
			complete: function() {
				$('.preview').removeClass('loading');
				$('.lblTimeWarn').hide();
				$('#btnRegister').show();
			},
			success:function(jArray){
				
				if(jArray[0].status=="capachaError"){
				    //jAlert("Invalid captcha. Please try again.");
				    errorAlertBootstrap("Invalid captcha. Please try again.");
				    refreshImg();
				    return;
				}if(jArray[0].status=="alreadyAdded"){
				    //jAlert("Invalid captcha. Please try again.");
				    swal("You have already applied for this course!\n You may use your credentials to login for the same.");
				    //window.location.reload();
				    return;
				}
				if(jArray[0].status=="ApplicationLimit")
     	   		{
     	   			//jAlert('Applicant registration fulfilled. Please try next time');
	     	   		errorAlertBootstrap('Applicant registration fulfilled. Please try next time');
	  				 return;
     	   		}
				if(jArray.isError!=undefined && jArray.isError==true){
					swal("Sorry for inconvenience! Please contact to Admission department!");
					return ;
				}
				if(jArray[0].status){
					if(jArray[0].status!="capachaError"){
						var msg='';
						if($('#customerHelper').val()=="MGMU_SERVER"){
							msg="Thank you for your interest in MGM University. <br> Your login credentials sent on your Mobile - \''"+mobileNo+"'\' & E-Mail - \''"+emailIdName+"'\'.";
						}else if($('#customerHelper').val()=="RCOEM_SERVER"){
							msg="Successfully Registered! <br> Please go to registered email address for getting password. <br>You will be required to use this credentials for login/ process.";
						}else if(($('#customerHelper').val()=="BML_SERVER") && courseName=="BBP"){
							msg="Thank You for registering with BMU Axis Bank Bharat Banking Programme.<p style='text-align:left;'>You have successfully completed the first step of the registration process.</p> <p style='text-align:left;'>We have sent your login details to the registered email address.</p><p style='text-align:left;'>Please check your email for login information.</p><p style='text-align:left;'> Login through the link provided in your mail to complete your application.</p> Thanks & Regards,\n BML Munjal University"
						}else{
							msg="Successfully Registered! <br> Please go to registered email address or phone number for getting password. <br>You will be required to use this credentials for login process.";
						}
						if($('#customerHelper').val()!="MATS_SERVER"){
						
						jAlert(msg,"Success",function(r) {
						  	if (r) {
						  		window.location.href = "login.htm";
							  }
							 });
						}else{
							window.location.href = "login.htm";
						}
						/* alert('Successfully Registered Please go to registered Email Id/Mobile No for getting password');
						$("#applicantRegistrationDiv").hide();
						$("#applicantGuidelinesDiv").html(getGuidelinesBoiler()).show();	 */					
					}
				} else{
					errorAlertBootstrap('Oops something went wrong, please try again.');
				}
			},
			error:function(){
				//jAlert("in Error Admission Type CMB","Error");
			}
		}); 
	}else{
		if(validationLogStr.length>0)
			errorAlertBootstrap(validationLogStr.join(', ')+' are not valid!');
		setTimeout(function(){
			$('#applicantRegistrationTLB input[type=text]').css("border","");
		},5000);
	}
}

function addNITIEValidationsAndPop(){
	
	
	//var customerId = $('#customerId').val();
	var urlDomainName = window.location.host;
	var customerId = 0;
	var validationLogStr = new Array();	
	if(urlDomainName=="kjedu.in")
		customerId="db32227ae3fc2517e96ad2a3eed93d62";
	else	
		customerId=$('#customerId').val();

	var firstName = $('#firstName').val().trim();
	var middleName = $('#middleName').val().trim();
	var lastName = $('#lastName').val().trim();
	var emailIdName = $('#emailIdName').val().trim();
	var mobileNo = $('#mobileNo').val().trim();
	var InstituteCourseStrength = '';
	var txtCapecha = $("#txtCapecha").val().trim();
	var FixedGender = $("#fixedGender").val().trim();
	var StudentElective = $("#StudentElective").val().trim();
	var isEntrance = $("#isEntrance").val().trim();
	var Aadharno = $('#AdharcardNo').val();
	var DteNo = $('#Dteno').val();
	var countryCode = ini_CountryCode.getSelectedCountryData().dialCode;
	var instName = $('#cmbInstitute option:selected').text();
	var courseName = $('#selInstituteCourseStrength option:selected').attr('courseName');
	var courseType = $('#selInstituteCourseStrength option:selected').attr('courseType');
	var instAbb = $('#cmbInstitute option:selected').attr('instAbbr');
	var candidateCategory = '';
	var CATRegistrationNumber = '';
	var categoryReservationId = 0;
	var admissionTypeId = 0;
	var physicallyHandicapped = '';
	var dateofBirth='';
	if(window.location.toString().indexOf("ictmumbai") != -1){
		InstituteCourseStrength = $('#selInstituteCourseStrength1').val();
	}else {
		InstituteCourseStrength =  $('#selInstituteCourseStrength').val();
	}
	
	var txtCapecha1 = $("#captchaInfo");
	var isValid = true;
	$('#applicantRegistrationTLB input[type=text]').each(function(){
		if(!($(this).val()!=undefined && $(this).val().trim()!='')){
			if($(this).attr('id') != 'middleName' && $(this).attr('id') != 'lastName' && $(this).attr('id') != 'CATRegistrationNumber'){
				$(this).css("border","1px solid red");
				isValid = false;
				if($(this).prop('placeholder')!=undefined){
					validationLogStr.push($(this).prop('placeholder'));
				}
			}
		}else{
			$(this).css("border","1px solid #CCCCCC");
		}
		
	});
	
	 if (validateEmail() && firstName != "" && emailIdName != "" && mobileNo != "") {
		 
		var regExp = /^[A-Za-z]+$/;
		var firstNameInfo = $("#firstNameInfo");
		var mobileInfo = $("#mobileInfo");
		var mobileNoObk = $("#mobileNo");
		var emailIdObk = $("#emailIdName");
		var txtCapecha1 = $("#captchaInfo");
		
 		if (mobileNo.length < 10 && countryCode=="91") 
 		{
 			//lastNameInfo.hide();
			//mobileInfo.text("Enter valid mobile number");
			
			errorAlertBootstrap("Enter valid mobile number");
			mobileNoObk.css("border","1px solid red");
			//mobileInfo.show();
			mobileInfo.hide();
			isValid = false;
			return ;
		}else{
			mobileNoObk.css("border","");
			// mobileInfo.hide();
			isValid = true;
		}
 		
 		if(window.flow.isMobileNoVerify=="true"||window.flow.isMobileNoVerify){
			var isOtpVer = mobileNoObk.data('isOTPVerified');
			if(isOtpVer!=true){
				isValid = false;
				errorAlertBootstrap("Please verify OTP of mobile number!");
				mobileNoObk.css("border","1px solid red");
				//mobileInfo.show();
				// mobileInfo.hide();
				
			}else{
				mobileNoObk.css("border","");
				mobileInfo.hide();
				isValid = true;
			}
		}	
 		if($('#customerHelper').val()=="HPTU_SERVER" || $('#customerHelper').val()=="ATAL_MEDICAL_SERVER"||$('#customerHelper').val()=="NITIE_SERVER"){
			var isOtpVer = emailIdObk.data('isOTPVerified');
			if(isOtpVer!=true){
				errorAlertBootstrap("Please verify OTP of Email Id!");
				 $("#emailIdName").css("border","1px solid red");
				//mobileInfo.show();
			//	mobileInfo.hide();
				isValid = false;
			}else{
				emailIdObk.css("border","");
				// mobileInfo.hide();
				isValid = true;
			}
		}	
		if($('#customerHelper').val()=="NITIE_SERVER"||$('#customerHelper').val()=="LOCAL_SERVER"){
			
			if($("#ApplicantBirthDayTR").is(":visible") &&  $('#ApplicantBirthdate').val().trim() == ""){
				$('#ApplicantBirthdate').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#ApplicantBirthdate').css('border', 'none');
			}
			if($("#candidateCategoryTRCls").is(":visible") && $('[name="candidateCategory"]:checked').val()==undefined){
				$('#candidateCategoryTRCls').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#candidateCategoryTRCls').css('border', 'none');
			}
			
			if($("#CATRegistrationNumber").is(":visible") && $('#CATRegistrationNumber').val().trim() == ""){
				$('#CATRegistrationNumber').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#CATRegistrationNumber').css('border', 'none');
			}
			
			if($("#cmbCategory").is(":visible") && $('#cmbCategory').val() == "0"){
				$('#cmbCategory').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#cmbCategory').css('border', 'none');
			}
			if($("#physicallyHandicappedTR").is(":visible") && $('[name="physicallyHandicapped"]:checked').val()==undefined){
				$('#physicallyHandicappedTR').css("border", "1px solid red");
				isValid = false;
			}else{
				$('#physicallyHandicappedTR').css('border', 'none');
			}
			
			
		}
		 //isValid = true;
	 } else {
		 isValid = false;
	 }
	
	if(InstituteCourseStrength==null || InstituteCourseStrength=='null' || InstituteCourseStrength=="0")
	{
		swal("Please Select Course Name");
		isValid = false;
	}
	if($('#customerHelper').val()=="NITIE_SERVER"){
		 candidateCategory = $('input[name = "candidateCategory"]:checked').val();
		 CATRegistrationNumber = $('#CATRegistrationNumber').val().trim();
		 categoryReservationId =  $('#cmbCategory option:selected').val();	
		 admissionTypeId = $('#cmbCategory option:selected').attr("admissionTypeId");	
		 physicallyHandicapped = $('input[name = "physicallyHandicapped"]:checked').val();
		 dateofbirth= $('#ApplicantBirthdate').val().trim();
	}
	
	 if (emailIdName != ""  && emailIdName.includes(',')){
		 $('#emailIdName').css("border","1px solid red");
		 isValid = false;
				errorAlertBootstrap('EmailId are not valid!');
			setTimeout(function(){
				$('#applicantRegistrationTLB input[type=text]').css("border","");
			},5000);
		 
	 }else{
		 $('#emailIdName').css("border","");		
	 }
	
	if(isValid)
	{
		guideLinePopup();
	}else{
		if(validationLogStr.length>0)
			errorAlertBootstrap(validationLogStr.join(', ')+' are not valid!');
		setTimeout(function(){
			$('#applicantRegistrationTLB input[type=text]').css("border","");
		},5000);
	}
}

var errorTimeoutId = null;
function errorAlertBootstrap(txt){
	if(errorTimeoutId!=null){clearTimeout(errorTimeoutId);}
	$('#errorAlert').html('<div class="alert alert-danger alertHideShow" style="margin-top:10px;margin-bottom: -5px;"><a href="javascript:void(0)" class="close" data-dismiss="alert" aria-label="close">&times;</a><strong>'+txt+'</strong></div>');
	errorTimeoutId = setTimeout(function(){$('.alertHideShow').fadeOut('slow');	},5000);
	
}
function showGuidelines(){
	
	var InstituteCourseStrength = $('#selInstituteCourseStrength option:selected').val();
	var guidelines = '';
	$.ajax({
			url:'restApi/getInstituteContactDetailsRegistration.json?InstituteCourseStrength='+InstituteCourseStrength,
			dataType:'json',
			async:false,
			beforeSend: function() {
				$('#guideLineIcon').show();
			},
			complete: function() {
				$('#guideLineIcon').fadeOut(2000);
			},
			success:function(reply){
			var instructions="";
            try{instructions=decodeURIComponent(reply.profileInstructions)}catch(e){instructions=reply.profileInstructions}
            guidelines = instructions;
			},
 			error:function(){
 				//jAlert("in Error Admission Type CMB","Error");
 			}
 		});
	
	$("#applicantRegistrationDiv").hide();
	//$("#applicantGuidelinesDiv").html(getGuidelinesBoiler()).show();
	//$("#applicantGuidelinesDiv").html(guidelines).show();
	
	if(guidelines=='' || guidelines=='-'){
		$("#applicantGuidelinesDiv").html(getGuidelinesBoiler()).show();
	}else{
		$("#applicantGuidelinesDiv").html(guidelines).show();
	}
	
	$("#applicantGuidelinesDiv").prepend('<a href="javascript:void(0);" class="btn btn-info btn-mini" onclick="hideGuidelines()"><i class="fa fa-arrow-circle-left" aria-hidden="true"></i> Back to Registration Form</a><br/>');
	
}
function hideGuidelines(){
	$("#applicantRegistrationDiv").show();
	$("#applicantGuidelinesDiv").html('').hide();
}
</script>
