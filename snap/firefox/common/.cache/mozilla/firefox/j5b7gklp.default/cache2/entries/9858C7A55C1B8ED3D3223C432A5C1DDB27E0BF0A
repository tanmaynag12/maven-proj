						//|| $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" 
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
						if(gIsProfileFreezed==true){
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAchievmentCertificate(\''+trId+'\',\''+documentId+'\',\''+achievmentId+'\')" /></td></tr>';
						}else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAchievmentCertificate(\''+trId+'\',\''+documentId+'\',\''+achievmentId+'\')" /></td></tr>';
						}
					}else{
						certificateBoiler += 
							'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeAchievmentCertificate(\''+trId+'\',\''+documentId+'\',\''+achievmentId+'\')" /></td></tr>';
					}
					}
					certificateBoiler += '<table>';
					$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		//alert(e);
	}
}

