function showDegreeGAPUploadAttachment(categoryId,elementId,gapId){
	//////alert(degreeLabel);
	var uploadDocument = new UploadStudentDocument();
	var certificateBoiler = '';
	try{
		//gExtensionArray = ['pdf'];
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
			 gExtensionArray = ['pdf'];	
		}else{
			 gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip'];
		}
		uploadDocument.init({
			elementId:elementId,
			categoryId:categoryId,
			fileSize:AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS
		},
		function(docId){
			tempArray = new Array();
			uploadDocument.getDocument(categoryId, function(jArray){
				if(jArray.length > 0){
					for(var i = 0 ; i < jArray.length ; i++) {
						certificateBoiler = '<table>';
						for(var i = 0 ; i < jArray.length ; i++){
							var documentId = jArray[i].documentId;
							var attachmentFileName = jArray[i]["documentName"].toString(); 
							var trId = 'trAttachmentFile-'+documentId+'-'+i;
							var fileType = jArray[i]["extension"];
							iconPath = gFileTypeIconMapping[fileType];
							//clientCondition
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
							if(gIsProfileFreezed==true){
								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGAPCertificate(\''+trId+'\',\''+documentId+'\',\''+gapId+'\')" /></td></tr>';
							}
							else{
								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGAPCertificate(\''+trId+'\',\''+documentId+'\',\''+gapId+'\')" /></td></tr>';
							}
						}else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGAPCertificate(\''+trId+'\',\''+documentId+'\',\''+gapId+'\')" /></td></tr>';
						}
						}
						certificateBoiler += '<table>';
						$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
					}
				} else {
					$('#certificateAttachmentsDisplayOnDialog').html("");
				}
			});
		});
	} catch(e){
		jAlert(e);
	}
	try{
		//alert('Hi');
		tempArray = new Array();
		uploadDocument.getDocument(categoryId, function(jArray){
			if(jArray.length > 0){
				for(var i = 0 ; i < jArray.length ; i++){
					certificateBoiler = '<table>';
					for(var i = 0 ; i < jArray.length ; i++){
						var documentId = jArray[i].documentId;
						var attachmentFileName = jArray[i]["documentName"].toString(); 
						var trId = 'trAttachmentFile-'+documentId+'-'+i;
						var fileType = jArray[i]["extension"];
						iconPath = gFileTypeIconMapping[fileType];
						//clientCondition
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
						if(gIsProfileFreezed==true){
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGAPCertificate(\''+trId+'\',\''+documentId+'\',\''+gapId+'\')" /></td></tr>';
						}else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGAPCertificate(\''+trId+'\',\''+documentId+'\',\''+gapId+'\')" /></td></tr>';
						}
					}else{
						certificateBoiler += 
							'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGAPCertificate(\''+trId+'\',\''+documentId+'\',\''+gapId+'\')" /></td></tr>';
					}
						
					}
					certificateBoiler += '<table>';
					$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		//alert(e);
	}
}

