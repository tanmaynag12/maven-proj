/**
 * The JsonObject which maps the tabName with key
 * @memberOf {JsonObject}
 */

function achorDownloadTag(eDocId, attachmentFileName){
	return (juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
}
var gTabDegreeName = {
		TabName_SSC:'SSC',
		TabName_HSC:'HSC',
		TabName_UG:'UG',
		TabName_PG:'PG',
		TabName_CERTIFICATION:'CERTIFICATION',
		TabName_PROJECT:'PROJECT',
		TabName_WORKEX:'WORKEX',
		TabName_TECHNICALKNOWLEDGE:'TECHNICALKNOWLEDGE',
		TabName_GAP:'GAP',
		TabName_TRAINING:'TRAINING',
		TabName_ACHIEVEMENT:'ACHIEVEMENT',
		TabName_RESEARCHBOOK:'RESEARCHBOOK',
		TabName_RESEARCHPUB:'RESEARCHPUB',
		TabName_DIPLOMA:'DIPLOMA',
		TabName_MEDICAL:'MEDICAL',
		TabName_PASSPORTVISA:'PASSPORTVISA',
		TabName_EXTRAACHIEVEMENT:'EXTRAACHIEVEMENT',
		TabName_POSITIONRESPONSIBILITY:'POSITIONRESPONSIBILITY',
};


function openCertficateShowUploadDialog(){
	$('#certificateAttachmentsDisplayOnDialog').html('');
	$('#studentCertficateShowUploadDialog').modal('show');
}
