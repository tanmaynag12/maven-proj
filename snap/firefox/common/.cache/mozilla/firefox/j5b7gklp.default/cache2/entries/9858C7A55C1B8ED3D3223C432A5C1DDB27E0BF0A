								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removePassportVisaCertificate(\''+trId+'\',\''+documentId+'\',\''+studentId+'\')" /></td></tr>';
