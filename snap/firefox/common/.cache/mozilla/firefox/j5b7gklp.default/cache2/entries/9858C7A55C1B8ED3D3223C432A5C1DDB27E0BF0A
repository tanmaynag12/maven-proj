				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		//alert(e);
	}
}

function removeExtraActivityCertificate(trId,documentId,studentId) {
	removeUploadedDocumentById(documentId,function(){
		$('#'+trId).data('DeletedCertificateHtml',$('#'+trId).html());
		var boiler = 
			'<td>This document has been deleted. You can <a href = "javascript:void(0)" onclick = "undoExtraActivityCertificateRemoveAction(\''+trId+'\',\''+documentId+'\')"> <span class="help-inline">undo</span></a> this action.</td>';	
		$('#'+trId).html(boiler);
	});
}

function undoExtraActivityCertificateRemoveAction(trId,documentId) {
	var uploadDocument = new UploadStudentDocument();
	uploadDocument.undoUploadedDocumentDelete(documentId,function(flag){
		//alert(flag);
		if(flag.toString() == "pass"){
			$('#'+trId).html($('#'+trId).data('DeletedCertificateHtml'));
		} else {
			$('#'+trId).html('Oops! undo failed. Sorry for the inconvenience');
		}
	});
}

function getDocumentCategoryByResponsibilityId(id,callback){
	$.ajax({
		url:'getDocumentCategoryByResponsibilityId.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}

