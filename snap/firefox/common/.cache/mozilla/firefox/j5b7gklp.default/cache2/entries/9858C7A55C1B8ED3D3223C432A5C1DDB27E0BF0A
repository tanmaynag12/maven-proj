/**
 * The JsonObject which maps the tabName with key
 * @memberOf {JsonObject}
 */

function achorDownloadTag(eDocId, attachmentFileName){
	return (juno.label['a.download.json'].setIndexedArgs(eDocId, "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
}
var gTabDegreeName = {
		TabName_SSC:'SSC',
		TabName_HSC:'HSC',
		TabName_UG:'UG',
		TabName_PG:'PG',
		TabName_CERTIFICATION:'CERTIFICATION',
		TabName_PROJECT:'PROJECT',
		TabName_WORKEX:'WORKEX',
		TabName_TECHNICALKNOWLEDGE:'TECHNICALKNOWLEDGE',
		TabName_GAP:'GAP',
		TabName_TRAINING:'TRAINING',
		TabName_ACHIEVEMENT:'ACHIEVEMENT',
		TabName_RESEARCHBOOK:'RESEARCHBOOK',
		TabName_RESEARCHPUB:'RESEARCHPUB',
		TabName_DIPLOMA:'DIPLOMA',
		TabName_MEDICAL:'MEDICAL',
		TabName_PASSPORTVISA:'PASSPORTVISA',
		TabName_EXTRAACHIEVEMENT:'EXTRAACHIEVEMENT',
		TabName_POSITIONRESPONSIBILITY:'POSITIONRESPONSIBILITY',
};


function openCertficateShowUploadDialog(){
	$('#certificateAttachmentsDisplayOnDialog').html('');
	$('#studentCertficateShowUploadDialog').modal('show');
}
//label-1 Upload 10th(SSC) certificates here:
//label-2 10th(SSC) certificates:


/**
 * function to get document category from the object id of the tab selected OR the row selected. It also 
 * initialize the upload bar
 * 
 * @param trId
 * @param documentId
 * @param degreeLabel
 * @param schoolAndCollegeId
 */
function getDegreeCategoryId(degreeName,degreeId,isCurrentDegree,studentType){
	var dailyMonitoringflag = $('#dailyMonitoringflag').val()!=undefined || $('#dailyMonitoringflag').val()!='' ? $('#dailyMonitoringflag').val():false;
	if(degreeName == gTabDegreeName.TabName_SSC){
		degreeId = $('#sub10th').data('schoolAndCollegeId');
		if(degreeId != undefined){
			$('.documentAttachmentSSC').show();
			getDocumentCategoryBySchoolAndCollegeId(degreeId,function(categoryId){
				if(categoryId !=undefined && categoryId != '' && categoryId != null && categoryId != "null" )
					showDegree1012thUploadAttachment(categoryId,'certificateAttachmentSSC',degreeId,degreeName);
				else {
					if(dailyMonitoringflag != true && dailyMonitoringflag != "true"){
						jAlert('No document found');
					}
				}
			});
		} else {
			$('.documentAttachmentSSC').hide();
		}
	} else if(degreeName == gTabDegreeName.TabName_HSC){
		degreeId = $('#sub12th').data('schoolAndCollegeId');
		if(degreeId != undefined){
			$('.documentAttachmentHSC').show();
			getDocumentCategoryBySchoolAndCollegeId(degreeId,function(categoryId){
				if(categoryId !=undefined && categoryId != '' && categoryId != null && categoryId != "null" )
					showDegree1012thUploadAttachment(categoryId,'certificateAttachmentHSC',degreeId,degreeName);
				else {
					if(dailyMonitoringflag != true && dailyMonitoringflag != "true"){
						jAlert('No document found');
					}
				}
			});
		} else {
			$('.documentAttachmentHSC').hide();
		}

	} else if(degreeName == gTabDegreeName.TabName_UG){
		//alert("HI");
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload UG certificates here:");
		//$('#label-2').html("UG certificates:");
		
		 if(isCurrentDegree == 'current'){
			 
			if(studentType == 'academic'){ 
				getDocumentCategoryByCurrentDegreeAcademic(degreeId,function(categoryId){
					if(categoryId != '')
						showDegreeGraduationUploadAttachment(categoryId,'certificateAttachment',degreeId);
					else {
						jAlert('No document category found');
					}
				});
			} else {
				getDocumentCategoryByCurrentDegreeId(degreeId,function(categoryId){
					if(categoryId != '')
						showDegreeGraduationUploadAttachment(categoryId,'certificateAttachment',degreeId);
					else {
						jAlert('No document category found');
					}
				});
			}
			
		 } else {
			getDocumentCategoryByGraduationId(degreeId,function(categoryId){
				if(categoryId != '')
					showDegreeGraduationUploadAttachment(categoryId,'certificateAttachment',degreeId);
				else {
					jAlert('No document category found');
				}
			});
		 }
	} else if(degreeName == gTabDegreeName.TabName_PG){
		//alert("student Type: "+studentType);
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload PG certificates here:");
		//$('#label-2').html("PG certificates:");
		 if(isCurrentDegree == 'current'){
			if(studentType == 'academic'){ 
					getDocumentCategoryByCurrentDegreeAcademic(degreeId,function(categoryId){
						if(categoryId != '')
							showDegreeGraduationUploadAttachment(categoryId,'certificateAttachment',degreeId);
						else {
							jAlert('No document category found');
						}
					});
			} else {
				getDocumentCategoryByCurrentDegreeId(degreeId,function(categoryId){
					if(categoryId != '')
						showDegreeGraduationUploadAttachment(categoryId,'certificateAttachment',degreeId);
					else {
						jAlert('No document category found');
					}
				});
			}
		} else {
			getDocumentCategoryByGraduationId(degreeId,function(categoryId){
				if(categoryId != '')
					showDegreeGraduationUploadAttachment(categoryId,'certificateAttachment',degreeId);
				else {
					jAlert('No document category found');
				}
			});
		}
		 
		
		
	} else if(degreeName == gTabDegreeName.TabName_CERTIFICATION){

		openCertficateShowUploadDialog();
		$('#label-1').html("Upload certificates here:");
		//$('#label-2').html("Certificates:");
		getDocumentCategoryByCertificateId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeCertificateUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	} else if(degreeName == gTabDegreeName.TabName_PROJECT){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload project certificates here:");
		//$('#label-2').html("Project certificates:");
		getDocumentCategoryByProjectId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeProjectUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	} else if(degreeName == gTabDegreeName.TabName_WORKEX){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload work experience certificates here:");
		//$('#label-2').html("Work experience certificates:");
		getDocumentCategoryByWorkExId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeWorkExUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
		
	} else if(degreeName == gTabDegreeName.TabName_TECHNICALKNOWLEDGE){

		openCertficateShowUploadDialog();
		$('#label-1').html("Upload technical certificates here:");
		//$('#label-2').html("Technical experience certificates:");
		getDocumentCategoryBySkillSetId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeSkillSetUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
		
	} else if(degreeName == gTabDegreeName.TabName_GAP){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload your document for gap here:");
		//$('#label-2').html("Documents:");
		getDocumentCategoryByGAPId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeGAPUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	} else if(degreeName == gTabDegreeName.TabName_TRAINING){

		openCertficateShowUploadDialog();
		$('#label-1').html("Upload training certificates here:");
		//$('#label-2').html("Training experience certificates:");
		getDocumentCategoryByTrainingId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeTrainingUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
		
	} else if(degreeName == gTabDegreeName.TabName_ACHIEVEMENT){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload achievement certificates here:");
		//$('#label-2').html("Achievement certificates:");
		getDocumentCategoryByAchievmentId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeAchievmentUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	} else if(degreeName == gTabDegreeName.TabName_RESEARCHBOOK){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload book publication publication certificates/attachments here:");
		//$('#label-2').html("Book publication certificates/attachments:");
		getDocumentCategoryByBookPublicationId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeBookPublicationUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	} else if(degreeName == gTabDegreeName.TabName_RESEARCHPUB){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload research publication certificates/attachments here:");
		//$('#label-2').html("Research publication certificates/attachments:");
		getDocumentCategoryResearchPublicationId(degreeId,function(categoryId){
			if(categoryId != '')
				showDegreeResearchPublicationUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	} else if(degreeName == gTabDegreeName.TabName_DIPLOMA){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload diploma certificates here:");
		//$('#label-2').html("Diploma certificates:");
		if(isCurrentDegree == 'current'){
			if(studentType == 'academic'){ 
				getDocumentCategoryByCurrentDegreeAcademic(degreeId,function(categoryId){
					if(categoryId != '')
						showDegreeDiplomaUploadAttachment(categoryId,'certificateAttachment',degreeId);
					else {
						jAlert('No document category found');
					}
				});
			} else {
				getDocumentCategoryByCurrentDegreeId(degreeId,function(categoryId){
					if(categoryId != '')
						showDegreeDiplomaUploadAttachment(categoryId,'certificateAttachment',degreeId);
					else {
						jAlert('No document category found');
					}
				});
			}
		} else {
			getDocumentCategoryByDiplomaId(degreeId,function(categoryId){
				if(categoryId != '')
					showDegreeDiplomaUploadAttachment(categoryId,'certificateAttachment',degreeId);
				else {
					jAlert('No document category found');
				}
			});
		}
		
	}
	else if(degreeName == gTabDegreeName.TabName_MEDICAL){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload medical certificates/attachments here:");
		//$('#label-2').html("Medical certificates/attachments:");
		getDocumentCategoryByMedicalCertificateId(degreeId,function(categoryId){
			if(categoryId != '')
				showMedicalUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	}
	else if(degreeName == gTabDegreeName.TabName_PARENTDETAILS){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload Parent certificates/attachments here:");
		//$('#label-2').html("Medical certificates/attachments:");
		getDocumentCategoryByParentCertificateId(degreeId,function(categoryId){
			if(categoryId != '')
				showParentUploadAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	}
	else if(degreeName == gTabDegreeName.TabName_PASSPORTVISA){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload Passport and Visa <br>certificates / attachments here:");
		//$('#label-2').html("Medical certificates/attachments:");
		getDocumentCategoryByPassportVisaId(degreeId,function(categoryId){
			if(categoryId != '')
				showPassportVisaAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	}else if(degreeName == gTabDegreeName.TabName_EXTRAACHIEVEMENT){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload Extra Curricular Activity attachments here:");
		//$('#label-2').html("Medical certificates/attachments:");
		getDocumentCategoryByExtraActivityId(degreeId,function(categoryId){
			if(categoryId != '')
				showExtraActivityAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	}else if(degreeName == gTabDegreeName.TabName_POSITIONRESPONSIBILITY){
		openCertficateShowUploadDialog();
		$('#label-1').html("Upload Position/Responsibility attachments here:");
		//$('#label-2').html("Medical certificates/attachments:");
		getDocumentCategoryByResponsibilityId(degreeId,function(categoryId){
			if(categoryId != '')
				showResponsibilityAttachment(categoryId,'certificateAttachment',degreeId);
			else {
				jAlert('No document category found');
			}
		});
	}
}


/******************************************************************************************************SSC-HSC tab*/ 
/**
 * It get the document category with the help of the School and College Id
 * This function has to be written for each tab.
 * @param id
 * @param callback
 */
function getDocumentCategoryBySchoolAndCollegeId(id,callback){
	$.ajax({
		url:'getDocumentCategoryBySchoolAndCollegeId.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}

/**
 * function to init the upload bar and show the certificates(in any) of the tab clicked or the the row selected
 * @param categoryId
 * @param elementId
 * @param schoolAndCollegeId
 * @param degreeLabel
 */
function showDegree1012thUploadAttachment(categoryId,elementId,schoolAndCollegeId,degreeLabel){
	////alert(degreeLabel+"gh");
	var uploadDocument = new UploadStudentDocument();
	var certificateBoiler = '';
	var tempArray = new Array();
	try{
		//gExtensionArray = ['pdf'];
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
			 gExtensionArray = ['pdf'];	
		}else{
			 gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','png','jpeg'];
		}
		uploadDocument.init({
			elementId:elementId,
			categoryId:categoryId,
			fileSize:AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS
		},
		function(docId){
	
			$.ajax({
				url:'updateStudentDegreeCerficateWithDocument.json?d__docId='+docId+'&schoolAndCollegeId='+schoolAndCollegeId+'&flag='+degreeLabel,
				dataType:'json',
				success:function(jObject){
	
					tempArray = new Array();
					var mappedObjectArray =  jObject.degreeMappings;
					if(mappedObjectArray != undefined){
						//making entry of all 
						for(var i = 0 ; i < mappedObjectArray.length ; i++){
							if(mappedObjectArray[i].degreeLabel == degreeLabel)
								tempArray.push(parseInt(mappedObjectArray[i].degreeDocumentId));
						}
	
						////alert(tempArray);
						uploadDocument.getDocument(categoryId, function(jArray) {
							for(var i = 0 ; i < jArray.length ; i++){
								if($.inArray(parseInt(jArray[i].documentId),tempArray) != -1){
									certificateBoiler = '<table>';
									for(var i = 0 ; i < jArray.length ; i++){
										if($.inArray(parseInt(jArray[i].documentId),tempArray) != -1) {
											var documentId = jArray[i].documentId;
											var encDocumentId = jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId;;
											var attachmentFileName = jArray[i]["documentName"].toString(); 
											var trId = 'trAttachmentFile-'+encDocumentId+'-'+i;
											var fileType = jArray[i]["extension"];
											iconPath = gFileTypeIconMapping[fileType];
										//clientCondition
											var downloadLinkTmp = (juno.label['a.download.json'].setIndexedArgs((encDocumentId), "UPLOADED", encodeURIComponent(attachmentFileName), attachmentFileName));
											if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
											if(gIsProfileFreezed==true){
												certificateBoiler += 
													'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeSchoolAndCollegeCertificate(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')" /></td></tr>';
											}
											else{
												certificateBoiler += 
													'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeSchoolAndCollegeCertificate(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')" /></td></tr>';
											}
										}else{
											certificateBoiler += 
												'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeSchoolAndCollegeCertificate(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')" /></td></tr>';
										}
										}
									}
									certificateBoiler += '<table>';
									if(degreeLabel == gTabDegreeName.TabName_SSC)
										$('#certificateAttachmentsDisplaySSC').html(certificateBoiler);
									else
										$('#certificateAttachmentsDisplayHSC').html(certificateBoiler);
								}
							}
						});
	
					}
				}
			});
		});
	} catch(e){
		jAlert(e);
	}

	try{
		$.ajax({
			url:'getStudentSchoolAndCollegeById.json?schoolAndCollegeId='+schoolAndCollegeId,
			dataType:'json',
			beforeSend : function() {
				if(degreeLabel == gTabDegreeName.TabName_SSC)
					$('#certificateAttachmentsDisplaySSC').html('<div class = "preview-large"></div>');
				else
					$('#certificateAttachmentsDisplayHSC').html('<div class = "preview-large"></div>');
				$('.preview-large').addClass('loading-large');
			},
			complete : function() {
				$('.preview-large').removeClass('loading-large');
			},
			success:function(jObject){
				var mappedObjectArray =  jObject.degreeMappings;
				if(mappedObjectArray != undefined) {
					for(var i = 0 ; i < mappedObjectArray.length ; i++) {
						if(mappedObjectArray[i].degreeLabel == degreeLabel)
							tempArray.push(parseInt(mappedObjectArray[i].degreeDocumentId));
					}
					////alert("tempArray: "+tempArray);
					uploadDocument.getDocument(categoryId, function(jArray){
						certificateBoiler = '<table>';
						for(var i = 0 ; i < jArray.length ; i++){
							if($.inArray(parseInt(jArray[i].documentId),tempArray) != -1){
								var documentId = jArray[i].documentId;
								var attachmentFileName = jArray[i]["documentName"].toString(); 
								var trId = 'trAttachmentFile-'+documentId+'-'+i;
								var fileType = jArray[i]["extension"];
								iconPath = gFileTypeIconMapping[fileType];
								//clientCondition
								if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
								if(gIsProfileFreezed==true){
									certificateBoiler += 
										'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeSchoolAndCollegeCertificate(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')" /></td></tr>';
								}
								else{
									certificateBoiler += 
										'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeSchoolAndCollegeCertificate(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')" /></td></tr>';
								}
							}
								else{
									certificateBoiler += 
										'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeSchoolAndCollegeCertificate(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')" /></td></tr>';
								}
								
							}
						}
						certificateBoiler += '<table>';
						if(degreeLabel == gTabDegreeName.TabName_SSC)
							$('#certificateAttachmentsDisplaySSC').html(certificateBoiler);
						else
							$('#certificateAttachmentsDisplayHSC').html(certificateBoiler);
					});
				}
			}
		});
	} catch(e) {
		//alert(e);
	}
}

/**
 * function to remove the uploaded school and college certificates.
 * @param trId
 * @param documentId
 * @param degreeLabel
 * @param schoolAndCollegeId
 */
function removeSchoolAndCollegeCertificate(trId,documentId,schoolAndCollegeId,degreeLabel) {

	removeUploadedDocumentById(documentId,function(){
		$.ajax({
			url:'removeSchoolAndCollegeCertificate.json?d__docId='+documentId+'&schoolAndCollegeId='+schoolAndCollegeId,
			dataType:'json',
			success:function(jArray){
				$('#'+trId).data('DeletedCertificateHtml',$('#'+trId).html());
				var boiler = 
					'<td>This document has been deleted. You can <a href = "javascript:void(0)" onclick = "undoSchoolAndCollegeCertificateRemoveAction(\''+trId+'\',\''+documentId+'\',\''+schoolAndCollegeId+'\',\''+degreeLabel+'\')"> <span class="help-inline">undo</span></a> this action.</td>';	
				$('#'+trId).html(boiler);
			}
		});
	});
}

function undoSchoolAndCollegeCertificateRemoveAction(trId,documentId,schoolAndCollegeId,degreeLabel) {
	$.ajax({
		url:'undoSchoolAndCollegeCertificateRemoveAction.json',
		data:{d__documentId:documentId,schoolAndCollegeId:schoolAndCollegeId,degreeLabel:degreeLabel},
		type:'POST',
		dataType:'text',
		success:function(flag){
			if(flag.toString() == "pass"){
				$('#'+trId).html($('#'+trId).data('DeletedCertificateHtml'));
			} else {
				$('#'+trId).html('Oops! undo failed. Sorry for the inconvenience');
			}
		}
	});
}

/******************************************************************************************************UG tab*/ 


function getDocumentCategoryByGraduationId(id,callback){
	$.ajax({
		url:'getDocumentCategoryByGraduation.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}


function showDegreeGraduationUploadAttachment(categoryId,elementId,graduationId){
	//alert('iiin init attachment');
	var uploadDocument = new UploadStudentDocument();
	var certificateBoiler = '';
	try{
		//gExtensionArray = ['pdf'];
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
			 gExtensionArray = ['pdf'];	
		}else{
			 gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip'];
		}
		uploadDocument.init({
			elementId:elementId,
			categoryId:categoryId,
			fileSize:AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS
		},
		function(docId){
			tempArray = new Array();
			uploadDocument.getDocument(categoryId, function(jArray){
				if(jArray.length > 0){
					for(var i = 0 ; i < jArray.length ; i++) {
						certificateBoiler = '<table>';
						for(var i = 0 ; i < jArray.length ; i++) {
							var documentId = jArray[i].documentId;
							var attachmentFileName = jArray[i]["documentName"].toString(); 
							var trId = 'trAttachmentFile-'+documentId+'-'+i;
							var fileType = jArray[i]["extension"];
							iconPath = gFileTypeIconMapping[fileType];
							//clientCondition
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
							if(gIsProfileFreezed==true){
								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
							}
							else{
								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
							}
						}else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
						}
							
						}
						certificateBoiler += '<table>';
						$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
					}
				} else {
					$('#certificateAttachmentsDisplayOnDialog').html("");
				}
			});
		});
	} catch(e){
		alert(e);
	}

	try{
		tempArray = new Array();
		uploadDocument.getDocument(categoryId, function(jArray){
			if(jArray.length > 0){
				for(var i = 0 ; i < jArray.length ; i++){
					certificateBoiler = '<table>';
					for(var i = 0 ; i < jArray.length ; i++){
						var documentId = jArray[i].documentId;
						var attachmentFileName = jArray[i]["documentName"].toString(); 
						var trId = 'trAttachmentFile-'+documentId+'-'+i;
						var fileType = jArray[i]["extension"];
						iconPath = gFileTypeIconMapping[fileType];
						//clientCondition
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
						if(gIsProfileFreezed==true){
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
						}
						else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
						}
					}else{
						certificateBoiler += 
							'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
					}
					}
					certificateBoiler += '<table>';
					$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		alert(e);
	}
}


function removeGraduationCertificate(trId,documentId,graduationId) {
	removeUploadedDocumentById(documentId,function(){
		$('#'+trId).data('DeletedCertificateHtml',$('#'+trId).html());
		var boiler = 
			'<td>This document has been deleted. You can <a href = "javascript:void(0)" onclick = "undoGraduationCertificateRemoveAction(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')"> <span class="help-inline">undo</span></a> this action.</td>';	
		$('#'+trId).html(boiler);
	});
}

function undoGraduationCertificateRemoveAction(trId,documentId,graduationId) {
	var uploadDocument = new UploadStudentDocument();
	uploadDocument.undoUploadedDocumentDelete(documentId,function(flag){
		//alert(flag);
		if(flag.toString() == "pass"){
			$('#'+trId).html($('#'+trId).data('DeletedCertificateHtml'));
		} else {
			$('#'+trId).html('Oops! undo failed. Sorry for the inconvenience');
		}
	});
}

/************************************************************************************** DIMPLOMA *******************************/

/**
 * It get the document category with the help of the School and College Id
 * This function has to be written for each tab.
 * @param id
 * @param callback
 */
function getDocumentCategoryByDiplomaId(id,callback){
	$.ajax({
		url:'getDocumentCategoryByDiploma.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}

function getDocumentCategoryByCurrentDegreeId(id,callback){
	$.ajax({
		url:'getDocumentCategoryByCurrentPlacement.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}

function getDocumentCategoryByCurrentDegreeAcademic(id,callback) {
	//getDocumentCategoryByCurrent.json
	$.ajax({
		url:'getDocumentCategoryByCurrent.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}


function showDegreeDiplomaUploadAttachment(categoryId,elementId,diplomaId){
	//////alert(degreeLabel);
	var uploadDocument = new UploadStudentDocument();
	var certificateBoiler = '';
	try{
		//gExtensionArray = ['pdf'];
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
			 gExtensionArray = ['pdf'];	
		}else{
			 gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip'];
		}
		uploadDocument.init({
			elementId:elementId,
			categoryId:categoryId,
			fileSize:AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS
		},
		function(docId){
			tempArray = new Array();
			uploadDocument.getDocument(categoryId, function(jArray){
				if(jArray.length > 0){
					for(var i = 0 ; i < jArray.length ; i++) {
						certificateBoiler = '<table>';
						for(var i = 0 ; i < jArray.length ; i++){
							var documentId = jArray[i].documentId;
							var attachmentFileName = jArray[i]["documentName"].toString(); 
							var trId = 'trAttachmentFile-'+documentId+'-'+i;
							var fileType = jArray[i]["extension"];
							iconPath = gFileTypeIconMapping[fileType];
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeDiplomaCertificate(\''+trId+'\',\''+documentId+'\',\''+diplomaId+'\')" /></td></tr>';
						}
						certificateBoiler += '<table>';
						$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
					}
				} else {
					$('#certificateAttachmentsDisplayOnDialog').html("");
				}
			});
		});
	} catch(e){
		jAlert(e);
	}
	try{
		tempArray = new Array();
		uploadDocument.getDocument(categoryId, function(jArray){
			if(jArray.length > 0){
				for(var i = 0 ; i < jArray.length ; i++){
					certificateBoiler = '<table>';
					for(var i = 0 ; i < jArray.length ; i++){
						var documentId = jArray[i].documentId;
						var attachmentFileName = jArray[i]["documentName"].toString(); 
						var trId = 'trAttachmentFile-'+documentId+'-'+i;
						var fileType = jArray[i]["extension"];
						iconPath = gFileTypeIconMapping[fileType];
						certificateBoiler += 
							'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeDiplomaCertificate(\''+trId+'\',\''+documentId+'\',\''+diplomaId+'\')" /></td></tr>';
					}
					certificateBoiler += '<table>';
					$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		//alert(e);
	}
}


