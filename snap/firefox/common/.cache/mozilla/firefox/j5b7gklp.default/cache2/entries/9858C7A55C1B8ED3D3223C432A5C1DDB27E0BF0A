	$.ajax({
		url:'getDocumentCategoryByGraduation.json?id='+id,
		dataType:'text',
		success:function(catId){
			callback(catId);
		}
	});
}


function showDegreeGraduationUploadAttachment(categoryId,elementId,graduationId){
	//alert('iiin init attachment');
	var uploadDocument = new UploadStudentDocument();
	var certificateBoiler = '';
	try{
		//gExtensionArray = ['pdf'];
		if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
			 gExtensionArray = ['pdf'];	
		}else{
			 gExtensionArray = ['doc','docx','xls','xlsx','xlsm','pdf','jpg','text','png','gif','ppt','zip'];
		}
		uploadDocument.init({
			elementId:elementId,
			categoryId:categoryId,
			fileSize:AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS
		},
		function(docId){
			tempArray = new Array();
			uploadDocument.getDocument(categoryId, function(jArray){
				if(jArray.length > 0){
					for(var i = 0 ; i < jArray.length ; i++) {
						certificateBoiler = '<table>';
						for(var i = 0 ; i < jArray.length ; i++) {
							var documentId = jArray[i].documentId;
							var attachmentFileName = jArray[i]["documentName"].toString(); 
							var trId = 'trAttachmentFile-'+documentId+'-'+i;
							var fileType = jArray[i]["extension"];
							iconPath = gFileTypeIconMapping[fileType];
							//clientCondition
							if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
							if(gIsProfileFreezed==true){
								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
							}
							else{
								certificateBoiler += 
									'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
							}
						}else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
						}
							
						}
						certificateBoiler += '<table>';
						$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
					}
				} else {
					$('#certificateAttachmentsDisplayOnDialog').html("");
				}
			});
		});
	} catch(e){
		alert(e);
	}

	try{
		tempArray = new Array();
		uploadDocument.getDocument(categoryId, function(jArray){
			if(jArray.length > 0){
				for(var i = 0 ; i < jArray.length ; i++){
					certificateBoiler = '<table>';
					for(var i = 0 ; i < jArray.length ; i++){
						var documentId = jArray[i].documentId;
						var attachmentFileName = jArray[i]["documentName"].toString(); 
						var trId = 'trAttachmentFile-'+documentId+'-'+i;
						var fileType = jArray[i]["extension"];
						iconPath = gFileTypeIconMapping[fileType];
						//clientCondition
						if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_NAGPUR" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_GAZIABAD" || $('#customerHelper').val()!=undefined && $('#customerHelper').val()=="IMT_HYDERABAD"){
						if(gIsProfileFreezed==true){
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
						}
						else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
						}
					}else{
						certificateBoiler += 
							'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeGraduationCertificate(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')" /></td></tr>';
					}
					}
					certificateBoiler += '<table>';
					$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		alert(e);
	}
}


function removeGraduationCertificate(trId,documentId,graduationId) {
	removeUploadedDocumentById(documentId,function(){
		$('#'+trId).data('DeletedCertificateHtml',$('#'+trId).html());
		var boiler = 
			'<td>This document has been deleted. You can <a href = "javascript:void(0)" onclick = "undoGraduationCertificateRemoveAction(\''+trId+'\',\''+documentId+'\',\''+graduationId+'\')"> <span class="help-inline">undo</span></a> this action.</td>';	
		$('#'+trId).html(boiler);
	});
}

function undoGraduationCertificateRemoveAction(trId,documentId,graduationId) {
	var uploadDocument = new UploadStudentDocument();
	uploadDocument.undoUploadedDocumentDelete(documentId,function(flag){
		//alert(flag);
		if(flag.toString() == "pass"){
			$('#'+trId).html($('#'+trId).data('DeletedCertificateHtml'));
		} else {
			$('#'+trId).html('Oops! undo failed. Sorry for the inconvenience');
		}
	});
}

