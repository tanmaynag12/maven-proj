							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeMedicalCertificate(\''+trId+'\',\''+documentId+'\',\''+studMedCertiId+'\')" /></td></tr>';
						}
						else{
							certificateBoiler += 
								'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeMedicalCertificate(\''+trId+'\',\''+documentId+'\',\''+studMedCertiId+'\')" /></td></tr>';
						}
					}else{
						certificateBoiler += 
							'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;'+achorDownloadTag((jArray[i].eDocId!=undefined?jArray[i].eDocId:jArray[i].documentId), attachmentFileName)+'</td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeMedicalCertificate(\''+trId+'\',\''+documentId+'\',\''+studMedCertiId+'\')" /></td></tr>';
					}
					}
					certificateBoiler += '<table>';
					$('#certificateAttachmentsDisplayOnDialog').html(certificateBoiler);
				}
			} else {
				$('#certificateAttachmentsDisplayOnDialog').html("");
			}
		});
	} catch(e) {
		//alert(e);
	}
}
function removeMedicalCertificate(trId,documentId,studMedCertiId) {
	removeUploadedDocumentById(documentId,function(){
		$('#'+trId).data('DeletedCertificateHtml',$('#'+trId).html());
		var boiler = 
			'<td>This document has been deleted. You can <a href = "javascript:void(0)" onclick = "undoMedicalCertificateRemoveAction(\''+trId+'\',\''+documentId+'\')"> <span class="help-inline">undo</span></a> this action.</td>';	
		$('#'+trId).html(boiler);
	});
}

function undoMedicalCertificateRemoveAction(trId,documentId) {
	var uploadDocument = new UploadStudentDocument();
	uploadDocument.undoUploadedDocumentDelete(documentId,function(flag){
		//alert(flag);
		if(flag.toString() == "pass"){
			$('#'+trId).html($('#'+trId).data('DeletedCertificateHtml'));
		} else {
			$('#'+trId).html('Oops! undo failed. Sorry for the inconvenience');
		}
	});
}
