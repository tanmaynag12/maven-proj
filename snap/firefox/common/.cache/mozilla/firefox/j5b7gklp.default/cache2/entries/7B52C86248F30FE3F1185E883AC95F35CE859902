<script type="text/javascript">
$(function() {
	 
		var currentdate = new Date();
		var curYear = (currentdate.getFullYear() + 1);
		var yearListOf20 = '<option value="0">Select</option>';

		for ( var year = curYear; year > curYear - 50 ; year--)
			yearListOf20 += '<option value='+year+'>' + year + '</option>';
		$('#yrOfPassingId').html(yearListOf20);
		
});
function isNumberKey(evt, obj) {
    var charCode = (evt.which) ? evt.which : event.keyCode
    var value = obj.value;
    var dotcontains = value.indexOf(".") != -1;
    var num = value.split('.')[1];
    if (dotcontains)
        if (charCode == 46) return false;
    if (charCode == 46) return true;
    if (charCode > 31 && (charCode < 48 || charCode > 57))
        return false;
    if(num.length>1)
    	return false;
    return true;
}

</script>
						
