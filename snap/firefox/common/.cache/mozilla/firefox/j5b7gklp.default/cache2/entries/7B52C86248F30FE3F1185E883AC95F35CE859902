<script type="text/javascript">





var date = new Date();
var CurrentMonth = date.getMonth();
var CurrentDate = date.getDate();
var currentYear = date.getFullYear();
function systematicExamination()
{
	$('#studSystematicExaminationDiv').modal('show').css({
		width: '1000px',
		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});
}

function medicalExamination()
{
	$('#studMedicalExaminationDiv').modal('show').css({
		width: '1200px',
		'margin-left': function () {
			return -($(this).width() / 2);
		}
	});
}
$('#add :input').attr('disabled', true);
function studentFormEnabled() {
	$('#add :input').attr('disabled', false);
	$('#sub').show();
	$('#res').show();
}

function percentOrGrade(txtGrade, gradeConvMethod, txtMarks, txtOutOf,txtPercent, perGradFlag) {
	//alert("percent Or Grade "+perGradFlag);
	if ($('input[name = "' + perGradFlag + '"]:checked').val() == "percent") {
		//alert("in percent");
		$('#' + txtGrade).val("");
		$('#' + gradeConvMethod).val("");
		$('#' + txtMarks).attr('disabled', false);
		$('#' + txtPercent).attr('disabled', false);
		$('#' + txtOutOf).attr('disabled', false);
		$('#' + txtGrade).attr('disabled', true);
		$('#' + gradeConvMethod).attr('disabled', true);
	} else {
		//alert("in grade");
		$('#' + txtMarks).val("");
		$('#' + txtOutOf).val("");
		$('#' + txtPercent).val("");
		$('#' + txtGrade).attr('disabled', false);
		$('#' + gradeConvMethod).attr('disabled', false);
		$('#' + txtOutOf).attr('disabled', true);
		$('#' + txtMarks).attr('disabled', true);
	}
}


