				var keyCode = e.which ? e.which : e.keyCode;
				var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
				return result;
			});
		 
		 $('#txtStudentPermTaluka').keypress(function (e) {
				var keyCode = e.which ? e.which : e.keyCode;
				var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
				return result;
			});
					 
		 $('#txtStudentDistrict').keypress(function (e) {
				var keyCode = e.which ? e.which : e.keyCode;
				var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
				return result;
			});
		 
		 $('#txtStudentTaluka').keypress(function (e) {
				var keyCode = e.which ? e.which : e.keyCode;
				var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
				return result;
			});
		 
		 $('#txtFatherState').keypress(function (e) {
				var keyCode = e.which ? e.which : e.keyCode;
				var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
				return result;
			});
		 
		 $('#tenthplace').keypress(function (e) {
				var keyCode = e.which ? e.which : e.keyCode;
				var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
				return result;
			});
		 
		$('#studTwelthPlace').keypress(function (e) {
					var keyCode = e.which ? e.which : e.keyCode;
					var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
					return result;
		});
	
	}
		 initUploadCourseExemptionDocuments(); 
	});	 
	
	
		
</script>
		
