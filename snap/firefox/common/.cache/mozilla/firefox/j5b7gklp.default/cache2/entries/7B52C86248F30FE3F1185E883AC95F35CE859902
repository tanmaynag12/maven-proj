<script type="text/javascript">
	var ajaxRequestCount = 0;
	var ModalCount = 1;
	var isTest;
	
	$(document).ready(function(){ 
		$('#spinner').modal({ backdrop: 'static', keyboard: false }); 
		$('#spinner').show();
	});
	function startCommonAjaxSpinner(){
		try{
			ajaxRequestCount++;
			if (ModalCount <= 0) {
				if(isTest!="true"){	
					$('#spinner').modal('show'); 
				}else{
					$.fancybox.showLoading();
				}
				ModalCount++;
			}
				
		//$('#spinner').show();
		//$.fancybox.showLoading();
		}catch(e){
				
		}
	}
	function stopCommonAjaxSpinner(){
		try{
			if (ajaxRequestCount <= 1 ) {
				setTimeout(function() {
					if(isTest!="true"){	
						$('#spinner').modal('hide');
					}else{
						$.fancybox.hideLoading();
					}
				}, 1000);
				ModalCount--;
			}
			ajaxRequestCount--;
			//$('#spinner').modal('hide');
			//$('#spinner').hide();
			//$.fancybox.hideLoading();
		}catch(e){
			
		}
	}
	
	String.prototype.gemsDecode = function () {//Kiran Badave - Ticket No : #896167
		var dval = this;
		try{
			if (dval!=null && dval!=undefined && dval.toString().indexOf("%") != -1) {
				for(var i=0;i<5;i++){
	            	if(dval.indexOf('%')!=-1){
	                	try{dval = decodeURIComponent(dval.toString());}catch(e){}
	                }else{break;}
	            }  
			}
			return dval.toString();
		}catch(e){ return this; }
	};
	String.prototype.gemsEncode = function (count) {
		//Regex to check Basic Latin unicodes, Except any other unicode this will encode
		var regxPattCommonLang = /^([a-zA-Z0-9$@$!%*?&#^-_. +,<>\'\"`~()-/\\;:|{}[\]=\n\t]{0,})$/g;
		var data = this;
			try{
				if(data.toString().match(regxPattCommonLang)==null){
					data = (encodeURIComponent(data));
					if(count!=undefined){
						for(var i=1;i<count;i++){
							data = (encodeURIComponent(data));
						}
					}
				}
			}catch(e){ data = this; }
		return data;
	};
	setTimeout(function(){
		setInterval(function() {
			try{
				$('.gems_decode').each(function() {
					var gmstxt = $(this).html();
					var gmstxtDec = gmstxt.gemsDecode();
					$(this).data('original_value', gmstxt);
					if(gmstxt != gmstxtDec){
						$(this).html(gmstxtDec);
					}
					$(this).removeClass('gems_decode');
				});
			}catch(e){}
		}, 2000);
	},1500);
	
	var securityHelperObj = {'security-helper-prop':true};//headers:securityHelperObj,
	
	String.prototype.setIndexedArgs = function(){
		/* 	auth: Kiran Badave, Use: Replce args with indexed {0} values
			Example: 	From Validator.properties -- errors.maxlength={0} can not be greater than {1} characters.
						alert(juno.label["errors.maxlength"].setIndexedArgs("Address", "50"));
			Output: 	Address can not be greater than 50 characters.*/
			try{var str = this;for(var i in arguments){str = str.replace('{'+i+'}', arguments[i]);}return str;}catch(e){return this;}
		};
</script>

