<script type="text/javascript">
	jQuery(function($){
		var boiler = '<div id="log" style="max-height:16px; height:16px; text-align:center; background:none repeat scroll 0% 0% rgb(255, 255, 232); font-size:11px;">'+
					 '&nbsp;<a href="http://www.mozilla.org/en-US/firefox/new/" target="_blank">To download latest version of firefox click here</a>'+
					 '&nbsp;or&nbsp;<a href="#" onclick="$(\'#log\').remove();">dismiss</a>.'+
					 '<label style="cursor:pointer;float:right;margin-right:5px;" onclick="$(\'#log\').remove();">&times;</label></div>';
		$('body').before(boiler);
		
		if(navigator.userAgent.indexOf('Firefox') != -1 && !(parseFloat(navigator.userAgent.substring(navigator.userAgent.indexOf('Firefox') + 8)) >= 3.6)){
			 //Firefox < 3.6
			 $('#log').prepend( 'Some important features may not work in this version of your browser.');
		}else if(navigator.userAgent.indexOf('Firefox') != -1){
			//Firefox
		 	$('#log').hide();
		}
		else{
			$('#log').prepend( 'You browser is not mozilla firefox. Some important features may not work in your browser.');
		}
	});
</script>
