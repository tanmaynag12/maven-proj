var Maxsize =  5242880;	

function initStudentSignatureUploaded(targetElementId){

var boiler = '<div class="">'+
'<div class="span2">'+
'<div class="preview"></div></div>'+
'<div class="span4">'+
	'<div class="btn btn-info" style = "width:165px;height:20px">Upload Your Signature'+
		'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="profileSignatureUpload" accept="image/*">'+
	'</div>'+
'</div>'+
'</div>';
$('#'+targetElementId).html(boiler);

try{
	if($('#customerHelper').val()!=undefined && $('#customerHelper').val()=="XLRI"){
		$image_crop = $('#studentSign').croppie({
			enableExif: true,
			enableOrientation: true,
			viewport: {
				width:250,
				height:100,
				type:'square' //circle
			},
			boundary:{
				width:300,
				height:300
			}
		});
	}else{
		$image_crop = $('#studentSign').croppie({
		enableExif: true,
		enableOrientation: true,
		viewport: {
			width:200,
			height:200,
			type:'square' //circle
		},
		boundary:{
			width:300,
			height:300
		}
	});
  }
}catch(e){
	
}
$('#profileSignatureUpload').on('change', function(){
	var reader = new FileReader();
	reader.onload = function (event) {
		$image_crop.croppie('bind', {
			url: event.target.result
		}).then(function(){
			console.log('jQuery bind complete');
		});
	}
	reader.readAsDataURL(this.files[0]);
	$('#uploadSignModal').modal('show');
});

$(function() {
	$('.vanilla-rotate').on('click', function(ev) {
		$image_crop.croppie('rotate', parseInt($(this).data('deg')));
	});
});


$('.crop_sign').click(function(event){
	$image_crop.croppie('result', {
		type: 'canvas',
		size: 'viewport'
	}).then(function(response){
		console.log(response);
		var base64ImageContent = response.replace(/^data:image\/(png|jpg);base64,/, "")
		var blob = base64ToBlob(base64ImageContent, 'image/png');                
		var formData = new FormData();
		formData.append('fileData', blob);
		$.ajax({
			url:"uploadStudentSignatureImage.json",
			type: "POST",
			contentType: false,
			processData: false,
			data: formData,
		name : 'fileData',
		size: Maxsize,
		success:function(data)
		{
			$('#uploadSignModal').modal('hide');

			if(isNaN(data)){
				//showErrorMessage('File size is larger than specified',10000);
				jAlert("File size is larger than specified !!");
			} else {
				jAlert("Image uploaded successfully !!!");
				var imageBoiler = 
				'<img src="getStudentSignatureImage.json?date='+new Date()+'" alt="Sign" name="Sign" width="80px" height="40px" border="1" align="left" style="background-color: #FFFFFF" /></td>';
			}
			$('#'+targetElementId).html(imageBoiler);
			$('#deleteSignature').html('<img src="css/images/not.png"  style="margin-top:70%;margin-left:25%">');
			$('#deleteSignature').show();
			$('#deleteSignature').unbind();
			$('#deleteSignature').bind('click',function(){
				deleteStudentSignature(function(){
					
				});
			});
			$('#uploaded_image').html(imageBoiler);
			//$image_crop.croppie('destroy');
			//initStudentProfileImageUpload();
			//initStudentSignatureUploaded('wrongId');
		}
	});
});
});
}

function deleteStudentSignature(){
$.ajax({
	url:'deleteStudentSignature.json',
	dataType:'json',
	type:'POST',
	beforeSend: function() {
		$('#deleteSignature').html('<div class="preview" style="margin-top:10px;margin-left:2px"></div>');
		$('#deleteSignature').find('.preview').addClass('loading');
	},
	complete: function() {
		$('#deleteSignature').find('.preview').removeClass('loading');
	},
	success:function(response) {
		if(response.responseFlag == 1){
			initStudentSignatureUploaded('studentSignatureUpload');
			$('#deleteSignature').hide();
		}
	}
});
}

