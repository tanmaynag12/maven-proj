<script type="text/javascript" src="scripts/dms/dmsStudent.js"></script><!-- Included for student certificates management -->
<script type="text/javascript" src="js/ajaxupload.js"></script><!-- Included for Photo Upload -->
<script type="text/javascript" src="scripts/student/studentCertificateManagement.js"></script><!-- Included for student certificates management -->
<script type="text/javascript" src="scripts/student/studentProfileCertificates.js"></script>
<script type="text/javascript" src="scripts/student/studentProfile.js"></script>
<script type="text/javascript" src="js/jquery.elastic.js"></script>
<script type="text/javascript" src="js/jquery.mtz.monthpicker.js" ></script>
<script type="text/javascript">
$('document').ready(
		function() {
			
			
			$.ajax({
				url:'checkStudentProfileImage.json',
				dataType:'text',
				success:function(text) {
					initStudentProfileImageUpload();
					if(text == 'no'){
						$('#studentProfilePhoto').attr('src','img/boy.gif');
					}
				}
			});
			
			$.ajax({
				url:'checkForStudentSignature.json',
				dataType:'json',
				success:function(response) {
					if(response.responseFlag == 1 && !response.image){
						initStudentSignatureUploaded('studentSignatureUpload');
						$('#deleteSignature').hide();
					} else {
						var imageBoiler = 
							'<img src="getStudentSignatureImage.json?date='+new Date()+'" alt="Sign" name="Sign" width="80px" height="40px" border="1" align="right" style="background-color: #FFFFFF" /></td>';
						$('#studentSignatureUpload').html(imageBoiler);
						$('#deleteSignature').html('<img src="css/images/not.png"  style="margin-top:70%;margin-left:25%">');
						// setTimeout(function(){
							 $('#deleteSignature').show();
						// }, 500);
						
						$('#deleteSignature').unbind();
						$('#deleteSignature').bind('click',function(){
							deleteStudentSignature(function(){
								
							});
						});
					}
				}
			});
		});
function base64ToBlob(base64, mime) 
{
    mime = mime || '';
    var sliceSize = 1024;
    var byteChars = window.atob(base64);
    var byteArrays = [];

    for (var offset = 0, len = byteChars.length; offset < len; offset += sliceSize) {
        var slice = byteChars.slice(offset, offset + sliceSize);

        var byteNumbers = new Array(slice.length);
        for (var i = 0; i < slice.length; i++) {
            byteNumbers[i] = slice.charCodeAt(i);
        }

        var byteArray = new Uint8Array(byteNumbers);

        byteArrays.push(byteArray);
    }

    return new Blob(byteArrays, {type: mime});
}
