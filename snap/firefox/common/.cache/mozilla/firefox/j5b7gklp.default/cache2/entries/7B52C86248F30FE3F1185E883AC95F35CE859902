	<script>
		function getOtherUniversitiesNew() {
			$.ajax({
				url : 'getOtherUniversity.json',
				dataType : 'json',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					var j = jArray.length;
					var uniComboBoiler = '<option value="0">Select</option>';
					for ( var i = 0; i < j; i++) {
						uniComboBoiler += '<option value="' + jArray[i]["otherUniversityId"] + '">' + jArray[i]["uniName"] + '</option>';
					}
					$('#universityBoardName').html(uniComboBoiler);
				},
				error : function() {
				}
			});
		}
		function getOtherStreamsNew() {
			$.ajax({
				url : 'getOtherStream.json',
				dataType : 'json',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					var j = jArray.length;
					var uniComboBoiler = '<option value="0">Select</option>';
					for ( var i = 0; i < j; i++) {
						uniComboBoiler += '<option value="'	+ jArray[i]["degreeStream"] + '">'+ jArray[i]["degreeStream"] + '</option>';
					}
					$('#dStreamId').html(uniComboBoiler);
				},
				error : function() {
				}
			});
		}
		function getDegreeNameNew() {
			var streamName = $('#dStreamId :selected').val();
			var degreeType = "'COURSE_TYPE_RESEARCH'";
			$.ajax({
				url : 'getOtherDegreeForStudent.json',
				data : {
					streamName:streamName,
					degreeType:degreeType
				},
				dataType : 'json',
				async : false,
				beforeSend : function() {
					startCommonAjaxSpinner();
				},
				complete : function() {
					stopCommonAjaxSpinner();
				},
				success : function(jArray) {
					var boiler = '<option value="0">Select</option>';
					for ( var i = 0; i < jArray.length; i++) {
						boiler += '<option id="'+ jArray[i]["otherPlacementQualificationId"]+ '" value="'+ jArray[i]["otherPlacementQualificationId"]+ '" >' + jArray[i]["degreeName"]+ '</option>';
					}
					$('#degreeNamePhd').html(boiler);
				},
				error : function() {
				}
			});
		}
		function cancelPhdDetailTab() {
			$('#phdDetailtbl').hide();
			$('#addDetailbtn').show();
			$('#getPhdDetailsId').show();
		}
		function addDetails() {
			$('#phdDetailtbl').show();
			$('#addDetailbtn').hide();
		}
	</script>
	
					</div>
					 













 
