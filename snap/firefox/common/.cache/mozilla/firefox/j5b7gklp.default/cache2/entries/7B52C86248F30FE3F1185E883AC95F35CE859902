	$('#document').ready(function(){
		function OnLoad() {                
		    var options = {
		        sourceLanguage:
		        google.elements.transliteration.LanguageCode.ENGLISH,
		        destinationLanguage:
		        [google.elements.transliteration.LanguageCode.HINDI],
		        shortcutKey: 'ctrl+g',
		        transliterationEnabled: true
		    };

		    var control = new google.elements.transliteration.TransliterationControl(options);
		    

		control.makeTransliteratable(["firstNameHindi"]);
		control.makeTransliteratable(["motherNameHindi"]);
		control.makeTransliteratable(["fatherNameHindi"]);
		    var keyVal = 32; // Space key
		   /*  $("#txtfirstName").on('keydown', function(event) {
		        if(event.keyCode === 32) {
		            var engText = $("#txtfirstName").val() + " ";
		            var engTextArray = engText.split(" ");
		            $("#firstNameHindi").val($("#firstNameHindi").val() + engTextArray[engTextArray.length-2]);

		            document.getElementById("firstNameHindi").focus();
		            $("#firstNameHindi").trigger ( {
		                type: 'keypress', keyCode: keyVal, which: keyVal, charCode: keyVal
		            } );
		        }
		    }); */

		   
		} //end onLoad function

		google.setOnLoadCallback(OnLoad);
		  		
		$.ajax({
			url:'getStudentCurrentDegreeType.json',
			dataType:'json',
			success:function(jArray) {
				
				if(jArray.length){
					if(jArray[0]["DegreeType"]=="Diploma"){
						$('#degType').val("DIP");
					}else{
						$('#degType').val(jArray[0]["DegreeType"]);
					}
					if((jArray[0]["DegreeType"])=="PG"){
						$('#pgLI').css("text-decoration", "blink");
						$('#pgLI').css("font-weight", "bold");
						$('#pgLI').css("font-style", "italic");
					}
					if((jArray[0]["DegreeType"])=="UG"){
						$('#ugLI').css("text-decoration", "blink");
						$('#ugLI').css("font-weight", "bold");
						$('#ugLI').css("font-style", "italic");
					}
					if((jArray[0]["DegreeType"])=="Diploma"){
						$('#dipLI').css("text-decoration", "blink");
						$('#dipLI').css("font-weight", "bold");
						$('#dipLI').css("font-style", "italic");
					}
				}
			},
			error:function() {
			}
		});
		
		$('#fromYearTxt').datepicker({
		    changeMonth: true,
		        changeYear: true,
		        showButtonPanel: true,
		        dateFormat: 'MM yy',
		        justMonth: true,
		        create: function(input, inst) {
		            $(".ui-datepicker table").addClass("badbad");
		        },
		        onClose: function(dateText, inst) {
		            var month = $("#ui-datepicker-div .ui-datepicker-month :selected").val();
		            var year = $("#ui-datepicker-div .ui-datepicker-year :selected").val();
		            $(this).datepicker('setDate', new Date(year, month, 1));
		        }
		});
			
		$('#toYearTxt').datepicker({
		    changeMonth: true,
		        changeYear: true,
		        showButtonPanel: true,
		        dateFormat: 'MM yy',
		        justMonth: true,
		        create: function(input, inst) {
		            $(".ui-datepicker table").addClass("badbad");
		        },
		        onClose: function(dateText, inst) {
		            var month = $("#ui-datepicker-div .ui-datepicker-month :selected").val();
		            var year = $("#ui-datepicker-div .ui-datepicker-year :selected").val();
		            $(this).datepicker('setDate', new Date(year, month, 1));
		        }
		});

	}); 
