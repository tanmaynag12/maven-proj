	function showEmpOtherDocumentAttachment1(jArray,tableId,isCreator){
		var roleType = $('#roleType').val();
		var attachementBoiler = "<table class = 'table'>";
		for(var i = 0 ; i < jArray.length; i++){
			var attachmentFileName = jArray[i]["name"].toString(); 
			var documentId = jArray[i]["documentId"];
			var isTemporary = jArray[i]["isTemporary"];

			var trId = 'trAttachmentFile-'+documentId;

			var fileType = jArray[i]["type"];
			iconPath = gFileTypeIconMapping[fileType];
			if(!isTemporary){
				attachementBoiler += 
					'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;<a href = "download.json?id='+documentId+'&type=UPLOADED&name='+attachmentFileName+'">'+attachmentFileName+'</a></td>';
				if(isCreator){
					if(roleType=="financeadmin"){
						attachementBoiler += '<td style="display:none;"><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeOtherDocumentTabAttachmentAjax(\''+trId+'\',\''+documentId+'\',\'saved\')" /></td>';
					}else{
						attachementBoiler += '<td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeOtherDocumentTabAttachmentAjax(\''+trId+'\',\''+documentId+'\',\'saved\')" /></td>';
					}
				}
				attachementBoiler +=  '</tr>';
			} else {
				attachementBoiler += 
					'<tr id = "'+trId+'"><td><img src="'+iconPath+'" />&nbsp;&nbsp;&nbsp;&nbsp;<a href = "javascript:void(0)">'+attachmentFileName+'</a></td><td><img style = "cursor:pointer" title = "remove" src = "img/remove-icon.png" onclick = "removeOtherDocumentTabAttachmentAjax(\''+trId+'\',\''+documentId+'\',\'temporary\')" />&nbsp;&nbsp;&nbsp;&nbsp<img src = "img/unsaved.png" title = "Unsaved"/></td></tr>';
			}
		}
		attachementBoiler += "</table>";
		if(tableId == undefined)
			$('.divEmployeeLeaveUploadedFilesList').html(attachementBoiler);
		else
			$('.'+tableId).html(attachementBoiler);
	}

	function uploadPanCardPassportLicenceAadharCard(type)
	{
		checkDocumentUploadOrNotPersonalDetailTab();
		var empId =0;
		var boiler ='<div><table class="table table-bordered"><tr><td><b>Attached Docs:</b></td><td colspan="3" class = "divEmployeeLeaveUploadedFilesList">No docs attached</td></tr>'+
		'<tr><td><b>Upload</b></td><td colspan="3" class = "divEmployeeLeaveUploadedFilesUploadBar">'+
		'<div class="preview"></div>'+
		'<div class="btn btn-info" style = "width:165px;height:20px">'+
		'Upload attachments'+
		'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="txtFileAttachment1"></div><br><br>'+
		'<div class = "alert alert-info">'+
		'	<i class = "fa fa-hand-o-right iconColorBlack"></i>&nbsp;&nbsp;.pdf,.jpg,.jpeg,.png files only<br />'+
		'	<i class = "fa fa-hand-o-right iconColorBlack"></i>&nbsp;&nbsp; Maximum file size limit 3 <strong id = "fileSizeAllowed"></strong>&nbsp;MB<br />'+
		'</div>'+
		'</td></tr>'+
		'</table>'+
		'</div>';
		$('#viewPanAadharLiceAttachmentBody').html(boiler);
		showOtherEmpAttachmentsDialog1(type,0,'true','false','edit');
		$('#viewPanAadharLiceAttachmentHeader').html('Attached Other Employee document of&nbsp;<strong></strong>');
		$('#viewPanAadharLiceAttachmentDialog').modal('show');
	}


	function checkDocumentUploadOrNotPersonalDetailTab(){
		$.ajax({
			url:'checkDocumentUploadOrNotPersonalDetailsTabAttachmentstudentAdharcard.json',
			dataType:'json',
			type:'POST',
			success:function(jArray){
				$('#permanentAddr').html('<b><i class="icon-remove-sign"></i></b>');
				//$('#currAddr').html('<b><i class="icon-remove-sign"></i></b>');
				$('#AadharCardPersonalDet').html('<b><i class="icon-remove-sign"></i></b>');
				for(var i=0;i<jArray.length;i++){
					var isDocument=jArray[i]['isUploaded'];
					var type=jArray[i]['docType'];
					if(type=="AadhaarCard" && isDocument==true ){
						$('#AadharCardPersonalDet').html('<b><i class="icon-ok-sign"></i></b>');
					}else
						$('#AadharCardPersonalDet').html('<b>Document Not Found</i></b>');
			}
			},
		});
	}		
			
	function showOtherEmpAttachmentsDialog1(type,employeeName,showUploadBar,isPopUp,addEdit)
	{
		initFileUploadOtherDocTab(type);
		var boiler = getAttachementForm();
		if(isPopUp == 'true'){
			$('#viewPanAadharLiceAttachmentBody').html(boiler);
			$('#viewPanAadharLiceAttachmentHeader').html('Attached Other Employee document of&nbsp;<strong></strong>');
			$('#viewPanAadharLiceAttachmentDialog').modal('show');
		} else {
			if(addEdit == 'add'){
				$('.divEmployeeLeaveUploadedFiles').html(boiler);
				$('.divEmployeeLeaveUploadedFiles').show();
			} else {
				$('.divEmployeeLeaveUploadedFiles').html('');
			}
		}

		if(showUploadBar == 'true'){
			$('.divEmployeeLeaveUploadedFilesUploadBar').show();
			initFileUploadOtherDocTab(type);
		} else {
			$('.divEmployeeLeaveUploadedFilesUploadBar').hide();
		}

		if(type != '' && type != undefined){
			startLoading();
			getDocumentCategoryByemployeeId(type,function(categoryId){
				var uploaddocument = new UploadDocument();
				if(categoryId != 'null' && categoryId != undefined) {
					uploaddocument.getDocument(categoryId,function(jArray){
						stopLoading();
						if(jArray.length > 0){
							if(showUploadBar == 'true'){
								showEmpOtherDocumentAttachment1(jArray,'divEmployeeLeaveUploadedFilesList',true);
							} else {
								showEmpOtherDocumentAttachment1(jArray,'divEmployeeLeaveUploadedFilesList',false);
							}
						} else {
							$('.divEmployeeLeaveUploadedFilesList').html('<div class = "alert alert-info">No document found!!!</div>');
						}
					});
				}
				else {
					$('.divEmployeeLeaveUploadedFilesList').html('<div class = "alert alert-error">No document found!!!</div>');
				}

			});
		}

	}	

	function getAttachementForm(){
		var boiler = '<div class = "divEmployeeLeaveUploadedFilesList">'+
		'</div>'+
		'<div class = "divEmployeeLeaveUploadedFilesUploadBar">'+
		'	<div class="preview"></div>'+
		'<div class="btn btn-info" style = "width:165px;height:20px">'+
		'Upload attachments'+
		'<input style = "margin-top:-40px;margin-left:-10px;opacity:0;" type="file" name="fileData" id="txtFileAttachment"></div><br><br>'+
		'<div class = "alert alert-info">'+
		'	<i class = "fa fa-hand-o-right iconColorBlack"></i>&nbsp;&nbsp;.doc,.docx,.pdf,.jpg,.jpeg,.png,.gif files only<br />'+
		'	<i class = "fa fa-hand-o-right iconColorBlack"></i>&nbsp;&nbsp; Maximum file size limit &nbsp;&nbsp;<strong id = "fileSizeAllowed"></strong>&nbsp;MB<br />'+
		'</div>'+
		'</div>';

		return boiler;
	}


	function initFileUploadOtherDocTab(type){

		var elementId = 'txtFileAttachment';
		if(type!=undefined)
			elementId = 'txtFileAttachment1';

		$('document').ready(function() {
			new AjaxUpload(elementId,{
				action : 'attachFileOfOtherDocumentTabforAdhar.json',
				data : {},
				name : 'fileData',
				size : AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS,
				onSubmit : function(file, extension) {
					if(extension.toString().toLowerCase() == 'jpg'  || extension.toString().toLowerCase() == 'jpeg' 
						|| extension.toString().toLowerCase() == 'pdf' || extension.toString().toLowerCase() == 'png'
						|| extension.toString().toLowerCase() == 'gif'){
						startCommonAjaxSpinner();
						this.setData({
							'type':type,
							'fileSize':AllowedFileSize.PROFILE_UPLOAD_DOCUMENTS
						});
					
					} else {
						jAlert('Use only "jpg","jpeg","pdf","png","gif" format file!');
						return false;
					}
				},
				onComplete : function(file, response) {
					stopCommonAjaxSpinner();
					var jArray = eval('(' + response + ')');
					//alert("responseFlag="+jArray["responseFlag"]);
					if(jArray["responseFlag"]==0){
						jAlert(jArray["responseText"]);
					}
					else{			
						if(type == undefined || type == '')
							showEmpOtherDocumentAttachment1(jArray,undefined,true);
						else
							showEmpOtherDocumentAttachment1(jArray,'divEmployeeLeaveUploadedFilesList',true);
					}
			}
			});
		});
	}

	function getDocumentCategoryByemployeeIdOtherDocTab(type,callback){
		$.ajax({
			url:'getDocumentCategoryAdharBystudentId.json?type='+type,
			dataType:'text',
			success:function(categoryId){
				callback(categoryId);
			}
		});
	}
	function getDocumentCategoryByemployeeId(type,callback){
		$.ajax({
			url:'getDocumentCategoryAdharBystudentId.json?type='+type,
			dataType:'text',
			success:function(categoryId){
				callback(categoryId);
			}
		});
	}



	function removeOtherDocumentTabAttachmentAjax(trId,documentId,docuementType){
		if(docuementType == 'saved'){
			removeUploadedDocumentById(documentId,function(jObject){
				$('#'+trId).remove();
			});
		} else {
			removeTemporaryEmpOtherDocAttachmentAjax(documentId,function(){
				$('#'+trId).remove();
			});
		}
	}
	function removeTemporaryEmpOtherDocAttachmentAjax(documentId,callback){
		try{
			$.ajax({
				url:'removeTemporaryDocOtherTabAttachmentAdharcard.json',
				dataType:'json',
				type:'POST',
				data:{d__documentId:documentId},
				success:function(){
					try{
						callback();
					}catch(e){

					}
				}	
			});

		}catch(e){
			alert(e);
		}
	}
	function showuloaded()
	{
		var txtAdharCardNo=$('#txtAdharCardNo').val();
		 if(txtAdharCardNo.length==12)
			$('#uploadAdhar').show();
		 else
			 $('#uploadAdhar').hide();	
	}

	$('document').ready(function(){
		
		$("#studName").keypress(function (e) {
			var keyCode = e.which ? e.which : e.keyCode;
			var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
			return result;
		});
		
		$("#nameOfBank").keypress(function (e) {
			var keyCode = e.which ? e.which : e.keyCode;
			var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
			return result;
		});
		
		$("#branchName").keypress(function (e) {
			var keyCode = e.which ? e.which : e.keyCode;
			var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
			return result;
		});
		
		$("#NameAsPerAcc").keypress(function (e) {
			var keyCode = e.which ? e.which : e.keyCode;
			var result = !( keyCode == 9  || keyCode == 38 || keyCode == 126 || keyCode == 94 || ( keyCode >= 33 && keyCode <= 47) || ( keyCode >= 48 && keyCode <= 57) || ( keyCode >= 58 && keyCode <= 64));
			return result;
		});
		
		$("#BankAccountNo").keypress(function (e) {
			var keyCode = e.which ? e.which : e.keyCode;
			var result = !( ( keyCode >= 65 && keyCode <= 90) || ( keyCode >= 33 && keyCode <= 47) || keyCode == 126 || ( keyCode >= 97 && keyCode <= 122) || ( keyCode >= 58 && keyCode <= 64) || ( keyCode >= 91 && keyCode <= 96));
			return result;
		});
		
		 $("#txtAdharCardNo").keypress(function (e) {
			var keyCode = e.which ? e.which : e.keyCode;
			var result = !( ( keyCode >= 65 && keyCode <= 90) || ( keyCode >= 33 && keyCode <= 47) || keyCode == 126 || ( keyCode >= 97 && keyCode <= 122) || ( keyCode >= 58 && keyCode <= 64) || ( keyCode >= 91 && keyCode <= 96));
			return result;
		}); 
		 
