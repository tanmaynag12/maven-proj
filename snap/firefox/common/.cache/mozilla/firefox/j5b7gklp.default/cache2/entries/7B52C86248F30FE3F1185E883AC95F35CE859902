var alertArray = { "alert_create" : "Record(s) Created Successfully !!", //Save
				"alert_update": "Record(s) Updated Successfully !!", // edit/update/change
				"alert_delete" : "Record(s) Deleted Successfully !!",
				"alert_remove" : "Record(s) Removed Successfully !!",
				"alert_frozen" : "Record(s) Frozen Successfully !!",
				"alert_unfrozen" : "Record(s) Unfrozen Successfully !!",
				"alert_approved" : "Record(s) Approved Successfully !!",
				"alert_disapproved" : "Record(s) Disapproved Successfully !!",
				"alert_registered" : "Record(s) Registered Successfully !!",
				"alert_deregistered" : "Record(s) Deregistered Successfully !!",
				"alert_accepted" : "Record(s) Accepted Successfully !!",
				"alert_rejected" : "Record(s) Rejected Successfully !!",
				"alert_selection" : "Please select at least one record !!",
				"alert_duplicate" : "Record(s) Already Exists !!",
				"alert_upload" : "Document(s) Uploaded Successfully !!",
				"alert_error" : "Something went wrong... Please try again later !!",
				"alert_replicate" : "Record(s) Replicated Successfully !!",
				"alert_blocked" : "Record(s) Blocked Successfully !!",
				"alert_norecord" : "No Record(s) Found !!"};	
	
	
function getCustomAlertMessage(alertKey){
	if(alertArray[alertKey]!=undefined)
		return jAlert(alertArray[alertKey]);
	else
		return jAlert(alertKey);
}
function callScheduledMenthod(srcName)
{
	$.ajax({
		url:'callScheduleCronMethod.json?method='+srcName,
		dataType:'text',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(msg){
				jAlert(msg);
			},
			error:function(){
				getCustomAlertMessage("alert_error");
			}
	});
}
$(document).ajaxComplete(function() {
	if($("#successmsg").is(":visible")) {
	   	$('#successMsgSpanCommon').html('Record(s) Created Successfully !!');
	}else if($("#errormsg").is(":visible")){
		$('#errorMsgSpanCommon').html('Record(s) Already Exists !!');
	}
});
	
