var searchKeyword = new Array();
var globalArray = new Array();
var globalMyArrArray = new Array();
var countAnnouncement=0;


$('document').ready(function(){
	$('#replayToStudentMessageModal').modal({
		keyboard:false,
		show:false
	}).css({
		width:'700px',
		'margin-left':function(){
			return -($(this).width()/2);
		},
	});

	$('#annScroll').slimScroll({
		position: 'right',
		height: '500px',
		wheelStep: 2
	});
});
function typeAhead(){
	$('document').ready(function(){
		$('#searchBox').typeahead({
			source:searchKeyword
		});
	});
}
//getUnreadMessageAndAnnouncementForStudent();
function viewAnnouncement(divId){
	getUnreadAnnouncement();
	getUnreadStudentMessage();
	$("#viewAnnouncementAll").html('');
	$("#viewAnnouncement").html('');
	var boiler='<div>'+
	'<br><div id="autoComplete"></div><div id="announcementList">'+
	'</div><div id="paging"></div>';
	$("#"+divId).html(boiler);

	$.ajax({
		url:'getStudentAnnouncement.json',
		dataType:'json',
		async:false,
		beforeSend:function() {
			startCommonAjaxSpinner();
		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			if(jArray.length>0){
				globalArray = jArray;

				var searchBoiler='<table class="table table-bordered">' +
				'<tr>'+
				'<td>'+
				'<span id="searchDiv">'+
				'&nbsp;&nbsp;<input type="text" id="searchBox" placeholder="Search By Keyword" data-provide="typeahead"/>'+
				'</span>'+
				'&nbsp;&nbsp;<a class="btn" style="vertical-align: top;" onclick="showAnnouncement(\'search\', 0);">Search</a><span id="cartBtn" class="pull-right"></span>'+
				'&nbsp;&nbsp;<a class="btn" style="vertical-align: top;" onclick="showAnnouncement(\'no\', 0); ">Clear</a><span id="cartBtn" class="pull-right"></span>'+
				'</td>'+
				'</tr>'+
				'</table>';
				$("#autoComplete").html(searchBoiler);

				var myArr = new Array();
				for(var j=0;j<globalArray.length;j++)
				{
					myArr.push(decodeURIComponent(globalArray[j]["heading"]));
				}

				$.each(myArr, function(i, el){
					if($.inArray(el, searchKeyword) === -1) searchKeyword.push(el);
				});

				typeAhead();
				/*	$("#searchBox").keyup(function(e) {
							showAnnouncement('search', 0);
						});
						$("#searchBox").click(function(e) {
							showAnnouncement('search', 0);
						});*/

				showAnnouncement('no', 0);
			}
			else{
				$('#announcementList').html('<div class = "alert alert-info">No announcements</div>');

			}
		},
		error:function(){

		}
	});


}

function searchAnnouncement(divId,filterFlag){

	var SearchWord=$("#txtSearchWord").val();
	viewAnnouncementNew(divId,SearchWord,filterFlag,'');
}


function viewAnnouncementNew(divId,searchKeyword,filterFlag,flag){
	//alert(flag);
	$("#divalumpost").hide();
	$("#"+divId).show();
	$('#announcementDetails').html('');
	getUnreadAnnouncement();
	getUnreadStudentMessage();
	$.ajax({
		url:'getStudentAnnouncement.json',
		dataType:'json',
		data:{SearchWord:searchKeyword,filterFlag:filterFlag},
		async:false,
		beforeSend:function() {
			$("#"+divId).html('<div id="divPostLoading" style="text-align: center;width: 100%;"><i class="fa fa-spinner fa-pulse fa-fw fa-4x" style="color:#0b4971"></i></div>');
			
		},
		complete: function() {

		},
		success:function(jArray){

			var boilerNew='';
			var annBody = '';
			var allchecked = '';
			var unreadchecked = '';
			var todaysDate = new Date();
			var cnt = jArray.length;
			$("#announcementCount").text(cnt);
			$("#countAnnouncement").text(cnt);
			if(filterFlag=='unread')unreadchecked='checked';else allchecked='checked';
			
			if(flag == '')
			{
			boilerNew='<div class="row-fluid" style="margin-left:5px;">'+
			'<div class="span4">'+
			'<div class="switch-toggle well">'+
			'<input id="Unread" name="view" '+unreadchecked+' type="radio"><label for="Unread" onclick="viewAnnouncementNew(\''+divId+'\',\''+searchKeyword+'\',\'unread\',\'\')">Unread</label>'+
			'<input id="All" name="view" '+allchecked+' type="radio"><label for="All" onclick="viewAnnouncementNew(\''+divId+'\',\''+searchKeyword+'\',\'all\',\'\')">All</label><a class="btn btn-success"></a></div></div>'+

			'<div class="span2">'+'<p style="margin-bottom:10px;font-size:13px;text-align:left;">Showing '+cnt+' results</p></div>'+

			'<div class="span6" style="text-align:right;margin-top:-5px;">'+
			'<input type=text id="txtSearchWord" value="'+searchKeyword+'" placeholder="Search Announcement" style="margin:5px;border-radius:5px;">'+
			'<input type="button" class="btn btn-primary" value="Search" onclick="searchAnnouncement(\''+divId+'\',\''+filterFlag+'\')"></div>'+
			//'</div>'+
			'</div>';
			}
			//--------new---------
			if(jArray.length>0){				
				boilerNew+='<div id="announcementContainer"><section><div class="container" style="width:98%;">';
				for(var j=0;j<jArray.length;j++)
				{
					if(jArray[j]["readStatus"]==false)
					{
						var createdDate =new Date(jArray[j]["createdDate"]);
						if(todaysDate.getFullYear() == createdDate.getFullYear() && todaysDate.getMonth() == createdDate.getMonth()  && todaysDate.getDate() == createdDate.getDate() )
						{
							boilerNew+= '<div class="collapse-card" style="background-color:#dfeffc;" id="div'+jArray[j]["announcementId"]+'" >';
						}
						else
						{
							boilerNew+= '<div class="collapse-card" style="background-color:#F3F3F3;" id="div'+jArray[j]["announcementId"]+'" >';
						}
					}
					else
					{
						boilerNew+= '<div class="collapse-card" id="div'+jArray[j]["announcementId"]+'" >';
					}

					boilerNew+= '<div class="title"  onclick="viewAnnouncementDetailNew(\''+jArray[j]["announcementId"]+'\')">';
					boilerNew+= '<img src="getEmployeeProfileImageById.json?id='+jArray[j]["employeeId"]+'" style="border-radius:20px;float:left;border:0.5px solid lightgray;" height=40>';
					boilerNew+= '<span>On '+jArray[j]["createdDate"]+' by <b>'+jArray[j]["employeeName"]+'</b></span>';
					try{
						boilerNew+= '<strong>'+decodeURIComponent(jArray[j]["heading"])+'</strong>';
					}catch(e){
						boilerNew+= '<strong>'+jArray[j]["heading"]+'</strong>';
					}
					boilerNew+= '</div>';
					boilerNew+= '<div class="body"><div id="announcementBody-'+jArray[j]["announcementId"]+'"><div style="width:100%; text-align:center;"><img src="img/ajax.gif"></div></div>';

					boilerNew+= '<h1 style="font-size: 18px; padding-bottom: 10px; margin-bottom: 10px; line-height: 22px; border-bottom: 1px solid #E5E5E5;"></h1>';
					boilerNew+= '<div class="footer" style="float:right;margin-bottom:10px;">';
					boilerNew+= '<a title="Move to Trash" onclick="removeStudentAnnouncement(\''+jArray[j]["announcementId"]+'\',\'div\',\''+jArray[j]["isPermanent"]+'\'),setReadAnnouncement(\''+jArray[j]["announcementId"]+'\')"><img src="img/trash.png"></a>';
					boilerNew+= '</div><br></div></div>';
				}
				boilerNew+='</div></section></div>';
			}
			else
			{
				boilerNew+='<h4 style="text-align:center;font-size:14px;padding:5px;">No new Announcements</h4>';
			}
			$("#"+divId).html(boilerNew);
			$('.collapse-card .title').paperCollapse({});
			$('#announcementContainer').slimScroll({
				position: 'right',
				height: '450px',
				width: '100%',
				scrollBy: '10px',
				railVisible: true,
				wheelStep: 6,
				alwaysVisible: true
			});
			//-------------end--------

			/*for(var j=0;j<jArray.length;j++)
				{

				annBody = $('#body1').html(jArray[j]["announcementBody"] ).text();

				if(jArray[j]["status"]==false){
					boilerNew+= '<div style="background-color: #DDDDE5; padding: 5px; font-size: 11px; border: 1px solid #D5D5D5; border-radius: 2px; margin-bottom: 10px;" id="div'+jArray[j]["studentAnnouncmentId"]+'" >'+
					'<div style="" onclick="viewAnnouncementDetailNew(\''+jArray[j]["announcementId"]+'\'), setReadAnnouncement(\''+jArray[j]["announcementId"]+'\')" style="cursor: pointer;">';
					boilerNew+= '&nbsp;&nbsp;&nbsp;<span class="pull-right" id="new'+jArray[j]["announcementId"]+'"><img src="images/Icon_new.png"></span>'+
						'<p style="padding-bottom: 5px; border-bottom: 1px solid #F0F0F0; margin-bottom: 5px;" id="subject'+j+'"><strong>';
						//' '+decodeURIComponent(jArray[j]["heading"])+' '+
						try{
							boilerNew+= decodeURIComponent(jArray[j]["heading"]);
						}catch(e){
							boilerNew+= jArray[j]["heading"];
						}
						boilerNew+='</strong></p>';
				}
				else{
					boilerNew+= '<div style="background-color: #FFF; padding: 5px; font-size: 11px; border: 1px solid #D5D5D5; border-radius: 2px; margin-bottom: 10px;" id="div'+jArray[j]["studentAnnouncmentId"]+'" >'+
					'<div style="" onclick="viewAnnouncementDetailNew(\''+jArray[j]["announcementId"]+'\'), setReadAnnouncement(\''+jArray[j]["announcementId"]+'\')" style="cursor: pointer;">';
					//boilerNew+= '&nbsp;&nbsp;&nbsp;<span class="label label-info" id="new'+jArray[j]["announcementId"]+'">New</span>';
					boilerNew+= '<p style="padding-bottom: 5px; border-bottom: 1px solid #F0F0F0; margin-bottom: 5px;" id="subject'+j+'"><strong>';
					//' '+decodeURIComponent(jArray[j]["heading"])+' '+
					try{
						boilerNew+= decodeURIComponent(jArray[j]["heading"]);
					}catch(e){
						boilerNew+= jArray[j]["heading"];
					}
					boilerNew+=' </strong></p>';

				}
				//boilerNew+= '<p style="color: #A0A0A0; padding-bottom: 5px; border-bottom: 1px solid #F0F0F0; margin-bottom: 5px;">'+annBody+'</p>'+
				boilerNew+= '<p style="color: #555; padding-bottom: 5px; border-bottom: 1px solid #F0F0F0; margin-bottom: 5px;">On '+jArray[j]["createdDate"]+' by '+jArray[j]["employeeName"]+'</p>'+
				'</div>'+
				'<p><a style="color: #1D719F;" onclick="setReadAnnouncement(\''+jArray[j]["announcementId"]+'\')"><span class="icon-envelope"></span> Mark read</a> '+
				'<a style="color: #AF0000;" onclick="removeStudentAnnouncement(\''+jArray[j]["announcementId"]+'\',\'div\',\''+jArray[j]["isPermanent"]+'\'),setReadAnnouncement(\''+jArray[j]["announcementId"]+'\')"><span class="icon-trash"></span> Move to trash</a></p>'+
				'</div>';
			}
			//alert(boiler);
			$("#listAnn").html(boilerNew);*/
		},
		error:function(){
			return;
		}
	});


}


function viewAnnouncementDetailNew(announcementId){
	if(!$('#announcementBody-'+announcementId).hasClass("loaded")){
		setReadAnnouncement(announcementId);
		//alert(!$('#announcementBody-'+announcementId).hasClass("loaded"));
		$('#member'+announcementId).html('').hide();
		if(announId != announcementId)
			aCount=0;

		if(aCount == 0){
			aCount++;
			announId=announcementId;

			viewDetailsOfAnnouncementNew(announcementId,function(announcementArray){
				var isCreator = false;
				//alert('announcementArray: '+announcementArray.length);
				if(announcementArray.length > 0){
					var annoucementObj = announcementArray[0];

					if(annoucementObj.createdByEmployeeId == annoucementObj.viewerEmployeeId){
						isCreator = true;
					}

					var boiler = '';
					getDocumentCategoryByAnnoucementId(announcementId,function(categoryId){
						var uploaddocument = new UploadDocument();
						if(categoryId != 'null' && categoryId != undefined){
							uploaddocument.getDocument(categoryId,function(jArray){
								if(jArray.length > 0){
									showAnnouncementAttachment(jArray,'announcementAttachment-'+announcementId,isCreator);
								} else {
									boiler = '<div class = "alert alert-info">No attachments</div>';
									$('#announcementAttachment-'+announcementId).html(boiler);
								}
							});
						} else {
							boiler = '<div class = "alert alert-info">No attachments</div>';
							$('#announcementAttachment-'+announcementId).html(boiler);
						}
					});
				}

			});

		}
	}
}


function viewDetailsOfAnnouncementNew(announcementId,callback){
	//alert("gfdfg"+announcementId);
	$.ajax({
		url:'getAnnouncementByAnnouncementId.json?announcementId='+announcementId,
		dataType:'json',
		async:false,
		beforeSend: function() {
			/*startCommonAjaxSpinner();
*/
		},
		complete: function() {
			/*stopCommonAjaxSpinner();*/
		},
		success:function(jArray){
			var accordianBoiler='';
			//alert(+jArray[0]["heading"]);
			body='';
			for(var i=0;i<jArray.length;i++)
			{

				body=jArray[i]["announcementBody"];
				accordianBoiler+= '<div style="background-color: #FFF; border: 1px solid #D5D5D5; border-radius: 2px; padding: 20px; height: auto; overflow-y: auto; ">';

					accordianBoiler += '<div style="padding-bottom: 20px; border-bottom: 1px solid #E5E5E5; margin-bottom: 20px;">';
				//''+decodeURIComponent(jArray[i]["announcementBody"])+''+
				try{
					accordianBoiler+= decodeURIComponent(jArray[i]["announcementBody"]);
				}catch(e){
					accordianBoiler+= jArray[i]["announcementBody"];
				}
				accordianBoiler+='</div>';
				if(jArray[i]["hasPoll"]=='true'|| jArray[i]["hasPoll"]==true){
					if(jArray[i]["hasCustomOption"]=="true" || jArray[i]["hasCustomOption"]==true){
						accordianBoiler +='<strong style="font-size:10pt"> Cast your Vote:</strong>'+
						'<div class = "well" align="left"><strong style="font-size:10pt">'+jArray[i]["pollQuestion"]+'</strong>'+
						'<ul style="list-style-type: none;">';
						var customOptionJArray = jArray[i]["customOptionJArray"];											
						for(var j=0;j<customOptionJArray.length;j++)
							accordianBoiler +='<li><input id="rd_'+customOptionJArray[j]["OptionId"]+'_'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="'+customOptionJArray[j]["OptionId"]+'">'+customOptionJArray[j]["OptionName"]+'</li>';

						accordianBoiler += '<li>&nbsp;</li>'+
						'<li><input type="button" class="btn btn-inverse btn-mini" id="btnVote" value="Vote" onClick="voteForAnnouncementPollStudent(\'Custom\','+announcementId+')"/></li>'+
						'</ul>'+
						'<div id="voteAlertMsg'+announcementId+'"></div>'+
						'</div>';
					}else if(jArray[i]["hasCustomOption"]==undefined || jArray[i]["hasCustomOption"]=="false" || jArray[i]["hasCustomOption"]==false){
						accordianBoiler +='<strong style="font-size:10pt"> Cast your Vote:</strong>'+
						'<div class = "well" align="left"><strong style="font-size:10pt">'+jArray[i]["pollQuestion"]+'</strong>'+
						'<ul style="list-style-type: none;">'+
						'<li><input id="rd_yes'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="yes">Yes</li>'+
						'<li><input id="rd_no'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="no">No</li>'+
						'<li><input id="rd_maybe'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="maybe">Maybe</li>'+
						'<li><input id="rd_remindmelater'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="remindmelater">Remind me later</li>'+
						'<li><input id="rd_ignore'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="ignore">Ignore</li>'+
						'<li>&nbsp;</li>'+
						'<li><input type="button" class="btn btn-inverse btn-mini" id="btnVote" value="Vote" onClick="voteForAnnouncementPollStudent(\'Default\','+announcementId+')"/></li>'+
						'</ul>'+
						'<div id="voteAlertMsg'+announcementId+'"></div>'+
						'</div>';
					}
				}
				accordianBoiler +='<p style="padding-bottom: 20px; font-size: 11px; margin-bottom: 10px; border-bottom: 1px solid #E5E5E5;"id="announcementAttachment-'+announcementId+'"><img src="img/loading-small.gif"><a style="color: #1D719F;"></a> </p>'+				
				/*'<p style="padding-bottom: 10px; font-size: 11px;">'+
				'<a style="color: #AF0000;" onclick="removeAnnouncement(\''+jArray[i]["studentAnnouncmentId"]+'\',\'announcementDetails\',\''+jArray[i]["isPermanent"]+'\'),setReadAnnouncement(\''+jArray[i]["announcementId"]+'\')"><span class="icon-trash"></span> Move to trash</a>'+
				'<span style="float: right;">'+
					'<a style="color: #1D719F;" onclick="forwardAnnouncement(\''+jArray[i]["announcementId"]+'\',\''+jArray[i]["heading"]+'\')">Forward <span class="icon-chevron-right"></span></a>'+
				'</span>'+
				'</p>'+*/

				'</div>';
			}
			//alert(boiler);
			$("#announcementBody-"+announcementId).html(accordianBoiler);
			$("#announcementBody-"+announcementId).addClass("loaded");
			
			//$("#announcementDetails").html(accordianBoiler);


			$("input:radio[name=pollAnswer"+jArray[0]["employeeAnnouncementDetailId"]+"][value=" + jArray[0]["PollAnswer"] + "]").attr('checked', 'checked');
			callback(jArray);	
		},
		error:function(){
			return;
		}
	});

	//getUnreadMessageAndAnnouncementForStudent();
}


function removeAnnouncement(employeeAnnouncementDetailId,div,isPermanent) 
{
	if(isPermanent!="PERMANENT")
	{
		$.ajax({
			url:'removeAnnouncement.json?employeeAnnouncementDetailId='+employeeAnnouncementDetailId,
			dataType:'json',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray)
			{
				$('#div'+employeeAnnouncementDetailId).fadeOut(1200);
				$('#announcementDetails').html('');
			},
			error:function(){

			}
		});
	}
	else
		jAlert("The sender has not allow you to delete this announcement..","Warning");
}




function showAnnouncement(isHistory,index){
	aCount=0;
	var boiler = '';
	var flag = true;
	if(isHistory!='search')
	{
		$("#searchBox").val("");
		globalMyArrArray = globalArray;
	}
	else
	{
		var keyword = $("#searchBox").val();//.toUpperCase();
		var myArr = new Array();
		flag = false;
		for(var j=0;j<globalArray.length;j++)
		{
			if(keyword!="")
			{
				if(keyword==globalArray[j]["heading"])
				{
					myArr.push(globalArray[j]);
					flag = true;
				}
			}
		}
		globalMyArrArray = myArr;
		if(keyword=="")
			$("#announcementList").html('<br/><div class="alert alert-error"><button type="button" class="close" data-dismiss="alert">Ã—</button><strong>Sorry !</strong> Please enter some keyword.</div>');
		else if(flag==false)
			$("#announcementList").html('<br/><div class="alert alert-error"><button type="button" class="close" data-dismiss="alert">Ã—</button><strong>Sorry !</strong> No Result found.</div>');
	}
	if(flag==true)
	{
		var j = '';
		if((globalMyArrArray.length-index)<10)
			j = globalMyArrArray.length;
		else
			j = index+10;
		for(var i=index;i<j;i++)
		{
			if(i==globalMyArrArray.length)
			{
				break;
			}


			if(globalArray[i]["readStatus"]==false){
				boiler+=
					'<h3 id="h3'+globalMyArrArray[i]["announcementId"]+'">'+
					//'<a href="#" id ="a'+jArray[i]["documentId"]+'"  onclick="displayDocumentDetail(\''+jArray[i]["documentId"]+'\')">'+
					'<a href="#" id ="a'+globalMyArrArray[i]["announcementId"]+'" onclick="viewAnnouncementDetail(\''+globalMyArrArray[i]["announcementId"]+'\'),setReadAnnouncement(\''+globalMyArrArray[i]["announcementId"]+'\')">';
				try{
					boiler+=''+decodeURIComponent(globalMyArrArray[i]["heading"])+'';
				}
				catch(e){
					boiler+=''+globalMyArrArray[i]["heading"]+'';
				}
				boiler+=' Created on '+globalMyArrArray[i]["createdDate"]+' (Created By:-'+globalMyArrArray[i]["employeeName"]+')&nbsp;&nbsp;&nbsp;<span class="label label-info" id="new_'+globalMyArrArray[i]["announcementId"]+'">New</span>'+
				'</a></h3>'+
				'<div id="div'+globalMyArrArray[i]["announcementId"]+'" align="center"></div>';

			}
			else{
				boiler+=
					'<h3 id="h3'+globalMyArrArray[i]["announcementId"]+'">'+
					//'<a href="#" id ="a'+jArray[i]["documentId"]+'"  onclick="displayDocumentDetail(\''+jArray[i]["documentId"]+'\')">'+
					'<a href="#" id ="a'+globalMyArrArray[i]["announcementId"]+'" onclick="viewAnnouncementDetail(\''+globalMyArrArray[i]["announcementId"]+'\')">';
				//''+decodeURIComponent(globalMyArrArray[i]["heading"])+''+
				try{
					boiler+=''+decodeURIComponent(globalMyArrArray[i]["heading"])+'';
				}
				catch(e){
					boiler+=''+globalMyArrArray[i]["heading"]+'';
				}
				boiler+=' Created on '+globalMyArrArray[i]["createdDate"]+' (Created By:-'+globalMyArrArray[i]["employeeName"]+')'+
				'</a></h3>'+
				'<div id="div'+globalMyArrArray[i]["announcementId"]+'" align="center"></div>';
				/*boiler+='<div id="accordian"class="accordion-group">'+
							'<div class="accordion-heading">'+

							'<a class="accordion-toggle" data-toggle="collapse" data-parent="#viewAnnouncement" href="#collapse'+i+'"onclick="viewAnnouncementDetail(\''+globalArray[i]["announcementId"]+'\')">'+globalArray[i]["heading"]+' Created on '+globalArray[i]["createdDate"]+' (Created By:-'+globalArray[i]["employeeName"]+')'+
							'</a>'+
							'</div>'+
							'<div id="collapse'+i+'" class="accordion-body collapse">'+
								'<div class="accordion-inner">'+
								'<div id="div'+globalArray[i]["announcementId"]+'" class="mini-layout fluid"  style="height:450px"></div>'+	
								'</div>'+
							'</div>'+
						'</div>';*/
			}
			$("#announcementList").html(boiler);
			$('#announcementList').accordion('destroy');
			$('#announcementList').accordion({ header: "h3",
				autoHeight: false, collapsible: true, active:false

			});

			if(globalMyArrArray.length>10)
			{
				var totPages=(globalMyArrArray.length/10)+1;
				if(globalMyArrArray.length%10==0)
					totPages--;
				var pagerBoiler='<div class="pagination"><ul><li id="pagePrev"><a id="prevLink" onclick="showAnnouncement(\''+isHistory+'\', '+(index-10)+')"><<</a></li>';
				for(var j=1;j<=totPages;j++)
					pagerBoiler+='<li id="page'+j+'"><a href="#" onclick="showAnnouncement(\''+isHistory+'\', '+((j*10)-10)+')">'+j+'</a></li>';
				pagerBoiler+='<li id="pageNext"><a id="nextLink" onclick="showAnnouncement(\''+isHistory+'\', '+(index+10)+')">>></a></li></ul></div>';
				$("#paging").html(pagerBoiler);
			}
			if(index==0)
			{
				$('#prevLink').attr('onclick','').unbind('click');
				$('#pagePrev').addClass('disabled');
			}
			if((globalMyArrArray.length-index)<10)
			{
				$('#nextLink').attr('onclick','').unbind('click');
				$('#pageNext').addClass('disabled');
			}
			var temp=(index+10)/10;
			$('#page'+temp).addClass('active');
		}
	}
}
function viewDetailsOfAnnouncement(announcementId,callback){
	var accordianBoiler='';
	$.ajax({
		url:'getAnnouncementByAnnouncementId.json?announcementId='+announcementId,
		dataType:'json',
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){

			//alert(jArray.length);
			$('#div'+announcementId).html('');
			for(var i=0;i<jArray.length;i++){
				//style="margin-left:20%;margin-right:20%;width:60%;"
				accordianBoiler +=	'<div class="well">'+
				'<h3 style="background-color:#1EA6D7">';
				try{
					accordianBoiler +=''+decodeURIComponent(jArray[i]["heading"])+'';
				}
				catch(e){
					accordianBoiler +=''+jArray[i]["heading"]+'';
				}
				accordianBoiler +='</h3><br>'+
				'<span style="float:left;font-size:10pt"><strong>Created By:</strong>'+jArray[i]["employeeName"]+'</span>'+
				'<span style="float:right;font-size:10pt"><strong>Date:</strong>'+jArray[i]["createdDate"]+'</span><br><br>';
				if(jArray[i]["isForwarded"]){
					accordianBoiler+='<span style="float:left;font-size:10pt"><strong>Created By :</strong> '+jArray[i]["ForwardedByEmployeeName"]+'</span>'+
					'<span style="float:right;font-size:10pt"><strong>Date : </strong>'+jArray[i]["forwardedDate"]+'</span><br><br>';
				}
				accordianBoiler+='<div class = "well" align="left"><span style="font-size:10pt">';
				//''+jArray[i]["announcementBody"]+''+
				try{
					accordianBoiler +=''+decodeURIComponent(jArray[i]["announcementBody"])+'';
				}
				catch(e){
					accordianBoiler +=''+jArray[i]["announcementBody"]+'';
				}
				accordianBoiler +='</span></div>';
				if(jArray[i]["hasPoll"]=='true'|| jArray[i]["hasPoll"]==true){
					if(jArray[i]["hasCustomOption"]=="true" || jArray[i]["hasCustomOption"]==true){
						accordianBoiler +='<strong style="font-size:10pt"> Cast your Vote:</strong>'+
						'<div class = "well" align="left"><strong style="font-size:10pt">'+jArray[i]["pollQuestion"]+'</strong>'+
						'<ul style="list-style-type: none;">';
						var customOptionJArray = jArray[i]["customOptionJArray"];											
						for(var j=0;j<customOptionJArray.length;j++)
							accordianBoiler +='<li><input id="rd_'+customOptionJArray[j]["OptionId"]+'_'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="'+customOptionJArray[j]["OptionId"]+'">'+customOptionJArray[j]["OptionName"]+'</li>';

						accordianBoiler += '<li>&nbsp;</li>'+
						'<li><input type="button" class="btn btn-inverse btn-mini" id="btnVote" value="Vote" onClick="voteForAnnouncementPollStudent(\'Custom\','+announcementId+')"/></li>'+
						'</ul>'+
						'<div id="voteAlertMsg'+announcementId+'"></div>'+
						'</div>';
					}else if(jArray[i]["hasCustomOption"]==undefined || jArray[i]["hasCustomOption"]=="false" || jArray[i]["hasCustomOption"]==false){
						accordianBoiler +='<strong style="font-size:10pt"> Cast your Vote:</strong>'+
						'<div class = "well" align="left"><strong style="font-size:10pt">'+jArray[i]["pollQuestion"]+'</strong>'+
						'<ul style="list-style-type: none;">'+
						'<li><input id="rd_yes'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="yes">Yes</li>'+
						'<li><input id="rd_no'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="no">No</li>'+
						'<li><input id="rd_maybe'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="maybe">Maybe</li>'+
						'<li><input id="rd_remindmelater'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="remindmelater">Remind me later</li>'+
						'<li><input id="rd_ignore'+announcementId+'" type="radio" name="pollAnswer'+announcementId+'" value="ignore">Ignore</li>'+
						'<li>&nbsp;</li>'+
						'<li><input type="button" class="btn btn-inverse btn-mini" id="btnVote" value="Vote" onClick="voteForAnnouncementPollStudent(\'Default\','+announcementId+')"/></li>'+
						'</ul>'+
						'<div id="voteAlertMsg'+announcementId+'"></div>'+
						'</div>';
					}
				}
				accordianBoiler +='<strong style="font-size:10pt"> Attachments:</strong>'+
				'<div class = "well" id = "announcementAttachment-'+announcementId+'"></div>'+
				'</div>';


			}
			accordianBoiler +='</table>';
			$('#div'+announcementId).html(accordianBoiler);

			$("input:radio[name=pollAnswer"+announcementId+"][value=" + jArray[0]["PollAnswer"] + "]").attr('checked', 'checked');

			//$('#announcementAttachment').html('');
			callback();

		},
		error:function(){
		}
	});
}

function voteForAnnouncementPollStudent(optionsType,announcementId){
	if(optionsType=="Default"){
		var pollAnswer=	$("input:radio[name=pollAnswer"+announcementId+"]:checked").val();
		$.ajax({
			url:'voteForAnnouncementPollStudent.json?announcementId='+announcementId+'&pollAnswer='+pollAnswer+'&optionsType='+optionsType,
			dataType:'json',
			async:false,
			beforeSend:function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(){
				var boiler = '<div class="alert alert-info">Successfully Saved</div>';

				$('#voteAlertMsg'+announcementId).html(boiler);
				setTimeout("$('#voteAlertMsg"+announcementId+"').html('')",3000);
			},
			error:function(){
				jAlert("Something went wrong, Please try after some time","Error");
			}
		});
	}
	else{
		var pollAnswer=	$("input:radio[name=pollAnswer"+announcementId+"]:checked").val();
		$.ajax({
			url:'voteForAnnouncementPollStudent.json?announcementId='+announcementId+'&pollAnswer='+pollAnswer+'&optionsType='+optionsType,
			dataType:'json',
			async:false,
			beforeSend:function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(){
				var boiler = '<div class="alert alert-info">Successfully Saved</div>';

				$('#voteAlertMsg'+announcementId).html(boiler);
				setTimeout("$('#voteAlertMsg"+announcementId+"').html('')",3000);
			},
			error:function(){
				jAlert("Something went wrong, Please try after some time","Error");
			}
		});	
	}
}

var announId='';
var aCount=0;
function viewAnnouncementDetail(announcementId){
	if(announId != announcementId){
		aCount=0;
	}

	if(aCount == 0){
		aCount++;
		announId=announcementId;
		viewDetailsOfAnnouncement(announcementId,function(){
			var boiler = '';
			getDocumentCategoryByAnnoucementId(announcementId,function(categoryId){
				var uploaddocument = new UploadStudentDocument();
				if(categoryId != 'null' && categoryId != undefined){
					uploaddocument.getDocument(categoryId,function(jArray){
						if(jArray.length > 0){
							showAnnouncementAttachment(jArray,'announcementAttachment-'+announcementId);
						} else {
							boiler = '<div class = "alert alert-info">No attachments</div>';
							$('#announcementAttachment-'+announcementId+'').html(boiler);
						}
					});
				} else {
					boiler = '<div class = "alert alert-info">No attachments</div>';
					$('#announcementAttachment-'+announcementId+'').html(boiler);
				}
			});
		});
	}
}


function getUnreadAnnouncement(){
//	var instituteCourseId = $('#departmentCmb').val().split("-")[0];
	var hdnUserRole = $('#hdnUserRole').val();
		var url = 'getUnreadStudentAnnouncement.json';
		if(hdnUserRole == "student" || hdnUserRole == "Student"){
			url = 'stu_getUnreadStudentAnnouncement.json';
	}
	$.ajax({
		url:url,
		dataType:'json',
		async:false,
		beforeSend: function() {
		
		},
		complete: function() {
			
		},
		success:function(jArray)
		{
			if(jArray.length>0){
				$("#countAnnouncement").html(jArray[0]["count"]);
				$("#announcementCount").html(jArray[0]["count"]);
			}
			else
				$("#countAnnouncement").html('0');
		},
	});
}


function setReadAnnouncement(announcementId){

	if(!$('#announcementBody-'+announcementId).hasClass("loaded")) {

		$('#new'+announcementId).hide();
		if($("#countAnnouncement").text()!=0 || $("#announcementCount").text()!=0)
		{
			$.ajax({
				url:'setReadAnnouncement.json?announcementId='+announcementId,
				dataType:'json',
				async:false,
				beforeSend: function() {
					startCommonAjaxSpinner();
				},
				complete: function() {
					stopCommonAjaxSpinner();
				},
				success:function(jArray){
					$('#new_'+announcementId).remove();
					getUnreadAnnouncement();
					
					
				},
			});
		}
	}
}


function removeStudentAnnouncement(studentAnnouncmentId,div,isPermanent) 
{
	if(isPermanent!="PERMANENT")
	{
		$.ajax({
			url:'removeStudentAnnouncement.json?studentAnnouncmentId='+studentAnnouncmentId,
			dataType:'json',
			async:false,
			beforeSend: function() {
				startCommonAjaxSpinner();
			},
			complete: function() {
				stopCommonAjaxSpinner();
			},
			success:function(jArray){

				$('#div'+studentAnnouncmentId).fadeOut(1200);
				$('#announcementDetails').html('');

			},
			error:function(){

			}
		});
	}
	else
		jAlert("The sender has not allowed you to delete this announcement..","Warning");
}


function getStudentMessage(){

	$('#message').val('');
	$.ajax({
		url:'getStudentMessage.json',
		dataType:'json',
		async:false,

		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
			var subcnt=1;
			var j=jArray.length;
			var boiler='';
			boiler+='<table id="dMtbl1" cellpadding="0" cellspacing="0" border="1" class="table table-striped table-bordered table-condensed" style="width: 100%"><thead>'+
			'<tr><th>Sr No.</th><th>Date</th><th>Time</th><th>Message</th>'+
			'<th>Sent By</th><th>Reply</th><th style="display:none">employeeMessageId</th></tr></thead><tbody>';

			for( var i=0; i<j; i++)
			{
				var studentMessageId = jArray[i]["studentMessageId"];
				if(jArray[i]["status"]==false){
					boiler+='<tr id="'+studentMessageId+'" style="font-weight:bold">';
				}
				else{
					boiler+='<tr id="'+studentMessageId+'">';
				}

				boiler+='<td>'+subcnt+'</td>'+
				'<td>'+jArray[i]["date"]+'</td>'+
				'<td>'+jArray[i]["time"].split(".")[0]+'</td>'+
				'<td>'+jArray[i]["message"]+'</td>'+
				//'<td align="center"><input type="button" value="Message" class="viewMessage" onclick="showMessage(\''+jArray[i]["message"]+'\')"></td>'+
				'<td>'+jArray[i]["sentBy"]+'</td>'+
				'<td style="text-align:center">'+
				'<input type="button" value="Reply" class="btn btn-inverse btn-mini  " onclick="sendEmployeeMessage(\''+jArray[i]["employeeId"]+'\' ,\''+jArray[i]["sentBy"]+'\')"></td>'+
				'<td style="display:none">'+studentMessageId+'</td></tr>';
				subcnt++;
			}
			boiler+='</tbody></table><br /><div id="divMeetingForCoordinator"></div>';
			$("#message").html(boiler);


			$('#dMtbl1').dataTable( {
				"sDom": "<'row'<'margin-left-22'l><'pull-right'f>r>t<'row'<'margin-left-22'i><'pull-right'p>>",
				"sPaginationType": "bootstrap",
				"oLanguage": {
					"sLengthMenu": "_MENU_ records per page"
				}
			} );

		},
	});
	if($("#countMessage").text()!=0){
		setReadStudentMessage();
	}
}

function sendEmployeeMessage(employeeId,empName)
{
	$('#studName').html('');
	$('#replayDiv').html('');
	$('#replayToStudentMessageModal').modal('show');
	$('#studName').html("Reply To : "+empName);

	var boiler ='<table class="table table-bordered"><thead>'+
	'<tr><th>Reply Message</th></tr></thead>'+
	'<tr><td><textarea id="replyStudent" style="width:650px"></textarea></td></tr>'+
	'<tr><td><div align="center">'+
	'<input type="button" class="btn btn-inverse btn-mini" value="Send Reply" onclick="sendReplayToEmployee(\''+employeeId+'\')"></div></td></tr>'+
	'</table>'+
	'</div>';
	$('#replayDiv').html(boiler);
}

function sendReplayToEmployee(employeeId)
{
	var description = $('#replyStudent').val();
	$.ajax({      
		url:'sendReplayToEmployee.json',
		dataType:'text',
		type:'POST',
		data:{employeeId:employeeId,description:description.toString()},
		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(type)
		{	
			$('#replayToStudentMessageModal').modal('hide');
			jAlert("Successfully Send Message","Success");
		},
		error:function(){
			alert("Error");
		}
	});

}


