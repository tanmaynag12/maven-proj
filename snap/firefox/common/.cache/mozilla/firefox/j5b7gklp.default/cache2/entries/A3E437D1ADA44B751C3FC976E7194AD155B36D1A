		async:false,
		beforeSend: function() {
			startCommonAjaxSpinner();

		},
		complete: function() {
			stopCommonAjaxSpinner();
		},
		success:function(jArray){
				$("#countMessage").html(jArray[0]["studMessagecount"]);
		},
	});
